{
  CmsFolder folder=null;
  ResultSet res=null;
  PreparedStatement stmt=null;
  Connection conn=null;
  try {
    conn=m_sqlManager.getConnection(projectId);
    stmt=m_sqlManager.getPreparedStatement(conn,projectId,"C_RESOURCES_READBYID");
    stmt.setString(1,folderId.toString());
    res=stmt.executeQuery();
    if (res.next()) {
      folder=createCmsFolderFromResultSet(res,projectId,true);
      while (res.next()) {
      }
    }
 else {
      throw new CmsException("[" + this.getClass().getName() + "] "+ folderId,CmsException.C_NOT_FOUND);
    }
  }
 catch (  SQLException e) {
    String message="method: " + this.getClass().getName() + ".readFolder,";
    message+="message: " + e.getMessage() + ",";
    message+="query key: C_RESOURCES_READBYID";
    throw m_sqlManager.getCmsException(this,message,CmsException.C_SQL_ERROR,e,false);
  }
catch (  CmsException exc) {
    throw exc;
  }
catch (  Exception exc) {
    throw m_sqlManager.getCmsException(this,null,CmsException.C_UNKNOWN_EXCEPTION,exc,false);
  }
 finally {
    m_sqlManager.closeAll(conn,stmt,res);
  }
  return folder;
}
