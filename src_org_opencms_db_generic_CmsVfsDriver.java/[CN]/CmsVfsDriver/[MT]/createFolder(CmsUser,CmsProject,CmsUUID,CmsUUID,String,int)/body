{
  CmsFolder newFolder=new CmsFolder(new CmsUUID(),new CmsUUID(),parentId,CmsUUID.getNullUUID(),folderName,CmsResourceTypeFolder.C_RESOURCE_TYPE_ID,flags,project.getId(),com.opencms.core.I_CmsConstants.C_STATE_NEW,0,user.getId(),0,user.getId(),1);
  return createFolder(user,project,newFolder,parentId,folderName);
}
