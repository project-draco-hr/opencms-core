{
  CmsFolder newFolder=new CmsFolder(new CmsUUID(),new CmsUUID(),parentId,CmsUUID.getNullUUID(),folderName,CmsResourceTypeFolder.C_RESOURCE_TYPE_ID,flags,user.getId(),user.getDefaultGroupId(),project.getId(),com.opencms.core.I_CmsConstants.C_ACCESS_DEFAULT_FLAGS,com.opencms.core.I_CmsConstants.C_STATE_NEW,CmsUUID.getNullUUID(),0,0,user.getId(),project.getId());
  return createFolder(user,project,newFolder,parentId,folderName);
}
