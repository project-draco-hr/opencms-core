{
  Integer counterObj=internalReadResourceTypeCounter(dbc,resourceType);
  int result;
  if (counterObj == null) {
    internalCreateResourceTypeCounter(dbc,resourceType);
    result=0;
  }
 else {
    result=counterObj.intValue();
    internalUpdateResourceTypeCounter(dbc,resourceType);
  }
  return result;
}
