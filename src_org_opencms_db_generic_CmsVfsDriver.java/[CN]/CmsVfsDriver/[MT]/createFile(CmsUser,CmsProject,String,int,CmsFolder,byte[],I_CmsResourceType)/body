{
  CmsFile newFile=new CmsFile(new CmsUUID(),new CmsUUID(),parentFolder.getId(),new CmsUUID(),filename,resourceType.getResourceType(),flags,project.getId(),com.opencms.core.I_CmsConstants.C_STATE_NEW,resourceType.getLoaderId(),0,user.getId(),0,user.getId(),contents.length,1,contents);
  newFile.setFullResourceName(parentFolder.getFullResourceName() + newFile.getResourceName());
  return createFile(project,newFile,user.getId(),parentFolder.getId(),filename);
}
