{
  Iterator it=m_moduleInstances.keySet().iterator();
  while (it.hasNext()) {
    I_CmsModuleAction moduleAction=(I_CmsModuleAction)m_moduleInstances.get(it.next());
    if (OpenCms.getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_INIT).info(". Module configuration : shutting down module class " + moduleAction.getClass().getName());
    }
    try {
      moduleAction.shutDown();
    }
 catch (    Throwable t) {
      OpenCms.getLog(this).error("Error during module action instance shutDown for class '" + moduleAction.getClass().getName() + "'",t);
    }
  }
  if (OpenCms.getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
    OpenCms.getLog(CmsLog.CHANNEL_INIT).info(". Module configuration : " + m_moduleInstances.size() + " module classes have been shut down");
  }
}
