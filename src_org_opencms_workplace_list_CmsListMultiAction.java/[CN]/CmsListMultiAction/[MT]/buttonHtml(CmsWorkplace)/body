{
  if (isEnabled()) {
    String onClic=getListId() + "ListMultiAction('" + getId()+ "', '"+ CmsStringUtil.escapeJavaScript(wp.resolveMacros(getConfirmationMessage().key(wp.getLocale())))+ "');";
    return A_CmsHtmlIconButton.defaultButtonHtml(getId(),getName().key(wp.getLocale()),getHelpText().key(wp.getLocale()),isEnabled(),getIconPath(),onClic);
  }
  return "";
}
