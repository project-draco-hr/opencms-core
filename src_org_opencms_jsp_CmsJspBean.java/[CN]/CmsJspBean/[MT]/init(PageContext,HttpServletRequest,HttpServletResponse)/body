{
  m_controller=(CmsFlexController)req.getAttribute(CmsFlexController.ATTRIBUTE_NAME);
  if (m_controller == null) {
    throw new CmsRuntimeException(Messages.get().container("ERR_MISSING_CMS_CONTROLLER",CmsJspBean.class.getName()));
  }
  m_context=context;
  m_request=req;
  m_response=res;
  m_isNotInitialized=false;
}
