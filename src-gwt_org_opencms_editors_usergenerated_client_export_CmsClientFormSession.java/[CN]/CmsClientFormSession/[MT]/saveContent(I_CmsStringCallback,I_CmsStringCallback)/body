{
  m_apiRoot.getRpcHelper().executeRpc(CmsXmlContentFormApi.SERVICE.saveContent(m_content.getSessionId(),m_newValues,new AsyncCallback<Map<String,String>>(){
    public void onFailure(    Throwable caught){
      onFailure.call("An error occurred: " + caught);
    }
    public void onSuccess(    Map<String,String> result){
      if ((result == null) || result.isEmpty()) {
        onSuccess.call("");
      }
 else {
        onFailure.call("");
      }
    }
  }
));
}
