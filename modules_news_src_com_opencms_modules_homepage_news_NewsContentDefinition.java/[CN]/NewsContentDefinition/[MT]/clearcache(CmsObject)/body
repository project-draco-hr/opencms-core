{
  cms.getRequestContext().getElementCache().clearCache();
  URL u=null;
  String p=((HttpServletRequest)cms.getRequestContext().getRequest().getOriginalRequest()).getServletPath();
  String s=((HttpServletRequest)cms.getRequestContext().getRequest().getOriginalRequest()).getServerName();
  try {
    u=new URL("http://" + s + p+ "/system/workplace/action/login.html?_clearcache=all");
    URLConnection uc=u.openConnection();
    uc.connect();
    InputStream in=uc.getInputStream();
  }
 catch (  MalformedURLException e) {
    System.err.println("Exception1: " + e.getMessage());
  }
catch (  IOException e) {
    System.err.println("Exception2: " + e.getMessage());
  }
}
