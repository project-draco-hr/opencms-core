 * @see junit.extensions.TestSetup#setUp()
 */
    @Override protected void setUp(){
      setupOpenCms("simpletest","/sites/default/");
    }
    /** 
 * @see junit.extensions.TestSetup#tearDown()
 */
    @Override protected void tearDown(){
    }
  }
;
  return wrapper;
}
