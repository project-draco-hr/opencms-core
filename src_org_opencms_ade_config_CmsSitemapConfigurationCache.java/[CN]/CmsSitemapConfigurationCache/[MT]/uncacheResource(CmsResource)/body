{
  super.uncacheResource(resource);
  if (OpenCms.getResourceManager().getResourceType(resource).getTypeName().equals(CmsADEConfigurationManager.TYPE_SITEMAP_CONFIG) && !CmsResource.isTemporaryFileName(resource.getRootPath())) {
synchronized (OpenCms.getADEConfigurationManager()) {
      m_combinedConfigurations.get(Boolean.FALSE).clear();
    }
  }
}
