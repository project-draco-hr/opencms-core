{
  String siteRoot=getAdjustedSiteRoot(getSiteRoot(),rootPath);
  if ((siteRoot == getSiteRoot()) && rootPath.startsWith(siteRoot) && ((rootPath.length() == siteRoot.length()) || (rootPath.charAt(siteRoot.length()) == '/'))) {
    rootPath=rootPath.substring(siteRoot.length());
  }
  return rootPath;
}
