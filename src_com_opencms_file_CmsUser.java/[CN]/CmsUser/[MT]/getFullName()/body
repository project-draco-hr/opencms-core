{
  StringBuffer buf=new StringBuffer();
  String first=getFirstname();
  if ((null != first) && !"".equals(first.trim())) {
    buf.append(first);
    buf.append(" ");
  }
  String last=getLastname();
  if ((null != last) && !"".equals(last.trim())) {
    buf.append(getLastname());
    buf.append(" ");
  }
  buf.append("(");
  buf.append(getName());
  buf.append(")");
  return buf.toString();
}
