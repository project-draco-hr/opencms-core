{
  StringBuffer output=new StringBuffer();
  output.append("[User]:");
  output.append(m_name);
  output.append(" , Id=");
  output.append(m_id);
  output.append(" , flags=");
  output.append(getFlags());
  output.append(" , type=");
  output.append(getType());
  output.append(" :");
  output.append(m_description);
  return output.toString();
}
