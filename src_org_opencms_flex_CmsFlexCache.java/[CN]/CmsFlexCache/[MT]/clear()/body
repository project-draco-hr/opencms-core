{
  if (!isEnabled())   return;
  m_resourceMap.clear();
  m_resourceMap=java.util.Collections.synchronizedMap(new CmsLruHashMap(C_INITIAL_CAPACITY_CACHE));
  m_size=0;
  this.m_entryLruCache.clear();
  this.m_variationCache.clear();
  if (OpenCms.getLog(CmsLog.CHANNEL_FLEX).isInfoEnabled())   OpenCms.getLog(CmsLog.CHANNEL_FLEX).info("Complete FlexCache cleared - clear() called");
}
