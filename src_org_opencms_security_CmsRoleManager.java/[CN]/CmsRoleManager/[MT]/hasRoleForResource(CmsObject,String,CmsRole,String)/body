{
  CmsResource resource;
  try {
    resource=cms.readResource(resourceName);
  }
 catch (  CmsException e) {
    return false;
  }
  CmsUser user;
  try {
    user=cms.readUser(userName);
  }
 catch (  CmsException e) {
    return false;
  }
  return m_securityManager.hasRoleForResource(cms.getRequestContext(),user,role,resource);
}
