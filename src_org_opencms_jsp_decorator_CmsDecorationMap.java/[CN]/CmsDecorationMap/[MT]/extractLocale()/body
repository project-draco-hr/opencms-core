{
  Locale loc=null;
  int underscore=m_name.lastIndexOf("_");
  if (underscore > -1) {
    String localeName=m_name.substring(underscore + 1);
    if (localeName.lastIndexOf(".") > -1) {
      localeName=localeName.substring(0,localeName.lastIndexOf("."));
    }
    loc=new Locale(localeName);
  }
  return loc;
}
