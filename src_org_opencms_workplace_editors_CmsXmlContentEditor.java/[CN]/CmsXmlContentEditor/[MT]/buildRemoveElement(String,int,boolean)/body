{
  if (enabled) {
    StringBuffer href=new StringBuffer(4);
    href.append("javascript:removeElement('");
    href.append(elementName);
    href.append("', ");
    href.append(index);
    href.append(");");
    return button(href.toString(),null,"deletecontent.png","button.delete",0);
  }
 else {
    return "<td style=\"width: 22px;\">&nbsp;</td>";
  }
}
