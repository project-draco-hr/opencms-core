{
  Element nodeElement=parent.element(nodeName);
  if (value != null) {
    if (nodeElement == null) {
      nodeElement=parent.addElement(nodeName);
    }
    if (nodeElement != null) {
      nodeElement.clearContent();
      if (cdata) {
        nodeElement.addCDATA(value);
      }
 else {
        nodeElement.addText(value);
      }
    }
  }
 else {
    if (nodeElement != null) {
      parent.remove(nodeElement);
    }
  }
}
