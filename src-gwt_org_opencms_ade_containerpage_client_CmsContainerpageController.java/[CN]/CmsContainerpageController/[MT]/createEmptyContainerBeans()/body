{
  List<CmsContainer> result=new ArrayList<CmsContainer>();
  for (  CmsContainerJso containerJso : m_containers.values()) {
    CmsContainer container=new CmsContainer(containerJso.getName(),containerJso.getType(),containerJso.getWidth(),containerJso.getMaxElements(),null,null);
    result.add(container);
  }
  return result;
}
