{
  CmsRpcAction<CmsGalleryDataBean> dataAction=new CmsRpcAction<CmsGalleryDataBean>(){
    @Override public void execute(){
      getContainerpageService().getGalleryDataForPage(getEditableContainers(),getElementView(),CmsCoreProvider.get().getUri(),getData().getLocale(),this);
    }
    @Override protected void onResponse(    CmsGalleryDataBean result){
      m_handler.m_editor.getAdd().updateGalleryData(result);
      if (nextAction != null) {
        nextAction.run();
      }
    }
  }
;
  dataAction.execute();
}
