{
  if (m_lockStatus == LockStatus.locked) {
    return true;
  }
  if (m_lockStatus == LockStatus.failed) {
    return false;
  }
  String lockError=CmsCoreProvider.get().lockOrReturnError(CmsCoreProvider.get().getStructureId());
  if (lockError == null) {
    onLockSuccess();
    return true;
  }
 else {
    onLockFail(lockError);
    return false;
  }
}
