{
  if (hasPageChanged()) {
    CmsRpcAction<Void> action=new CmsRpcAction<Void>(){
      /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#execute()
 */
      @Override public void execute(){
        setLoadingMessage(org.opencms.gwt.client.Messages.get().key(org.opencms.gwt.client.Messages.GUI_SAVING_0));
        start(500,true);
        getContainerpageService().saveContainerpage(CmsCoreProvider.get().getStructureId(),getPageContent(),getLocale(),this);
      }
      /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#onResponse(java.lang.Object)
 */
      @Override protected void onResponse(      Void result){
        stop(false);
        setPageChanged(false,false);
      }
    }
;
    action.execute();
  }
}
