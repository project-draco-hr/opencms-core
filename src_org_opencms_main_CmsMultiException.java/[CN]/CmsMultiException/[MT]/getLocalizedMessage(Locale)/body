{
  if (m_exceptions.isEmpty()) {
    return null;
  }
  StringBuffer result=new StringBuffer(128);
  Iterator<CmsException> it=m_exceptions.iterator();
  while (it.hasNext()) {
    CmsException ex=it.next();
    result.append(ex.getLocalizedMessage(locale));
    if (it.hasNext()) {
      result.append('\n');
    }
  }
  return result.toString();
}
