{
  if (m_exceptions.isEmpty()) {
    return null;
  }
  StringBuffer result=new StringBuffer(128);
  Iterator it=m_exceptions.iterator();
  while (it.hasNext()) {
    CmsException ex=(CmsException)it.next();
    result.append(ex.getLocalizedMessage());
    if (it.hasNext()) {
      result.append('\n');
    }
  }
  return result.toString();
}
