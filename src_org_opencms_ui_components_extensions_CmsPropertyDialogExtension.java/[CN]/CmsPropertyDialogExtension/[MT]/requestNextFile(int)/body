{
  int newPos=m_position;
  int count=0;
  do {
    newPos=nextIndex(newPos,offset);
    count+=1;
    if (count > m_ids.size()) {
      newPos=m_position;
      break;
    }
  }
 while (!canEdit(m_ids.get(newPos)));
  m_position=newPos;
  getRpcProxy(I_CmsPropertyClientRpc.class).sendNextId("" + m_ids.get(m_position));
}
