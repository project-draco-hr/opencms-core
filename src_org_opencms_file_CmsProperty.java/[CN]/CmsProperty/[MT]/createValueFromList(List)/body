{
  if (valueList == null) {
    return null;
  }
  StringBuffer result=new StringBuffer(valueList.size() * 32);
  Iterator i=valueList.iterator();
  while (i.hasNext()) {
    result.append(i.next().toString());
    if (i.hasNext()) {
      result.append(VALUE_LIST_DELIMITER);
    }
  }
  return result.toString();
}
