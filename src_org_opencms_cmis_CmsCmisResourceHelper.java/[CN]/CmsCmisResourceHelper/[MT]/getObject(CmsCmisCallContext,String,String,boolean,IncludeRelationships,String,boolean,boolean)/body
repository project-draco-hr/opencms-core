{
  try {
    if (objectId == null) {
      throw new CmisInvalidArgumentException("Object Id must be set.");
    }
    CmsObject cms=m_repository.getCmsObject(context);
    CmsResource file=cms.readResource(new CmsUUID(objectId));
    Set<String> filterCollection=splitFilter(filter);
    return collectObjectData(context,cms,file,filterCollection,includeAllowableActions,includeAcl,includeRelationships);
  }
 catch (  CmsException e) {
    handleCmsException(e);
    return null;
  }
}
