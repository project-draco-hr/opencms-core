{
  if (!m_fieldconfiguration.getFields().isEmpty()) {
    Iterator itFields=m_fieldconfiguration.getFields().iterator();
    while (itFields.hasNext()) {
      CmsSearchField field=(CmsSearchField)itFields.next();
      if (field.getMappings().isEmpty()) {
        return false;
      }
    }
    return true;
  }
  return false;
}
