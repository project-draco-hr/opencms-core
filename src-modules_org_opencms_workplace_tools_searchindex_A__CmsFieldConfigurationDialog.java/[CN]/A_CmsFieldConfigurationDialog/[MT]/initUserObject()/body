{
  if (m_fieldconfiguration == null) {
    try {
      m_fieldconfiguration=m_searchManager.getFieldConfiguration(getParamFieldconfiguration());
      if (m_fieldconfiguration == null) {
        m_fieldconfiguration=new CmsLuceneSearchFieldConfiguration();
      }
    }
 catch (    Exception e) {
      m_fieldconfiguration=new CmsLuceneSearchFieldConfiguration();
    }
  }
}
