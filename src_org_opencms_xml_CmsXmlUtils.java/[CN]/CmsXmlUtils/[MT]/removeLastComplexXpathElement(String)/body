{
  int pos=path.lastIndexOf('/');
  if (pos < 0) {
    return path;
  }
  int p=pos;
  int count=-1;
  while (p > 0) {
    count++;
    p=path.indexOf("\'",p + 1);
  }
  String parentPath=path.substring(0,pos);
  if (count % 2 == 0) {
    return parentPath;
  }
  p=parentPath.lastIndexOf("'");
  if (p >= 0) {
    return removeLastComplexXpathElement(parentPath.substring(0,p));
  }
  return parentPath;
}
