{
  super(hoverbar);
  setImageClass(I_CmsImageBundle.INSTANCE.contextMenuIcons().gotoParent());
  setLabel(Messages.get().key(Messages.GUI_HOVERBAR_PARENT_0));
  setActive(true);
  setCommand(new Command(){
    /** 
 * @see com.google.gwt.user.client.Command#execute()
 */
    public void execute(){
      String sitemapLocation=CmsCoreProvider.get().getUri() + "?path=" + getHoverbar().getController().getData().getParentSitemap();
      getHoverbar().getController().leaveEditor(sitemapLocation);
    }
  }
);
}
