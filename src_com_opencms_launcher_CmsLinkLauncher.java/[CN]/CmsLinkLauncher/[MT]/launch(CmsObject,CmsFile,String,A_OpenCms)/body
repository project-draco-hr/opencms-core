{
  String link=new String(file.getContents());
  if (link.startsWith("/")) {
    CmsFile linkFile=cms.readFile(link);
    openCms.showResource(cms,linkFile);
  }
 else {
    HttpServletResponse response=(HttpServletResponse)cms.getRequestContext().getResponse().getOriginalResponse();
    try {
      response.sendRedirect(link);
    }
 catch (    IOException e) {
    }
  }
}
