{
  List resources=driverManager.readChildResources(cms.getRequestContext(),resource,CmsResourceFilter.ALL,true,true);
  super.changeLastModifiedProjectId(cms,driverManager,resource);
  for (int i=0; i < resources.size(); i++) {
    CmsResource childResource=(CmsResource)resources.get(i);
    if (childResource.isFolder()) {
      changeLastModifiedProjectId(cms,driverManager,childResource);
    }
 else {
      getResourceType(childResource.getTypeId()).changeLastModifiedProjectId(cms,driverManager,childResource);
    }
  }
}
