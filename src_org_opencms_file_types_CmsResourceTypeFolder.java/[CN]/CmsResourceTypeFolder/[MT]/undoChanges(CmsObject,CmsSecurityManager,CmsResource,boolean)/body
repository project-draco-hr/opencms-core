{
  super.undoChanges(cms,securityManager,resource,recursive);
  if (recursive) {
    List resources=securityManager.readChildResources(cms.getRequestContext(),resource,CmsResourceFilter.ALL,true,true);
    for (int i=0; i < resources.size(); i++) {
      CmsResource childResource=(CmsResource)resources.get(i);
      if (childResource.isFolder()) {
        undoChanges(cms,securityManager,childResource,recursive);
      }
 else {
        getResourceType(childResource.getTypeId()).undoChanges(cms,securityManager,childResource,recursive);
      }
    }
  }
}
