{
  CmsUser user;
  CmsGroup group;
  user=m_accessUser.readUser(username);
  if (user != null) {
    group=m_accessGroup.readGroup(groupname);
    if (group != null) {
      m_accessGroup.addUserToGroup(user.getId(),group.getId());
    }
 else {
      throw new CmsException("[" + this.getClass().getName() + "]"+ groupname,CmsException.C_NO_GROUP);
    }
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "]"+ username,CmsException.C_NO_USER);
  }
}
