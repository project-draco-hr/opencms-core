{
  Vector groups=new Vector();
  A_CmsUser user=null;
  user=m_accessUser.readUser(username);
  if (user != null) {
    groups=m_accessGroup.getGroupsOfUser(user.getId());
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "]"+ username,CmsException.C_NO_USER);
  }
  return groups;
}
