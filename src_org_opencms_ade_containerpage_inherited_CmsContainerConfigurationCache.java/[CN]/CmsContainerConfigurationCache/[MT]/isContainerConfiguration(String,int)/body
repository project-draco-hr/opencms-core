{
  try {
    int expectedId=OpenCms.getResourceManager().getResourceType(CmsResourceTypeXmlContainerPage.INHERIT_CONTAINER_CONFIG_TYPE_NAME).getTypeId();
    return (type == expectedId) && !CmsResource.isTemporaryFileName(rootPath) && rootPath.endsWith("/" + INHERITANCE_CONFIG_FILE_NAME);
  }
 catch (  CmsLoaderException e) {
    return false;
  }
}
