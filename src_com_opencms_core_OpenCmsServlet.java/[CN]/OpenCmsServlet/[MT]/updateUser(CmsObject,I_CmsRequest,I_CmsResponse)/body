{
  HttpSession session=null;
  HttpServletRequest req=(HttpServletRequest)cmsReq.getOriginalRequest();
  HttpServletResponse res=(HttpServletResponse)cmsRes.getOriginalResponse();
  session=req.getSession(false);
  if (session != null) {
    m_sessionStorage.putUser(session.getId(),cms.getRequestContext().currentUser().getName(),cms.getRequestContext().currentGroup().getName(),cms.getRequestContext().currentProject().getName());
  }
}
