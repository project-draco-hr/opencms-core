{
switch (getAction()) {
case ACTION_CANCEL:
    actionCancel();
  actionCloseDialog();
break;
case ACTION_ERROR:
break;
case ACTION_SAVE:
setParamAction(DIALOG_OK);
actionCommit();
if (closeDialogOnCommit()) {
actionCloseDialog();
break;
}
case ACTION_DEFAULT:
default :
setParamAction(DIALOG_SAVE);
JspWriter out=getJsp().getJspContext().getOut();
out.print(defaultActionHtml());
}
}
