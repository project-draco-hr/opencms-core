{
  if (enabled) {
    StringBuffer href=new StringBuffer(4);
    href.append("javascript:removeElement('");
    href.append(elementName);
    href.append("', ");
    href.append(index);
    href.append(");");
    return button(href.toString(),null,"deletecontent",Messages.GUI_DIALOG_BUTTON_DELETE_0,0);
  }
 else {
    return "";
  }
}
