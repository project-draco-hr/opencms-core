{
  if (enabled) {
    StringBuffer href=new StringBuffer(4);
    href.append("javascript:addElement('");
    href.append(elementName);
    href.append("', ");
    href.append(insertAfter);
    href.append(");");
    return button(href.toString(),null,"new.png",Messages.GUI_DIALOG_BUTTON_ADDNEW_0,0);
  }
 else {
    return "";
  }
}
