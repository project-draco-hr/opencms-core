{
  m_locale=contentDefinition.getLocale();
  m_entityId=contentDefinition.getEntityId();
  initClosingHandler();
  setContentDefinition(contentDefinition);
  initToolbar();
  if (inline && (formParnet != null)) {
    m_editor.renderInlineEntity(m_entityId,formParnet);
  }
 else {
    initFormPanel();
    renderFormContent();
  }
}
