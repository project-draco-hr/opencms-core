{
  Method meth[]=getClass().getMethods();
  for (int z=0; z < meth.length; z++) {
    if ((meth[z].getDeclaringClass() == getClass()) && (meth[z].getModifiers() == Modifier.PUBLIC) && (meth[z].getName().toLowerCase().indexOf(searchString.toLowerCase()) > -1)) {
      printMethod(meth[z]);
    }
  }
}
