{
  CmsShell shell=new CmsShell();
  try {
    int nargs=CmsConstants.USE_MULTISITE ? 2 : 1;
    if (args.length != nargs) {
      shell.usage();
    }
 else {
      shell.init(args);
      shell.version();
      shell.copyright();
      shell.commands();
    }
  }
 catch (  Exception exc) {
    exc.printStackTrace();
  }
}
