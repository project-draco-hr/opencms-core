{
  CmsShell shell;
  try {
    int nargs=((CmsConstants.USE_MULTISITE) ? 2 : 1);
    if (args.length < nargs) {
      CmsShell.usage();
    }
 else {
      shell=new CmsShell(args);
      shell.commands();
    }
  }
 catch (  ArrayIndexOutOfBoundsException e) {
    int nargs=((CmsConstants.USE_MULTISITE) ? 2 : 1);
    if (args.length < nargs)     CmsShell.usage();
 else     e.printStackTrace();
  }
catch (  Exception exc) {
    exc.printStackTrace();
  }
}
