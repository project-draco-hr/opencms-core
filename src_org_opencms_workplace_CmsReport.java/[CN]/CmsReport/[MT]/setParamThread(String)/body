{
  m_paramThread=CmsUUID.getNullUUID();
  if (value != null) {
    try {
      m_paramThread=new CmsUUID(value);
    }
 catch (    Exception e) {
      if (OpenCms.getLog(this).isInfoEnabled()) {
        OpenCms.getLog(this).info("Unable to create a report Thread with ID " + value,e);
      }
    }
  }
}
