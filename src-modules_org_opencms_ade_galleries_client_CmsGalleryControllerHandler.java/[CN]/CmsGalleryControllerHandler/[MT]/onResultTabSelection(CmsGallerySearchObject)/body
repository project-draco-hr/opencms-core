{
  m_galleryDialog.getResultsTab().updateContent(searchObj,m_galleryDialog.getTypesTab().getTypesParamsPanel(searchObj.getTypes()),m_galleryDialog.getGalleriesTab().getGallerisParamsPanel(searchObj.getGalleries()),m_galleryDialog.getCategoriesTab().getCategoriesParamsPanel(searchObj.getCategories()));
}
