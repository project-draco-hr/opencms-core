{
  m_galleryDialog.getGalleriesTab().fillContent(dialogBean);
  m_galleryDialog.getTypesTab().fillContent(dialogBean);
  m_galleryDialog.getCategoriesTab().fillContent(dialogBean);
  m_galleryDialog.getResultsTab().fillContent(searchObj,m_galleryDialog.getTypesTab().getTypesParamsPanel(searchObj.getTypes()),m_galleryDialog.getGalleriesTab().getGallerisParamsPanel(searchObj.getGalleries()),m_galleryDialog.getCategoriesTab().getCategoriesParamsPanel(searchObj.getCategories()));
  m_galleryDialog.selectTab(tabIdIndex,isOpenInResults);
}
