{
  String key=name.trim().toLowerCase();
switch (SCALER_ATTRS_LIST.indexOf(key)) {
case 0:
    setScaleColor(value);
  break;
case 1:
setScaleFilter(value);
break;
case 2:
setHeight(value);
break;
case 3:
setPartialTag(value);
break;
case 4:
setScalePosition(value);
break;
case 5:
setScaleQuality(value);
break;
case 6:
setScaleRendermode(value);
break;
case 7:
setSrc(value);
break;
case 8:
setScaleType(value);
break;
case 9:
setWidth(value);
break;
case 10:
setMaxHeight(value);
break;
case 11:
setMaxWidth(value);
break;
case 12:
setNoDim(value);
break;
default :
setAttribute(key,value);
}
}
