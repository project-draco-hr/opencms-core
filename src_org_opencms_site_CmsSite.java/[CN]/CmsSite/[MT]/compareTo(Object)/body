{
  if (that == this) {
    return 0;
  }
  if (that instanceof CmsSite) {
    float thatPos=((CmsSite)that).getPosition();
    if (m_position == thatPos) {
      if (m_position == Float.MAX_VALUE) {
        return m_title.compareTo(((CmsSite)that).getTitle());
      }
      return 0;
    }
    return (m_position < thatPos) ? -1 : 1;
  }
  return 0;
}
