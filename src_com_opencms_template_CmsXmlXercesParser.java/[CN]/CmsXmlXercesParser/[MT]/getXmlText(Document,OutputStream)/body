{
  OutputFormat of=new OutputFormat(doc,OutputFormat.DEFAULT_ENCODING,true);
  try {
    Printer printer=Printer.makePrinter(out,of);
    printer.print(doc);
  }
 catch (  Exception e) {
    if (A_OpenCms.isLogging()) {
      A_OpenCms.log(C_OPENCMS_CRITICAL,"[CmsXmlXerxesParser] " + e);
    }
  }
}
