{
  CmsUUID detailId=CmsContainerpageController.get().getData().getDetailId();
  List<CmsUUID> detailIdList=new ArrayList<CmsUUID>();
  if (detailId != null) {
    detailIdList.add(detailId);
  }
  CmsResourceInfoDialog.load(CmsCoreProvider.get().getStructureId(),true,detailIdList,new CloseHandler<PopupPanel>(){
    public void onClose(    CloseEvent<PopupPanel> event){
      deactivateCurrentButton();
      activateSelection();
    }
  }
);
}
