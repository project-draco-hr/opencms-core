{
  String header;
  List defaultLocales=OpenCms.getLocaleManager().getDefaultLocales();
  Iterator i=defaultLocales.iterator();
  header="";
  while (i.hasNext()) {
    Locale loc=(Locale)i.next();
    header+=loc.getLanguage() + ", ";
  }
  header=header.substring(0,header.length() - 2);
  return header;
}
