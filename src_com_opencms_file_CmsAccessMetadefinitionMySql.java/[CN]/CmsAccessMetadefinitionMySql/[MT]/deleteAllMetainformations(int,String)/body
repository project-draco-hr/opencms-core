{
  try {
    PreparedStatement statementDeleteAllMetainfo=m_con.prepareStatement(C_METAINFO_DELETEALL);
    statementDeleteAllMetainfo.setString(1,path);
    statementDeleteAllMetainfo.setInt(2,projectId);
    statementDeleteAllMetainfo.executeQuery();
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
