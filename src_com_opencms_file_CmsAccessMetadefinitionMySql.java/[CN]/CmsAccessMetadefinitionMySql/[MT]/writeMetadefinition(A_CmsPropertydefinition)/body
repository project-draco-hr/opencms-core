{
  try {
    PreparedStatement statementUpdateMetadef=m_con.prepareStatement(C_METADEF_UPDATE);
    statementUpdateMetadef.setInt(1,metadef.getPropertydefType());
    statementUpdateMetadef.setInt(2,metadef.getId());
    statementUpdateMetadef.executeUpdate();
    return (readMetadefinition(metadef.getName(),metadef.getType()));
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
