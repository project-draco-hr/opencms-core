{
  try {
    if (countMetainfos(metadef) != 0) {
      throw new CmsException("[" + this.getClass().getName() + "] "+ metadef.getName(),CmsException.C_MANDATORY_PROPERTY);
    }
    PreparedStatement statementDeleteMetadef=m_con.prepareStatement(C_METADEF_DELETE);
    statementDeleteMetadef.setString(1,metadef.getName());
    statementDeleteMetadef.setInt(2,metadef.getType());
    statementDeleteMetadef.executeUpdate();
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
