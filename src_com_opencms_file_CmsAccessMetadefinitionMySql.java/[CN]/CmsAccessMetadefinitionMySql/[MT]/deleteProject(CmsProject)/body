{
  try {
    PreparedStatement statementDeleteProject=m_con.prepareStatement(C_PROJECT_DELETE);
    statementDeleteProject.setInt(1,project.getId());
    statementDeleteProject.executeUpdate();
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
