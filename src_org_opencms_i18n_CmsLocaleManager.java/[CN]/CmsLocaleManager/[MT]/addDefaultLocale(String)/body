{
  Locale locale=getLocale(localeName);
  if (!m_defaultLocales.contains(locale)) {
    m_defaultLocales.add(locale);
    if (OpenCms.getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_INIT).info(Messages.get().key(Messages.INIT_I18N_CONFIG_DEFAULT_LOCALE_2,new Integer(m_defaultLocales.size()),locale));
    }
  }
}
