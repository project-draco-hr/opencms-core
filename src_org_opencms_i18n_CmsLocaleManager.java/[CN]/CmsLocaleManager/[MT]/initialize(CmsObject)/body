{
  m_localeHandler.initHandler(cms);
  m_defaultLocale=m_defaultLocales.get(0);
  File profilesFolder=new File(OpenCms.getSystemInfo().getConfigFolder() + DEFAULT_LANGUAGE_PROFILES_FOLDER_NAME);
  if (profilesFolder.exists()) {
    try {
      DetectorFactory.loadProfile(profilesFolder);
    }
 catch (    LangDetectException e) {
      LOG.error(Messages.get().getBundle().key(Messages.LOG_INIT_LANG_DETECT_FAILED_1,profilesFolder),e);
    }
  }
  m_initialized=true;
  if (CmsLog.INIT.isInfoEnabled()) {
    CmsLog.INIT.info(Messages.get().getBundle().key(Messages.INIT_I18N_CONFIG_VFSACCESS_0));
  }
}
