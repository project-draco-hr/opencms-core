{
  StringBuffer buf;
switch (format) {
case FORMAT_HEADLINE:
    buf=new StringBuffer();
  buf.append("------ ");
buf.append(value);
System.out.print(buf);
break;
case FORMAT_WARNING:
buf=new StringBuffer();
buf.append("!!! ");
buf.append(value);
System.out.print(buf);
addWarning(value);
break;
case FORMAT_ERROR:
buf=new StringBuffer();
buf.append("!!! ");
buf.append(value);
System.out.print(buf);
addError(value);
break;
case FORMAT_NOTE:
case FORMAT_OK:
case FORMAT_DEFAULT:
default :
System.out.print(value);
}
}
