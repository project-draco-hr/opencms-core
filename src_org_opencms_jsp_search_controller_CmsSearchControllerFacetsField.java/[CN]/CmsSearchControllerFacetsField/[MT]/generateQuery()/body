{
  String query="";
  if (!m_fieldFacets.isEmpty()) {
    query="facet=true";
    final Iterator<I_CmsSearchControllerFacetField> it=m_fieldFacets.values().iterator();
    query+=it.next().generateQuery();
    while (it.hasNext()) {
      query+=it.next().generateQuery();
    }
  }
  return query;
}
