{
  if (isNotInitialized()) {
    return C_NOT_INITIALIZED;
  }
  try {
    return CmsJspTagLabel.wpLabelTagAction(label,getRequest());
  }
 catch (  Throwable t) {
    handleException(t);
  }
  CmsMessageContainer msgContainer=new CmsMessageContainer(Messages.get(),Messages.LOG_ERR_WORKPL_LABEL_READ_1,new Object[]{label});
  return this.getMessage(msgContainer);
}
