{
  if (isNotInitialized()) {
    return;
  }
  if (parameterMap != null) {
    try {
      HashMap modParameterMap=new HashMap(parameterMap.size());
      Iterator i=parameterMap.entrySet().iterator();
      while (i.hasNext()) {
        Map.Entry entry=(Map.Entry)i.next();
        String key=(String)entry.getKey();
        Object value=entry.getValue();
        if (value instanceof String[]) {
          modParameterMap.put(key,value);
        }
 else {
          if (value == null) {
            value="null";
          }
          String[] newValue=new String[]{value.toString()};
          modParameterMap.put(key,newValue);
        }
      }
      parameterMap=modParameterMap;
    }
 catch (    UnsupportedOperationException e) {
    }
  }
  CmsJspTagInclude.includeTagAction(getJspContext(),target,element,editable,parameterMap,getRequest(),getResponse());
}
