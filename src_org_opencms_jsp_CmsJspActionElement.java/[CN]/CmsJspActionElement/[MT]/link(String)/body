{
  if (isNotInitialized()) {
    return C_NOT_INITIALIZED;
  }
  try {
    return CmsJspTagLink.linkTagAction(link,getRequest());
  }
 catch (  Throwable t) {
    handleException(t);
  }
  CmsMessageContainer msgContainer=new CmsMessageContainer(Messages.get(),Messages.LOG_ERR_GEN_LINK_0,new Object[]{link});
  return this.getMessage(msgContainer);
}
