{
  if (isNotInitialized()) {
    return null;
  }
  if (m_navigation == null) {
    if (OpenCms.getSitemapManager().getCurrentUri(getRequest()) == null) {
      m_navigation=new CmsJspNavBuilder(getCmsObject());
    }
 else {
      m_navigation=new CmsJspSitemapNavBuilder(getCmsObject(),getRequest());
    }
  }
  return m_navigation;
}
