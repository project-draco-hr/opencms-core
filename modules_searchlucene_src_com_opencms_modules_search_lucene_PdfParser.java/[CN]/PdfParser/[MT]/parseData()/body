{
  ByteArrayOutputStream temp=new ByteArrayOutputStream();
  while (true) {
    int b=read();
    if (b == -1)     break;
    char ch=(char)b;
    if (ch == '(')     break;
  }
  while (true) {
    int b=read();
    if (b == -1)     break;
    char ch=(char)b;
    if (ch == ')')     break;
    temp.write(b);
  }
  return new String(temp.toByteArray());
}
