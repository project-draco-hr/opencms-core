{
  String param;
  String result;
  param="+";
  result=CmsEncoder.encode(param,CmsEncoder.C_UTF8_ENCODING);
  result=CmsEncoder.decode(result,CmsEncoder.C_UTF8_ENCODING);
  assertEquals(param,result);
  param="+K?ln -D?sseldorf &value";
  result=CmsEncoder.encode(param,CmsEncoder.C_UTF8_ENCODING);
  result=CmsEncoder.decode(result,CmsEncoder.C_UTF8_ENCODING);
  assertEquals(param,result);
}
