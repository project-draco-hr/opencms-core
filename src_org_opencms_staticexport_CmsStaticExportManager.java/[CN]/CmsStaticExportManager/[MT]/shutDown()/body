{
  int count=0;
  while ((count < HANDLER_FINISH_TIME) && m_handler.isBusy()) {
    count++;
    try {
      if (CmsLog.LOG.isInfoEnabled()) {
        CmsLog.LOG.info(Messages.get().key(Messages.INIT_STATIC_EXPORT_SHUTDOWN_3,m_handler.getClass().getName(),String.valueOf(count),String.valueOf(HANDLER_FINISH_TIME)));
      }
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      count=HANDLER_FINISH_TIME;
    }
  }
  if (CmsLog.LOG.isInfoEnabled()) {
    CmsLog.LOG.info(Messages.get().key(Messages.INIT_SHUTDOWN_1,this.getClass().getName()));
  }
}
