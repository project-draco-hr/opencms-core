{
  Boolean relativeLinks=(useRelativeLinks == null ? null : Boolean.valueOf(useRelativeLinks));
  m_rfsRules.add(new CmsStaticExportRfsRule(name,description,source,rfsPrefix,exportPath,relativeLinks,m_rfsTmpRule.getRelatedSystemResources()));
  m_rfsTmpRule=new CmsStaticExportRfsRule("","","","","",null);
}
