{
  if (rfsName != null) {
    if (rfsName.startsWith(m_rfsPrefix + "/")) {
      return true;
    }
    Iterator it=m_rfsRules.iterator();
    while (it.hasNext()) {
      CmsStaticExportRfsRule rule=(CmsStaticExportRfsRule)it.next();
      String rfsPrefix=rule.getRfsPrefix() + "/";
      if (rfsName.startsWith(rfsPrefix)) {
        return true;
      }
    }
  }
  return false;
}
