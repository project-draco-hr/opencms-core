{
  AttributeConfiguration result=null;
  try {
    I_CmsWidget widget=schemaType.getContentDefinition().getContentHandler().getWidget(schemaType);
    result=new AttributeConfiguration(getLabel(schemaType),getHelp(schemaType),widget.getClass().getName(),widget.getConfiguration());
  }
 catch (  Exception e) {
    LOG.debug(e.getMessage(),e);
  }
  return result;
}
