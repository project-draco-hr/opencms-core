{
  I_CmsXmlSchemaType schemaType=parentValue.getContentDefinition().getSchemaType(parentValue.getName() + "/" + childName);
  StringBuffer result=new StringBuffer();
  if (schemaType != null) {
    result.append("about=\"");
    result.append(CmsContentDefinition.uuidToEntityId(parentValue.getDocument().getFile().getStructureId(),parentValue.getLocale().toString()));
    result.append("/").append(parentValue.getPath());
    result.append("\" property=\"");
    result.append(getTypeUri(schemaType.getContentDefinition())).append("/").append(childName);
    result.append("\"");
  }
  return result.toString();
}
