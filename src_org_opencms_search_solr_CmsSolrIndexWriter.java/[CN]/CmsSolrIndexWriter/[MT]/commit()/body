{
  if ((m_server != null) && (m_index != null)) {
    try {
      LOG.info(Messages.get().getBundle().key(Messages.LOG_INDEX_WRITER_MSG_COMMIT_2,m_index.getName(),m_index.getPath()));
      m_server.commit();
    }
 catch (    SolrServerException e) {
      throw new IOException(e.getLocalizedMessage(),e);
    }
  }
}
