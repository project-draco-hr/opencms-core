{
  List result=new ArrayList();
  try {
    int resourceTypeGenericXmlContent=OpenCms.getResourceManager().getResourceType("xmlcontent").getTypeId();
    Iterator xmlFiles=getCms().readResources(I_CmsWpConstants.C_VFS_PATH_MODULES,CmsResourceFilter.DEFAULT_FILES.addRequireType(resourceTypeGenericXmlContent),true).iterator();
    while (xmlFiles.hasNext()) {
      CmsResource xmlFile=(CmsResource)xmlFiles.next();
      if (xmlFile.getName().endsWith(TABLE_XSLT_SUFFIX)) {
        result.add(xmlFile);
      }
    }
  }
 catch (  CmsException e) {
    if (OpenCms.getLog(this).isErrorEnabled()) {
      OpenCms.getLog(this).error(e);
    }
  }
  return result;
}
