{
  Iterator iter=VALUES.iterator();
  while (iter.hasNext()) {
    CmsProjectResourcesDisplayMode target=(CmsProjectResourcesDisplayMode)iter.next();
    if (value.equals(target.getMode())) {
      return target;
    }
  }
  throw new CmsIllegalArgumentException(Messages.get().container(Messages.ERR_MODE_ENUM_PARSE_2,value,CmsProjectResourcesDisplayMode.class.getName()));
}
