{
  RootPanel.get().addStyleName(I_CmsToolbarButtonLayoutBundle.INSTANCE.toolbarButtonCss().toolbarSelection());
  RootPanel.get().addStyleName(org.opencms.gwt.client.ui.css.I_CmsLayoutBundle.INSTANCE.directEditCss().classicDirectEdit());
  installToolbar();
  Scheduler.get().scheduleDeferred(new ScheduledCommand(){
    public void execute(){
      initializeButtons();
    }
  }
);
}
