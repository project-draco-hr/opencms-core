{
  boolean exec=false;
  List wrappers=getWrappers();
  Iterator iter=wrappers.iterator();
  while (iter.hasNext()) {
    I_CmsResourceWrapper wrapper=(I_CmsResourceWrapper)iter.next();
    exec=wrapper.lockResource(m_cms,resourcename);
    if (exec) {
      break;
    }
  }
  if (!exec) {
    m_cms.lockResource(resourcename);
  }
}
