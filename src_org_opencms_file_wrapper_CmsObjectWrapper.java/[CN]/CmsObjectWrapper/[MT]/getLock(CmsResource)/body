{
  CmsLock lock=null;
  List wrappers=getWrappers();
  Iterator iter=wrappers.iterator();
  while (iter.hasNext()) {
    I_CmsResourceWrapper wrapper=(I_CmsResourceWrapper)iter.next();
    lock=wrapper.getLock(m_cms,resource);
    if (lock != null) {
      break;
    }
  }
  if (lock == null) {
    lock=m_cms.getLock(resource);
  }
  return lock;
}
