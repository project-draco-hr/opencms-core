{
  try {
    CmsUser user=null;
    user=(CmsUser)m_userCache.get(new Integer(id));
    if (user == null) {
      user=m_dbAccess.readUser(id);
      m_userCache.put(new Integer(id),user);
    }
    return user;
  }
 catch (  CmsException ex) {
    return new CmsUser(C_UNKNOWN_ID,id + "","deleted user");
  }
}
