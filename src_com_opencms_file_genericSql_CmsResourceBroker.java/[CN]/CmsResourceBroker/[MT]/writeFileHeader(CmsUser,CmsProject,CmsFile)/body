{
  if (accessWrite(currentUser,currentProject,(CmsResource)file)) {
    file.setState(C_STATE_CHANGED);
    m_dbAccess.writeFileHeader(currentProject,onlineProject(currentUser,currentProject),file,true);
    m_resourceCache.put(C_FILE + currentProject.getId() + file.getAbsolutePath(),file);
    m_subresCache.clear();
    fileSystemChanged();
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ file.getAbsolutePath(),CmsException.C_NO_ACCESS);
  }
}
