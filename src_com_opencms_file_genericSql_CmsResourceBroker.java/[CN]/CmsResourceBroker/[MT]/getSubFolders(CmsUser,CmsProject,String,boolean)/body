{
  Vector folders=new Vector();
  folders=(Vector)m_subresCache.get("FOLDERS" + foldername,currentProject.getId());
  if ((folders == null) || (folders.size() == 0)) {
    folders=new Vector();
    try {
      folders=helperGetSubFolders(currentUser,currentProject,foldername);
    }
 catch (    CmsException exc) {
    }
    if (!currentProject.equals(onlineProject(currentUser,currentProject))) {
      try {
        Vector onlineFolders=helperGetSubFolders(currentUser,onlineProject(currentUser,currentProject),foldername);
        folders=mergeResources(folders,onlineFolders);
      }
 catch (      CmsException exc) {
      }
    }
    m_subresCache.put("FOLDERS" + foldername,currentProject.getId(),folders);
  }
  return (folders);
}
