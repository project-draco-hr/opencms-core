{
  for (int i=0; i < classFiles.size(); i++) {
    try {
      CmsFile file=m_dbAccess.readFileHeader(projectId,(String)classFiles.elementAt(i),false);
      if ((file.getState() == C_STATE_CHANGED) || (file.getState() == C_STATE_DELETED)) {
        return true;
      }
    }
 catch (    CmsException exc) {
    }
  }
  return false;
}
