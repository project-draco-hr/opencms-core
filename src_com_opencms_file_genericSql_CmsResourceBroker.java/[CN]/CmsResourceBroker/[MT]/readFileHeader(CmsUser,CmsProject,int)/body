{
  CmsResource cmsFile;
  try {
    cmsFile=m_dbAccess.readFileHeader(id);
  }
 catch (  CmsException exc) {
    throw exc;
  }
  if (accessRead(currentUser,currentProject,cmsFile)) {
    return cmsFile;
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ idx,CmsException.C_ACCESS_DENIED);
  }
}
