{
  if (!accessProject(currentUser,currentProject,currentProject.getId())) {
    return (false);
  }
  do {
    if (accessOther(currentUser,currentProject,resource,C_ACCESS_PUBLIC_READ) || accessOwner(currentUser,currentProject,resource,C_ACCESS_OWNER_READ) || accessGroup(currentUser,currentProject,resource,C_ACCESS_GROUP_READ)) {
      if (resource.getParent() != null) {
        try {
          resource=(CmsResource)readFolder(currentUser,currentProject,resource.getParent());
        }
 catch (        CmsException exc) {
          resource=(CmsResource)readFolder(currentUser,onlineProject(currentUser,currentProject),resource.getParent());
        }
      }
    }
 else {
      return (false);
    }
  }
 while (resource.getParent() != null);
  return (true);
}
