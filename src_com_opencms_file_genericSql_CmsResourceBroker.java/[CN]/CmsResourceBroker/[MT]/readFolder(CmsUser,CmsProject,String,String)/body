{
  CmsFolder cmsFolder;
  try {
    cmsFolder=cmsFolder=m_dbAccess.readFolder(currentProject.getId(),folder + folderName);
  }
 catch (  CmsException exc) {
    if (currentProject.equals(onlineProject(currentUser,currentProject))) {
      throw exc;
    }
 else {
      cmsFolder=cmsFolder=m_dbAccess.readFolder(onlineProject(currentUser,currentProject).getId(),folder + folderName);
    }
  }
  if (accessRead(currentUser,currentProject,(CmsResource)cmsFolder)) {
    return cmsFolder;
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ folder+ folderName,CmsException.C_ACCESS_DENIED);
  }
}
