{
  if (folder == null)   return null;
  CmsFolder cmsFolder=(CmsFolder)m_resourceCache.get(folder,currentProject.getId());
  if (cmsFolder == null) {
    cmsFolder=m_dbAccess.readFolderInProject(project,folder);
    if (cmsFolder != null) {
      m_resourceCache.put(folder,currentProject.getId(),(CmsFolder)cmsFolder);
    }
  }
  if (cmsFolder != null) {
    if (!accessRead(currentUser,currentProject,(CmsResource)cmsFolder))     throw new CmsException("[" + this.getClass().getName() + "] "+ folder,CmsException.C_ACCESS_DENIED);
  }
  return cmsFolder;
}
