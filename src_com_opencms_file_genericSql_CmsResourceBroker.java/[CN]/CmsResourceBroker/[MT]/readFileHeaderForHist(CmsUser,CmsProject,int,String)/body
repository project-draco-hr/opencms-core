{
  CmsResource cmsFile;
  try {
    cmsFile=(CmsResource)m_resourceCache.get(C_FILE + projectId + filename);
    if (cmsFile == null) {
      cmsFile=m_dbAccess.readFileHeader(projectId,filename);
      m_resourceCache.put(C_FILE + projectId + filename,cmsFile);
    }
    return cmsFile;
  }
 catch (  CmsException exc) {
    throw exc;
  }
}
