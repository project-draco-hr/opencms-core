{
  Vector files;
  try {
    files=helperGetFilesInFolder(currentUser,currentProject,foldername,includeDeleted);
  }
 catch (  CmsException e) {
    if (e.getType() == CmsException.C_ACCESS_DENIED)     return new Vector();
 else     throw e;
  }
  if (files == null) {
    return new Vector();
  }
  return files;
}
