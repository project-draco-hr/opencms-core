{
  StringBuffer buffer=new StringBuffer(32);
  if (prefix != null) {
    buffer.append(prefix);
    buffer.append("_");
  }
  if (projectId >= I_CmsConstants.C_PROJECT_ONLINE_ID) {
    if (projectId == I_CmsConstants.C_PROJECT_ONLINE_ID) {
      buffer.append("on");
    }
 else {
      buffer.append("of");
    }
    buffer.append("_");
  }
  buffer.append(resource);
  return buffer.toString();
}
