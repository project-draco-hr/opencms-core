{
  m_workflowDriver.endTask(dbc,taskid);
  if (dbc.currentUser() == null) {
    m_workflowDriver.writeSystemTaskLog(dbc,taskid,"Task finished.");
  }
 else {
    m_workflowDriver.writeSystemTaskLog(dbc,taskid,"Task finished by " + dbc.currentUser().getFirstname() + " "+ dbc.currentUser().getLastname()+ ".");
  }
}
