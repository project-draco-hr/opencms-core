{
  CmsLogFilter filter=CmsLogFilter.ALL.filterResource(resource.getStructureId()).filterUser(user.getId()).includeType(CmsLogEntryType.USER_RESOURCE_VISITED);
  List<CmsLogEntry> entries=getProjectDriver(dbc).readLog(dbc,poolName,filter);
  if (!entries.isEmpty()) {
    return entries.get(0).getDate();
  }
  return 0;
}
