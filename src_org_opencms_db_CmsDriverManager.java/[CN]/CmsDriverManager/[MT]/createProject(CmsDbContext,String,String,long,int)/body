{
  CmsGroup role=null;
  if (CmsStringUtil.isNotEmpty(roleName)) {
    role=readGroup(dbc,roleName);
  }
  java.sql.Timestamp timestamp=new java.sql.Timestamp(timeout);
  java.sql.Timestamp now=new java.sql.Timestamp(System.currentTimeMillis());
  return m_workflowDriver.createTask(dbc,0,0,1,dbc.currentUser().getId(),dbc.currentUser().getId(),role.getId(),projectName,now,timestamp,priority);
}
