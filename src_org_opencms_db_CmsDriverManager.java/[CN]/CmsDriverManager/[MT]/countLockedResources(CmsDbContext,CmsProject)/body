{
  if (project.getFlags() == I_CmsConstants.C_PROJECT_STATE_UNLOCKED) {
    return m_lockManager.countExclusiveLocksInProject(project);
  }
 else {
    throw new CmsLockException("[" + this.getClass().getName() + "] countLockedResources() project="+ dbc.currentProject().getName(),CmsLockException.C_RESOURCE_LOCKED);
  }
}
