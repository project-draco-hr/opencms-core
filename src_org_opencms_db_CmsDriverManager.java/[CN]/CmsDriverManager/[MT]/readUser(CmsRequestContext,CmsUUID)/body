{
  CmsUser user=null;
  try {
    CacheId cacheId=new CacheId(id);
    user=(CmsUser)m_userCache.get(cacheId);
    if (user == null) {
      user=m_userDriver.readUser(id);
      m_userCache.put(cacheId,user);
    }
  }
 catch (  CmsException ex) {
    return new CmsUser(CmsUUID.getNullUUID(),id + "","deleted user");
  }
  return user;
}
