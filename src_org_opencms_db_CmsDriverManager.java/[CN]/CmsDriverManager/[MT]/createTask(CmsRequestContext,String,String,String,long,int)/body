{
  CmsGroup role=m_userDriver.readGroup(null,roleName);
  java.sql.Timestamp timestamp=new java.sql.Timestamp(timeout);
  java.sql.Timestamp now=new java.sql.Timestamp(System.currentTimeMillis());
  CmsUUID agentId=CmsUUID.getNullUUID();
  validTaskname(taskname);
  try {
    agentId=readUser(null,agentName,I_CmsConstants.C_USER_TYPE_SYSTEMUSER).getId();
  }
 catch (  Exception e) {
  }
  return m_workflowDriver.createTask(context.currentProject().getTaskId(),context.currentProject().getTaskId(),1,context.currentUser().getId(),agentId,role.getId(),taskname,now,timestamp,priority);
}
