{
  StringBuffer b=new StringBuffer(64);
  int len=keys.length;
  if (len > 0) {
    for (int i=0; i < len; i++) {
      b.append(keys[i]);
      b.append('_');
    }
  }
  CmsUUID id=dbc.currentProject().getUuid();
  if (!dbc.getProjectId().isNullUUID()) {
    id=dbc.getProjectId();
  }
  b.append(id.toString());
  return b.toString();
}
