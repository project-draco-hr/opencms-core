{
  StringBuffer buffer=new StringBuffer(32);
  if (prefix != null) {
    buffer.append(prefix);
    if (flag) {
      buffer.append("_t_");
    }
 else {
      buffer.append("_f_");
    }
  }
  if (projectId >= CmsProject.ONLINE_PROJECT_ID) {
    if (projectId == CmsProject.ONLINE_PROJECT_ID) {
      buffer.append("on");
    }
 else {
      buffer.append("of");
    }
    buffer.append("_");
  }
  buffer.append(resource);
  return buffer.toString();
}
