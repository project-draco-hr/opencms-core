{
  return m_vfsDriver.readResourceTree(dbc,dbc.currentProject().getId(),folder,CmsDriverManager.READ_IGNORE_TYPE,CmsDriverManager.READ_IGNORE_STATE,starttime,endtime,CmsDriverManager.READ_IGNORE_TIME,CmsDriverManager.READ_IGNORE_TIME,CmsDriverManager.READ_IGNORE_TIME,CmsDriverManager.READ_IGNORE_TIME,CmsDriverManager.READMODE_INCLUDE_TREE);
}
