{
  try {
    CmsGroup group=m_userDriver.readGroup(null,principalName);
    if (group != null) {
      return group;
    }
  }
 catch (  Exception e) {
  }
  try {
    CmsUser user=readUser(null,principalName,I_CmsConstants.C_USER_TYPE_SYSTEMUSER);
    if (user != null) {
      return user;
    }
  }
 catch (  Exception e) {
  }
  return null;
}
