{
  try {
    CmsGroup group=m_userDriver.readGroup(principalName);
    if (group != null) {
      return (I_CmsPrincipal)group;
    }
  }
 catch (  Exception e) {
  }
  try {
    CmsUser user=readUser(principalName,I_CmsConstants.C_USER_TYPE_SYSTEMUSER);
    if (user != null) {
      return (I_CmsPrincipal)user;
    }
  }
 catch (  Exception e) {
  }
  return null;
}
