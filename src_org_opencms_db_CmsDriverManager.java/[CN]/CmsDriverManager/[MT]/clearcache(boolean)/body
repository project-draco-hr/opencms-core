{
  m_userCache.clear();
  m_groupCache.clear();
  m_userGroupsCache.clear();
  m_accessControlListCache.clear();
  m_securityManager.clearPermissionCache();
  if (!principalsOnly) {
    m_projectCache.clear();
    m_resourceCache.clear();
    m_resourceListCache.clear();
    m_propertyCache.clear();
  }
}
