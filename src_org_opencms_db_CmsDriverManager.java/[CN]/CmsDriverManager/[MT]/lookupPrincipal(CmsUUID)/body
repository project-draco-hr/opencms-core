{
  try {
    CmsGroup group=m_userDriver.readGroup(principalId);
    if (group != null) {
      return (I_CmsPrincipal)group;
    }
  }
 catch (  Exception e) {
  }
  try {
    CmsUser user=m_userDriver.readUser(principalId);
    if (user != null) {
      return (I_CmsPrincipal)user;
    }
  }
 catch (  Exception e) {
  }
  return null;
}
