{
  try {
    CmsGroup group=m_userDriver.readGroup(CmsRuntimeInfoFactory.getNullRuntimeInfo(),principalId);
    if (group != null) {
      return group;
    }
  }
 catch (  Exception e) {
  }
  try {
    CmsUser user=readUser(CmsRuntimeInfoFactory.getNullRuntimeInfo(),principalId);
    if (user != null) {
      return user;
    }
  }
 catch (  Exception e) {
  }
  return null;
}
