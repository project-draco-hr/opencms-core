{
  String newFile=new String();
  if (wholePath != null && !("".equals(wholePath))) {
    if (wholePath.startsWith("/")) {
      newFile=wholePath.substring(wholePath.lastIndexOf("/") + 1);
      if (newFile == null || "".equals(newFile)) {
        newFile=file.getName();
      }
    }
 else {
      newFile=wholePath;
    }
  }
  return newFile;
}
