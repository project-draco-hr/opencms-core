{
  if (m_loginManager == null) {
    m_loginManager=new CmsLoginManager(CmsLoginManager.DISABLE_MINUTES_DEFAULT,CmsLoginManager.MAX_BAD_ATTEMPTS_DEFAULT,CmsLoginManager.ENABLE_SECURITY_DEFAULT,"" + CmsLoginManager.DEFAULT_TOKEN_LIFETIME,null);
  }
  if (m_loginMessage != null) {
    try {
      m_loginManager.setLoginMessage(null,m_loginMessage);
    }
 catch (    CmsRoleViolationException e) {
    }
  }
  return m_loginManager;
}
