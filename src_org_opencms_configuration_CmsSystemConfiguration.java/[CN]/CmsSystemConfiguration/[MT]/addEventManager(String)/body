{
  Object initClass;
  try {
    initClass=Class.forName(clazz).newInstance();
  }
 catch (  Throwable t) {
    LOG.error(Messages.get().getBundle().key(Messages.INIT_EVENTMANAGER_CLASS_INVALID_2,clazz,m_resourceInitHandlers.getClass().getName()),t);
    return;
  }
  if (initClass instanceof CmsEventManager) {
    m_eventManager=(CmsEventManager)initClass;
    if (CmsLog.INIT.isInfoEnabled()) {
      CmsLog.INIT.info(Messages.get().getBundle().key(Messages.INIT_EVENTMANAGER_CLASS_SUCCESS_1,initClass));
    }
  }
 else {
    if (CmsLog.INIT.isErrorEnabled()) {
      CmsLog.INIT.error(Messages.get().getBundle().key(Messages.INIT_EVENTMANAGER_CLASS_INVALID_2,initClass,m_resourceInitHandlers.getClass().getName()));
    }
  }
}
