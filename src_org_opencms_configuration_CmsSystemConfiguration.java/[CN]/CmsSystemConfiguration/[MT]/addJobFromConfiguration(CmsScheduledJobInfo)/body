{
  m_configuredJobs.add(jobInfo);
  if (OpenCms.getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
    OpenCms.getLog(CmsLog.CHANNEL_INIT).info(". Scheduler config     : configured job named '" + jobInfo.getJobName() + "' for class '"+ jobInfo.getClassName()+ "' with user "+ jobInfo.getContextInfo().getUserName());
  }
}
