{
  if (attrs != null) {
    attrs=" " + attrs;
  }
 else {
    attrs="";
  }
  if (segment == HTML_START) {
    return "<table class=\"dialog\" cellpadding=\"0\" cellspacing=\"0\"" + attrs + ">\n"+ "<tr><td>\n<table class=\"dialogbox\" cellpadding=\"0\" cellspacing=\"0\">\n"+ "<tr><td>";
  }
 else {
    return "</td></tr></table>\n</td></tr></table>\n<p>&nbsp;</p>";
  }
}
