{
  NamedNodeMap attrs=node.getAttributes();
  if (attrs.item(number).getNodeValue().equalsIgnoreCase(CmsStringUtil.TRUE)) {
    return true;
  }
 else   if (attrs.item(number).getNodeValue().equalsIgnoreCase(CmsStringUtil.FALSE)) {
    return false;
  }
  System.err.println("Fatal configuration error: Attribute value \"" + attrs.item(number).getNodeValue() + "\" not recognized in tag <"+ node.getNodeName()+ ">.");
  System.exit(1);
  return false;
}
