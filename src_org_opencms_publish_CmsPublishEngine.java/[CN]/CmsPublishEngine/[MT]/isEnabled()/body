{
  try {
    if (m_engineState == CmsPublishEngineState.ENGINE_STOPPED || m_engineState == CmsPublishEngineState.ENGINE_STARTED) {
      OpenCms.getLoginManager().checkLoginAllowed();
      return true;
    }
 else {
      return false;
    }
  }
 catch (  CmsAuthentificationException e) {
    return false;
  }
}
