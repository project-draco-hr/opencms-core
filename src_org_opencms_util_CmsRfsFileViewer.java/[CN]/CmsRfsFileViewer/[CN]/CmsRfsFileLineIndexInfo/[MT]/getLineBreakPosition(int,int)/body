{
  int sz=m_breakPositions.size();
  if (breakNumber <= sz) {
    if (sz - breakNumber < linesToRead) {
      breakNumber=sz - linesToRead;
      if (breakNumber < 0) {
        breakNumber=0;
      }
    }
    return ((Number)m_breakPositions.get(breakNumber)).longValue();
  }
 else {
    if (m_windowSize > sz) {
      breakNumber=sz - linesToRead;
    }
 else {
      breakNumber=0;
    }
    return ((Number)m_breakPositions.get(breakNumber)).longValue();
  }
}
