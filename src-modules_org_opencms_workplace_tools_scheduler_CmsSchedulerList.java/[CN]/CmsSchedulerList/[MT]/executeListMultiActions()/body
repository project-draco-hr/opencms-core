{
  CmsListItem listItem=null;
  if (getParamListAction().equals(LIST_ACTION_MDELETE)) {
    List removedItems=new ArrayList();
    try {
      Iterator itItems=getSelectedItems().iterator();
      while (itItems.hasNext()) {
        listItem=(CmsListItem)itItems.next();
        OpenCms.getScheduleManager().unscheduleJob(getCms(),listItem.getId());
        removedItems.add(listItem.getId());
      }
      writeConfiguration(false);
    }
 catch (    CmsException e) {
      throw new CmsRuntimeException(Messages.get().container(Messages.ERR_UNSCHEDULE_JOB_1,(listItem == null) ? (Object)"?" : new Integer(listItem.getId())),e);
    }
 finally {
      getList().removeAllItems(removedItems,getLocale());
    }
  }
 else   if (getParamListAction().equals(LIST_ACTION_MACTIVATE) || getParamListAction().equals(LIST_ACTION_MDEACTIVATE)) {
    try {
      Iterator itItems=getSelectedItems().iterator();
      boolean activate=getParamListAction().equals(LIST_ACTION_MACTIVATE);
      while (itItems.hasNext()) {
        listItem=(CmsListItem)itItems.next();
        CmsScheduledJobInfo job=(CmsScheduledJobInfo)OpenCms.getScheduleManager().getJob(listItem.getId()).clone();
        job.setActive(activate);
        OpenCms.getScheduleManager().scheduleJob(getCms(),job);
      }
      writeConfiguration(true);
    }
 catch (    CmsException e) {
      throw new CmsRuntimeException(Messages.get().container(Messages.ERR_SCHEDULE_JOB_1,(listItem == null) ? (Object)"?" : new Integer(listItem.getId())),e);
    }
  }
 else {
    throwListUnsupportedActionException();
  }
  listSave();
}
