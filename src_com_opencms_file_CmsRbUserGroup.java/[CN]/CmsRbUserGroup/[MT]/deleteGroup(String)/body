{
  Vector childs=null;
  Vector users=null;
  childs=getChild(delgroup);
  users=getUsersOfGroup(delgroup);
  if ((childs == null) && ((users == null) || (users.size() == 0))) {
    m_accessUserGroup.deleteGroup(delgroup);
  }
 else {
    throw new CmsException(delgroup,CmsException.C_GROUP_NOT_EMPTY);
  }
}
