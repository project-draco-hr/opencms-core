{
  String backgroundColor=CmsDomUtil.getCurrentStyle(element,Style.backgroundColor);
  String backgroundImage=CmsDomUtil.getCurrentStyle(element,Style.backgroundImage);
  if ((backgroundColor.equals(StyleValue.transparent.toString())) && ((backgroundImage == null) || (backgroundImage.trim().length() == 0) || backgroundImage.equals(StyleValue.none.toString()))) {
    return false;
  }
  return true;
}
