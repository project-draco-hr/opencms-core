{
  String backgroundColor=CmsDomUtil.getCurrentStyle(element,Style.backgroundColor);
  if ((Document.get().getBody() != element) && (CmsStringUtil.isEmptyOrWhitespaceOnly(backgroundColor) || backgroundColor.equals(StyleValue.transparent.toString()) || backgroundColor.equals(StyleValue.inherit.toString()))) {
    backgroundColor=getEffectiveBackgroundColor(element.getParentElement());
  }
  return backgroundColor;
}
