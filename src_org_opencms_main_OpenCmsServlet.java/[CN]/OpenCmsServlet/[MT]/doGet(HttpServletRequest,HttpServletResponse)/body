{
  String path=req.getPathInfo();
  if ((path != null) && path.startsWith(C_HANDLE)) {
    try {
      OpenCmsCore.getInstance().initStartupClasses(req,res);
    }
 catch (    CmsException e) {
    }
    invokeHandler(req,res);
  }
 else {
    OpenCmsCore.getInstance().showResource(req,res);
  }
}
