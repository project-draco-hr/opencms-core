def createXmlHeader(self, command, resourceType, currentFolder):
    self.setXmlHeaders()
    s = ''
    s += '<?xml version="1.0" encoding="utf-8" ?>'
    s += ('<Connector command="%s" resourceType="%s">' % (command, resourceType))
    s += ('<CurrentFolder path="%s" url="%s" />' % (self.convertToXmlAttribute(currentFolder), self.convertToXmlAttribute(self.getWebUrlFromPath(resourceType, currentFolder))))
    return s
