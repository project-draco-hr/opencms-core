{
  if (!value.equals("")) {
    String pattern="01234567890 (abcdefghijklmnopqrstuvwxyz)[ABCDEFGHIJKLMNOPQRSTUVWXYZ]{???????}<.@:,;?$|!&+-#_=%*/>";
    for (int i=0; i < value.length(); i++) {
      if (value.indexOf(value.charAt(i)) == -1) {
        throw new Exception(parameter);
      }
    }
  }
}
