{
  if (!isJavaScriptPresent(javascriptLink)) {
    FromUrl injector=ScriptInjector.fromUrl(javascriptLink);
    injector.setCallback(callback);
    injector.inject();
  }
 else {
    Scheduler.get().scheduleDeferred(new ScheduledCommand(){
      public void execute(){
        callback.onSuccess(null);
      }
    }
);
  }
}
