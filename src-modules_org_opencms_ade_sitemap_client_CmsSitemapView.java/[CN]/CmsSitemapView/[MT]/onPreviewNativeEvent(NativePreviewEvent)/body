{
  Event nativeEvent=Event.as(event.getNativeEvent());
  if (event.getTypeInt() != Event.ONKEYUP) {
    return;
  }
  if ((nativeEvent.getKeyCode() == 'z') || (nativeEvent.getKeyCode() == 'Z')) {
    m_controller.undo();
  }
  if ((nativeEvent.getKeyCode() == 'r') || (nativeEvent.getKeyCode() == 'R')) {
    m_controller.redo();
  }
}
