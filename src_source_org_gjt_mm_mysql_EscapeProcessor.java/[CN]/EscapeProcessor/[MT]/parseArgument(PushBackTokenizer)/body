{
  StringBuffer Argument=new StringBuffer();
  boolean done_parsing=false;
  boolean seen_one_quote=false;
  while (!done_parsing) {
    String Tok=Tokenizer.nextToken();
    if (Tok.equals("'")) {
      if (seen_one_quote) {
        done_parsing=true;
      }
 else {
        seen_one_quote=true;
      }
    }
 else {
      Argument.append(Tok);
    }
  }
  return Argument.toString();
}
