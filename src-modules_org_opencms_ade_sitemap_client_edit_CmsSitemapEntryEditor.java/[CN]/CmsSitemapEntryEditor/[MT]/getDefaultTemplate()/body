{
  CmsSitemapTemplate template=m_controller.getDefaultTemplate(m_handler.getEntry().getSitePath());
  if (template != null) {
    CmsSitemapTemplate result=new CmsSitemapTemplate(template.getTitle(),template.getDescription(),DEFAULT_TEMPLATE_VALUE,template.getImgPath());
    result.setShowWeakText(true);
    return result;
  }
  return null;
}
