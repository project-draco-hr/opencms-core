{
  BasePrinter printer;
  if (format == null) {
    format=new OutputFormat("xml","UTF8",false);
    printer=new XMLPrinter();
  }
 else {
    if (format.getMethod().equalsIgnoreCase("html"))     printer=new HTMLPrinter(false);
 else     if (format.getMethod().equalsIgnoreCase("xhtml"))     printer=new HTMLPrinter(true);
 else     printer=new XMLPrinter();
  }
  return printer;
}
