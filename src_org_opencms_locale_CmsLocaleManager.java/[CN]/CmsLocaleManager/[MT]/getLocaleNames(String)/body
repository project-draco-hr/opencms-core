{
  String details[]=Utils.split(fullName,"_");
  String ext;
  if (details.length > 1) {
    ext=details[1];
    details[1]=details[0];
    details[0]=details[0] + "_" + ext;
  }
  if (details.length > 2) {
    ext=details[2];
    details[2]=details[1];
    details[1]=details[0];
    details[0]=details[0] + "_" + ext;
  }
  return details;
}
