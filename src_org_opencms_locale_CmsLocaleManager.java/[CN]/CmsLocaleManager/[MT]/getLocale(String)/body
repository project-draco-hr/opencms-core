{
  String name[]=Utils.split(fullName,"_");
  Locale l;
  l=(Locale)m_availableLocales.get(fullName);
  if (l != null) {
    return l;
  }
  l=(Locale)m_availableLocales.get(name[0] + "_" + name[1]);
  if (l != null) {
    return l;
  }
  l=(Locale)m_availableLocales.get(name[0]);
  if (l != null) {
    return l;
  }
  return null;
}
