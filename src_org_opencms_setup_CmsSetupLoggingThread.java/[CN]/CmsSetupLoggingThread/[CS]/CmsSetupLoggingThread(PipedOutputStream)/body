{
  super("OpenCms: Setup logging");
  m_pipedOut=pipedOut;
  messages=new Vector();
  m_stopThread=false;
  try {
    m_pipedIn=new PipedInputStream();
    m_pipedIn.connect(m_pipedOut);
    m_lineReader=new LineNumberReader(new BufferedReader(new InputStreamReader(m_pipedIn)));
  }
 catch (  Exception e) {
    messages.addElement(e.toString());
  }
}
