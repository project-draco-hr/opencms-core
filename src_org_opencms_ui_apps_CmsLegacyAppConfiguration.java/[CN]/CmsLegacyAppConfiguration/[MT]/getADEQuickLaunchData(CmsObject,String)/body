{
  CmsUserSettings settings=new CmsUserSettings(cms);
  if (!settings.usesNewWorkplace() || !getVisibility(cms).isActive()) {
    return Optional.absent();
  }
 else {
    String wpToolsUrl=OpenCms.getSystemInfo().getWorkplaceContext() + "#!" + m_toolHandler.getPath();
    return Optional.of(new CmsQuickLaunchData(m_toolHandler.getPath(),wpToolsUrl,CmsGwtConstants.WIN_WORKPLACE,getName(OpenCms.getWorkplaceManager().getWorkplaceLocale(cms)),getImageLink(),true));
  }
}
