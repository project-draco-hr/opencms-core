{
  m_uriLocator=new CmsUriLocator(uriCachesize);
  m_elementLocator=new CmsElementLocator(elementCachesize);
  if (variantCachesize < 2) {
    variantCachesize=100;
  }
  m_variantCachesize=variantCachesize;
  OpenCms.addCmsEventListener(this,new int[]{I_CmsEventListener.EVENT_PUBLISH_PROJECT,I_CmsEventListener.EVENT_PUBLISH_RESOURCE,I_CmsEventListener.EVENT_CLEAR_CACHES,I_CmsEventListener.EVENT_CLEAR_ONLINE_CACHES,I_CmsEventListener.EVENT_CLEAR_OFFLINE_CACHES});
}
