{
  SchemaField idField=OpenCms.getSearchManager().getSolrServerConfiguration().getSolrSchema().getUniqueKeyField();
  int[] luceneIds=new int[m_rows.intValue()];
  int docs=0;
  for (  SolrDocument doc : list) {
    String idString=(String)doc.getFirstValue(CmsSearchField.FIELD_ID);
    int id=m_solrQueryRequest.getSearcher().getFirstMatch(new Term(idField.getName(),idField.getType().toInternal(idString)));
    luceneIds[docs++]=id;
  }
  return new DocSlice(0,docs,luceneIds,null,docs,0);
}
