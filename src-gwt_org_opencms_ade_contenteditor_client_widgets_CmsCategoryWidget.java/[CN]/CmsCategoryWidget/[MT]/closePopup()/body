{
  m_previewHandlerRegistration.removeHandler();
  m_previewHandlerRegistration=null;
  String selected="";
  if (m_isSingelValue) {
    selected=m_cmsCategoryTree.getSelected();
  }
 else {
    for (    String s : m_cmsCategoryTree.getAllSelected()) {
      if (!s.isEmpty()) {
        selected+=m_categoryFolder + s + ",";
      }
    }
  }
  setValue(selected);
  m_cmsPopup.hide();
}
