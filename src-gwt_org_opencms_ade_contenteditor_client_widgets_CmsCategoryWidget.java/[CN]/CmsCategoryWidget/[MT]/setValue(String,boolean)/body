{
  value=value.replace(m_categoryFolder,"");
  String[] selectedValues=value.split("/");
  String prefix="";
  for (int i=0; i < selectedValues.length; i++) {
    prefix+=selectedValues[i] + "/";
    m_selectedValue.put(Integer.valueOf(i),prefix);
  }
  if (fireEvents) {
    fireChangeEvent();
  }
}
