{
  Iterator<String> i=m_selected.iterator();
  String result="";
  int y=0;
  if (m_isSingelValue) {
    int max_length=0;
    while (i.hasNext()) {
      String value=i.next();
      if (value.split("/").length > max_length) {
        max_length=value.split("/").length;
        result=m_categoryFolder + value;
      }
    }
  }
 else {
    while (i.hasNext()) {
      if (y != 0) {
        result+=",";
      }
      result+=m_categoryFolder + i.next();
      y++;
    }
  }
  return result;
}
