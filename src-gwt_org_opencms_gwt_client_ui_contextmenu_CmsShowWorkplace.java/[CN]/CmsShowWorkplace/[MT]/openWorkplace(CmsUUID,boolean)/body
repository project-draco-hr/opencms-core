{
  CmsRpcAction<String> callback=new CmsRpcAction<String>(){
    /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#execute()
 */
    @Override public void execute(){
      start(0,true);
      CmsCoreProvider.getService().getWorkplaceLink(structureId,classic,this);
    }
    /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#onResponse(java.lang.Object)
 */
    @Override protected void onResponse(    String result){
      stop(false);
      int width=Window.getClientWidth();
      int height=Window.getClientHeight();
      int left=Window.getScrollLeft();
      int top=Window.getScrollTop();
      openWorkplace(result,width,height,left,top);
    }
  }
;
  callback.execute();
}
