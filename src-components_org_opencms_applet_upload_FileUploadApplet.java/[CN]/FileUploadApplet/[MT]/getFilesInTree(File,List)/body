{
  if (file.isDirectory()) {
    File[] children=file.listFiles();
    for (int i=0; i < children.length; i++) {
      getFilesInTree(children[i],fileNames);
    }
  }
 else {
    FileFilter filter=m_fileSelector.getFileFilter();
    if (filter.accept(file)) {
      fileNames.add(file);
    }
  }
}
