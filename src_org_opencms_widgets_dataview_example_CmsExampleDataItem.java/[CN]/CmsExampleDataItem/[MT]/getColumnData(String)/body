{
switch (column) {
case "id":
    return getId();
case "title":
  return getTitle();
case "description":
return getDescription();
case "size":
return new Double(m_size);
case "good":
return new Boolean(m_isGood);
case "image":
return m_image;
case "category":
return m_category;
default :
return null;
}
}
