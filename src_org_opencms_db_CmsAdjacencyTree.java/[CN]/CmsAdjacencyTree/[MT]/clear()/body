{
  if (m_treeMap == null) {
    return;
  }
  Iterator i=m_treeMap.keySet().iterator();
  while (i.hasNext()) {
    CmsUUID currentParentId=(CmsUUID)i.next();
    List currentAdjacencyList=(List)m_treeMap.get(currentParentId);
    currentAdjacencyList.clear();
    m_treeMap.remove(currentParentId);
  }
  m_treeMap.clear();
  m_size=0;
}
