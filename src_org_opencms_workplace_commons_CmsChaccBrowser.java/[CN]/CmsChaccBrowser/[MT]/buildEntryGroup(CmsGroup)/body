{
  StringBuffer retValue=new StringBuffer(256);
  retValue.append("<span class=\"dialogunmarked maxwidth\" onmouseover=\"className='dialogmarked maxwidth';\"" + " onmouseout=\"className='dialogunmarked maxwidth'\" onclick=\"top.selectForm('0','" + group.getName() + "');\">");
  retValue.append("<img src=\"" + getSkinUri() + "commons/group.png\">&nbsp;");
  retValue.append(group.getName());
  retValue.append("</span>");
  return retValue;
}
