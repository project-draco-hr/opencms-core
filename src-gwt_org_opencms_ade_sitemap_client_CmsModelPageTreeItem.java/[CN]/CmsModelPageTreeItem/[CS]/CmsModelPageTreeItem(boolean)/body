{
  super(true);
  m_isContainerModel=isContainerModel;
  CmsListInfoBean infoBean=m_isContainerModel ? new CmsListInfoBean("Container models","Click on the plus symbol at the right to create a new container model.",null) : new CmsListInfoBean(Messages.get().key(Messages.GUI_MODEL_PAGE_TREE_ROOT_TITLE_0),Messages.get().key(Messages.GUI_MODE_PAGE_TREE_ROOT_SUBTITLE_0),null);
  CmsListItemWidget content=new CmsListItemWidget(infoBean);
  content.setIcon(CmsIconUtil.getResourceIconClasses("modelpage",false));
  initContent(content);
}
