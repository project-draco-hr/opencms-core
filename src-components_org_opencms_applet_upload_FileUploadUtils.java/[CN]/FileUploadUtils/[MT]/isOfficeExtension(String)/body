{
  if (extension != null) {
    return (extension.equals(".odt") || extension.equals(".ods") || extension.equals(".odp")|| extension.equals(".odg")|| extension.equals(".pdf")|| extension.equals(".doc")|| extension.equals(".xls")|| extension.equals(".vsd")|| extension.equals(".ppt"));
  }
 else {
    return false;
  }
}
