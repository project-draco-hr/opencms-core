{
  if (extension != null) {
    return (extension.equals(".txt") || extension.equals(".ini") || extension.equals(".bat")|| extension.equals(".cmd")|| extension.equals(".sh")|| extension.equals(".java")|| extension.equals(".log")|| extension.equals(".xml")|| extension.equals(".html")|| extension.equals(".sys"));
  }
 else {
    return false;
  }
}
