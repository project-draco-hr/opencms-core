{
  if (extension != null) {
    return (extension.equals(".gif") || extension.equals(".tiff") || extension.equals(".tif")|| extension.equals(".jpeg")|| extension.equals(".jpg")|| extension.equals(".bmp")|| extension.equals(".pnm")|| extension.equals(".pbm")|| extension.equals(".pgm")|| extension.equals(".ppm")|| extension.equals(".png"));
  }
 else {
    return false;
  }
}
