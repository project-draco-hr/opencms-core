{
  if (CmsStringUtil.isEmptyOrWhitespaceOnly(getLockedByName())) {
    return 0;
  }
  if (!getLockedByName().equals(m_request.currentUser().getName()) || !getLockedInProjectId().equals(m_request.currentProject().getUuid())) {
    return 1;
  }
  if (getLock().getType().isShared()) {
    return 2;
  }
  return 3;
}
