{
  StringBuffer result=new StringBuffer(32);
  if (m_filterVisible) {
    result.append(" Vi");
  }
  if (m_filterWritable) {
    result.append(" Wr");
  }
  if (m_filterTimerange) {
    result.append(" Ti");
  }
switch (m_filterState) {
case REQUIRED:
    result.append(" Sr");
  result.append(m_state);
break;
case EXCLUDED:
result.append(" Sx");
result.append(m_state);
break;
default :
}
switch (m_filterType) {
case REQUIRED:
result.append(" Tr");
result.append(m_type);
break;
case EXCLUDED:
result.append(" Tx");
result.append(m_type);
break;
default :
}
if (m_onlyFolders != null) {
if (m_onlyFolders.booleanValue()) {
result.append(" Fo");
}
 else {
result.append(" Fi");
}
}
if (m_filterLastModified) {
result.append(" Lt");
result.append(m_modifiedAfter);
result.append("-");
result.append(m_modifiedBefore);
}
m_cacheId=result.toString();
}
