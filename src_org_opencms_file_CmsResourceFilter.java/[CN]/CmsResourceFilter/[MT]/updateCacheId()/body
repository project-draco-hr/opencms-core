{
  StringBuffer result=new StringBuffer(32);
  if (m_filterVisible) {
    result.append(" Vis");
  }
switch (m_filterState) {
case REQUIRED:
    result.append(" Sr");
  result.append(m_state);
case EXCLUDED:
result.append(" Sx");
result.append(m_state);
default :
}
switch (m_filterType) {
case REQUIRED:
result.append(" Tr");
result.append(m_type);
case EXCLUDED:
result.append(" Tx");
result.append(m_type);
default :
}
if (m_filterTimerange) {
result.append(" Time");
result.append(m_modifiedAfter);
result.append("-");
result.append(m_modifiedBefore);
}
m_cacheId=result.toString();
}
