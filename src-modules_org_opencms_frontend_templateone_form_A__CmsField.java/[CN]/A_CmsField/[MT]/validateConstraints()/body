{
  if (isMandatory()) {
    if (needsItems()) {
      Iterator k=m_items.iterator();
      boolean isSelected=false;
      while (k.hasNext()) {
        CmsFieldItem currentItem=(CmsFieldItem)k.next();
        if (currentItem.isSelected()) {
          isSelected=true;
          continue;
        }
      }
      if (!isSelected) {
        return CmsFormHandler.ERROR_MANDATORY;
      }
    }
 else {
      if (CmsStringUtil.isEmpty(m_value)) {
        return CmsFormHandler.ERROR_MANDATORY;
      }
    }
  }
  return null;
}
