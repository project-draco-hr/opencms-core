{
  if (CmsStringUtil.isEmptyOrWhitespaceOnly(colSizeTwo) || !colSizeTwo.trim().equalsIgnoreCase("true")) {
    return true;
  }
  boolean result=false;
  if (m_position != 0) {
    result=true;
  }
  if (m_position == 0)   m_position=1;
 else   m_position=0;
  if (m_position == 1 && m_placeholder >= 1) {
    result=true;
    m_position=0;
    m_placeholder--;
  }
  return result;
}
