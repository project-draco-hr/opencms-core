{
  I_CmsFormatterBean result=null;
  String formatterConfigId=element.getSettings() != null ? element.getSettings().get(CmsFormatterConfig.getSettingsKeyForContainer(container.getName())) : null;
  if (CmsUUID.isValidUUID(formatterConfigId)) {
    result=formatters.get(new CmsUUID(formatterConfigId));
  }
  return result;
}
