{
  int containerWidth=-1;
  if (container.getWidth() == null) {
    containerWidth=CmsFormatterConfiguration.MATCH_ALL_CONTAINER_WIDTH;
  }
 else {
    try {
      containerWidth=Integer.parseInt(container.getWidth());
    }
 catch (    NumberFormatException e) {
    }
  }
  CmsJspTagContainer.ensureValidFormatterSettings(cms,element,config,container.getName(),container.getType(),containerWidth);
  I_CmsFormatterBean result=null;
  String formatterConfigId=element.getSettings() != null ? element.getSettings().get(CmsFormatterConfig.getSettingsKeyForContainer(container.getName())) : null;
  if (CmsUUID.isValidUUID(formatterConfigId)) {
    result=formatters.get(new CmsUUID(formatterConfigId));
  }
  return result;
}
