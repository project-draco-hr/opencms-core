{
  CmsUploader uploader=new CmsUploader();
  CmsFileInfo info=m_fileInput.getFiles()[0];
  info.setOverrideFileName(CmsResource.getName(m_replaceInfo.getSitepath()));
  uploader.uploadFiles(CmsCoreProvider.get().link(I_CmsUploadConstants.UPLOAD_ACTION_JSP_URI),CmsResource.getFolderPath(m_replaceInfo.getSitepath()),Collections.singletonList(info),Collections.<String>emptyList(),this);
  showProgress();
}
