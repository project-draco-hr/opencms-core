{
  int scopeValue;
switch (SCOPES_LIST.indexOf(scope)) {
case 3:
    scopeValue=PageContext.APPLICATION_SCOPE;
  break;
case 2:
scopeValue=PageContext.SESSION_SCOPE;
break;
case 1:
scopeValue=PageContext.REQUEST_SCOPE;
break;
default :
scopeValue=PageContext.PAGE_SCOPE;
break;
}
return scopeValue;
}
