{
  m_previewVisible=visible;
  if (m_previewVisible) {
    useMaxDimensions();
    m_parentPanel.removeStyleName(I_CmsLayoutBundle.INSTANCE.previewDialogCss().hidePreview());
  }
 else {
    m_parentPanel.addStyleName(I_CmsLayoutBundle.INSTANCE.previewDialogCss().hidePreview());
    updateSizes();
  }
}
