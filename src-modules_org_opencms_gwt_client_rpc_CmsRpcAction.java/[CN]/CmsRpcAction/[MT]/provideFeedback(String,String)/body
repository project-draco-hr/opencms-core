{
  String title="Error";
  String text="An error occured:\n" + message + "\n"+ "Provided ticket '"+ ticket+ "'"+ "\n"+ "You may retry this operation by clicking 'OK'.\n"+ "Or retry later by clicking 'Cancel'.\n"+ "However if the error persists, contact Technical Support.";
  CmsPopupDialog dialog=getDialog(title,text);
  dialog.setAutoHideEnabled(false);
  dialog.addCloseHandler(new CloseHandler<PopupPanel>(){
    public void onClose(    CloseEvent<PopupPanel> event){
      execute();
    }
  }
);
}
