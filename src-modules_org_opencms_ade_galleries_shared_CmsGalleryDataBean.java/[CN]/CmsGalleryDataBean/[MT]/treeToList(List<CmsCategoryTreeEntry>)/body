{
  if (entries != null) {
    for (    CmsCategoryTreeEntry entry : entries) {
      CmsCategoryInfoBean bean=new CmsCategoryInfoBean(entry.getTitle(),entry.getPath(),null,entry.getPath(),entry.getIconResource());
      m_categoriesList.add(bean);
      treeToList(entry.getChildren());
    }
  }
}
