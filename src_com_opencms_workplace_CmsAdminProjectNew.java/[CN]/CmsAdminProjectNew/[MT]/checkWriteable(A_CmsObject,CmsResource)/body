{
  boolean access=false;
  int accessflags=res.getAccessFlags();
  if (((accessflags & C_ACCESS_PUBLIC_WRITE) > 0) || (cms.getRequestContext().isAdmin()) || (cms.readOwner(res).equals(cms.getRequestContext().currentUser()) && (accessflags & C_ACCESS_OWNER_WRITE) > 0)|| (cms.readGroup(res).equals(cms.getRequestContext().currentGroup()) && (accessflags & C_ACCESS_GROUP_WRITE) > 0)) {
    access=true;
  }
  return access;
}
