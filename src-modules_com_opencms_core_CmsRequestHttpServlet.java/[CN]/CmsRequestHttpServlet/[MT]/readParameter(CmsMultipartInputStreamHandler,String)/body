{
  StringBuffer sbuf=new StringBuffer();
  String line;
  while ((line=in.readLine()) != null) {
    if (line.startsWith(boundary)) {
      break;
    }
    sbuf.append(line + "\r\n");
  }
  if (sbuf.length() == 0) {
    return null;
  }
  sbuf.setLength(sbuf.length() - 2);
  return sbuf.toString();
}
