{
  if (m_siteRoot == null) {
    return resourcename;
  }
  String siteRoot=CmsRequestContext.getAdjustedSiteRoot(m_siteRoot,resourcename);
  if ((siteRoot == m_siteRoot) && resourcename.startsWith(siteRoot)) {
    resourcename=resourcename.substring(siteRoot.length());
  }
  return resourcename;
}
