{
  Entity result=null;
  if (m_id.equals(entityId)) {
    result=this;
  }
 else {
    for (    List<Entity> children : m_entityAttributes.values()) {
      for (      Entity child : children) {
        result=child.getEntityById(entityId);
        if (result != null) {
          break;
        }
      }
      if (result != null) {
        break;
      }
    }
  }
  return result;
}
