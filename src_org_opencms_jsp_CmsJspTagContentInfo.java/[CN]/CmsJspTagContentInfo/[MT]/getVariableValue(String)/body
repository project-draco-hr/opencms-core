{
  int dotIndex=-1;
  String variableValue=null;
  if (value.startsWith(CmsStringUtil.C_MACRO_DELIMITER + CmsStringUtil.C_MACRO_START) && value.endsWith(CmsStringUtil.C_MACRO_END) && (dotIndex=value.indexOf(".")) > 0) {
    variableValue=value.substring(dotIndex + 1,value.length() - 1);
  }
  return variableValue;
}
