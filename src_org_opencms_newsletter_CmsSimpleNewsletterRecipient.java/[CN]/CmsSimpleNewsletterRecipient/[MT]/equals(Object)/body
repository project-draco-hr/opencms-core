{
  if (!(obj instanceof CmsSimpleNewsletterRecipient)) {
    return false;
  }
  CmsSimpleNewsletterRecipient recipient=(CmsSimpleNewsletterRecipient)obj;
  if (getEmail() != recipient.getEmail()) {
    return false;
  }
  if (!getName().equals(recipient.getName())) {
    return false;
  }
  return true;
}
