{
  if (m_target.toString().indexOf("_") != 0) {
    return "<a href='#' title='" + getName() + "' onClick=\"return openView('"+ getId()+ "', '"+ page.getJsp().link(page.resolveMacros(m_link))+ "', '"+ m_target+ "');\" onMouseOver=\"mouseHelpEvent('"+ getId()+ "Help', true)\" onMouseOut=\"mouseHelpEvent('"+ getId()+ "Help', false)\">";
  }
  return "<a target='" + m_target + "' href='"+ page.getJsp().link(page.resolveMacros(m_link))+ "' title='"+ getName()+ "' onMouseOver=\"mouseHelpEvent('"+ getId()+ "Help', true)\" onMouseOut=\"mouseHelpEvent('"+ getId()+ "Help', false)\">";
}
