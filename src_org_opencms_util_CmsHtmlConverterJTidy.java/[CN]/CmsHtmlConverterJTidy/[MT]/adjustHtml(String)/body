{
  if ((htmlInput.toLowerCase().indexOf("<html>") == -1) && (htmlInput.toLowerCase().indexOf("</html>") == -1)) {
    StringBuffer tmp=new StringBuffer();
    tmp.append("<html xmlns:o=\"\"><body>");
    tmp.append(htmlInput);
    tmp.append("</body></html>");
    htmlInput=tmp.toString();
  }
  return htmlInput;
}
