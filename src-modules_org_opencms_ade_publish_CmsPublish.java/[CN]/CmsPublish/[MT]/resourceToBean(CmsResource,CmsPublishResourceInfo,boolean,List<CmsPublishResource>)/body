{
  CmsResourceUtil resUtil=new CmsResourceUtil(m_cms,resource);
  CmsPublishResource pubResource=new CmsPublishResource(resource.getStructureId(),resUtil.getFullPath(),resUtil.getTitle(),CmsWorkplace.getResourceUri(resUtil.getIconPathExplorer()),"" + resUtil.getStateAbbreviation(),removable,info,related);
  return pubResource;
}
