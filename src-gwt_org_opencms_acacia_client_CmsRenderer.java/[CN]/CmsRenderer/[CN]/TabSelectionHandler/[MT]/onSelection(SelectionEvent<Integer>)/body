{
  Scheduler.get().scheduleDeferred(new ScheduledCommand(){
    public void execute(){
      FlowPanel tab=m_tabsPanel.getWidget(event.getSelectedItem().intValue());
      for (      Widget w : tab) {
        if (w instanceof I_HasResizeOnShow) {
          ((I_HasResizeOnShow)w).resizeOnShow();
        }
      }
    }
  }
);
}
