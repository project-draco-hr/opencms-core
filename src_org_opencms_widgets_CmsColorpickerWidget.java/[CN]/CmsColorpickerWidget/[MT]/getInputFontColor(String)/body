{
  if (backgroundColor != null && backgroundColor.indexOf("#") == 0) {
    backgroundColor=backgroundColor.substring(1);
    int colorValue=50001;
    try {
      colorValue=Integer.parseInt(backgroundColor,16);
    }
 catch (    NumberFormatException nf) {
    }
    if (colorValue < 50000) {
      return "#FFFFFF";
    }
 else {
      return "#000000";
    }
  }
  return "#000000";
}
