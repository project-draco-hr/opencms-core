{
  CmsFile file;
  try {
    file=readFile(project.getId(),source);
  }
 catch (  SQLException se) {
    throw new CmsException(CmsException.C_SQL_ERROR,se);
  }
  if (file != null)   createFile(project,onlineProject,file,userId,parentId,destination,true);
 else   throw new CmsException(this.getClass().getName() + ".copyFile():" + source,CmsException.C_NOT_FOUND);
}
