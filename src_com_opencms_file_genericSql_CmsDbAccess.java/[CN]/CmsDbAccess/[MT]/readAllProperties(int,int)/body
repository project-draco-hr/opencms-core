{
  Hashtable returnValue=new Hashtable();
  ResultSet result=null;
  PreparedStatement statement=null;
  Connection con=null;
  try {
    con=DriverManager.getConnection(m_poolName);
    statement=con.prepareStatement(m_cq.C_PROPERTIES_READALL);
    statement.setInt(1,resourceId);
    statement.setInt(2,resourceType);
    result=statement.executeQuery();
    while (result.next()) {
      returnValue.put(result.getString(m_cq.C_PROPERTYDEF_NAME),result.getString(m_cq.C_PROPERTY_VALUE));
    }
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
 finally {
    if (result != null) {
      try {
        result.close();
      }
 catch (      SQLException exc) {
      }
    }
    if (statement != null) {
      try {
        statement.close();
      }
 catch (      SQLException exc) {
      }
    }
    if (con != null) {
      try {
        con.close();
      }
 catch (      SQLException exc) {
      }
    }
  }
  return (returnValue);
}
