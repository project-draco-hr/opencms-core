{
  PreparedStatement statement=null;
  try {
    statement=m_pool.getPreparedStatement(m_cq.C_RESOURCES_UNLOCK_KEY);
    statement.setInt(1,project.getId());
    statement.executeUpdate();
  }
 catch (  Exception exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
 finally {
    if (statement != null) {
      m_pool.putPreparedStatement(m_cq.C_RESOURCES_UNLOCK_KEY,statement);
    }
  }
}
