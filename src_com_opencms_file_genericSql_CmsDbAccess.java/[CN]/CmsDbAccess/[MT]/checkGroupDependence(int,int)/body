{
  int id=group1;
  do {
    id=readGroup(id).getParentId();
    if (id == group2) {
      return group1;
    }
  }
 while (id != C_UNKNOWN_ID);
  id=group2;
  do {
    id=readGroup(id).getParentId();
    if (id == group1) {
      return group2;
    }
  }
 while (id != C_UNKNOWN_ID);
  return -1;
}
