{
  Hashtable returnValue=new Hashtable();
  ResultSet result=null;
  PreparedStatement statement=null;
  Connection con=null;
  String usedPool;
  String usedStatement;
  int resourceId=resource.getResourceId();
  int onlineProject=I_CmsConstants.C_PROJECT_ONLINE_ID;
  if (projectId == onlineProject) {
    usedPool=m_poolNameOnline;
    usedStatement="_ONLINE";
  }
 else {
    usedPool=m_poolName;
    usedStatement="";
  }
  try {
    con=DriverManager.getConnection(usedPool);
    statement=con.prepareStatement(m_cq.get("C_PROPERTIES_READALL" + usedStatement));
    statement.setInt(1,resourceId);
    statement.setInt(2,resourceType);
    result=statement.executeQuery();
    while (result.next()) {
      returnValue.put(result.getString(m_cq.get("C_PROPERTYDEF_NAME")),result.getString(m_cq.get("C_PROPERTY_VALUE")));
    }
  }
 catch (  SQLException exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
 finally {
    if (result != null) {
      try {
        result.close();
      }
 catch (      SQLException exc) {
      }
    }
    if (statement != null) {
      try {
        statement.close();
      }
 catch (      SQLException exc) {
      }
    }
    if (con != null) {
      try {
        con.close();
      }
 catch (      SQLException exc) {
      }
    }
  }
  return (returnValue);
}
