{
  String s2="";
switch (m_conversionCharacter) {
case 'f':
    s2=printFFormat(s);
  break;
case 'E':
case 'e':
s2=printEFormat(s);
break;
case 'G':
case 'g':
s2=printGFormat(s);
break;
default :
throw new IllegalArgumentException("Cannot " + "format a double with a format using a " + m_conversionCharacter + " conversion character.");
}
return s2;
}
