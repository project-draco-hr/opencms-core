{
  boolean carry=true;
  for (int i=cLast; carry && i >= cFirst; i--) {
    carry=false;
switch (ca[i]) {
case '0':
      ca[i]='1';
    break;
case '1':
  ca[i]='2';
break;
case '2':
ca[i]='3';
break;
case '3':
ca[i]='4';
break;
case '4':
ca[i]='5';
break;
case '5':
ca[i]='6';
break;
case '6':
ca[i]='7';
break;
case '7':
ca[i]='8';
break;
case '8':
ca[i]='9';
break;
case '9':
ca[i]='0';
carry=true;
break;
default :
}
}
return carry;
}
