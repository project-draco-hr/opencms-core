{
  m_optionalh=false;
  m_optionall=false;
  m_optionalL=false;
  if (m_pos < m_fmt.length()) {
    char c=m_fmt.charAt(m_pos);
    if (c == 'h') {
      m_optionalh=true;
      m_pos++;
    }
 else     if (c == 'l') {
      m_optionall=true;
      m_pos++;
    }
 else     if (c == 'L') {
      m_optionalL=true;
      m_pos++;
    }
  }
}
