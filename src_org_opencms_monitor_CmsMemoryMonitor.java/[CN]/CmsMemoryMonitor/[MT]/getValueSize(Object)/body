{
  if (obj instanceof CmsLruCache) {
    return ((CmsLruCache)obj).size();
  }
  if (obj instanceof Map) {
    return getValueSize((Map)obj,1);
  }
  if (obj instanceof List) {
    return getValueSize((List)obj,1);
  }
  try {
    return getMemorySize(obj);
  }
 catch (  Exception exc) {
    return 0;
  }
}
