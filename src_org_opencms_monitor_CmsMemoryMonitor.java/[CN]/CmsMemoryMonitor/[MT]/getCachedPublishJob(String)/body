{
synchronized (m_publishQueue) {
    for (Iterator<CmsPublishJobInfoBean> i=m_publishQueue.iterator(); i.hasNext(); ) {
      CmsPublishJobInfoBean publishJob=i.next();
      if (publishJob.getPublishHistoryId().toString().equals(key)) {
        return publishJob;
      }
    }
  }
  return null;
}
