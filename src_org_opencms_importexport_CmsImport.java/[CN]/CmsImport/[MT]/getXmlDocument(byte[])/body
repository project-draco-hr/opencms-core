{
  ByteArrayInputStream stream=null;
  Document doc=null;
  try {
    stream=new ByteArrayInputStream(content);
    SAXReader saxReader=new SAXReader();
    doc=saxReader.read(stream);
  }
 catch (  Exception e) {
    throw new CmsException(CmsException.C_UNKNOWN_EXCEPTION,e);
  }
 finally {
    try {
      if (stream != null) {
        stream.close();
      }
    }
 catch (    Exception e) {
    }
  }
  return doc;
}
