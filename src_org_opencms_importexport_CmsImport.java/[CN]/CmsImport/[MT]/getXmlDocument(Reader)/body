{
  Document doc=null;
  try {
    SAXReader saxReader=new SAXReader();
    doc=saxReader.read(reader);
  }
 catch (  Exception e) {
    throw new CmsException(CmsException.C_UNKNOWN_EXCEPTION,e);
  }
 finally {
    try {
      if (reader != null) {
        reader.close();
      }
    }
 catch (    Exception e) {
    }
  }
  return doc;
}
