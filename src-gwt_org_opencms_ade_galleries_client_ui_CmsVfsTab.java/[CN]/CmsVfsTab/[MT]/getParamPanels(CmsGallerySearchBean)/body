{
  List<CmsSearchParamPanel> result=new ArrayList<CmsSearchParamPanel>();
  for (  String folder : searchObj.getFolders()) {
    CmsSearchParamPanel panel=new CmsSearchParamPanel(Messages.get().key(Messages.GUI_PARAMS_LABEL_FOLDERS_0),this);
    panel.setContent(folder,folder);
    result.add(panel);
  }
  return result;
}
