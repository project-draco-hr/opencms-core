{
  PreparedStatement stmt=null;
  try {
    stmt=conn.prepareStatement("UPDATE CMS_SYSTEMID SET ID=? WHERE CMS_SYSTEMID.TABLE_KEY=? AND CMS_SYSTEMID.ID=?");
    stmt.setInt(1,newId);
    stmt.setString(2,tableName);
    stmt.setInt(3,oldId);
    int amount=stmt.executeUpdate();
    return (amount == 1);
  }
 catch (  SQLException e) {
    throw new CmsDbSqlException(org.opencms.db.generic.Messages.get().container(org.opencms.db.generic.Messages.ERR_GENERIC_SQL_1,CmsDbSqlException.getErrorQuery(stmt)),e);
  }
 finally {
    if (stmt != null) {
      try {
        stmt.close();
      }
 catch (      SQLException exc) {
      }
    }
  }
}
