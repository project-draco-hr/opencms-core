{
  int i=0;
  for (; ; ) {
    try {
      return (result.getTimestamp(column));
    }
 catch (    SQLException exc) {
      i++;
      if (i >= 10) {
        throw exc;
      }
 else {
        if (OpenCms.isLogging(CmsLog.CHANNEL_MODULE,CmsLog.LEVEL_WARN)) {
          OpenCms.log(CmsLog.CHANNEL_MODULE,CmsLog.LEVEL_WARN,"Trying to get timestamp " + column + " #"+ i);
        }
      }
    }
  }
}
