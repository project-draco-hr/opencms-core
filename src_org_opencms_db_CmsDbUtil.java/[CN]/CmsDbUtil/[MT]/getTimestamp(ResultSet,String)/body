{
  int i=0;
  for (; ; ) {
    try {
      return (result.getTimestamp(column));
    }
 catch (    SQLException exc) {
      i++;
      if (i >= 10) {
        throw exc;
      }
 else {
        if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isWarnEnabled()) {
          OpenCms.getLog(CmsLog.CHANNEL_MAIN).warn("Trying to get timestamp " + column + " #"+ i);
        }
      }
    }
  }
}
