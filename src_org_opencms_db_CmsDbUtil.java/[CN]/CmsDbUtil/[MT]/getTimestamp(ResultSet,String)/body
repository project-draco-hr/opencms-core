{
  int i=0;
  for (; ; ) {
    try {
      return (result.getTimestamp(column));
    }
 catch (    SQLException exc) {
      i++;
      if (i >= 10) {
        throw exc;
      }
 else {
        if (LOG.isWarnEnabled()) {
          LOG.warn(Messages.get().container(Messages.LOG_GET_TIMESTAMP_2,column,String.valueOf(i)));
        }
      }
    }
  }
}
