{
  String resourceIds="";
  if (resources != null) {
    for (    CmsUUID id : resources) {
      resourceIds+=id.toString() + ";";
    }
  }
  m_frame=new CmsIFrame("embeddedDialogFrame",CmsCoreProvider.get().getEmbeddedDialogsUrl() + dialogId + "?resources="+ resourceIds+ "&contextType="+ contextType);
  m_frame.setStyleName(I_CmsLayoutBundle.INSTANCE.dialogCss().embeddedDialogFrame());
  RootPanel.get().add(m_frame);
  initIFrame();
}
