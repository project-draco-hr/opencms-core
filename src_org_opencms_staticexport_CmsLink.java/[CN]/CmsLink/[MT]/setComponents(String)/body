{
  String[] components=CmsRequestUtil.splitUri(uri);
  if (components != null) {
    m_target=components[0];
    m_anchor=components[1];
    setQuery(components[2]);
  }
 else {
    m_target=uri;
    m_anchor=null;
    setQuery(null);
  }
  m_parameters=CmsRequestUtil.createParameterMap(m_query);
}
