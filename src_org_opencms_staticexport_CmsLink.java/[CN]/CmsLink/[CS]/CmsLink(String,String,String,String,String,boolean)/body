{
  m_name=name;
  m_type=type;
  m_target=target;
  m_anchor=anchor;
  m_query=query;
  m_internal=internal;
  if (m_query != null) {
    m_parameters=getParameters(m_query);
  }
 else {
    m_parameters=new HashMap();
  }
  m_uri=m_target + ((m_query != null) ? "?" + m_query : "") + ((m_anchor != null) ? "#" + m_anchor : "");
}
