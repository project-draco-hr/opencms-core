{
  m_name=name;
  m_type=type;
  m_target=target;
  m_anchor=anchor;
  m_query=query;
  m_internal=internal;
  if (m_query != null) {
    m_parameters=getParameters(m_query);
  }
 else {
    m_parameters=Collections.EMPTY_MAP;
  }
  StringBuffer uri=new StringBuffer(64);
  uri.append(m_target);
  if (m_query != null) {
    uri.append('?');
    uri.append(m_query);
  }
  if (m_anchor != null) {
    uri.append('#');
    uri.append(m_anchor);
  }
  m_uri=uri.toString();
}
