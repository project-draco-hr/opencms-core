{
  m_name=name;
  m_type=type;
  m_uri=uri;
  m_internal=internal;
  String components[]=split(uri);
  if (components != null) {
    m_target=components[0];
    m_anchor=components[1];
    m_query=components[2];
  }
 else {
    m_target=uri;
    m_anchor=null;
    m_query=null;
  }
  if (m_query != null) {
    m_parameters=getParameters(m_query);
  }
 else {
    m_parameters=Collections.EMPTY_MAP;
  }
}
