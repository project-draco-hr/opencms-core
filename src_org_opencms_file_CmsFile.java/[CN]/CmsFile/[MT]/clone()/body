{
  byte[] newContent=new byte[this.getContents().length];
  System.arraycopy(getContents(),0,newContent,0,getContents().length);
  CmsFile clone=new CmsFile(getStructureId(),getResourceId(),getParentStructureId(),getContentId(),getName(),getTypeId(),getFlags(),getProjectLastModified(),getState(),getDateCreated(),getUserCreated(),getDateLastModified(),getUserLastModified(),getDateReleased(),getDateExpired(),getSiblingCount(),getLength(),newContent);
  if (isTouched()) {
    clone.setDateLastModified(getDateLastModified());
  }
  if (hasFullResourceName()) {
    clone.setRootPath(getRootPath());
  }
  return clone;
}
