{
  if (search) {
    String f=fileName;
    String prop=cms.readProperty(f,propertyName);
    while ((prop == null) && (!"".equals(f))) {
      f=f.substring(0,f.lastIndexOf("/"));
      prop=cms.readProperty(f + "/",propertyName);
    }
    return prop;
  }
 else {
    return cms.readProperty(fileName,propertyName);
  }
}
