{
  if (search) {
    String f=resource;
    String prop=cms.readProperty(f,property);
    while ((prop == null) && (!"".equals(f))) {
      f=f.substring(0,f.lastIndexOf("/"));
      prop=cms.readProperty(f + "/",property);
    }
    return prop;
  }
 else {
    return cms.readProperty(resource,property);
  }
}
