{
  if (detectWindowsPhone7()) {
    return false;
  }
  if ((m_userAgent.indexOf(deviceWinMob) != -1) || (m_userAgent.indexOf(deviceWinMob) != -1) || (m_userAgent.indexOf(deviceIeMob) != -1)|| (m_userAgent.indexOf(enginePie) != -1)|| ((m_userAgent.indexOf(manuHtc) != -1) && (m_userAgent.indexOf(deviceWindows) != -1))|| (detectWapWml() && (m_userAgent.indexOf(deviceWindows) != -1))) {
    return true;
  }
  if ((m_userAgent.indexOf(devicePpc) != -1) && !(m_userAgent.indexOf(deviceMacPpc) != -1)) {
    return true;
  }
  return false;
}
