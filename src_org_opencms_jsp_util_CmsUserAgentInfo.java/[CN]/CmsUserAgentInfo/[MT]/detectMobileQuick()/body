{
  if (isTierTablet) {
    return false;
  }
  if (detectSmartphone()) {
    return true;
  }
  if (detectWapWml() || detectBrewDevice() || detectOperaMobile()) {
    return true;
  }
  if ((m_userAgent.indexOf(engineNetfront) != -1) || (m_userAgent.indexOf(engineUpBrowser) != -1) || (m_userAgent.indexOf(engineOpenWeb) != -1)) {
    return true;
  }
  if (detectDangerHiptop() || detectMidpCapable() || detectMaemoTablet()|| detectArchos()) {
    return true;
  }
  if ((m_userAgent.indexOf(devicePda) != -1) && (m_userAgent.indexOf(disUpdate) < 0)) {
    return true;
  }
  if (m_userAgent.indexOf(mobile) != -1) {
    return true;
  }
  if (detectKindle() || detectAmazonSilk()) {
    return true;
  }
  return false;
}
