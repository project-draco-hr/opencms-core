{
  if (detectBlackBerryWebKit()) {
    return false;
  }
  if (detectBlackBerry()) {
    if (detectBlackBerryTouch() || (m_userAgent.indexOf(deviceBBBold) != -1) || (m_userAgent.indexOf(deviceBBTour) != -1)|| (m_userAgent.indexOf(deviceBBCurve) != -1)) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
