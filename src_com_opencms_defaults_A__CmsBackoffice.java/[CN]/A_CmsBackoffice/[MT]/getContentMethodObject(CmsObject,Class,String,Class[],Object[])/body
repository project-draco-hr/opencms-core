{
  Object retObject=null;
  if (method != "") {
    try {
      retObject=cdClass.getMethod(method,paramClasses).invoke(null,params);
    }
 catch (    InvocationTargetException ite) {
      if (OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).isWarnEnabled()) {
        OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).warn(method + " caused an InvocationTargetException",ite);
      }
      ite.getTargetException().printStackTrace();
    }
catch (    NoSuchMethodException nsm) {
      if (OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).isWarnEnabled()) {
        OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).warn(method + ": Requested method was not found",nsm);
      }
    }
catch (    Exception e) {
      if (OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).isWarnEnabled()) {
        OpenCms.getLog(CmsLog.CHANNEL_MODULE_MASTER).warn(method + ": Other Exception",e);
      }
    }
  }
  return retObject;
}
