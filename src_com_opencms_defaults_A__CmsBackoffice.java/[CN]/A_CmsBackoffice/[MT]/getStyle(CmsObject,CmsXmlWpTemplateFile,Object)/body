{
  int state=0;
  int projectId=1;
  int actProjectId=cms.getRequestContext().currentProject().getId();
  String style=new String();
  try {
    projectId=((I_CmsExtendedContentDefinition)entryObject).getProjectId();
  }
 catch (  Exception e) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice setFontFormat: Method getProjectId throwed an exception: " + e.toString());
    }
  }
  if (actProjectId == I_CmsConstants.C_PROJECT_ONLINE_ID) {
    style=C_STYLE_UNCHANGED;
  }
 else   if (projectId != actProjectId) {
    style=C_STYLE_NOTINPROJECT;
  }
 else {
    try {
      ((A_CmsContentDefinition)entryObject).getLockstate();
    }
 catch (    Exception e) {
      if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
        OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice setFontFormat: Method getLockstate throwed an exception: " + e.toString());
      }
    }
    try {
      state=((I_CmsExtendedContentDefinition)entryObject).getState();
    }
 catch (    Exception e) {
      if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
        OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice setFontFormat: Method getState throwed an exception: " + e.toString());
      }
    }
switch (state) {
case I_CmsConstants.C_STATE_NEW:
      style=C_STYLE_NEW;
    break;
case I_CmsConstants.C_STATE_CHANGED:
  style=C_STYLE_CHANGED;
break;
case I_CmsConstants.C_STATE_DELETED:
style=C_STYLE_DELETED;
break;
default :
style=C_STYLE_UNCHANGED;
break;
}
}
return style;
}
