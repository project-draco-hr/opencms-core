{
  Vector filterMethods=new Vector();
  Class cdClass=getContentDefinitionClass();
  try {
    filterMethods=(Vector)cdClass.getMethod("getFilterMethods",new Class[]{CmsObject.class}).invoke(null,new Object[]{cms});
  }
 catch (  InvocationTargetException ite) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice getContentHead: InvocationTargetException!");
    }
  }
catch (  NoSuchMethodException nsm) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice getContentHead: Requested method was not found!");
    }
  }
catch (  Exception e) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).info(getClassName() + ": Backoffice getContentHead: Problem occured with your filter methods: " + e.toString());
    }
  }
  return filterMethods;
}
