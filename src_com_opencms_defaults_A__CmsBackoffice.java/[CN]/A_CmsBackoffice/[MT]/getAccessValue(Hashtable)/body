{
  int accessFlag=0;
  for (int i=0; i <= 8; i++) {
    String permissionsAtI=(String)parameters.get("permission_" + i);
    if (permissionsAtI != null) {
      if (permissionsAtI.equals("on")) {
        accessFlag+=new Integer(this.C_ACCESS_FLAGS[i]).intValue();
      }
    }
  }
  if (accessFlag == 0) {
    accessFlag=this.C_DEFAULT_PERMISSIONS;
  }
  return accessFlag;
}
