{
  return new CmsResource(null,null,xmlPage.getRootPath() + "/" + locale+ "/"+ name+ "."+ EXTENSION_ELEMENT,CmsResourceTypePlain.getStaticTypeId(),false,xmlPage.getFlags(),xmlPage.getProjectLastModified(),xmlPage.getState(),xmlPage.getDateCreated(),xmlPage.getUserCreated(),xmlPage.getDateLastModified(),xmlPage.getUserLastModified(),xmlPage.getDateReleased(),xmlPage.getDateExpired(),xmlPage.getSiblingCount(),length);
}
