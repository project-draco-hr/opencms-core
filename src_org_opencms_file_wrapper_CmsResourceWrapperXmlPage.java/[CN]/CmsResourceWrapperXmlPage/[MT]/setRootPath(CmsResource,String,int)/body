{
  return new CmsResource(res.getStructureId(),res.getResourceId(),rootPath,typeId,res.isFolder(),res.getFlags(),res.getProjectLastModified(),res.getState(),res.getDateCreated(),res.getUserCreated(),res.getDateLastModified(),res.getUserLastModified(),res.getDateReleased(),res.getDateExpired(),res.getSiblingCount(),res.getLength());
}
