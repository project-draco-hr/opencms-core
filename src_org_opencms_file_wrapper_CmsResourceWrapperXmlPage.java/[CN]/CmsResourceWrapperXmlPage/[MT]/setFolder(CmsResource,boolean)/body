{
  String path=res.getRootPath();
  if (!path.endsWith("/")) {
    path+="/";
  }
  return new CmsResource(res.getStructureId(),res.getResourceId(),path,res.getTypeId(),isFolder,res.getFlags(),res.getProjectLastModified(),res.getState(),res.getDateCreated(),res.getUserCreated(),res.getDateLastModified(),res.getUserLastModified(),res.getDateReleased(),res.getDateExpired(),res.getSiblingCount(),res.getLength());
}
