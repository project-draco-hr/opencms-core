{
switch (galleryMode) {
case widget:
    CmsPreviewUtil.setResourcePath(m_infoBean.getResourcePath() + m_handler.getScaleParam());
  break;
case editor:
CmsPreviewUtil.setImage(CmsCoreProvider.get().link(m_infoBean.getResourcePath() + m_handler.getScaleParam()),m_handler.getImageAttributes());
break;
case ade:
case view:
default :
break;
}
}
