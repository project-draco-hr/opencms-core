{
  if (binaryData.length != 16) {
    return null;
  }
  char[] buffer=new char[32];
  for (int i=0; i < 16; i++) {
    int low=(binaryData[i] & 0x0f);
    int high=((binaryData[i] & 0xf0) >> 4);
    buffer[i * 2]=HEXADECIMAL[high];
    buffer[i * 2 + 1]=HEXADECIMAL[low];
  }
  return new String(buffer);
}
