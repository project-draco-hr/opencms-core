{
  CmsShell shell=new CmsShell();
  try {
    if ((args.length == 0) || (args.length > 3)) {
      shell.usage();
    }
 else {
      shell.init(args);
      shell.version();
      shell.initDb();
      shell=new CmsShell();
      args[0]="com.opencms.file.CmsInitMySql";
      shell.init(args);
      shell.initDb2();
    }
  }
 catch (  Exception exc) {
    System.out.println(exc);
    exc.printStackTrace();
  }
}
