{
  ResultSet result;
  try {
synchronized (m_statementReadTaskLog) {
      m_statementReadTaskLog.setInt(1,id);
      result=m_statementReadTaskLog.executeQuery();
      if (result.next()) {
        return new CmsTaskLog(result.getInt("id"),result.getString("comment"),result.getInt("task"),result.getInt("user"),result.getTimestamp("starttime"),result.getInt("type"));
      }
 else {
        return (null);
      }
    }
  }
 catch (  SQLException exc) {
    throw new CmsException(exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
