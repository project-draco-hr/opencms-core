{
  ResultSet res=null;
  int id=C_UNKNOWN_ID;
  try {
synchronized (m_statementGetLastInsertId) {
      res=m_statementGetLastInsertId.executeQuery();
      id=res.getInt(C_TASK_ID);
    }
  }
 catch (  SQLException e) {
    throw new CmsException(e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  return id;
}
