{
  Node node=document.selectSingleNode(xpath);
  if (node == null) {
    if (xpath.equals(getXPathsToUpdate().get(0))) {
      CmsSetupXmlHelper.setValue(document,xpath + "/@" + I_CmsXmlConfiguration.A_CLASS,CmsDetailPageResourceHandler.class.getName());
    }
 else {
      return false;
    }
    return true;
  }
  return false;
}
