{
  Vector users=new Vector();
  PreparedStatement stmt=null;
  ResultSet res=null;
  Connection conn=null;
  int i=0;
  if (lastname == null)   lastname="";
  try {
    conn=m_sqlManager.getConnection();
    if (wasLoggedIn == C_AT_LEAST_ONCE)     stmt=m_sqlManager.getPreparedStatement(conn,"C_USERS_GETUSERS_BY_LASTNAME_ONCE");
 else     if (wasLoggedIn == C_NEVER)     stmt=m_sqlManager.getPreparedStatement(conn,"C_USERS_GETUSERS_BY_LASTNAME_NEVER");
 else     stmt=m_sqlManager.getPreparedStatement(conn,"C_USERS_GETUSERS_BY_LASTNAME_WHATEVER");
    stmt.setString(1,lastname + "%");
    stmt.setInt(2,userType);
    stmt.setInt(3,userStatus);
    res=stmt.executeQuery();
    while (res.next() && (i++ < nMax)) {
      users.addElement(createCmsUserFromResultSet(res,true));
    }
  }
 catch (  SQLException e) {
    throw m_sqlManager.getCmsException(this,null,CmsException.C_SQL_ERROR,e);
  }
catch (  Exception e) {
    throw m_sqlManager.getCmsException(this,null,CmsException.C_UNKNOWN_EXCEPTION,e);
  }
 finally {
    m_sqlManager.closeAll(conn,stmt,res);
  }
  return users;
}
