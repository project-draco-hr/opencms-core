{
  if (column == null) {
    return null;
  }
  if (column.equals(LIST_COLUMN_FULLNAME)) {
    return SortKey.fullName;
  }
 else   if (column.equals(LIST_COLUMN_NAME)) {
    return SortKey.loginName;
  }
 else   if (column.equals(LIST_COLUMN_ORGUNIT)) {
    return SortKey.orgUnit;
  }
  return null;
}
