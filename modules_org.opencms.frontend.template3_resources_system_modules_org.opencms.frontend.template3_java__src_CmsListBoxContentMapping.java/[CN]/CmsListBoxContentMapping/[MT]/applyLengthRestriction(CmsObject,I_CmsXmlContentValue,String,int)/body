{
  if (value.length() <= maxLength) {
    return value;
  }
  String result;
  if (xmlContentValue instanceof CmsXmlHtmlValue) {
    result=xmlContentValue.getPlainText(cms);
  }
 else {
    result=value;
  }
  if (result.length() > maxLength) {
    result=CmsStringUtil.trimToSize(result,maxLength);
  }
  return result;
}
