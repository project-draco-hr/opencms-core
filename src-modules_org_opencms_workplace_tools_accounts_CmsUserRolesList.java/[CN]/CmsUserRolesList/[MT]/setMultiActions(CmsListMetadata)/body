  removeMultiAction.setConfirmationMessage(Messages.get().container(Messages.GUI_USERROLES_LIST_MACTION_REMOVE_CONF_0));
  removeMultiAction.setIconPath(ICON_MULTI_DEACTIVATE);
  metadata.addMultiAction(removeMultiAction);
}
