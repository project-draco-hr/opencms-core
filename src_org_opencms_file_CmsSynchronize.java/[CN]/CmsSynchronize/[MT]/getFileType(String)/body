{
  String suffix=filename.substring(filename.lastIndexOf('.') + 1);
  suffix=suffix.toLowerCase();
  Hashtable extensions=m_cms.readFileExtensions();
  String resType=new String();
  if (extensions != null) {
    resType=(String)extensions.get(suffix);
  }
  if (resType == null) {
    resType="plain";
  }
  return resType;
}
