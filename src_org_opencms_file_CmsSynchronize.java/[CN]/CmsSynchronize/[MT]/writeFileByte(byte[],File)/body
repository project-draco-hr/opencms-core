{
  FileOutputStream fOut=null;
  DataOutputStream dOut=null;
  try {
    fOut=new FileOutputStream(file);
    dOut=new DataOutputStream(fOut);
    dOut.write(content);
    dOut.flush();
  }
 catch (  IOException e) {
    throw e;
  }
 finally {
    try {
      if (fOut != null)       fOut.close();
    }
 catch (    IOException e) {
    }
  }
}
