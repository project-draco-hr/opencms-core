{
  List interfaceList=new ArrayList();
  try {
    Hashtable syncmodificationNode=m_cms.getRegistry().getSystemValues("syncmodification");
    if (syncmodificationNode != null) {
      for (int i=1; i <= syncmodificationNode.size(); i++) {
        String currentClass=(String)syncmodificationNode.get("class" + i);
        try {
          interfaceList.add(Class.forName(currentClass).newInstance());
          if (C_LOGGING && A_OpenCms.isLogging(C_OPENCMS_INFO))           A_OpenCms.log(C_OPENCMS_INFO,". CmsSyncModification class init : " + currentClass + " instanciated");
        }
 catch (        Exception e1) {
          if (C_LOGGING && A_OpenCms.isLogging(C_OPENCMS_INFO))           A_OpenCms.log(C_OPENCMS_INFO,". CmsSyncModification class init : non-critical error " + e1.toString());
        }
      }
    }
  }
 catch (  Exception e2) {
    if (C_LOGGING && A_OpenCms.isLogging(C_OPENCMS_INFO))     A_OpenCms.log(C_OPENCMS_INFO,". CmsSyncModification  init : non-critical error " + e2.toString());
  }
  return interfaceList;
}
