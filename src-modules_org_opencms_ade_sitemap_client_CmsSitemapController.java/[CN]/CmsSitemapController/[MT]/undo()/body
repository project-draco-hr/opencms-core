{
  if (!isDirty()) {
    return;
  }
  if (m_undone.isEmpty()) {
    getToolbar().getRedoButton().setEnabled(true);
  }
  I_CmsSitemapChange change=m_changes.remove(m_changes.size() - 1);
  m_undone.add(change);
  if (m_handler != null) {
    m_handler.onChange(change);
  }
  if (!isDirty()) {
    internalReset(false);
  }
}
