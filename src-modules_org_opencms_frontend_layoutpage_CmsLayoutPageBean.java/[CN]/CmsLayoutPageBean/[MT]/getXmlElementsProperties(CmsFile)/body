{
  CmsConfigurationParameter properties=new CmsConfigurationParameter();
  try {
    String typeName=OpenCms.getResourceManager().getResourceType(linkToFile.getTypeId()).getTypeName();
    if (m_typeMappings.get(typeName) == null) {
      properties.load(new ByteArrayInputStream(getCmsObject().readFile(CmsWorkplace.VFS_PATH_MODULES + MODULE_NAME + "/mappings/"+ typeName+ ".properties").getContents()));
      m_typeMappings.put(typeName,properties);
    }
 else {
      properties=(CmsConfigurationParameter)m_typeMappings.get(typeName);
    }
  }
 catch (  Exception e) {
  }
  return properties;
}
