{
  CmsGroup role=m_dbAccess.readGroup(roleName);
  java.sql.Timestamp timestamp=new java.sql.Timestamp(timeout);
  java.sql.Timestamp now=new java.sql.Timestamp(System.currentTimeMillis());
  return m_dbAccess.createTask(currentProject.getTaskId(),currentProject.getTaskId(),1,currentUser.getId(),C_UNKNOWN_ID,role.getId(),taskname,now,timestamp,priority);
}
