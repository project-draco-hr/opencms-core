{
  List<String> result=new ArrayList<String>();
  try {
    Query q=m_sqlManager.createQuery(dbc,C_PROJECTRESOURCES_READ_BY_ID_1);
    q.setParameter(1,project.getUuid().toString());
    @SuppressWarnings("unchecked") List<String> res=q.getResultList();
    for (    String r : res) {
      result.add(r);
    }
  }
 catch (  PersistenceException e) {
    throw new CmsDataAccessException(Messages.get().container(Messages.ERR_JPA_PERSITENCE,e),e);
  }
  return result;
}
