{
  String type=null;
  CmsSolrIndex index=CmsSearchManager.getIndexSolr(cms,null);
  if (index != null) {
    I_CmsSearchDocument doc=index.getDocument(I_CmsSearchField.FIELD_PATH,rootPath);
    if (doc != null) {
      type=doc.getFieldValueAsString(I_CmsSearchField.FIELD_TYPE);
    }
  }
  return type;
}
