{
  return (tagName.equals("BODY") || tagName.equals("HEAD") || tagName.equals("HTML")|| tagName.equals("P")|| tagName.equals("DT")|| tagName.equals("DD")|| tagName.equals("LI")|| tagName.equals("OPTION")|| tagName.equals("THEAD")|| tagName.equals("TFOOT")|| tagName.equals("TBODY")|| tagName.equals("TR")|| tagName.equals("COLGROUP")|| tagName.equals("TH")|| tagName.equals("TD"));
}
