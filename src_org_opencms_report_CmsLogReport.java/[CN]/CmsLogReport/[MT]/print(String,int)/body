{
switch (format) {
case FORMAT_HEADLINE:
    m_buffer.append("[ ");
  m_buffer.append(value);
m_buffer.append(" ]");
break;
case FORMAT_WARNING:
m_buffer.append("!!! ");
m_buffer.append(value);
m_buffer.append(" !!!");
addWarning(value);
break;
case FORMAT_ERROR:
m_buffer.append("!!! ");
m_buffer.append(value);
m_buffer.append(" !!!");
addError(value);
break;
case FORMAT_NOTE:
case FORMAT_OK:
case FORMAT_DEFAULT:
default :
m_buffer.append(value);
}
}
