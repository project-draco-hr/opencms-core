{
switch (format) {
case C_FORMAT_HEADLINE:
    m_buffer.append("[ ");
  m_buffer.append(value);
m_buffer.append(" ]");
break;
case C_FORMAT_WARNING:
m_buffer.append("!!! ");
m_buffer.append(value);
m_buffer.append(" !!!");
break;
case C_FORMAT_ERROR:
m_buffer.append("!!! ");
m_buffer.append(value);
m_buffer.append(" !!!");
addError(value);
break;
case C_FORMAT_NOTE:
case C_FORMAT_OK:
case C_FORMAT_DEFAULT:
default :
m_buffer.append(value);
}
}
