{
  if (!DIALOG_WAIT.equals(getParamAction())) {
    try {
      CmsFolder folder=getCms().readFolder(getParamResource(),true);
      if (folder.isFolder()) {
        return false;
      }
    }
 catch (    CmsException e) {
    }
  }
  getCms().undeleteResource(getParamResource());
  return true;
}
