{
  if (filterName == null) {
    filterName=m_filterNames[0];
  }
switch (m_filters.indexOf(filterName)) {
case 0:
    return null;
case 1:
  return getCreateInFolder(cms,param);
case 2:
return getCreateInFolder(cms,param);
case 3:
return null;
case 4:
return null;
default :
throw new CmsException("Invalid XML content filter selected: " + filterName);
}
}
