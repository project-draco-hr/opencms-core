{
  CmsPathSelectField field=new CmsPathSelectField();
  field.setUseRootPaths(true);
  if (value != null) {
    field.setValue(value);
  }
  CmsRemovableFormRow<CmsPathSelectField> row=new CmsRemovableFormRow<CmsPathSelectField>(field,"Remove resource");
  row.setCaption("Resource");
  m_resources.addComponent(row);
}
