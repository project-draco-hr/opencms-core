{
  if (resourceUtil[0].isInsideProject()) {
    CmsLock lock=resourceUtil[0].getLock();
    return (!resourceUtil[0].getProjectState().isLockedForPublishing() && !lock.isShared() && lock.isOwnedInProjectBy(cms.getRequestContext().currentUser(),cms.getRequestContext().currentProject()));
  }
  return false;
}
