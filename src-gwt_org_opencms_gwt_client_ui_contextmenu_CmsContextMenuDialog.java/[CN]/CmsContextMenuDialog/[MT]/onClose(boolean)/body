{
  if (m_dialog != null) {
    m_dialog.hide();
    m_dialog=null;
  }
  if (m_form != null) {
    m_form.removeFromParent();
    m_form=null;
  }
  if (reload) {
    Scheduler.get().scheduleDeferred(new ScheduledCommand(){
      public void execute(){
        String url=Window.Location.getHref();
        m_menuHandler.leavePage(url);
      }
    }
);
  }
}
