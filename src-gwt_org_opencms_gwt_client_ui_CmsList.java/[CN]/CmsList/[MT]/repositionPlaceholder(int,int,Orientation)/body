{
switch (orientation) {
case HORIZONTAL:
    m_placeholderIndex=CmsDomUtil.positionElementInside(m_placeholder,getElement(),m_placeholderIndex,x,-1);
  break;
case VERTICAL:
m_placeholderIndex=CmsDomUtil.positionElementInside(m_placeholder,getElement(),m_placeholderIndex,-1,y);
break;
case ALL:
default :
m_placeholderIndex=CmsDomUtil.positionElementInside(m_placeholder,getElement(),m_placeholderIndex,x,y);
break;
}
}
