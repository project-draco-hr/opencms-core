{
  CmsRpcAction<CmsContainerElementData> action=new CmsRpcAction<CmsContainerElementData>(){
    /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#execute()
 */
    @Override public void execute(){
      start(200,false);
      getContainerpageService().getElementWithProperties(CmsContainerpageController.getCurrentUri(),getRequestParams(),clientId,properties,m_containerBeans,getLocale(),this);
    }
    /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#onResponse(java.lang.Object)
 */
    @Override protected void onResponse(    CmsContainerElementData result){
      stop(false);
      callback.execute(result);
    }
  }
;
  action.execute();
}
