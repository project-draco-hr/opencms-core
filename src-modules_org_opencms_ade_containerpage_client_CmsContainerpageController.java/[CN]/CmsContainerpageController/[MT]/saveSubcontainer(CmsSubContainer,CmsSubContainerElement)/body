{
  if (getSubcontainer() != null) {
    CmsRpcAction<Map<String,CmsContainerElementData>> action=new CmsRpcAction<Map<String,CmsContainerElementData>>(){
      /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#execute()
 */
      @Override public void execute(){
        getContainerpageService().saveSubContainer(getCurrentUri(),getRequestParams(),subContainer,m_containerBeans,this);
      }
      /** 
 * @see org.opencms.gwt.client.rpc.CmsRpcAction#onResponse(java.lang.Object)
 */
      @Override protected void onResponse(      Map<String,CmsContainerElementData> result){
        m_elements.putAll(result);
        CmsNotification.get().send(Type.NORMAL,Messages.get().key(Messages.GUI_NOTIFICATION_SUB_CONTAINER_SAVED_0));
      }
    }
;
    action.execute();
  }
}
