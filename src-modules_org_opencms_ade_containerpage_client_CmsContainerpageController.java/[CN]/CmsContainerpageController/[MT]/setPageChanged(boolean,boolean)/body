{
  if (changed) {
    if (!m_pageChanged) {
      m_pageChanged=changed;
      if (lockContainerpage()) {
        m_handler.enableSaveReset(!isEditingDisabled());
      }
    }
  }
 else {
    m_pageChanged=changed;
    m_handler.enableSaveReset(false);
    if (unlock) {
      unlockContainerpage();
    }
  }
}
