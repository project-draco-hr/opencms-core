{
  if (m_lockStatus == LockStatus.locked) {
    return true;
  }
  if (m_lockStatus == LockStatus.failed) {
    return false;
  }
  String errorMessage=CmsCoreProvider.get().lockAndCheckModification(getCurrentUri(),m_data.getDateLastModified(),false);
  if (errorMessage == null) {
    onLockSuccess();
    return true;
  }
 else {
    onLockFail(errorMessage);
    return false;
  }
}
