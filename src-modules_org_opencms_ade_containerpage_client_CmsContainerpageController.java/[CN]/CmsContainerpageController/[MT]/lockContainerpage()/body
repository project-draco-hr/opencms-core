{
  if (m_lockStatus == LockStatus.locked) {
    return true;
  }
  if (m_lockStatus == LockStatus.failed) {
    return false;
  }
  CmsLockInfo lockInfo=CmsCoreProvider.get().lockTempAndCheckModification(getCurrentUri(),m_data.getDateLastModified());
  if (lockInfo.couldLock()) {
    onLockSuccess(lockInfo);
    return true;
  }
 else {
    onLockFail(lockInfo);
    return false;
  }
}
