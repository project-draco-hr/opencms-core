{
  String strKey=getStrKey(value);
  if (strKey != null) {
    if (cache.size() < max_objects) {
      cache.put(strKey,(new CmsCachedObject(value)).clone());
    }
 else {
      removeLRU();
      cache.put(strKey,(new CmsCachedObject(value)).clone());
    }
    index.put(new Integer(key),strKey);
  }
}
