{
  DefaultFormatters[] formatters=DefaultFormatters.values();
  for (int i=0; i < formatters.length; i++) {
    if (formatters[i].getKey().equals(key)) {
      return formatters[i];
    }
  }
  throw new IllegalArgumentException(Messages.get().getBundle().key(Messages.ERR_DEFAULT_FORMATTER_NOT_DEFINED_1,key));
}
