{
  getConfigFile(cms);
  filelistTemplate.fastSetXmlData(C_FILELIST_ICON_VALUE,CmsXmlTemplateLoader.getRequest(cms.getRequestContext()).getServletUrl() + getGalleryIconPath(cms));
  filelistTemplate.setData(C_FILELIST_NAME_VALUE,res.getName());
  filelistTemplate.setData(C_FILELIST_TITLE_VALUE,cms.readProperty(cms.getSitePath(res),C_PROPERTY_TITLE));
}
