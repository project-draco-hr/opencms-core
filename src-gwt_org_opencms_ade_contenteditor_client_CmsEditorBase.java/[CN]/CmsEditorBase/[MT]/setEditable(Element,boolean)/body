{
  List<Element> children=Vie.getInstance().select("[property^=\"opencms://\"]",element);
  if (children.size() > 0) {
    for (    Element child : children) {
      if (editable) {
        child.setAttribute("contentEditable","true");
      }
 else {
        child.removeAttribute("contentEditable");
      }
    }
    return true;
  }
  return false;
}
