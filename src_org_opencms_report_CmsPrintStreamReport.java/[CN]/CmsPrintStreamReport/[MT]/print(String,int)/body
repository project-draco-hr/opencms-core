{
  if (m_writeHtml) {
    super.print(value,format);
    m_printStream.print(super.getReportUpdate());
    return;
  }
  StringBuffer buf;
switch (format) {
case FORMAT_HEADLINE:
    buf=new StringBuffer();
  buf.append("------ ");
buf.append(value);
m_printStream.print(buf);
break;
case FORMAT_WARNING:
buf=new StringBuffer();
buf.append("!!! ");
buf.append(value);
m_printStream.print(buf);
addWarning(value);
break;
case FORMAT_ERROR:
buf=new StringBuffer();
buf.append("!!! ");
buf.append(value);
m_printStream.print(buf);
addError(value);
break;
case FORMAT_NOTE:
case FORMAT_OK:
case FORMAT_DEFAULT:
default :
m_printStream.print(value);
}
}
