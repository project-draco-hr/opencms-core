{
  String currentSite=null;
  Hashtable userinfo=getUser(sessionId);
  if (userinfo != null) {
    currentSite=(String)userinfo.get(I_CmsConstants.C_SESSION_CURRENTSITE);
  }
  if (currentSite == null) {
    return OpenCms.getSiteManager().getDefaultSite().getSiteRoot();
  }
 else {
    return currentSite;
  }
}
