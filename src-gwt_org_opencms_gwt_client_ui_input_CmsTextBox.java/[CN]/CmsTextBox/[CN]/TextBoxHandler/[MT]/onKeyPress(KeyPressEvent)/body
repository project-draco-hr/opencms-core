{
  int keyCode=event.getNativeEvent().getKeyCode();
  if (!isNavigationKey(keyCode)) {
    setGhostMode(false);
  }
  if (isTriggerChangeOnKeyPress()) {
    Scheduler.get().scheduleDeferred(new ScheduledCommand(){
      public void execute(){
        checkForChange(m_inhibitValidationForKeypresses);
      }
    }
);
  }
}
