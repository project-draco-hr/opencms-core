{
  if (newValue == null) {
    newValue="";
  }
  if ("".equals(newValue) && (m_ghostValue != null)) {
    m_ghostMode=true;
    setGhostStyleEnabled(true,true);
    m_textbox.setValue(m_ghostValue);
  }
 else {
    setGhostMode(false);
    setGhostStyleEnabled(false,true);
    setFormValue(newValue);
  }
}
