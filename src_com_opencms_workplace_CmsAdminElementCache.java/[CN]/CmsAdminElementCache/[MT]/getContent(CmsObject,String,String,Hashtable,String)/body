{
  if (com.opencms.boot.I_CmsLogChannels.C_PREPROCESSOR_IS_LOGGING && C_DEBUG && A_OpenCms.isLogging()) {
    A_OpenCms.log(C_OPENCMS_DEBUG,this.getClassName() + "getting content of element " + ((elementName == null) ? "<root>" : elementName));
    A_OpenCms.log(C_OPENCMS_DEBUG,this.getClassName() + "template file is: " + templateFile);
    A_OpenCms.log(C_OPENCMS_DEBUG,this.getClassName() + "selected template section is: " + ((templateSelector == null) ? "<default>" : templateSelector));
  }
  CmsXmlWpTemplateFile xmlTemplateDocument=new CmsXmlWpTemplateFile(cms,templateFile);
  CmsElementCache cache=cms.getOnlineElementCache();
  String action=(String)parameters.get("action");
  if ((action == null) || ("".equals(action))) {
    Vector cacheInfo=cache.getCacheInfo();
    xmlTemplateDocument.setData("urisize",((Integer)cacheInfo.elementAt(1)).toString() + " | " + ((Integer)cacheInfo.elementAt(0)).toString());
    xmlTemplateDocument.setData("elementsize",((Integer)cacheInfo.elementAt(3)).toString() + " | " + ((Integer)cacheInfo.elementAt(2)).toString());
  }
 else {
    cache.clearCache();
    templateSelector="done";
  }
  return startProcessing(cms,xmlTemplateDocument,elementName,parameters,templateSelector);
}
