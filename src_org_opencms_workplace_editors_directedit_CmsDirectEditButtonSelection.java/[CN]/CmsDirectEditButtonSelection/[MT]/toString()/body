{
  if (m_stringValue == null) {
    StringBuffer result=new StringBuffer(32);
    if (m_showEdit) {
      result.append(VALUE_EDIT);
    }
    result.append('|');
    if (m_showDelete) {
      result.append(VALUE_DELETE);
    }
    result.append('|');
    if (m_showNew) {
      result.append(VALUE_NEW);
    }
    m_stringValue=result.toString();
  }
  return m_stringValue;
}
