{
  if (n == root) {
    return null;
  }
  Node nextnode=null;
  Node parent=null;
  nextnode=n.getNextSibling();
  parent=n.getParentNode();
  while (nextnode == null && parent != null && parent != root) {
    nextnode=parent.getNextSibling();
    parent=parent.getParentNode();
  }
  return nextnode;
}
