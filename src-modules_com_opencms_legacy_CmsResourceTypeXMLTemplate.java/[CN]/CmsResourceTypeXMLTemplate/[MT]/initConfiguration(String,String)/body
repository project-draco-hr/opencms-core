{
  if ((OpenCms.getRunLevel() > OpenCms.RUNLEVEL_2_INITIALIZING) && m_staticFrozen) {
    throw new CmsConfigurationException("Resource type " + this.getClass().getName() + " with static name='"+ getStaticTypeName()+ "' static id='"+ getStaticTypeId()+ "' can't be reconfigured");
  }
  if (!C_RESOURCE_TYPE_NAME.equals(name)) {
    throw new CmsConfigurationException("Resource type " + this.getClass().getName() + " must be configured with resource type name '"+ C_RESOURCE_TYPE_NAME+ "' (not '"+ name+ "')");
  }
  m_staticFrozen=true;
  super.initConfiguration(C_RESOURCE_TYPE_NAME,id);
  m_staticTypeId=m_typeId;
}
