{
  if (m_app == null) {
    if (!m_appConfig.getVisibility(A_CmsUI.getCmsObject()).isActive()) {
      m_app=new DummyApp();
    }
 else {
      m_app=m_appConfig.getAppInstance();
    }
    CmsAppViewLayout layout=new CmsAppViewLayout();
    layout.setAppTitle(m_appConfig.getName(UI.getCurrent().getLocale()));
    m_app.initUI(layout);
    return layout;
  }
  return null;
}
