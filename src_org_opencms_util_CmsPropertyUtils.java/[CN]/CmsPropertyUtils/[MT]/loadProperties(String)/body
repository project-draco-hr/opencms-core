{
  FileInputStream input=null;
  ExtendedProperties properties=null;
  try {
    input=new FileInputStream(new File(file));
    properties=new ExtendedProperties();
    properties.load(input);
    input.close();
  }
 catch (  IOException e) {
    try {
      if (input != null) {
        input.close();
      }
    }
 catch (    Exception ex) {
    }
    throw e;
  }
  unescapeProperties(properties);
  return properties;
}
