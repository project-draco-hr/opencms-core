{
  if (resourceUtil[0].isInsideProject()) {
    CmsLock lock=resourceUtil[0].getLock();
    return !(!resourceUtil[0].getProjectState().isLockedForPublishing() && !lock.isNullLock() && lock.isExclusiveOwnedInProjectBy(cms.getRequestContext().currentUser(),cms.getRequestContext().currentProject()));
  }
  return false;
}
