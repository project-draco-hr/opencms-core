{
  Locale theLocale=locale;
  if (!m_sitemaps.containsKey(theLocale)) {
    LOG.warn(Messages.get().container(Messages.LOG_SITEMAP_LOCALE_NOT_FOUND_2,cms.getSitePath(getFile()),theLocale.toString()).key());
    theLocale=OpenCms.getLocaleManager().getDefaultLocales(cms,getFile()).get(0);
    if (!m_sitemaps.containsKey(theLocale)) {
      LOG.error(Messages.get().container(Messages.LOG_SITEMAP_LOCALE_NOT_FOUND_2,cms.getSitePath(getFile()),theLocale).key());
      return null;
    }
  }
  return m_sitemaps.get(theLocale);
}
