{
  Locale theLocale=locale;
  CmsSitemapBean result=m_sitemaps.get(theLocale);
  if (result != null) {
    return result;
  }
  LOG.warn(Messages.get().container(Messages.LOG_SITEMAP_LOCALE_NOT_FOUND_2,getFile().getRootPath(),theLocale.toString()).key());
  theLocale=OpenCms.getLocaleManager().getDefaultLocale(cms,getFile());
  result=m_sitemaps.get(theLocale);
  if (result != null) {
    return result;
  }
  if (getLocales().isEmpty()) {
    LOG.error(Messages.get().container(Messages.LOG_SITEMAP_LOCALE_NOT_FOUND_2,getFile().getRootPath(),theLocale).key());
    return null;
  }
  return m_sitemaps.get(getLocales().get(0));
}
