{
  Element entryElement=getElement(cms,change.getSitePath());
  if (entryElement == null) {
    throw createEntryNotFoundException(cms,change.getSitePath());
  }
  entryElement.detach();
  return getSubTreePaths(CmsResource.getParentFolder(cms.getRequestContext().addSiteRoot(change.getSitePath())),entryElement);
}
