{
  ArrayList result=new ArrayList();
  if (value == null) {
    return result;
  }
  String[] names=CmsStringUtil.splitAsArray(value,';');
  for (int i=0; i < names.length; i++) {
    String name=names[i].trim();
    if (CmsStringUtil.isNotEmpty(name)) {
      result.add(name);
    }
  }
  return result;
}
