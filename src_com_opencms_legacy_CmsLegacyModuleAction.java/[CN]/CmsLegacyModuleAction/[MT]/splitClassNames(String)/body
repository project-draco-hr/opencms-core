{
  ArrayList result=new ArrayList();
  if (value == null) {
    return result;
  }
  String[] names=CmsStringUtil.split(value,";");
  for (int i=0; i < names.length; i++) {
    String name=names[i].trim();
    if (!CmsStringUtil.isEmpty(name)) {
      result.add(name);
    }
  }
  return result;
}
