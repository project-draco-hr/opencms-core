{
  if (m_plainTextValue == null) {
    try {
      m_plainTextValue=CmsHtmlExtractor.extractText(this.getStringValue(cms),m_document.getEncoding());
    }
 catch (    Exception exc) {
      m_plainTextValue=NULL_VALUE;
    }
  }
  if (m_plainTextValue == NULL_VALUE) {
    return null;
  }
  return m_plainTextValue;
}
