{
  StringBuffer previewRow=new StringBuffer();
  previewRow.append("<tr align=\"left\">\n");
  previewRow.append("<td>\n<b>");
  previewRow.append(column1);
  previewRow.append("</b>\n</td>\n");
  previewRow.append("<td>\n");
