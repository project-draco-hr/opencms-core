{
  Vector resources=new Vector();
  CmsFile file;
  CmsFolder folder;
  ResultSet res;
  try {
    PreparedStatement statementProjectReadFiles=m_Con.prepareStatement(C_PROJECT_READ_FILES);
    statementProjectReadFiles.setInt(1,project.getId());
    res=statementProjectReadFiles.executeQuery();
    while (res.next()) {
      file=new CmsFile(res.getString(C_RESOURCE_NAME),res.getInt(C_RESOURCE_TYPE),res.getInt(C_RESOURCE_FLAGS),res.getInt(C_USER_ID),res.getInt(C_GROUP_ID),onlineProject.getId(),res.getInt(C_ACCESS_FLAGS),res.getInt(C_STATE),res.getInt(C_LOCKED_BY),res.getInt(C_LAUNCHER_TYPE),res.getString(C_LAUNCHER_CLASSNAME),res.getLong(C_DATE_CREATED),res.getLong(C_DATE_LASTMODIFIED),res.getBytes(C_FILE_CONTENT),res.getInt(C_SIZE));
      if ((file.getState() == C_STATE_CHANGED) || (file.getState() == C_STATE_NEW)) {
        deleteFile(file);
        createFile(onlineProject,onlineProject,file,file.getAbsolutePath());
        resources.addElement(file.getAbsolutePath());
      }
 else       if (file.getState() == C_STATE_DELETED) {
        deleteFile(file);
        resources.addElement(file.getAbsolutePath());
      }
    }
    PreparedStatement statementProjectReadFolders=m_Con.prepareStatement(C_PROJECT_READ_FOLDER);
    statementProjectReadFolders.setInt(1,project.getId());
    statementProjectReadFolders.setInt(2,C_TYPE_FOLDER);
    res=statementProjectReadFolders.executeQuery();
    while (res.next()) {
      folder=new CmsFolder(res.getString(C_RESOURCE_NAME),res.getInt(C_RESOURCE_TYPE),res.getInt(C_RESOURCE_FLAGS),res.getInt(C_USER_ID),res.getInt(C_GROUP_ID),onlineProject.getId(),res.getInt(C_ACCESS_FLAGS),res.getInt(C_STATE),res.getInt(C_LOCKED_BY),res.getLong(C_DATE_CREATED),res.getLong(C_DATE_LASTMODIFIED));
      if ((folder.getState() == C_STATE_CHANGED) || (folder.getState() == C_STATE_NEW) || (folder.getState() == C_STATE_UNCHANGED)) {
        deleteFolder(folder);
        createFolder(onlineProject,folder,folder.getAbsolutePath());
        resources.addElement(folder.getAbsolutePath());
      }
 else       if (folder.getState() == C_STATE_DELETED) {
        deleteFolder(folder);
        resources.addElement(folder.getAbsolutePath());
      }
    }
  }
 catch (  SQLException e) {
    throw new CmsException("[" + this.getClass().getName() + "]"+ e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  return resources;
}
