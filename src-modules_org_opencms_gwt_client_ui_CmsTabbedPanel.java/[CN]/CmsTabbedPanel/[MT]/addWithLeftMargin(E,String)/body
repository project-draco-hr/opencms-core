{
  tabContent.addStyleName(I_CmsLayoutBundle.INSTANCE.generalCss().cornerAll());
  m_tabPanel.add(tabContent,tabName);
  int tabIndex=m_tabPanel.getWidgetIndex(tabContent);
  Element tabElement=getTabElement(tabIndex);
  if (tabElement != null) {
    tabElement.addClassName(I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().tabLeftMargin() + " " + I_CmsLayoutBundle.INSTANCE.generalCss().buttonCornerAll()+ " "+ I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().borderAll());
  }
}
