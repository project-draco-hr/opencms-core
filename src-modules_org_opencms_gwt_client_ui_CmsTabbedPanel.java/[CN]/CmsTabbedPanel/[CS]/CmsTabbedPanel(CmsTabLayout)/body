{
  m_tabPanel=new TabLayoutPanel(tabbarHeight.getBarHeight(),Unit.PX);
  initWidget(m_tabPanel);
  Element tabRootEl=m_tabPanel.getElement();
  List<Element> tabBarDivs=CmsDomUtil.getElementsByClass(I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().cmsTabLayoutPanelTabs(),CmsDomUtil.Tag.div,tabRootEl);
  if (tabBarDivs.size() == 1) {
    tabBarDivs.get(0).getParentElement().setClassName(I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().cmsTabLayoutPanelTabBar());
  }
  m_tabPanel.setStyleName(I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().cmsTabLayoutPanel());
}
