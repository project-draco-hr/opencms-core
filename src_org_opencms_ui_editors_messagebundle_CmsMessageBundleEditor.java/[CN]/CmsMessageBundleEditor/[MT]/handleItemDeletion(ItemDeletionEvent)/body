{
  Item it=m_table.getItem(e.getItemId());
  Property<?> keyProp=it.getItemProperty(TableProperty.KEY);
  String key=(String)keyProp.getValue();
  if (m_model.handleKeyDeletion(key)) {
    return true;
  }
  showWarning(Messages.GUI_NOTIFICATION_MESSAGEBUNDLEEDITOR_REMOVE_ENTRY_FAILED_CAPTION_0,Messages.GUI_NOTIFICATION_MESSAGEBUNDLEEDITOR_REMOVE_ENTRY_FAILED_DESCRIPTION_0);
  return false;
}
