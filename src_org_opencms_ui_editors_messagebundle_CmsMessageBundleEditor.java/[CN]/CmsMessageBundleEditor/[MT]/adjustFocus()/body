{
  Map<Integer,TextField> firstEditableCol=m_fieldFactories.get(m_model.getEditMode()).getValueFields().get(Integer.valueOf(1));
  if (null != firstEditableCol) {
    TextField firstTextField=firstEditableCol.get(Integer.valueOf(1));
    if (null != firstTextField) {
      firstTextField.focus();
    }
  }
}
