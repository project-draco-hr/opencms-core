{
  List errors=new ArrayList();
  try {
    m_searchManager.removeSearchIndexSource(m_indexsource);
    clearDialogObject();
    writeConfiguration();
    setParamCloseLink(CmsToolManager.linkForToolPath(getJsp(),"/searchindex/indexsources"));
  }
 catch (  Throwable t) {
    errors.add(t);
  }
  setCommitErrors(errors);
}
