{
  if (o1 == o2) {
    return true;
  }
  if ((o1 == null) || (o2 == null)) {
    return false;
  }
  if (!o1.getLocation().equals(o2.getLocation())) {
    return false;
  }
  if (!o1.getText().equals(o2.getText())) {
    return false;
  }
  if (!o1.getType().equals(o2.getType())) {
    return false;
  }
  return true;
}
