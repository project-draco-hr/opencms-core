{
  CmsObject cms=getCmsObject();
  Map<String,CmsXmlContentProperty> propertiesConf=m_manager.getElementPropertyConfiguration(cms,cms.readResource(structureId));
  Map<String,String> cnfProps=new HashMap<String,String>();
  if (properties != null) {
    Iterator<String> itProperties=properties.keys();
    while (itProperties.hasNext()) {
      String propertyName=itProperties.next();
      cnfProps.put(propertyName,CmsXmlContentProperty.getPropValueIds(cms,propertiesConf.get(propertyName).getPropertyType(),properties.getString(propertyName)));
    }
  }
  return new CmsContainerElementBean(structureId,null,cnfProps);
}
