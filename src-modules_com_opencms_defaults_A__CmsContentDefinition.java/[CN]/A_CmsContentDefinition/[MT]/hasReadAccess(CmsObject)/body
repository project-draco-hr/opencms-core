{
  CmsUser currentUser=cms.getRequestContext().currentUser();
  if (!accessOther(com.opencms.core.I_CmsConstants.C_ACCESS_PUBLIC_READ) && !accessOwner(cms,currentUser,CmsPermissionSet.PERMISSION_READ) && !accessGroup(cms,currentUser,com.opencms.core.I_CmsConstants.C_ACCESS_GROUP_READ)) {
    return false;
  }
  return true;
}
