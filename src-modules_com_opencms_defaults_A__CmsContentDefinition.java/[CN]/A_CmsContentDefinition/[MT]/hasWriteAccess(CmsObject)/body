{
  CmsUser currentUser=cms.getRequestContext().currentUser();
  if (isLockable() && (!getLockstate().equals(currentUser.getId()))) {
    return false;
  }
  if (!(accessOther(com.opencms.core.I_CmsConstants.C_ACCESS_PUBLIC_WRITE) || accessOwner(cms,currentUser,CmsPermissionSet.PERMISSION_WRITE) || accessGroup(cms,currentUser,com.opencms.core.I_CmsConstants.C_ACCESS_GROUP_WRITE))) {
    return false;
  }
  return true;
}
