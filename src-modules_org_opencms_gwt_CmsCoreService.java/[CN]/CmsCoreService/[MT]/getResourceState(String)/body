{
  CmsObject cms=getCmsObject();
  CmsResourceState result=null;
  try {
    try {
      CmsResource res=cms.readResource(path);
      result=res.getState();
    }
 catch (    CmsVfsResourceNotFoundException e) {
      result=CmsResourceState.STATE_DELETED;
    }
  }
 catch (  CmsException e) {
    error(e);
  }
  return result;
}
