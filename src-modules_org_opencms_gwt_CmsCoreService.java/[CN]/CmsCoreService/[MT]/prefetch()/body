{
  CmsObject cms=getCmsObject();
  String navigationUri=cms.getRequestContext().getUri();
  String uploadUri=OpenCms.getLinkManager().substituteLinkForUnknownTarget(cms,CmsUploadBean.UPLOAD_JSP_URI);
  long uploadFileSizeLimit=OpenCms.getWorkplaceManager().getFileBytesMaxUploadSize(getCmsObject());
  CmsCoreData data=new CmsCoreData(EDITOR_URI,BACKLINK_URI,OpenCms.getSystemInfo().getOpenCmsContext(),cms.getRequestContext().getSiteRoot(),cms.getRequestContext().getLocale().toString(),OpenCms.getWorkplaceManager().getWorkplaceLocale(cms).toString(),cms.getRequestContext().getUri(),navigationUri,new HashMap<String,String>(OpenCms.getResourceManager().getExtensionMapping()),uploadUri,uploadFileSizeLimit,System.currentTimeMillis());
  return data;
}
