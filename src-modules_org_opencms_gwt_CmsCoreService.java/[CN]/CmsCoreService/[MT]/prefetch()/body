{
  CmsObject cms=getCmsObject();
  CmsCoreData data=new CmsCoreData(OpenCms.getSystemInfo().getOpenCmsContext(),cms.getRequestContext().getSiteRoot(),cms.getRequestContext().getLocale().toString(),OpenCms.getWorkplaceManager().getWorkplaceLocale(cms).toString(),cms.getRequestContext().getUri());
  return data;
}
