{
  Vector projects=new Vector();
  try {
    ResultSet result;
synchronized (m_statementGetProjectsByGroup) {
      m_statementGetProjectsByGroup.setInt(1,group.getId());
      result=m_statementGetProjectsByGroup.executeQuery();
    }
    while (result.next()) {
      projects.addElement(new CmsProject(result.getInt(C_PROJECT_ID),result.getString(C_PROJECT_NAME),result.getString(C_PROJECT_DESCRIPTION),result.getInt(C_GLOBETASK_ID),result.getInt(C_USER_ID),result.getInt(C_GROUP_ID),result.getInt(C_PROJECT_FLAGS)));
    }
    return (projects);
  }
 catch (  SQLException exc) {
    throw new CmsException(this.getClass().getName() + ": " + exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
