{
  Vector projects=new Vector();
  try {
    ResultSet result;
    PreparedStatement statementGetProjectsByGroup=m_con.prepareStatement(C_PROJECT_GET_BY_MANAGERGROUP);
    statementGetProjectsByGroup.setInt(1,managergroup.getId());
    result=statementGetProjectsByGroup.executeQuery();
    while (result.next()) {
      projects.addElement(new CmsProject(result.getInt(C_PROJECT_ID),result.getString(C_PROJECT_NAME),result.getString(C_PROJECT_DESCRIPTION),result.getInt(C_TASK_ID),result.getInt(C_USER_ID),result.getInt(C_GROUP_ID),result.getInt(C_MANAGERGROUP_ID),result.getInt(C_PROJECT_FLAGS),SqlHelper.getTimestamp(result,C_PROJECT_CREATEDATE),SqlHelper.getTimestamp(result,C_PROJECT_PUBLISHDATE),result.getInt(C_PROJECT_PUBLISHED_BY),result.getInt(C_PROJECT_COUNT_LOCKED)));
    }
    return (projects);
  }
 catch (  Exception exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
