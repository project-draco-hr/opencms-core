{
  Vector projects=new Vector();
  try {
    ResultSet result;
    PreparedStatement statementGetProjectsByFlag=m_con.prepareStatement(C_PROJECT_GET_BY_FLAG);
    statementGetProjectsByFlag.setInt(1,flag);
    result=statementGetProjectsByFlag.executeQuery();
    while (result.next()) {
      projects.addElement(new CmsProject(result.getInt(C_PROJECT_ID),result.getString(C_PROJECT_NAME),result.getString(C_PROJECT_DESCRIPTION),result.getInt(C_TASK_ID),result.getInt(C_USER_ID),result.getInt(C_GROUP_ID),result.getInt(C_MANAGERGROUP_ID),result.getInt(C_PROJECT_FLAGS),SqlHelper.getTimestamp(result,C_PROJECT_CREATEDATE),SqlHelper.getTimestamp(result,C_PROJECT_PUBLISHDATE)));
    }
    return (projects);
  }
 catch (  Exception exc) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ exc.getMessage(),CmsException.C_SQL_ERROR,exc);
  }
}
