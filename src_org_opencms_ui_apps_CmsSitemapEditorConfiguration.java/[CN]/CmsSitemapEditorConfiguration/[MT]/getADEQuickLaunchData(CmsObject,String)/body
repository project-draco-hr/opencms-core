{
  if (!getVisibility(cms).isActive() || !OpenCms.getRoleManager().hasRole(cms,CmsRole.EDITOR)) {
    return Optional.absent();
  }
 else {
    return Optional.of(new CmsQuickLaunchData(Q_SITEMAP,null,null,getName(OpenCms.getWorkplaceManager().getWorkplaceLocale(cms)),getImageLink(),false));
  }
}
