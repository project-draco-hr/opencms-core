{
  CmsProjectBean manProj=new CmsProjectBean(project.getUuid(),project.getType().getMode(),org.opencms.ade.publish.Messages.get().getBundle(OpenCms.getWorkplaceManager().getWorkplaceLocale(cms)).key(org.opencms.ade.publish.Messages.GUI_NORMAL_PROJECT_1,getOuAwareName(cms,project.getName())),project.getDescription());
  return manProj;
}
