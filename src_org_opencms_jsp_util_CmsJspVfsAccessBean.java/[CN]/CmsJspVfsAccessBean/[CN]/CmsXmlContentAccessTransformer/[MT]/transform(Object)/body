{
  CmsJspContentAccessBean result=null;
  CmsResource resource=(CmsResource)getReadResource().get(input);
  if ((resource != null) && (CmsResourceTypeXmlPage.isXmlPage(resource) || CmsResourceTypeXmlContent.isXmlContent(resource) || CmsResourceTypeContainerPage.isContainerPage(resource))) {
    result=new CmsJspContentAccessBean(getCmsObject(),resource);
  }
  return result;
}
