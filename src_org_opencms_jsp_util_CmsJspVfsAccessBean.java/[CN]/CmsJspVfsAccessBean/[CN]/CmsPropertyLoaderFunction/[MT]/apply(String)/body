{
  Map<String,String> result=null;
  CmsResource resource=getReadResource().get(input);
  if (resource != null) {
    result=CmsCollectionUtil.makeComputingMap(new CmsPropertyLoaderSingleFunction(resource,m_search));
  }
  if (result == null) {
    result=Collections.emptyMap();
  }
  return result;
}
