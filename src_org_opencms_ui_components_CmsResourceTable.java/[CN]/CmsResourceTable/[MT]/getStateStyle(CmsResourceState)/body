{
  String stateStyle="";
  if (state != null) {
    if (state.isDeleted()) {
      stateStyle=OpenCmsTheme.STATE_DELETED;
    }
 else     if (state.isNew()) {
      stateStyle=OpenCmsTheme.STATE_NEW;
    }
 else     if (state.isChanged()) {
      stateStyle=OpenCmsTheme.STATE_CHANGED;
    }
  }
  return stateStyle;
}
