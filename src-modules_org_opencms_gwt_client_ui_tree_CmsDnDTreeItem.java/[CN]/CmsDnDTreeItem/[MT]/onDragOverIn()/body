{
  getListItemWidget().getContentPanel().addStyleName(I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().itemActive());
  if (isOpen()) {
    return;
  }
  m_timer=new Timer(){
    /** 
 * @see com.google.gwt.user.client.Timer#run()
 */
    @Override public void run(){
      m_timer=null;
      setOpen(true);
    }
  }
;
  m_timer.schedule(1000);
}
