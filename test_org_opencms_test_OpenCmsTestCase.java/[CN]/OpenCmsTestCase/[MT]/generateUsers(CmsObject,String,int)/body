{
  CmsGroup group=null;
  try {
    group=cms.readGroup(groupName);
  }
 catch (  Exception e) {
  }
  if (group == null) {
    group=cms.createGroup(groupName,groupName,0,null);
  }
  for (int i=0; i < n; i++) {
    String name=generateName(10) + i;
    cms.createUser(name,"pwd" + i,"test user " + i,null);
    cms.addUserToGroup(name,groupName);
  }
}
