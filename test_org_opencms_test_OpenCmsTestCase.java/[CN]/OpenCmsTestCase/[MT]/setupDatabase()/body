{
  CmsSetupDb setupDb;
  if (C_DB_ORACLE.equals(m_dbProduct)) {
    setupDb=dropTables();
  }
 else {
    setupDb=dropDatabase();
  }
  setupDb.closeConnection();
  setupDb=createDatabase();
  if (!"oracle".equals(m_dbProduct)) {
    checkErrors(setupDb);
  }
  setupDb.closeConnection();
  setupDb=createTables();
  checkErrors(setupDb);
  setupDb.closeConnection();
}
