{
  Method runMethod=null;
  try {
    runMethod=getClass().getMethod(getName(),(Class[])null);
    printInfoBox("Running OpenCms test case:",getClass().getName() + "#" + runMethod.getName());
  }
 catch (  NoSuchMethodException e) {
    fail("Method \"" + getName() + "\" not found");
  }
  super.runTest();
}
