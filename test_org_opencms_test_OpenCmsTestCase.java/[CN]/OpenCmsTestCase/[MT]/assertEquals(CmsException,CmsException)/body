{
  if (e1 == null && e2 == null) {
    return;
  }
  if ((e1 == null && e2 != null) || (e1 != null && e2 == null)) {
    fail("Exceptions not equal (not both null)");
  }
  if (!(e1.getClass().equals(e2.getClass()) || !(e1.getType() == e2.getType()))) {
    fail("Exception " + e1.toString() + " does not equal "+ e2.toString());
  }
}
