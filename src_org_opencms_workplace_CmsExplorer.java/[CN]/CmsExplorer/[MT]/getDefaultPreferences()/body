{
  int filelist;
  String explorerSettings=(String)getSettings().getUser().getAdditionalInfo(I_CmsConstants.C_ADDITIONAL_INFO_EXPLORERSETTINGS);
  if (explorerSettings != null) {
    filelist=new Integer(explorerSettings).intValue();
  }
 else {
    filelist=I_CmsWpConstants.C_FILELIST_NAME + I_CmsWpConstants.C_FILELIST_TITLE + I_CmsWpConstants.C_FILELIST_TYPE+ I_CmsWpConstants.C_FILELIST_DATE_LASTMODIFIED;
  }
  return filelist;
}
