{
  if ("vfslink".equals(mode)) {
    try {
      return new Vector(cms.fetchVfsLinksForResource(folder));
    }
 catch (    CmsException e) {
      return new Vector();
    }
  }
 else {
    try {
      return cms.getResourcesInFolder(folder);
    }
 catch (    CmsException e) {
      return new Vector();
    }
  }
}
