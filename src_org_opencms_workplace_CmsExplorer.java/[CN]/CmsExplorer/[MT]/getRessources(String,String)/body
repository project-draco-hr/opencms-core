{
  if ("vfslink".equals(mode)) {
    try {
      return new Vector(getCms().fetchVfsLinksForResource(resource));
    }
 catch (    CmsException e) {
      return new Vector();
    }
  }
 else {
    try {
      return getCms().getResourcesInFolder(resource);
    }
 catch (    CmsException e) {
      return new Vector();
    }
  }
}
