{
  List<CmsContainerElementData> result=null;
  try {
    ensureSession();
    CmsResource containerpage=getCmsObject().readResource(pageStructureId);
    String containerpageUri=getCmsObject().getSitePath(containerpage);
    result=getListElementsData(OpenCms.getADEManager().getFavoriteList(getCmsObject()),containerpageUri,detailContentId,containers,CmsLocaleManager.getLocale(locale));
  }
 catch (  Throwable e) {
    error(e);
  }
  return result;
}
