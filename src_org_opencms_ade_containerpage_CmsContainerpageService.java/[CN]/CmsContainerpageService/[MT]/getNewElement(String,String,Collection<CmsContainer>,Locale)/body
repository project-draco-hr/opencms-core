{
  CmsObject cms=getCmsObject();
  CmsElementUtil elemUtil=new CmsElementUtil(cms,uriParam,getRequest(),getResponse(),locale);
  CmsContainerElementBean elementBean=CmsContainerElementBean.createElementForResourceType(cms,OpenCms.getResourceManager().getResourceType(resourceTypeName),"/",locale);
  return elemUtil.getElementData(elementBean,containers);
}
