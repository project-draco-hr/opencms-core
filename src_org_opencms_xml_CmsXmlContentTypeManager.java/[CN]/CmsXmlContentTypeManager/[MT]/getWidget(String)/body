{
  I_CmsWidget result=(I_CmsWidget)m_registeredWidgets.get(name);
  if (result == null) {
    String className=(String)m_widgetAliases.get(name);
    if (className != null) {
      result=(I_CmsWidget)m_registeredWidgets.get(className);
    }
  }
  if (result != null) {
    result=result.newInstance();
  }
  return result;
}
