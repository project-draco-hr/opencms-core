{
  try {
    getCms().readProject(new CmsUUID(getParamProjectid()));
  }
 catch (  Exception e) {
    if (!getCms().getRequestContext().currentProject().isOnlineProject()) {
      m_paramProjectid=getCms().getRequestContext().currentProject().getUuid().toString();
    }
 else {
      throw e;
    }
  }
}
