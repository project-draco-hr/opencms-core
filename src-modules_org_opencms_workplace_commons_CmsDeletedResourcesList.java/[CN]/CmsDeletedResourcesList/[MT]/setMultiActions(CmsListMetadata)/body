{
  CmsListMultiAction restoreMultiAction=new CmsListMultiAction(LIST_MACTION_RESTORE);
  restoreMultiAction.setName(Messages.get().container(Messages.GUI_DELETED_RESOURCES_LIST_MACTION_RESTORE_NAME_0));
