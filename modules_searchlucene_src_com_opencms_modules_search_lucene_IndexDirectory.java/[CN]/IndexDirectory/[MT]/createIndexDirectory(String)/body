{
  IndexWriter writer=null;
  if (debug) {
    System.err.println("Index " + indexPath + " created");
  }
  try {
    writer=new IndexWriter(indexPath,null,true);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  try {
    writer.close();
  }
 catch (  IOException ex) {
    ex.printStackTrace();
  }
}
