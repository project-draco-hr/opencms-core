{
  setVisible(true);
  if (m_helper != null) {
    m_helper.removeFromParent();
    m_helper=null;
  }
  if (m_provisionalParent != null) {
    m_provisionalParent.removeFromParent();
    m_provisionalParent=null;
  }
  if (m_placeholder != null) {
    m_placeholder.removeFromParent();
    m_placeholder=null;
  }
}
