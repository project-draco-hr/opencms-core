{
  List<CmsSelectWidgetOption> options=new ArrayList<CmsSelectWidgetOption>();
  for (  CmsEditGroup group : OpenCms.getADEManager().getEditGroups(cms).values()) {
    options.add(new CmsSelectWidgetOption(cms.getSitePath(group.getResource()),false,group.getTitle(cms,widgetDialog.getLocale()),group.getDescription(cms,widgetDialog.getLocale())));
  }
  return options;
}
