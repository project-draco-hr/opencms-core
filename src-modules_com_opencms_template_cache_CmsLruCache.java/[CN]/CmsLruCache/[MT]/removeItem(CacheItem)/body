{
  if (C_DEBUG) {
    System.err.println("--remove item from cache: " + item.key);
  }
  if (m_size < 2) {
    clearCache();
  }
 else {
    removeFromTable(item);
    if ((item != head) && (item != tail)) {
      item.previous.next=item.next;
      item.next.previous=item.previous;
    }
 else {
      if (item == head) {
        head=item.next;
        head.previous=null;
      }
      if (item == tail) {
        tail=item.previous;
        tail.next=null;
      }
    }
    m_size--;
  }
}
