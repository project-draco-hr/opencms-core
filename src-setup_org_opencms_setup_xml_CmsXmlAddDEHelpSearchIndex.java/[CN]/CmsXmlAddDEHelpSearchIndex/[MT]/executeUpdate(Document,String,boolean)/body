{
  Node node=document.selectSingleNode(xpath);
  if (node == null) {
    if (xpath.equals(getXPathsToUpdate().get(0))) {
      CmsSetupXmlHelper.setValue(document,xpath + "/" + I_CmsXmlConfiguration.N_NAME,"German online help");
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_REBUILD,"auto");
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_PROJECT,CmsProject.ONLINE_PROJECT_NAME);
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_LOCALE,"de");
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_SOURCES+ "/"+ CmsSearchConfiguration.N_SOURCE,"source2");
    }
 else     if (xpath.equals(getXPathsToUpdate().get(1))) {
      CmsSetupXmlHelper.setValue(document,xpath + "/" + I_CmsXmlConfiguration.N_NAME,"source2");
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_INDEXER+ "/@"+ I_CmsXmlConfiguration.A_CLASS,CmsVfsIndexer.class.getName());
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_RESOURCES+ "/"+ I_CmsXmlConfiguration.N_RESOURCE,CmsWorkplace.VFS_PATH_LOCALES);
      CmsSetupXmlHelper.setValue(document,xpath + "/" + CmsSearchConfiguration.N_DOCUMENTTYPES_INDEXED+ "/"+ I_CmsXmlConfiguration.N_NAME,CmsResourceTypeXmlPage.getStaticTypeName());
    }
    return true;
  }
  return false;
}
