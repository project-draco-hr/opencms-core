{
  if (CmsSite.LocalizationMode.singleTree.equals(targetSite.getLocalizationMode())) {
    return CmsStringUtil.joinPaths(OpenCms.getStaticExportManager().getVfsPrefix(),cms.getRequestContext().getLocale().toString(),vfsName);
  }
 else {
    return super.addVfsPrefix(cms,vfsName,targetSite);
  }
}
