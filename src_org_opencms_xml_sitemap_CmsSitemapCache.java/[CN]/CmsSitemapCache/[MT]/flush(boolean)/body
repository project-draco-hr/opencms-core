{
  if (online) {
    m_documentsOnline.clear();
    m_defPropsOnline=null;
    if (m_activeOnline != null) {
      m_activeOnline.clear();
      m_activeOnline=null;
    }
  }
 else {
    m_documentsOffline.clear();
    m_defPropsOffline=null;
    if (m_activeOffline != null) {
      m_activeOffline.clear();
      m_activeOffline=null;
    }
  }
}
