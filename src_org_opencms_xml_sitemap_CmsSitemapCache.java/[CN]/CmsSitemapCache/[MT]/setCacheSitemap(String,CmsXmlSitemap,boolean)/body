{
  if (online) {
    m_sitemapsOnline.put(key,sitemap);
    if (LOG.isDebugEnabled()) {
      LOG.debug(Messages.get().getBundle().key(Messages.LOG_DEBUG_CACHE_SET_ONLINE_2,new Object[]{key,sitemap}));
    }
  }
 else {
    m_sitemapsOffline.put(key,sitemap);
    if (LOG.isDebugEnabled()) {
      LOG.debug(Messages.get().getBundle().key(Messages.LOG_DEBUG_CACHE_SET_OFFLINE_2,new Object[]{key,sitemap}));
    }
  }
}
