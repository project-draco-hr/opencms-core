{
  StringBuffer result=new StringBuffer(8);
  if (m_category != null) {
    if (result.length() > 0) {
      result.append("|");
    }
    result.append(CONFIGURATION_CATEGORY);
    result.append("=");
    result.append(m_category);
  }
  if (m_keyPrefix != null) {
    if (result.length() > 0) {
      result.append("|");
    }
    result.append(CONFIGURATION_KEY_PREFIX);
    result.append("=");
    result.append(m_keyPrefix);
  }
  return result.toString();
}
