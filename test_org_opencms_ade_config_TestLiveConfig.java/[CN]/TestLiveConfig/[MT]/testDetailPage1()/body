{
  CmsObject cms=rootCms();
  String detailPage=OpenCms.getADEConfigurationManager().getDetailPageFinder().getDetailPage(cms,"/sites/default/.content/a1/blarg.html","/sites/default/today/news");
  assertEquals("/sites/default/",detailPage);
  cms=getCmsObject();
  detailPage=OpenCms.getADEConfigurationManager().getDetailPageFinder().getDetailPage(cms,"/sites/default/.content/a1/blarg.html","/today/news");
  assertEquals("/sites/default/",detailPage);
}
