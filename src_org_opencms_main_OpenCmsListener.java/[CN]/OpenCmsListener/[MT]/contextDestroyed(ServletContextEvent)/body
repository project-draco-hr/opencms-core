{
  try {
    OpenCmsCore.getInstance().shutDown();
    shutDownSqlDrivers();
  }
 catch (  CmsInitException e) {
    if (e.isNewError()) {
      LOG.error(e.getLocalizedMessage(),e);
    }
  }
catch (  Throwable t) {
    LOG.error(Messages.get().getBundle().key(Messages.LOG_ERROR_GENERIC_0),t);
  }
}
