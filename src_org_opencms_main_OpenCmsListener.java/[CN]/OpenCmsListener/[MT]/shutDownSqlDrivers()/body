{
  Enumeration<Driver> drivers=DriverManager.getDrivers();
  while (drivers.hasMoreElements()) {
    Driver driver=drivers.nextElement();
    try {
      DriverManager.deregisterDriver(driver);
      System.out.println(Messages.get().getBundle().key(Messages.LOG_ERROR_DERIGISTERING_JDBC_DRIVER_1,driver.getClass().getName()));
    }
 catch (    Throwable e) {
      System.out.println(Messages.get().getBundle().key(Messages.ERR_DERIGISTERING_JDBC_DRIVER_1,driver.getClass().getName()));
    }
  }
}
