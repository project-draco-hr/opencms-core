{
  Object value=repository.get(key);
  if (value instanceof String) {
    return (String)value;
  }
 else   if (value == null) {
    if (defaults != null) {
      return defaults.getString(key,defaultValue);
    }
 else {
      return defaultValue;
    }
  }
 else {
    throw new ClassCastException(key + " doesn't map to a String object");
  }
}
