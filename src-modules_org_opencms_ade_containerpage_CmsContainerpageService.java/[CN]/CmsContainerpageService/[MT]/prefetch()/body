{
  CmsCntPageData data=null;
  CmsObject cms=getCmsObject();
  HttpServletRequest request=getRequest();
  try {
    CmsResource cntPage=getContainerpage(cms);
    long lastModified=cntPage.getDateLastModified();
    String cntPageUri=cms.getSitePath(cntPage);
    data=new CmsCntPageData(cms.getSitePath(cntPage),getNoEditReason(cms,cntPage),CmsRequestUtil.encodeParams(request),CmsADEManager.PATH_SITEMAP_EDITOR_JSP,cntPageUri,getNewTypes(cms,request),lastModified,cms.getRequestContext().getLocale().toString());
  }
 catch (  Throwable e) {
    error(e);
  }
  return data;
}
