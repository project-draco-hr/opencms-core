{
  CmsCntPageData data=null;
  CmsObject cms=getCmsObject();
  HttpServletRequest request=getRequest();
  try {
    CmsResource cntPage=getContainerpage(cms);
    data=new CmsCntPageData(cms.getSitePath(cntPage),getNoEditReason(cms,cntPage),CmsRequestUtil.encodeParams(request),getSitemapUri(cms,request),getNewTypes(cms,request),getSessionCache().isToolbarVisible());
  }
 catch (  Throwable e) {
    error(e);
  }
  return data;
}
