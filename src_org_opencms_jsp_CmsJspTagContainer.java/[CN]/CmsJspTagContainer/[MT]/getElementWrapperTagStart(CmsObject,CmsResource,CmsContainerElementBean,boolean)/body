{
  StringBuffer result=new StringBuffer("<div class='");
  if (isSubcontainer) {
    result.append(CLASS_SUB_CONTAINER_ELEMENTS);
  }
 else {
    result.append(CLASS_CONTAINER_ELEMENTS);
  }
  String noEditReason=new CmsResourceUtil(cms,resource).getNoEditReason(OpenCms.getWorkplaceManager().getWorkplaceLocale(cms));
  result.append("' title='").append(elementBean.getClientId()).append("' alt='").append(elementBean.getSitePath()).append("' rel='").append(CmsStringUtil.escapeHtml(noEditReason)).append("'>");
  return result.toString();
}
