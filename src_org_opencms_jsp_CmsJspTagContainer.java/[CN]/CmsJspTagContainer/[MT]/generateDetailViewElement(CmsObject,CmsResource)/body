{
  CmsContainerElementBean element=null;
  if (detailContent != null) {
    CmsFormatterBean formatter=OpenCms.getADEManager().getFormatterForContainer(cms,detailContent,getType(),getContainerWidth());
    if (formatter != null) {
      element=new CmsContainerElementBean(detailContent.getStructureId(),cms.readResource(formatter.getJspRootPath()).getStructureId(),null,false);
    }
  }
  return element;
}
