{
  CmsContainerElementBean element=null;
  if (detailContent != null) {
    CmsFormatterConfiguration formatters=OpenCms.getADEManager().getFormattersForResource(cms,cms.getRequestContext().getRootUri(),detailContent);
    CmsFormatterBean formatter=formatters.getFormatter(getType(),getContainerWidth());
    if (formatter != null) {
      element=new CmsContainerElementBean(detailContent.getStructureId(),formatter.getJspStructureId(),null,false);
    }
  }
  return element;
}
