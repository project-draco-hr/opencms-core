{
  CmsContainerElementBean element=null;
  if (detailContent != null) {
    String elementFormatter=OpenCms.getADEManager().getFormatterForContainer(cms,detailContent,getType(),getContainerWidth());
    if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(elementFormatter)) {
      element=new CmsContainerElementBean(detailContent.getStructureId(),cms.readResource(elementFormatter).getStructureId(),null,false);
    }
  }
  return element;
}
