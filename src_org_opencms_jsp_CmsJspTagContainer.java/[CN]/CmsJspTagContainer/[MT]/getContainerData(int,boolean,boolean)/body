{
  int width=-1;
  try {
    if (getWidth() != null) {
      width=Integer.parseInt(getWidth());
    }
  }
 catch (  NumberFormatException e) {
  }
  CmsContainer cont=new CmsContainer(getName(),getType(),width,maxElements,isDetailView,null,m_parentContainer != null ? m_parentContainer.getName() : null,m_parentElement != null ? m_parentElement.getInstanceId() : null);
  cont.setDeatilOnly(isDetailOnly);
  String result="";
  try {
    result=CmsContainerpageService.getSerializedContainerInfo(cont);
  }
 catch (  Exception e) {
    LOG.error(e.getLocalizedMessage(),e);
  }
  return result;
}
