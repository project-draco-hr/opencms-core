{
  int width=-1;
  try {
    if (getWidth() != null) {
      width=Integer.parseInt(getWidth());
    }
  }
 catch (  NumberFormatException e) {
  }
  CmsContainer cont=new CmsContainer(getName(),getType(),width,maxElements,isDetailView,m_parentContainer != null,null);
  cont.setDeatilOnly(m_detailOnly);
  String result="";
  try {
    result=CmsContainerpageService.getSerializedContainerInfo(cont);
  }
 catch (  Exception e) {
    LOG.error(e.getLocalizedMessage(),e);
  }
  return result;
}
