{
  if (m_sessionCache == null) {
    m_sessionCache=(CmsADESessionCache)((HttpServletRequest)pageContext.getRequest()).getSession().getAttribute(CmsADESessionCache.SESSION_ATTR_ADE_CACHE);
    if (m_sessionCache == null) {
      m_sessionCache=new CmsADESessionCache(cms);
      ((HttpServletRequest)pageContext.getRequest()).getSession().setAttribute(CmsADESessionCache.SESSION_ATTR_ADE_CACHE,m_sessionCache);
    }
  }
  return m_sessionCache;
}
