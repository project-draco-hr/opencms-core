{
  for (  ReqParam param : params) {
    String value=request.getParameter(param.getName());
    if (value == null) {
      if (result != null) {
        CmsMessages bundle=Messages.get().getBundle();
        result.put(JsonResponse.ERROR.getName(),bundle.key(Messages.ERR_JSON_MISSING_PARAMETER_1,param.getName()));
      }
      return false;
    }
  }
  return true;
}
