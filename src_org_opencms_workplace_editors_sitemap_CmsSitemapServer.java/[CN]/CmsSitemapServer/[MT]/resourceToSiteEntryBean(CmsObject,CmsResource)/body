{
  CmsProperty titleProp=cms.readPropertyObject(resource,CmsPropertyDefinition.PROPERTY_TITLE,false);
  String title=titleProp.getValue();
  String name=resource.getName();
  int dotPos=name.lastIndexOf('.');
  if (dotPos != -1) {
    name=name.substring(0,dotPos);
  }
  CmsSitemapEntry entryBean=new CmsSitemapEntry(new CmsUUID(),null,resource.getStructureId(),name,title,new HashMap<String,String>(),new ArrayList<CmsSitemapEntry>(),null);
  return entryBean;
}
