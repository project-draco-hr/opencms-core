{
  CmsProperty titleProp=cms.readPropertyObject(resource,"Title",false);
  String title=titleProp.getValue();
  String name=resource.getName();
  String extension="";
  int dotPos=name.lastIndexOf('.');
  if (dotPos != -1) {
    extension=name.substring(dotPos + 1,name.length());
  }
  CmsSiteEntryBean entryBean=new CmsSiteEntryBean(resource.getStructureId(),name.substring(0,dotPos),extension,title,new HashMap<String,String>(),new ArrayList<CmsSiteEntryBean>());
  return entryBean;
}
