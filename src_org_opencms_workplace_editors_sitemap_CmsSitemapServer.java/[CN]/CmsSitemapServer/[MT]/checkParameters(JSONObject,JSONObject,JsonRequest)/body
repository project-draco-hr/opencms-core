{
  for (  JsonRequest param : params) {
    if (!data.has(param.getName())) {
      if (result != null) {
        result.put(CmsSitemapServer.JsonResponse.ERROR.getName(),Messages.get().getBundle().key(Messages.ERR_JSON_MISSING_PARAMETER_1,param.getName()));
      }
      return false;
    }
  }
  return true;
}
