{
  if (m_file == null) {
    m_thumbnail=null;
    return;
  }
  ImageIcon tmpIcon=new ImageIcon(m_file.getPath());
  if (tmpIcon != null) {
    if (tmpIcon.getIconWidth() > 190) {
      m_thumbnail=new ImageIcon(tmpIcon.getImage().getScaledInstance(190,-1,Image.SCALE_DEFAULT));
    }
 else {
      m_thumbnail=tmpIcon;
    }
  }
}
