{
  try {
    File f=new File(rfsName);
    if (f.exists()) {
      long age=f.lastModified();
      if ((System.currentTimeMillis() - age) > 3600000) {
        f.setLastModified(System.currentTimeMillis());
      }
      return CmsFileUtil.readFile(f);
    }
  }
 catch (  IOException e) {
  }
  return null;
}
