{
  if (name == null || name.length() == 0 || name.trim().length() == 0) {
    throw new CmsException("[" + getClass().getName() + "] "+ name,CmsException.C_BAD_NAME);
  }
  if (!blank) {
    int l=name.length();
    for (int i=0; i < l; i++) {
      char c=name.charAt(i);
      if (c == ' ') {
        throw new CmsException("[" + getClass().getName() + "] "+ name,CmsException.C_BAD_NAME);
      }
    }
  }
}
