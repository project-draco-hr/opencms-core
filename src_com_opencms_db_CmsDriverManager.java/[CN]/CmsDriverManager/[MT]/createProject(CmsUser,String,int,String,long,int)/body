{
  CmsGroup role=null;
  if (roleName != null && !roleName.equals("")) {
    role=readGroup(currentUser,null,roleName);
  }
  java.sql.Timestamp timestamp=new java.sql.Timestamp(timeout);
  java.sql.Timestamp now=new java.sql.Timestamp(System.currentTimeMillis());
  return m_workflowDriver.createTask(0,0,1,currentUser.getId(),currentUser.getId(),role.getId(),projectName,now,timestamp,priority);
}
