{
  if (isAdmin(currentUser,currentProject)) {
    name=name.trim();
    validFilename(name);
    m_propertyDefVectorCache.clear();
    return (m_vfsDriver.createPropertydefinition(name,getResourceType(currentUser,currentProject,resourcetype).getResourceType()));
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ name,CmsException.C_NO_ACCESS);
  }
}
