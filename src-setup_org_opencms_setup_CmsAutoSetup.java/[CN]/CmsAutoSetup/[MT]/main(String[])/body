{
  String path=null;
  for (int i=0; i < args.length; i++) {
    String arg=args[i];
    if (arg.startsWith(PARAM_CONFIG_PATH)) {
      path=arg.substring(PARAM_CONFIG_PATH.length());
    }
  }
  CmsAutoSetupProperties.initialize(path);
  new CmsAutoSetup(CmsAutoSetupProperties.getInstance()).run();
  System.exit(0);
}
