{
  String requestedUri=cms.getRequestContext().getUri();
  String value="";
  try {
    value=cms.readProperty(requestedUri,tagcontent);
  }
 catch (  Exception e) {
    if (OpenCms.isLogging(CmsLog.CHANNEL_MAIN,CmsLog.LEVEL_INFO)) {
      OpenCms.log(CmsLog.CHANNEL_MAIN,CmsLog.LEVEL_INFO,"[CmsXmlTemplate] usermethod getProperty throwed an Exception getting " + tagcontent + ": "+ e.toString());
    }
  }
  if (value == null) {
    value="";
  }
  return value;
}
