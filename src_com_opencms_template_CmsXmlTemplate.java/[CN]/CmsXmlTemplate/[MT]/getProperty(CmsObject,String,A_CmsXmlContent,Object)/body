{
  String requestedUri=cms.getRequestContext().getUri();
  String value="";
  try {
    value=cms.readProperty(requestedUri,tagcontent);
  }
 catch (  Exception e) {
    if (OpenCms.getLog(CmsLog.CHANNEL_TEMPLATE_XML).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_TEMPLATE_XML).info("[CmsXmlTemplate] usermethod getProperty throwed an Exception getting " + tagcontent + ": "+ e.toString());
    }
  }
  if (value == null) {
    value="";
  }
  return value;
}
