{
  StringBuffer retValue=new StringBuffer(256);
  retValue.append("<span class=\"dialogunmarked maxwidth\" onmouseover=\"className='dialogmarked maxwidth'\"" + " onmouseout=\"className='dialogunmarked maxwidth'\" onclick=\"javascript:top.selectForm('0','" + group.getName() + "');\">");
