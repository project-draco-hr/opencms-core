{
  if (CmsLocaleManager.getDefaultLocale().equals(getLocale()) || !m_languageVersionLocales.contains(CmsLocaleManager.getDefaultLocale())) {
    return ListUtils.removeAll(OpenCms.getLocaleManager().getAvailableLocales(),m_languageVersionLocales);
  }
  List<Locale> result=new ArrayList<Locale>();
  result.add(getLocale());
  return result;
}
