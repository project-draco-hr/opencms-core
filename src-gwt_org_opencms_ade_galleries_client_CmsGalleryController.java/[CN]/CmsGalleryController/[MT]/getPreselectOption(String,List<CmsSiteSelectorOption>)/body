{
  int i=0;
  if ((siteRoot == null) || options.isEmpty()) {
    return null;
  }
  for (  CmsSiteSelectorOption option : options) {
    String key="" + i;
    if (CmsStringUtil.joinPaths(siteRoot,"/").equals(CmsStringUtil.joinPaths(option.getSiteRoot(),"/"))) {
      return key;
    }
    i+=1;
  }
  return null;
}
