{
  try {
    return m_index.getExcerpt(this);
  }
 catch (  Exception exc) {
    if (OpenCms.getLog(this).isErrorEnabled()) {
      String message="Could not generate excerpt for query [" + m_query + "], and resource "+ m_resource+ ":";
      OpenCms.getLog(this).error(message,exc);
    }
  }
  return null;
}
