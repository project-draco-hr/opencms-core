{
  int result=8;
  if (m_resource != null) {
    result+=CmsMemoryMonitor.getMemorySize(m_resource);
  }
  if (m_query != null) {
    result+=CmsMemoryMonitor.getMemorySize(m_query);
  }
  if (m_document != null) {
    result+=1024 * 10;
  }
  if (m_index != null) {
    result+=1024;
  }
  return result;
}
