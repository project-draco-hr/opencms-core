{
  String temp=(CmsStringUtil.isNotEmptyOrWhitespaceOnly(request.getParameter(ReqParam.dialogmode.name())) ? request.getParameter(ReqParam.dialogmode.name()) : (String)request.getAttribute(ReqParam.dialogmode.name()));
  GalleryMode mode=null;
  try {
    mode=GalleryMode.valueOf(temp);
  }
 catch (  Exception e) {
    mode=GalleryMode.ade;
  }
  return mode;
}
