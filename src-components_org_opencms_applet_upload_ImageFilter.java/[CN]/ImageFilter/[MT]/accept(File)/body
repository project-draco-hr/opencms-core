{
  if (f.isDirectory()) {
    return true;
  }
  String extension=FileUploadUtils.getExtension(f);
  if (extension != null) {
    return (extension.equals("gif") || extension.equals("tiff") || extension.equals("tif")|| extension.equals("jpeg")|| extension.equals("jpg")|| extension.equals("png"));
  }
 else {
    return false;
  }
}
