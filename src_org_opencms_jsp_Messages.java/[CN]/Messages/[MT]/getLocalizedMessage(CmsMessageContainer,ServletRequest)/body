{
  String msg;
  CmsRequestContext requestContext;
  Locale locale;
  CmsObject cms=CmsFlexController.getCmsObject(request);
  if ((cms == null) || ((requestContext=cms.getRequestContext()) == null) || ((locale=requestContext.getLocale()) == null)) {
    msg=container.key();
  }
 else {
    msg=container.key(locale);
  }
  return msg;
}
