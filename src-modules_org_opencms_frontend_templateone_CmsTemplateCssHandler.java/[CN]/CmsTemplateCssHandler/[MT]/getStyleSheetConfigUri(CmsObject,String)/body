{
  String originalUri=cms.getRequestContext().getUri();
  String confUri=null;
  try {
    cms.getRequestContext().setUri(editedResourcePath);
    confUri=cms.readPropertyObject(editedResourcePath,CmsTemplateStyleSheet.PROPERTY_CONFIGFILE,true).getValue("");
    if (CmsStringUtil.isEmpty(confUri)) {
      confUri=cms.readPropertyObject(editedResourcePath,CmsTemplateBean.PROPERTY_CONFIGPATH,true).getValue("/") + CmsTemplateStyleSheet.FILENAME_CONFIGFILE;
    }
  }
 catch (  CmsException e) {
    if (LOG.isWarnEnabled()) {
      LOG.warn(org.opencms.frontend.templateone.Messages.get().getBundle().key(org.opencms.frontend.templateone.Messages.LOG_XMLCONTEN_CONFIG_NOT_FOUND_2,CmsTemplateStyleSheet.FILENAME_CONFIGFILE,editedResourcePath));
    }
  }
 finally {
    cms.getRequestContext().setUri(originalUri);
  }
  return confUri;
}
