{
  StringBuffer buf=new StringBuffer();
  for (int i=0; i < values.length; i++) {
    values[i]=CmsStringUtil.substitute(values[i],",","\\,");
    values[i]=CmsStringUtil.substitute(values[i],"=","\\=");
    buf.append("\t" + values[i] + ((i < values.length - 1) ? ",\\\n" : ""));
  }
  return buf.toString();
}
