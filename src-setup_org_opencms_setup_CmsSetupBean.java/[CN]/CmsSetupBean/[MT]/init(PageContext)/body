{
  String webAppRfsPath=pageContext.getServletConfig().getServletContext().getRealPath("/");
  String servletMapping=pageContext.getServletContext().getInitParameter(OpenCmsServlet.SERVLET_PARAM_OPEN_CMS_SERVLET);
  String defaultWebApplication=pageContext.getServletContext().getInitParameter(OpenCmsServlet.SERVLET_PARAM_DEFAULT_WEB_APPLICATION);
  m_servletConfig=pageContext.getServletConfig();
  init(webAppRfsPath,servletMapping,defaultWebApplication);
}
