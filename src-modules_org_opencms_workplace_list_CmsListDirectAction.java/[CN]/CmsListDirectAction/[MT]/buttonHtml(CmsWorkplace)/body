{
  if (!isVisible()) {
    return "";
  }
  return A_CmsHtmlIconButton.defaultButtonHtml(resolveButtonStyle(),getId() + getItem().getId(),getId(),resolveName(wp.getLocale()),resolveHelpText(wp.getLocale()),isEnabled(),getIconPath(),null,resolveOnClic(wp.getLocale()),getColumnForTexts() == null,null);
}
