{
  Iterator i=projectResources.iterator();
  while (i.hasNext()) {
    String projectResource=(String)i.next();
    if (CmsResource.isFolder(projectResource)) {
      if (resourcename.startsWith(projectResource)) {
        return true;
      }
    }
 else {
      if (resourcename.equals(projectResource)) {
        return true;
      }
    }
  }
  return false;
}
