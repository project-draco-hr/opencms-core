{
  m_id=projectId;
  m_name=name;
  m_description=description;
  m_taskId=taskId;
  m_ownerId=ownerId;
  m_groupUsersId=groupId;
  m_groupUsersId=groupId;
  m_groupManagersId=managerGroupId;
  m_flags=flags;
  m_type=type;
  if (dateCreated != null) {
    m_dateCreated=dateCreated.getTime();
  }
 else {
    m_dateCreated=I_CmsConstants.C_UNKNOWN_LONG;
  }
}
