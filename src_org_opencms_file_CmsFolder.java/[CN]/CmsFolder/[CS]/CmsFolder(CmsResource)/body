{
  this(resource.getStructureId(),resource.getResourceId(),resource.getParentStructureId(),resource.getFileId(),resource.getName(),resource.getType(),resource.getFlags(),resource.getProjectLastModified(),resource.getState(),resource.getDateCreated(),resource.getUserCreated(),resource.getDateLastModified(),resource.getUserLastModified(),resource.getLinkCount(),0,0);
  if (resource.hasFullResourceName()) {
    setFullResourceName(resource.getRootPath());
  }
}
