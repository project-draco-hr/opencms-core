{
  String file=m_basePath + "setup" + File.separator+ "database"+ File.separator+ database+ File.separator+ "drop_db.sql";
  if (file != null) {
    m_errorLogging=true;
    parseScript(file,replacer);
  }
 else {
    m_errors.addElement("Could not open database drop script: " + database + ".dropdb");
  }
}
