{
  String file=m_basePath + "setup" + File.separator+ "database"+ File.separator+ database+ File.separator+ "drop_tables.sql";
  if (file != null) {
    m_errorLogging=true;
    parseScript(file,null);
  }
 else {
    m_errors.addElement("No drop tables script found: " + database + ".droptables \n");
  }
}
