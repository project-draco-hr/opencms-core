{
  if (name == null || name.length() == 0 || name.trim().length() == 0) {
    return false;
  }
  for (int i=0; i < name.length(); i++) {
    char c=name.charAt(i);
    if (((c < 'a') || (c > 'z')) && ((c < '0') || (c > '9')) && ((c < 'A') || (c > 'Z'))&& (c != '-')&& (c != '.')&& (c != '|')&& (c != '_')&& (c != '~')) {
      return false;
    }
  }
  return true;
}
