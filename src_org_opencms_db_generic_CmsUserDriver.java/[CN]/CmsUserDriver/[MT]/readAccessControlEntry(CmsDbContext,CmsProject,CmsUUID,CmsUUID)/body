{
  CmsAccessControlEntry ace=null;
  PreparedStatement stmt=null;
  Connection conn=null;
  ResultSet res=null;
  try {
    conn=m_sqlManager.getConnection(dbc);
    stmt=m_sqlManager.getPreparedStatement(conn,project,"C_ACCESS_READ_ENTRY");
    stmt.setString(1,resource.toString());
    stmt.setString(2,principal.toString());
    res=stmt.executeQuery();
    if (res.next()) {
      ace=internalCreateAce(res);
    }
 else {
      res.close();
      res=null;
      throw new CmsObjectNotFoundException("Access Control Entry not found for resource " + resource + " and principal "+ principal);
    }
    return ace;
  }
 catch (  SQLException e) {
    throw new CmsSqlException(this,stmt,e);
  }
 finally {
    m_sqlManager.closeAll(dbc,conn,stmt,res);
  }
}
