{
  if (colFlag == LIST_COLUMN_TYPEICON.hashCode()) {
    return true;
  }
  if (colFlag == LIST_COLUMN_NAME.hashCode()) {
    return true;
  }
  if (colFlag == CmsUserSettings.FILELIST_TITLE) {
    return true;
  }
  if (colFlag == CmsUserSettings.FILELIST_DATE_LASTMODIFIED) {
    return true;
  }
  return false;
}
