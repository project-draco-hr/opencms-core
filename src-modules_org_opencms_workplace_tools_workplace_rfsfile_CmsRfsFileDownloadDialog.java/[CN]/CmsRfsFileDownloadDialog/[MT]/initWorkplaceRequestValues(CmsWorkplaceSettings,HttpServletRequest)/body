{
  m_downloadFile=getDownloadFileInternal();
  fillParamValues(request);
  setParamDialogtype(DIALOG_TYPE);
  if (DIALOG_CANCEL.equals(getParamAction())) {
    setAction(ACTION_CANCEL);
  }
 else   if (DIALOG_OK.equals(getParamAction())) {
    setAction(ACTION_OK);
  }
 else {
    setAction(ACTION_DEFAULT);
    setParamTitle(key(Messages.GUI_WORKPLACE_LOGVIEW_DOWNLOAD_START_TITLE_0,new Object[]{getDownloadFile().getName()}));
  }
}
