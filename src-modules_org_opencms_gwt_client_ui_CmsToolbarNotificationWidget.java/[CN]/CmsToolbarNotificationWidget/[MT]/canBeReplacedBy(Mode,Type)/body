{
  if (mode == Mode.FIXED) {
    Element element=getElement();
switch (type) {
case ERROR:
      if (CmsDomUtil.hasClass(classForType(Type.ERROR),element)) {
        return false;
      }
    break;
case NORMAL:
  if (CmsDomUtil.hasClass(classForType(Type.ERROR),element) || CmsDomUtil.hasClass(classForType(Type.WARNING),element) || CmsDomUtil.hasClass(classForType(Type.NORMAL),element)) {
    return false;
  }
break;
case WARNING:
if (CmsDomUtil.hasClass(classForType(Type.ERROR),element) || CmsDomUtil.hasClass(classForType(Type.WARNING),element)) {
return false;
}
break;
default :
}
}
return true;
}
