{
  if (m_timer != null) {
    m_timer.cancel();
    m_timer=null;
  }
  if ((m_mode == Mode.FIXED) && !force) {
    return;
  }
  if (m_animation != null) {
    m_animation.cancel();
  }
  if (!isVisible()) {
    return;
  }
  if (!force) {
    m_animation=getHideAnimation();
    m_animation.run();
  }
 else {
    onHideComplete();
  }
}
