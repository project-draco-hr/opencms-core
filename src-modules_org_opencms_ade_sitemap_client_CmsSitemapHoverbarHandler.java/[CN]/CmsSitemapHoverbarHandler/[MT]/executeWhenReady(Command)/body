{
  CmsRpcAction<?> rpcAction=m_controller.getInitAction();
  if (rpcAction != null) {
    rpcAction.start(0);
    Timer t=new Timer(){
      /** 
 * @see com.google.gwt.user.client.Timer#run()
 */
      @Override public void run(){
        if (m_controller.getInitAction() != null) {
          return;
        }
        cancel();
        command.execute();
      }
    }
;
    t.scheduleRepeating(200);
  }
 else {
    command.execute();
  }
}
