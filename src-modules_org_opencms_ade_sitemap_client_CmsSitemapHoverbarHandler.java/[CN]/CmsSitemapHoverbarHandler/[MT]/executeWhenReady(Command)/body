{
  CmsSitemapData data=m_controller.getEditData();
  if (data == null) {
    Timer t=new Timer(){
      /** 
 * @see com.google.gwt.user.client.Timer#run()
 */
      @Override public void run(){
        if (m_controller.getEditData() == null) {
          return;
        }
        cancel();
        command.execute();
      }
    }
;
    t.scheduleRepeating(200);
  }
 else {
    command.execute();
  }
}
