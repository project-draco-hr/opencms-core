{
  while (element.equals(RootPanel.getBodyElement()) || !CmsDomUtil.hasClass(I_CmsLayoutBundle.INSTANCE.stateCss().cmsHovering(),element)) {
    element=element.getParentElement();
  }
  if (!element.equals(RootPanel.getBodyElement())) {
    element.removeClassName(I_CmsLayoutBundle.INSTANCE.stateCss().cmsHovering());
  }
}
