{
  if (!m_handler.isVisible(wp.getCms())) {
    return "";
  }
  String link=CmsToolManager.linkForToolPath(wp.getJsp(),getHandler().getPath(),getHandler().getParameters(wp));
  String onClic="openPage('" + link + "');";
  return A_CmsHtmlIconButton.defaultButtonHtml(wp.getJsp(),CmsHtmlIconButtonStyleEnum.BIG_ICON_TEXT,getId(),m_handler.getName(),m_handler.isEnabled(wp.getCms()) ? m_handler.getHelpText() : m_handler.getDisabledHelpText(),m_handler.isEnabled(wp.getCms()),m_handler.getIconPath(),m_handler.getConfirmationMessage(),onClic);
}
