{
  if (!m_handler.isVisible(wp.getCms())) {
    return "";
  }
  String link=CmsToolManager.linkForToolPath(wp.getJsp(),this.getHandler().getPath());
  if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(getHandler().getParameters())) {
    if (!link.endsWith("&")) {
      link+="&";
    }
    link+=wp.resolveMacros(getHandler().getParameters());
  }
  String onClic="openPage('" + link + "');";
  return A_CmsHtmlIconButton.defaultButtonHtml(CmsHtmlIconButtonStyleEnum.BIG_ICON_TEXT,getId(),m_handler.getName(),m_handler.isEnabled(wp.getCms()) ? m_handler.getHelpText() : m_handler.getDisabledHelpText(),m_handler.isEnabled(wp.getCms()),m_handler.getIconPath(),onClic);
}
