{
  List<CmsListItem> result=new ArrayList<CmsListItem>();
  CmsListItem item;
  int idCounter=0;
  for (  CmsResource resource : this.getResources()) {
    item=getList().newItem(resource.getRootPath());
    this.fillItem(resource,item,idCounter);
    idCounter++;
    result.add(item);
  }
  return result;
}
