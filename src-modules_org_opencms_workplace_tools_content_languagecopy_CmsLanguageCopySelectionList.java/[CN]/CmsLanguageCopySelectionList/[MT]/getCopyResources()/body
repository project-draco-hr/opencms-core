{
  List<CmsListItem> items=this.getSelectedItems();
  String paths="";
  boolean initial=true;
  for (  CmsListItem item : items) {
    if (!initial) {
      paths.concat(",");
    }
    paths.concat(String.valueOf(item.get(LIST_COLUMN_PATH)));
    initial=false;
  }
  return CmsStringUtil.splitAsArray(paths,",");
}
