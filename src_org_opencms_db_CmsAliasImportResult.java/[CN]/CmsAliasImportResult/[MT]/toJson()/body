{
  try {
    JSONObject obj=new JSONObject();
    if (m_line != null) {
      obj.put(JSON_LINE,m_line);
    }
    if (m_message != null) {
      obj.put(JSON_MESSAGE,m_message);
    }
    if (m_status != null) {
      obj.put(JSON_STATUS,m_status.toString());
    }
    return obj;
  }
 catch (  JSONException e) {
    return null;
  }
}
