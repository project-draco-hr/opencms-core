{
  try {
    JSONObject obj=new JSONObject();
    if (m_line != null) {
      obj.put("line",m_line);
    }
    if (m_error != null) {
      obj.put("error",m_error);
    }
    if (m_status != null) {
      obj.put("status",m_status.toString());
    }
    return obj;
  }
 catch (  JSONException e) {
    return null;
  }
}
