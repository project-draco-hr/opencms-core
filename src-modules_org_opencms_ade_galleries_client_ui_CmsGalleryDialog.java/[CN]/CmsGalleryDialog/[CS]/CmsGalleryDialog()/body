{
  initCss();
  m_dragHandler=null;
  m_isInitialSearch=false;
  m_parentPanel=new FlowPanel();
  m_parentPanel.setStyleName(I_CmsLayoutBundle.INSTANCE.galleryDialogCss().parentPanel());
