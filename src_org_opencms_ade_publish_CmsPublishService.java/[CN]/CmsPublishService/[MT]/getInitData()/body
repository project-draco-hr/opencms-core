{
  CmsPublishData result=null;
  try {
    CmsPublishOptions options=getCachedOptions();
    result=new CmsPublishData(options,getProjects(),getResourceGroups(options),OpenCms.getWorkflowManager().getAvailableActions(getCmsObject()));
  }
 catch (  Throwable e) {
    error(e);
  }
  return result;
}
