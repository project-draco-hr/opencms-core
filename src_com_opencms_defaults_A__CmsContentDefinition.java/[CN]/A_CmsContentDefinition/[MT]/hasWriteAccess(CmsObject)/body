{
  CmsUser currentUser=cms.getRequestContext().currentUser();
  if (isLockable() && (getLockstate() != currentUser.getId())) {
    return (false);
  }
  if (!(accessOther(C_ACCESS_PUBLIC_WRITE) || accessOwner(cms,currentUser,C_ACCESS_OWNER_WRITE) || accessGroup(cms,currentUser,C_ACCESS_GROUP_WRITE))) {
    return false;
  }
  return true;
}
