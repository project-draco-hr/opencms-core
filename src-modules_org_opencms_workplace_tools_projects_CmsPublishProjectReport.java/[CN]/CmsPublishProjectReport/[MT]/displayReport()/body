{
switch (getAction()) {
case ACTION_REPORT_BEGIN:
case ACTION_CONFIRMED:
case ACTION_DEFAULT:
default :
    try {
      if (getCms().readProject(new Integer(getParamProjectid()).intValue()).getType() == CmsProject.PROJECT_TYPE_TEMPORARY) {
        setParamRefreshWorkplace(CmsStringUtil.TRUE);
      }
    }
 catch (    Exception e) {
    }
}
super.displayReport();
}
