{
switch (getAction()) {
case ACTION_REPORT_BEGIN:
case ACTION_CONFIRMED:
case ACTION_DEFAULT:
default :
    try {
      if (getCms().readProject(new Integer(getParamProjectid()).intValue()).getType() == I_CmsConstants.C_PROJECT_TYPE_TEMPORARY) {
        setParamRefreshWorkplace("true");
      }
    }
 catch (    Exception e) {
    }
}
super.displayReport();
}
