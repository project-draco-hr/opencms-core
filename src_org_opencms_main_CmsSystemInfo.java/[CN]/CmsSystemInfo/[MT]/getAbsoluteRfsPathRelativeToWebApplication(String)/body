{
  if (path == null) {
    return null;
  }
  File f=new File(path);
  if (f.isAbsolute()) {
    path=f.getAbsolutePath();
    if (f.isDirectory() && !path.endsWith(File.separator)) {
      path=path.concat(File.separator);
    }
    return path;
  }
  return CmsFileUtil.normalizePath(getWebApplicationRfsPath() + path);
}
