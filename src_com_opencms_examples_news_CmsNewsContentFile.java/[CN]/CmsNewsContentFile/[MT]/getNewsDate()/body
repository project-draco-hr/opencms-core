{
  long result=0;
  String dateText=getDataValue(C_NEWS_XML_DATE);
  if (dateText != null && !"".equals(dateText)) {
    String splittetDate[]=Utils.split(dateText,".");
    GregorianCalendar cal=new GregorianCalendar(Integer.parseInt(splittetDate[2]),Integer.parseInt(splittetDate[1]) - 1,Integer.parseInt(splittetDate[0]),0,0,0);
    result=cal.getTime().getTime();
  }
  return result;
}
