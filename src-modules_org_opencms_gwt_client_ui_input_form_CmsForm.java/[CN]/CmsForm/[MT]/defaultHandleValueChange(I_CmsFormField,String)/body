{
  m_editedFields.add(field.getId());
  I_CmsStringModel model=field.getModel();
  if (model != null) {
    model.setValue(newValue,true);
  }
  field.setValidationStatus(I_CmsFormField.ValidationStatus.unknown);
  if (!m_pressedEnter) {
    validateField(field);
  }
 else {
    validateAndSubmit();
  }
}
