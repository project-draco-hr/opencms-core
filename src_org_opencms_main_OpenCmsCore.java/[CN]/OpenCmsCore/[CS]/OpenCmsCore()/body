{
synchronized (LOCK) {
    if (m_instance != null && (m_instance.getRunLevel() > OpenCms.RUNLEVEL_0_OFFLINE)) {
      throw new CmsInitException(Messages.get().container(Messages.ERR_ALREADY_INITIALIZED_0));
    }
    initMembers();
    m_instance=this;
    setRunLevel(OpenCms.RUNLEVEL_1_CORE_OBJECT);
  }
}
