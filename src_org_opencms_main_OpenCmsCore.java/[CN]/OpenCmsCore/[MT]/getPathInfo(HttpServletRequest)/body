{
  String path=req.getPathInfo();
  if ((path == null) && (m_requestErrorPageAttribute != null)) {
    path=(String)req.getAttribute(m_requestErrorPageAttribute);
    if (path != null) {
      int pos=path.indexOf("/",1);
      if (pos > 0) {
        path=path.substring(pos);
      }
    }
  }
  if (path == null) {
    path="/";
  }
  return path;
}
