{
  try {
    m_configurationManager.writeConfiguration(clazz);
  }
 catch (  IOException e) {
    getLog(CmsConfigurationManager.class).error(Messages.get().key(Messages.LOG_ERROR_WRITING_CONFIG_1,clazz.getName()),e);
  }
catch (  CmsConfigurationException e) {
    getLog(CmsConfigurationManager.class).error(Messages.get().key(Messages.LOG_ERROR_WRITING_CONFIG_1,clazz.getName()),e);
  }
}
