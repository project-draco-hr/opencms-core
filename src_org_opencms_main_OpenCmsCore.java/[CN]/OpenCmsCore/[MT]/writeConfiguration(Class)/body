{
  try {
    m_configurationManager.writeConfiguration(clazz);
  }
 catch (  IOException e) {
    getLog(CmsConfigurationManager.class).error("Error writing configuration for class '" + clazz.getName() + "'",e);
  }
catch (  CmsConfigurationException e) {
    getLog(CmsConfigurationManager.class).error("Error writing configuration for class '" + clazz.getName() + "'",e);
  }
}
