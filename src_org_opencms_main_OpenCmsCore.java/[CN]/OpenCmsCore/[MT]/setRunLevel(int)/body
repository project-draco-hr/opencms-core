{
  if (m_instance != null) {
    if (m_instance.m_runLevel >= OpenCms.RUNLEVEL_1_CORE_OBJECT) {
      if (CmsLog.INIT.isInfoEnabled()) {
        CmsLog.INIT.info(Messages.get().key(Messages.INIT_RUNLEVEL_CHANGE_2,new Integer(m_instance.m_runLevel),new Integer(level)));
      }
    }
    m_instance.m_runLevel=level;
  }
}
