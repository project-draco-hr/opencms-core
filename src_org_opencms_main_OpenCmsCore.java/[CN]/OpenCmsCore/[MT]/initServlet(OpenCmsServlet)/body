{
synchronized (LOCK) {
    addRequestHandler(servlet);
    if (CmsLog.LOG.isInfoEnabled()) {
      CmsLog.LOG.info(". OpenCms is running!  : Total startup time was " + CmsStringUtil.formatRuntime(getSystemInfo().getRuntime()));
      CmsLog.LOG.info(".                      ...............................................................");
      CmsLog.LOG.info(".");
    }
  }
}
