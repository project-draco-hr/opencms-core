{
  if (m_lastInitErrorTime > 0) {
    if ((System.currentTimeMillis() - m_lastInitErrorTime) < 10000) {
      throw new RuntimeException("Unable to initialize OpenCms");
    }
    m_lastInitErrorTime=0;
  }
  if (m_instance == null) {
    try {
      new OpenCmsCore();
    }
 catch (    CmsInitException e) {
    }
  }
  return m_instance;
}
