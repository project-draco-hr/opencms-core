{
  CmsUser user=contextInfo.getUser();
  if (user == null) {
    user=m_securityManager.readUser(null,contextInfo.getUserName());
  }
  CmsProject project=contextInfo.getProject();
  if (project == null) {
    project=m_securityManager.readProject(contextInfo.getProjectName());
  }
  CmsRequestContext context=new CmsRequestContext(user,project,contextInfo.getRequestedUri(),contextInfo.getSiteRoot(),contextInfo.getLocale(),contextInfo.getEncoding(),contextInfo.getRemoteAddr(),contextInfo.getRequestTime(),m_resourceManager.getFolderTranslator(),m_resourceManager.getFileTranslator(),contextInfo.getOuFqn());
  context.setDetailResource(contextInfo.getDetailResource());
  return new CmsObject(m_securityManager,context);
}
