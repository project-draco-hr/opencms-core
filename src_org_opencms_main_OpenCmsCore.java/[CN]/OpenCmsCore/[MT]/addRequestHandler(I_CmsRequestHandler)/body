{
  if (handler == null) {
    return;
  }
  String[] names=handler.getHandlerNames();
  for (int i=0; i < names.length; i++) {
    String name=names[i];
    if (m_requestHandlers.get(name) != null) {
      CmsLog.LOG.error(Messages.get().key(Messages.LOG_DUPLICATE_REQUEST_HANDLER_1,name));
      continue;
    }
    m_requestHandlers.put(name,handler);
    if (CmsLog.LOG.isInfoEnabled()) {
      CmsLog.LOG.info(Messages.get().key(Messages.INIT_ADDED_REQUEST_HANDLER_2,name,handler.getClass().getName()));
    }
  }
}
