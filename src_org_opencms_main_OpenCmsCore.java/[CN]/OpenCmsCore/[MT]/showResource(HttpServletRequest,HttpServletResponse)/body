{
  CmsObject cms=null;
  try {
    cms=initCmsObject(req,res);
    CmsResource resource=initResource(cms,cms.getRequestContext().getUri(),req,res);
    if (resource != null) {
      m_loaderManager.loadResource(cms,resource,req,res);
      updateUser(cms,req);
    }
  }
 catch (  Throwable t) {
    errorHandling(cms,req,res,t);
  }
}
