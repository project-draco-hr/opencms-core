{
  path=path.replace('\\','/');
  if (!path.endsWith("/")) {
    path=path + "/";
  }
  m_basePath=CmsLinkManager.normalizeRfsPath(path);
  if (getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
    getLog(CmsLog.CHANNEL_INIT).info("OpenCms: Base application path is " + m_basePath);
  }
  return m_basePath;
}
