{
  String onClic="listIndepAction('" + getListId() + "','"+ getId()+ "', '"+ CmsStringUtil.escapeJavaScript(wp.resolveMacros(getConfirmationMessage().key(wp.getLocale())))+ "');";
  return A_CmsHtmlIconButton.defaultButtonHtml(CmsHtmlIconButtonStyleEnum.SMALL_ICON_TEXT,getId(),getName().key(wp.getLocale()),getHelpText().key(wp.getLocale()),isEnabled(),getIconPath(),onClic);
}
