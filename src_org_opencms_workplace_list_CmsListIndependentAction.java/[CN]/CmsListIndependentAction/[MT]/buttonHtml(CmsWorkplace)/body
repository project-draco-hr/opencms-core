{
  if (isEnabled()) {
    String onClic=getList().getId() + "ListIndepAction('" + getId()+ "', '"+ CmsStringUtil.escapeJavaScript(wp.resolveMacros(getConfirmationMessage()))+ "');";
    return A_CmsHtmlIconButton.defaultButtonHtml(getId(),getName(),getHelpText(),isEnabled(),getIconPath(),onClic);
  }
  return "";
}
