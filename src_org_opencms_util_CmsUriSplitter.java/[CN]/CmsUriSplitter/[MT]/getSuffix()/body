{
  if (m_suffix != null) {
    StringBuffer result=new StringBuffer();
    if (m_query != null) {
      result.append('?');
      result.append(m_query);
    }
    if (m_anchor != null) {
      result.append('#');
      result.append(m_anchor);
    }
    m_suffix=result.toString();
  }
  return m_suffix;
}
