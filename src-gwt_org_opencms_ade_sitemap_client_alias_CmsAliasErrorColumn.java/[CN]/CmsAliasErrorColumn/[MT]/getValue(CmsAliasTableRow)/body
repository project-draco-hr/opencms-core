{
  String internalValue=getValueInternal(row);
  String v=internalValue;
  if (v == null) {
    v="";
  }
 else {
    v=SafeHtmlUtils.htmlEscape(v);
  }
  String html="<div class='" + (internalValue == null ? "" : WARNING_CLASS) + "' title='"+ v+ "'></div>";
  return SafeHtmlUtils.fromSafeConstant(html);
}
