{
  if (m_inputBuffer != null) {
    return new ByteArrayInputStream(m_inputBuffer);
  }
  ByteArrayOutputStream out=new ByteArrayOutputStream(4096);
  int c=0;
  while (true) {
    c=in.read();
    if (c < 0) {
      break;
    }
    out.write(c);
  }
  m_inputBuffer=out.toByteArray();
  return new ByteArrayInputStream(m_inputBuffer);
}
