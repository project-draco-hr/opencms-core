{
  m_hasMoreResults=searchObj.hasMore();
  if (searchObj.getPage() == 1) {
    getList().getElement().getStyle().setDisplay(Display.NONE);
    clearList();
    showParams(paramPanels);
    addContent(searchObj);
    getList().getElement().getStyle().clearDisplay();
    setScrollPosition(0);
  }
 else {
    showParams(paramPanels);
    addContent(searchObj);
  }
}
