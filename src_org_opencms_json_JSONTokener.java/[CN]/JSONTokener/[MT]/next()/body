{
  if (this.m_useLastChar) {
    this.m_useLastChar=false;
    if (this.m_lastChar != 0) {
      this.m_index+=1;
    }
    return this.m_lastChar;
  }
  int c;
  try {
    c=this.m_reader.read();
  }
 catch (  IOException exc) {
    throw new JSONException(exc);
  }
  if (c <= 0) {
    this.m_lastChar=0;
    return 0;
  }
  this.m_index+=1;
  this.m_lastChar=(char)c;
  return this.m_lastChar;
}
