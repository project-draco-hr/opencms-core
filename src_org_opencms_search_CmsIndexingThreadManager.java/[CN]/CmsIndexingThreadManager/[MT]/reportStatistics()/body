{
  CmsMessageContainer message=Messages.get().container(Messages.RPT_SEARCH_INDEXING_STATS_4,new Object[]{new Integer(m_fileCounter),new Integer(m_returnedCounter),new Integer(m_abandonedCounter),m_report.formatRuntime()});
  if (LOG.isInfoEnabled()) {
    LOG.info(message.key());
  }
  if (m_report != null) {
    m_report.println(Messages.get().container(Messages.RPT_SEARCH_INDEXING_END_0),I_CmsReport.FORMAT_HEADLINE);
    m_report.println(message);
  }
}
