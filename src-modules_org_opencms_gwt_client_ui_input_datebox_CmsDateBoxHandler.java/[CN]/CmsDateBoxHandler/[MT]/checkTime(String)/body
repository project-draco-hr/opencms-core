{
  if (!CmsDateConverter.validateTime(time)) {
    m_dateBox.setTimeError(Messages.get().key(Messages.ERR_DATEBOX_INVALID_TIME_FORMAT_1,CmsDateConverter.cutSuffix(time)));
    updateCloseBehavior(false);
    m_dateBox.syncPopup();
  }
 else {
    m_dateBox.setTimeError(null);
    updateCloseBehavior(true);
    m_dateBox.syncPopup();
  }
}
