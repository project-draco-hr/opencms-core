{
  addListener(ContextMenuOpenedOnTableRowEvent.class,contextMenuTableListener,ContextMenuOpenedListener.TableListener.MENU_OPENED_FROM_TABLE_ROW_METHOD);
  addListener(ContextMenuOpenedOnTableHeaderEvent.class,contextMenuTableListener,ContextMenuOpenedListener.TableListener.MENU_OPENED_FROM_TABLE_HEADER_METHOD);
  addListener(ContextMenuOpenedOnTableFooterEvent.class,contextMenuTableListener,ContextMenuOpenedListener.TableListener.MENU_OPENED_FROM_TABLE_FOOTER_METHOD);
}
