{
  int i=0;
  for (; ; ) {
    try {
      return (result.getTimestamp(column));
    }
 catch (    SQLException exc) {
      i++;
      if (i >= C_MAX_RETRIES) {
        throw exc;
      }
 else {
        if ((A_OpenCms.isLogging() && I_CmsLogChannels.C_PREPROCESSOR_IS_LOGGING)) {
          A_OpenCms.log(I_CmsLogChannels.C_MODULE_INFO,"Trying to get timestamp " + column + " #"+ i);
        }
      }
    }
  }
}
