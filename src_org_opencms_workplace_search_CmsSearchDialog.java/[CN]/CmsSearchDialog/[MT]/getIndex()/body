{
  CmsSearchIndex index=null;
  if (isInitialCall()) {
    index=OpenCms.getSearchManager().getIndexGeneral(getSettings().getUserSettings().getWorkplaceSearchIndexName());
  }
 else {
    index=OpenCms.getSearchManager().getIndexGeneral(getJsp().getRequest().getParameter("indexName.0"));
  }
  return index;
}
