{
  List retVal=new ArrayList();
  try {
    String[] names=CmsSearchParameters.SORT_NAMES;
    for (int i=0; i < names.length; i++) {
      retVal.add(new CmsSelectWidgetOption(names[i],(i == 0),key(A_CmsWidget.LABEL_PREFIX + names[i].toLowerCase())));
    }
  }
 catch (  Exception e) {
  }
  return retVal;
}
