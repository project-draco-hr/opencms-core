{
  CmsResource sitemapRes=cms.readResource(OpenCms.getSitemapManager().getSitemapForUri(cms,sitePath));
  Map<String,CmsXmlContentProperty> propertyConfig=OpenCms.getSitemapManager().getElementPropertyConfiguration(cms,sitemapRes,true);
  CmsClientSitemapEntry startEntry=toClientEntry(getNavBuilder().getNavigationForResource(sitePath),propertyConfig,false);
  if (startEntry != null) {
    startEntry.setSubEntries(getChildren(sitePath,-1,propertyConfig));
  }
  return startEntry;
}
