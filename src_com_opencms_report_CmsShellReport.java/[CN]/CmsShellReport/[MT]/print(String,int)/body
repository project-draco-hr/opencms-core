{
  StringBuffer buf;
switch (format) {
case C_FORMAT_HEADLINE:
    buf=new StringBuffer();
  buf.append("------ ");
buf.append(value);
System.out.print(buf);
break;
case C_FORMAT_WARNING:
buf=new StringBuffer();
buf.append("!!! ");
buf.append(value);
System.out.print(buf);
break;
case C_FORMAT_NOTE:
case C_FORMAT_OK:
case C_FORMAT_DEFAULT:
default :
System.out.print(value);
}
}
