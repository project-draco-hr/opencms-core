{
  String userName=getSettings().getUser().getName();
  if (m_typesLocalized[0] == null) {
    m_typesLocalized[0]=key("label.group");
    m_typesLocalized[1]=key("label.user");
  }
  setEditable(false);
  setShowInherit(false);
  try {
    setCurPermissions(getCms().getPermissions(getParamFile(),userName));
    CmsResource resource=getCms().readFileHeader(getParamFile());
    if (resource.isFolder()) {
      setShowInherit(true);
    }
  }
 catch (  CmsException e) {
  }
  try {
    if (getCms().isAdmin() || ((m_curPermissions.getAllowedPermissions() & I_CmsConstants.C_PERMISSION_CONTROL) > 0 && !((m_curPermissions.getDeniedPermissions() & I_CmsConstants.C_PERMISSION_CONTROL) > 0))) {
      setEditable(true);
    }
  }
 catch (  CmsException e) {
  }
}
