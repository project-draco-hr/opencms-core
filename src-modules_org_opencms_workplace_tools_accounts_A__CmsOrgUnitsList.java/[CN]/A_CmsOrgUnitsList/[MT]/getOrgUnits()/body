{
  List ous=OpenCms.getRoleManager().getOrgUnitsForRole(getCms(),CmsRole.ACCOUNT_MANAGER.forOrgUnit(""),true);
  Iterator it=ous.iterator();
  while (it.hasNext()) {
    CmsOrganizationalUnit ou=(CmsOrganizationalUnit)it.next();
    if (ou.hasFlagHide()) {
      it.remove();
    }
  }
  return ous;
}
