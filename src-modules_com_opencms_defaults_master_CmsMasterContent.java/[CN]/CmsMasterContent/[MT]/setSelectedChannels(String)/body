{
  StringTokenizer tk=new StringTokenizer(channels,",");
  Vector v=new Vector();
  int tokens=tk.countTokens();
  if (channels != null && channels.equals("empty")) {
    m_selectedChannels=v;
  }
 else   if (tokens > 0) {
    for (int i=0; i < tokens; i++) {
      v.addElement(tk.nextToken());
    }
    m_selectedChannels=v;
  }
}
