{
  if (m_exitCalled) {
    return;
  }
  m_exitCalled=true;
  try {
    if (m_additionaShellCommands != null) {
      m_additionaShellCommands.shellExit();
    }
 else {
      m_shellCommands.shellExit();
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
  try {
    m_opencms.shutDown();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
