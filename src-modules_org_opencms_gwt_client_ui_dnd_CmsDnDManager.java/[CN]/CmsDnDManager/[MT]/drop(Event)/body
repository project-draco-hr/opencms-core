{
  if (!m_dragging) {
    return;
  }
  storeEventPos(event);
  if ((m_currentTarget == null) || (m_position == null) || !m_draggable.canDrop(m_currentTarget,m_position)) {
    cancel();
    return;
  }
  if ((event.getButton() & NativeEvent.BUTTON_LEFT) == 0) {
    cancel();
    return;
  }
  stopDragging(event);
}
