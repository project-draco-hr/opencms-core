{
switch (getBundleType()) {
case DESCRIPTOR:
    return m_cms.getSitePath(m_desc);
case PROPERTY:
  return null != m_bundleFiles.get(getLocale()) ? m_cms.getSitePath(m_bundleFiles.get(getLocale()).getFile()) : m_cms.getSitePath(m_resource);
case XML:
return m_cms.getSitePath(m_resource);
default :
throw new IllegalArgumentException();
}
}
