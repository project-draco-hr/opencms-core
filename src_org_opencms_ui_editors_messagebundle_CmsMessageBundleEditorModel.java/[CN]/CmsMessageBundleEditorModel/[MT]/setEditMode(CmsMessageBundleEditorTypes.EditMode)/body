{
  try {
    if ((mode == CmsMessageBundleEditorTypes.EditMode.MASTER) && (null == m_descFile)) {
      m_descFile=CmsMessageBundleEditorTypes.LockedFile.lockResource(m_cms,m_desc);
    }
    m_editMode=mode;
  }
 catch (  @SuppressWarnings("unused") CmsException e) {
    return false;
  }
  return true;
}
