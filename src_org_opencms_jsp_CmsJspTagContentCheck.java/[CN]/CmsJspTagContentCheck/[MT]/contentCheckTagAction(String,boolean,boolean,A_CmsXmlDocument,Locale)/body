{
  boolean found=false;
  String elements[]=CmsStringUtil.splitAsArray(elementList,',');
  for (int i=(elements.length - 1); i >= 0; i--) {
    String element=elements[i].trim();
    found=found || content.hasValue(element,locale);
    if (found && checknone) {
      return false;
    }
    if (found && !checkall && !checknone) {
      return true;
    }
  }
  if (!found && checknone) {
    return true;
  }
  return found;
}
