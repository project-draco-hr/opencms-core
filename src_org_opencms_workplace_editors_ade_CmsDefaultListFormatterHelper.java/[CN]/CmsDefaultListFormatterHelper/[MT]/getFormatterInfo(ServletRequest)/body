{
  CmsFormatterInfoBean info=null;
  try {
    info=(CmsFormatterInfoBean)req.getAttribute(CmsADEManager.ATTR_FORMATTER_INFO);
  }
 catch (  Exception e) {
    throw new CmsException(org.opencms.xml.containerpage.Messages.get().container(org.opencms.xml.containerpage.Messages.ERR_READING_FORMATTER_INFO_FROM_REQUEST_0),e);
  }
  return info;
}
