{
  List retVal=new ArrayList();
  CmsMessages messages=Messages.get().getBundle(getLocale());
  retVal.add(new CmsSelectWidgetOption(CmsProjectResourcesDisplayMode.ALL_CHANGES.getMode(),true,messages.key(Messages.GUI_PROJECT_MODE_ALLCHANGES_0)));
  retVal.add(new CmsSelectWidgetOption(CmsProjectResourcesDisplayMode.NEW_FILES.getMode(),false,messages.key(Messages.GUI_PROJECT_MODE_NEWFILES_0)));
  retVal.add(new CmsSelectWidgetOption(CmsProjectResourcesDisplayMode.MODIFIED_FILES.getMode(),false,messages.key(Messages.GUI_PROJECT_MODE_MODFILES_0)));
  retVal.add(new CmsSelectWidgetOption(CmsProjectResourcesDisplayMode.DELETED_FILES.getMode(),false,messages.key(Messages.GUI_PROJECT_MODE_DELFILES_0)));
  return retVal;
}
