{
  if (getItem() != null) {
    try {
      String usrName=getItem().get(LIST_COLUMN_LOGIN).toString();
      CmsUser user=getCms().readUser(usrName);
      if (user.getDisabled()) {
        return "buttons/apply_in.gif";
      }
      return "buttons/apply.gif";
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  return super.getIconPath();
}
