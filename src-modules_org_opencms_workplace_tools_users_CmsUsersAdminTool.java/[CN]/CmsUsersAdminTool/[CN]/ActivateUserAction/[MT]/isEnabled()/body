{
  if (getItem() != null) {
    try {
      String usrName=getItem().get(LIST_COLUMN_LOGIN).toString();
      return !m_cms.userInGroup(usrName,OpenCms.getDefaultUsers().getGroupAdministrators());
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  return super.isEnabled();
}
