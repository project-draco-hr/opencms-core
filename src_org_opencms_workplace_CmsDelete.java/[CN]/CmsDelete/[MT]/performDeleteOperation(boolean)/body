{
  int deleteOption=-1;
  if (isFolder && !DIALOG_WAIT.equals(getParamAction())) {
    return false;
  }
  deleteOption=Boolean.valueOf(getParamDeleteVfsLinks()).booleanValue() ? I_CmsConstants.C_DELETE_OPTION_DELETE_SIBLINGS : I_CmsConstants.C_DELETE_OPTION_PRESERVE_SIBLINGS;
  checkLock(getParamResource());
  getCms().deleteResource(getParamResource(),deleteOption);
  return true;
}
