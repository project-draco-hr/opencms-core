{
  CmsLock lock=null;
  try {
    lock=getCms().getLock(getParamResource());
  }
 catch (  CmsException e) {
    lock=CmsLock.getNullLock();
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isErrorEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).error("Error getting lock state for resource " + getParamResource(),e);
    }
    return false;
  }
  int type=lock.getType();
  return (type == CmsLock.C_TYPE_EXCLUSIVE || type == CmsLock.C_TYPE_INHERITED);
}
