{
  Date date=null;
  if (isEnabled()) {
    try {
      if (m_dateOnly) {
        date=CmsDateConverter.toDayDate(m_box.getText());
      }
 else {
        date=CmsDateConverter.toDate(m_box.getText());
      }
      setErrorMessage(null);
    }
 catch (    Exception e) {
      setErrorMessage(Messages.get().key(Messages.ERR_DATEBOX_INVALID_DATE_FORMAT_0));
    }
  }
  return date;
}
