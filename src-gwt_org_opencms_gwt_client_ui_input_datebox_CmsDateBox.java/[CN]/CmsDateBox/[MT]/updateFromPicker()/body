{
  try {
    checkTime();
    Date date=m_picker.getValue();
    String timeAsString=getTimeText();
    date=CmsDateConverter.getDateWithTime(date,timeAsString);
    setValue(date);
    setErrorMessage(null);
    fireChange(date);
  }
 catch (  Exception e) {
    Window.alert(e.getMessage());
  }
}
