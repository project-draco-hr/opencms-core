{
  List result=new ArrayList();
  int count=1;
  Object o;
  String xpath=CmsXmlUtils.createXpath(path,1);
  xpath=CmsXmlUtils.removeXpathIndex(xpath);
  do {
    String subpath=CmsXmlUtils.createXpathElement(xpath,count);
    o=getBookmark(subpath,locale);
    if (o != null) {
      result.add(o);
      count++;
    }
  }
 while (o != null);
  return result;
}
