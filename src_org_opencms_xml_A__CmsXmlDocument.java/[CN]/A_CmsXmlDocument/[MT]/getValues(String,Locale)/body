{
  List result=new ArrayList();
  int count=0;
  Object o;
  name=CmsXmlUtils.removeXpathIndex(name);
  do {
    String path=CmsXmlUtils.createXpathElement(name,count);
    o=getBookmark(path,locale);
    if (o != null) {
      result.add(o);
      count++;
    }
  }
 while (o != null);
  return result;
}
