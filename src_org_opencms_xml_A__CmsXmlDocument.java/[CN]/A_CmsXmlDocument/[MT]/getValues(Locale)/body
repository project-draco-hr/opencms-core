{
  List result=new ArrayList();
  String prefix='/' + locale.toString() + '/';
  Iterator i=m_bookmarks.keySet().iterator();
  while (i.hasNext()) {
    String key=(String)i.next();
    if (key.startsWith(prefix)) {
      result.add(m_bookmarks.get(key));
    }
  }
  Collections.sort(result);
  return result;
}
