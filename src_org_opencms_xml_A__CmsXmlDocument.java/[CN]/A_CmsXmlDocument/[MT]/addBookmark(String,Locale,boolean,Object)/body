{
  addLocale(locale);
  m_bookmarks.put(getBookmarkName(name,locale),value);
  Object o;
  if (enabled) {
    o=m_elementLocales.get(name);
    if (o != null) {
      ((Set)o).add(locale);
    }
 else {
      Set set=new HashSet();
      set.add(locale);
      m_elementLocales.put(name,set);
    }
  }
  o=m_elementNames.get(locale);
  if (o != null) {
    ((Set)o).add(name);
  }
 else {
    Set set=new HashSet();
    set.add(name);
    m_elementNames.put(locale,set);
  }
}
