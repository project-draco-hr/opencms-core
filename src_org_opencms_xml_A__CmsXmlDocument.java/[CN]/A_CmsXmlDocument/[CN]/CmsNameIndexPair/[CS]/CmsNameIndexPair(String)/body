{
  int end=name.length() - 1;
  if (name.charAt(end) == ']') {
    int pos=name.lastIndexOf('[');
    if (pos > 0) {
      String number=name.substring(pos + 1,end);
      try {
        m_index=Integer.parseInt(number,10);
        if (m_index < 0) {
          m_index=0;
        }
        m_name=name.substring(0,pos);
      }
 catch (      Exception e) {
        m_index=0;
        m_name=name;
      }
    }
  }
 else {
    m_index=0;
    m_name=name;
  }
}
