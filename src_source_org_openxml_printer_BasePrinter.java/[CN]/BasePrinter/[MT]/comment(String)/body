{
  StringBuffer buffer;
  int index;
  ElementState state;
  state=content();
  buffer=new StringBuffer(40);
  index=text.indexOf("-->");
  if (index >= 0)   buffer.append("<!--").append(text.substring(0,index)).append("-->");
 else   buffer.append("<!--").append(text).append("-->");
  if (state == null) {
    if (_preRoot == null)     _preRoot=new Vector();
    _preRoot.addElement(buffer.toString());
  }
 else {
    indent();
    printText(buffer,false);
    unindent();
  }
}
