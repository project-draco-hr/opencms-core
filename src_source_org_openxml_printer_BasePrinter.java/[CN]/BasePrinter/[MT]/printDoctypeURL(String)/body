{
  StringBuffer result;
  int i;
  _text.append('"');
  for (i=0; i < url.length(); ++i) {
    if (url.charAt(i) == '"' || url.charAt(i) < 0x20 || url.charAt(i) > 0x7F)     _text.append("%").append(Integer.toHexString(url.charAt(i)));
 else     _text.append(url.charAt(i));
  }
  _text.append('"');
}
