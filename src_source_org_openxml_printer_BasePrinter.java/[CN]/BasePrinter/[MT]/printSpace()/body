{
  if (_text.length() > 0) {
    if (_format.getLineWidth() > 0 && _thisIndent + _line.length() + _spaces+ _text.length() > _format.getLineWidth())     flushLine();
    while (_spaces > 0) {
      _line.append(' ');
      --_spaces;
    }
    _line.append(_text);
    _text=new StringBuffer(20);
  }
  ++_spaces;
}
