{
  if (m_window != null) {
    m_window.close();
    m_window=null;
  }
  if (ids == null) {
    ids=Lists.newArrayList();
    for (    CmsResource res : getResources()) {
      ids.add(res.getStructureId());
    }
  }
  for (  CmsUUID id : ids) {
    if (id.isNullUUID()) {
      m_explorer.updateAll();
    }
  }
  m_explorer.update(ids);
}
