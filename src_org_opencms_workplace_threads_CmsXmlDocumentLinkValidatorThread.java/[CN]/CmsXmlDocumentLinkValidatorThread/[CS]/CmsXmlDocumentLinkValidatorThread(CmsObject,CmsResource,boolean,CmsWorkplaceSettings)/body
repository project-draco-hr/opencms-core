{
  super(cms,Messages.get().key(cms.getRequestContext().getLocale(),Messages.GUI_HTML_LINK_VALIDATOR_THREAD_NAME_1,new Object[]{cms.getRequestContext().currentProject().getName()}));
  m_directPublishResource=directPublishResource;
