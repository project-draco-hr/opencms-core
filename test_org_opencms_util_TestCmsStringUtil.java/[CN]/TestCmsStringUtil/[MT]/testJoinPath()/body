{
  assertEquals("/system/",CmsStringUtil.joinPaths("/system/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("/system","/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("/system",""));
  assertEquals("/system/",CmsStringUtil.joinPaths("/system/","/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("/","/system/","/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("/","system","/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("","/system/",""));
  assertEquals("/system/",CmsStringUtil.joinPaths("/system/","/","/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("/","/","/system/"));
  assertEquals("/system/",CmsStringUtil.joinPaths("","","/system/"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo/","/bar","baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo","bar","baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo/","/bar/","/baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo","/bar/","baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo//bar/","/baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("/foo/////////bar/","/baz"));
  assertEquals("/foo/bar/baz",CmsStringUtil.joinPaths("//////////foo/////////bar/////","//////baz"));
}
