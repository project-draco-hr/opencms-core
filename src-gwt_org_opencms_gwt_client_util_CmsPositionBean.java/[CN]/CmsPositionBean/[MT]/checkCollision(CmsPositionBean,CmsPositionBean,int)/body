{
  if ((posA.getLeft() - margin) > (posB.getLeft() + posB.getWidth())) {
    return false;
  }
  if ((posA.getLeft() + posA.getWidth()) < (posB.getLeft() - margin)) {
    return false;
  }
  if ((posA.getTop() - margin) > (posB.getTop() + posB.getHeight())) {
    return false;
  }
  if ((posA.getTop() + posA.getHeight()) < (posB.getTop() - margin)) {
    return false;
  }
  return true;
}
