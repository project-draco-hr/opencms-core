{
  if (posA.getLeft() > (posB.getLeft() + posB.getWidth())) {
    return false;
  }
  if ((posA.getLeft() + posA.getWidth()) < posB.getLeft()) {
    return false;
  }
  if (posA.getTop() > (posB.getTop() + posB.getHeight())) {
    return false;
  }
  if ((posA.getTop() + posA.getHeight()) < posB.getTop()) {
    return false;
  }
  return true;
}
