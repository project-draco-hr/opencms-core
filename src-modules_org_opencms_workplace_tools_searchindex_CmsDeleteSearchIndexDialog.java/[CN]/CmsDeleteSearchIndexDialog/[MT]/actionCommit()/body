{
  List errors=new ArrayList();
  try {
    m_searchManager.removeSearchIndex(m_index);
    clearDialogObject();
    writeConfiguration();
    setParamCloseLink(CmsToolManager.linkForToolPath(getJsp(),getParentPath()));
  }
 catch (  Throwable t) {
    errors.add(t);
  }
  setCommitErrors(errors);
}
