{
  if (dbc == null) {
    LOG.error(Messages.get().getBundle().key(Messages.LOG_NULL_DB_CONTEXT_0));
  }
  try {
    if (con != null && !con.isClosed()) {
      con.close();
    }
  }
 catch (  SQLException e) {
  }
 finally {
    con=null;
  }
  try {
    if (stmnt != null) {
      stmnt.close();
    }
  }
 catch (  SQLException e) {
  }
 finally {
    stmnt=null;
  }
  try {
    if (res != null) {
      res.close();
    }
  }
 catch (  SQLException e) {
  }
 finally {
    res=null;
  }
}
