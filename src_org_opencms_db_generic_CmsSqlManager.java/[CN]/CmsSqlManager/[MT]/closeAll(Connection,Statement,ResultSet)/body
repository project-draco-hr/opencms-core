{
  try {
    if (con != null && !con.isClosed()) {
      con.close();
    }
  }
 catch (  SQLException e) {
  }
  try {
    if (stmnt != null) {
      stmnt.close();
    }
  }
 catch (  SQLException e) {
  }
  try {
    if (res != null) {
      res.close();
    }
  }
 catch (  SQLException e) {
  }
  res=null;
  stmnt=null;
  con=null;
}
