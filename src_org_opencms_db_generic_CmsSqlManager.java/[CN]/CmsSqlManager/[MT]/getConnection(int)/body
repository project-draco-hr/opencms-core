{
  Connection conn=null;
  if (id > 1) {
    conn=DriverManager.getConnection(m_offlinePoolUrl);
  }
 else   if (id == 1) {
    conn=DriverManager.getConnection(m_onlinePoolUrl);
  }
 else   if (id < 0) {
    conn=DriverManager.getConnection(m_backupPoolUrl);
  }
 else {
    conn=DriverManager.getConnection(m_offlinePoolUrl);
  }
  return conn;
}
