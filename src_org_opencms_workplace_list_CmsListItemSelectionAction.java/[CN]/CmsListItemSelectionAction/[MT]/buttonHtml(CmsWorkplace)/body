{
  if (!isVisible()) {
    return "";
  }
  String html="<input type='radio' value='" + getItem().getId() + "' name='"+ getListId()+ getId()+ "'";
  if (!isEnabled()) {
    html+=" disabled";
  }
  if (getItem().getId().equals(getSelectedItemId())) {
    html+=" checked";
  }
  html+=">\n";
  return A_CmsHtmlIconButton.defaultButtonHtml(wp.getJsp(),CmsHtmlIconButtonStyleEnum.SMALL_ICON_TEXT,getId(),html,getHelpText().key(wp.getLocale()),true,null,null,null);
}
