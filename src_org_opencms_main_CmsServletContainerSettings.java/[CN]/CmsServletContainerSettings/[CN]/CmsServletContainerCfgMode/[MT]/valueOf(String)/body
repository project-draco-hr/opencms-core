{
  if (mode == null) {
    return null;
  }
  if (mode.equalsIgnoreCase(MODE_NONE.getMode())) {
    return MODE_NONE;
  }
  if (mode.equalsIgnoreCase(MODE_MANUAL.getMode())) {
    return MODE_MANUAL;
  }
  if (mode.equalsIgnoreCase(MODE_AUTO.getMode())) {
    return MODE_AUTO;
  }
  return null;
}
