{
  if (getItem() != null) {
    try {
      String usrName=getCms().readUser(new CmsUUID(getItem().getId())).getName();
      return !getCms().userInGroup(usrName,OpenCms.getDefaultUsers().getGroupAdministrators());
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  return super.isEnabled();
}
