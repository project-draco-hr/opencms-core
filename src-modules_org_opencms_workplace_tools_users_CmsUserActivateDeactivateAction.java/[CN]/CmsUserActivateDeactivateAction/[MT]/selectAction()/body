{
  if (getItem() != null) {
    String usrName=getItem().get(CmsUsersAdminTool.LIST_COLUMN_LOGIN).toString();
    try {
      CmsUser user=m_cms.readUser(usrName);
      if (user.getDisabled()) {
        return m_actAction;
      }
    }
 catch (    CmsException e) {
    }
  }
  return m_deactAction;
}
