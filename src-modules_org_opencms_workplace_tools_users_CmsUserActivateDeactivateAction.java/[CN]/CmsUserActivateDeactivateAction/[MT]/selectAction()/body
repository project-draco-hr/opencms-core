{
  if (getItem() != null) {
    String usrName=getItem().get(CmsUsersAdminTool.LIST_COLUMN_LOGIN).toString();
    try {
      CmsUser user=getCms().readUser(usrName);
      if (user.getDisabled()) {
        return getFirstAction();
      }
    }
 catch (    CmsException e) {
    }
  }
  return getSecondAction();
}
