{
  Node node=document.selectSingleNode(xpath);
  if (node == null) {
    if (xpath.equals(getXPathsToUpdate().get(0))) {
      CmsSetupXmlHelper.setValue(document,xpath + "/@" + I_CmsXmlConfiguration.A_CLASS,CmsXmlContainerPageLoader.class.getName());
    }
 else     if (xpath.equals(getXPathsToUpdate().get(1))) {
      CmsSetupXmlHelper.setValue(document,xpath + "/@" + I_CmsXmlConfiguration.A_CLASS,CmsXmlSitemapLoader.class.getName());
    }
    return true;
  }
  return false;
}
