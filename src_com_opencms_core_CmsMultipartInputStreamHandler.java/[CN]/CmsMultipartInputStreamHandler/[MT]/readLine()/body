{
  StringBuffer sbuf=new StringBuffer();
  int result;
  String line;
  do {
    result=this.readLine(buf,0,buf.length);
    if (result != -1) {
      sbuf.append(new String(buf,0,result,"ISO-8859-1"));
    }
  }
 while (result == buf.length);
  if (sbuf.length() == 0) {
    return null;
  }
  sbuf.setLength(sbuf.length() - 2);
  return sbuf.toString();
}
