{
  A_CmsGroupUsersList wp1=(list1.isActive() ? (A_CmsGroupUsersList)list1 : (A_CmsGroupUsersList)list2);
  A_CmsGroupUsersList wp2=(!list1.isActive() ? (A_CmsGroupUsersList)list1 : (A_CmsGroupUsersList)list2);
  wp1.updateGroupList();
  wp1.actionDialog();
  wp1.refreshList();
  wp2.refreshList();
}
