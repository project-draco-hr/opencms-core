{
  m_lockRecord=CmsLockUtil.ensureLock(cms,resource);
  m_file=cms.readFile(resource);
  m_new=false;
  CmsProperty encodingProperty=cms.readPropertyObject(m_file,CmsPropertyDefinition.PROPERTY_CONTENT_ENCODING,true);
  m_encoding=CmsEncoder.lookupEncoding(encodingProperty.getValue(""),OpenCms.getSystemInfo().getDefaultEncoding());
}
