{
  CmsContainerPageElementPanel dragElement=new CmsContainerPageElementPanel(element,dragParent,elementData.getClientId(),elementData.getSitePath(),elementData.getNoEditReason(),elementData.getTitle(),elementData.getSubTitle(),elementData.getResourceType(),elementData.hasSettings(dragParent.getContainerId()),elementData.hasViewPermission(),elementData.hasWritePermission(),elementData.isReleasedAndNotExpired(),elementData.isNewEditorDisabled(),elementData.getElementView());
  if (m_controller.isInlineEditable(dragElement,dragParent)) {
    addOptionBar(dragElement);
  }
  if (m_controller.requiresOptionBar(dragElement,dragParent)) {
    dragElement.initInlineEditor(m_controller);
  }
  return dragElement;
}
