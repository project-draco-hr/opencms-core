{
  HttpSession session=getRequest().getSession(false);
  if (session != null) {
    session.invalidate();
  }
  if (LOG.isInfoEnabled()) {
    LOG.info(Messages.get().key(Messages.LOG_LOGOUT_SUCCESFUL_2,getRequestContext().currentUser().getName(),getRequestContext().addSiteRoot(getRequestContext().getUri())));
  }
  getResponse().sendRedirect(getFormLink());
}
