{
  HttpSession session=getRequest().getSession(false);
  if (session != null) {
    session.invalidate();
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug(Messages.get().key(Messages.LOG_LOGOUT_SUCCESFUL_2,getRequestContext().addSiteRoot(getRequestContext().getUri())));
  }
  getResponse().sendRedirect(getFormLink());
}
