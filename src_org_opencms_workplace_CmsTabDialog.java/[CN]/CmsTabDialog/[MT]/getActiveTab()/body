{
  if (m_activeTab < 0) {
    String paramTab=getParamTab();
    int tab=1;
    if (paramTab != null && !"".equals(paramTab)) {
      try {
        tab=Integer.parseInt(paramTab);
      }
 catch (      NumberFormatException e) {
        if (LOG.isInfoEnabled()) {
          LOG.info(e.getLocalizedMessage());
        }
      }
    }
    setParamTab("" + tab);
    m_activeTab=tab;
    return tab;
  }
 else {
    return m_activeTab;
  }
}
