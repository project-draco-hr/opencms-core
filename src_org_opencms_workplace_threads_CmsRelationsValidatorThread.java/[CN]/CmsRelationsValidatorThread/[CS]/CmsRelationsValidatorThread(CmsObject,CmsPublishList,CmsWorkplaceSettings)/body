{
  super(cms,Messages.get().getBundle().key(Messages.GUI_HTML_LINK_VALIDATOR_THREAD_NAME_1,new Object[]{cms.getRequestContext().currentProject().getName()}));
  m_publishList=publishList;
  if (m_publishList == null) {
    try {
      m_publishList=OpenCms.getPublishManager().getPublishList(cms);
    }
 catch (    CmsException e) {
    }
  }
  m_savePublishList=true;
  m_settings=settings;
  initHtmlReport(cms.getRequestContext().getLocale());
}
