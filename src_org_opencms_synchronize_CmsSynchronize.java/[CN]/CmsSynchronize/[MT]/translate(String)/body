{
  String translation=null;
  Iterator i=m_synchronizeModifications.iterator();
  while (i.hasNext()) {
    try {
      translation=((I_CmsSynchonizeModification)i.next()).translate(m_cms,name);
    }
 catch (    CmsSynchronizeException e) {
      if (OpenCms.isLogging(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN))       OpenCms.log(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN,". CmsSyncModification class : external translation " + name + ":"+ e.toString());
      break;
    }
  }
  if (translation == null) {
    translation=m_cms.getRequestContext().getFileTranslator().translateResource(name);
  }
  return translation;
}
