{
  String xpath=xpathForType("otheroptions");
  if (doc.selectSingleNode(xpath) != null) {
    return false;
  }
  createExplorerType(doc,"/opencms/workplace/explorertypes/explorertype[@name='otheroptions']","otheroptions","fileicon.otheroptions","menu.png","folder");
  createEtNewResource(doc,xpath,"newresource.jsp?page=otheroptions",19,false,false,"desc.otheroptions",null);
  return true;
}
