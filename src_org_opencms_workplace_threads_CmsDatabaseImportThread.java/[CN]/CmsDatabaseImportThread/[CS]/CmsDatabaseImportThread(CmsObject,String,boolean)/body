{
  super(cms,Messages.get().key(cms.getRequestContext().getLocale(),Messages.GUI_DB_IMPORT_THREAD_NAME_1,new Object[]{importFile}));
  m_importFile=importFile;
  if (old) {
    initOldHtmlReport(cms.getRequestContext().getLocale());
  }
 else {
    initHtmlReport(cms.getRequestContext().getLocale());
  }
}
