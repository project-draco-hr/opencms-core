{
  String modelFolderPath=CmsStringUtil.joinPaths(m_adeConfig.getBasePath(),".content/.new");
  try {
    CmsResource result=m_cms.readFolder(modelFolderPath);
    return result;
  }
 catch (  CmsVfsResourceNotFoundException e) {
    CmsResource result=m_cms.createResource(modelFolderPath,getType(CmsResourceTypeFolder.getStaticTypeName()),null,new ArrayList<CmsProperty>());
    tryUnlock(result);
    return result;
  }
}
