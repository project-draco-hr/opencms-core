{
  StringBuffer html=new StringBuffer();
  if (hasSubmenu) {
    html.append("<div class=\"");
    html.append(I_CmsLayoutBundle.INSTANCE.contextmenuCss().arrow() + " " + I_CmsLayoutBundle.INSTANCE.iconsCss().uiIcon()+ " "+ I_CmsButton.UiIcon.triangle_1_e.name());
    html.append("\"></div>");
  }
  html.append("<div class=\"");
  html.append(I_CmsLayoutBundle.INSTANCE.contextmenuCss().label());
  html.append("\">" + text + "</div>");
  return html.toString();
}
