{
  ElementState state;
  state=content();
  if (state != null && (state.tagName.equalsIgnoreCase("SCRIPT") || state.tagName.equalsIgnoreCase("STYLE"))) {
    if (_xhtml)     super.characters(text,true,false);
 else     super.characters(text,false,true);
  }
 else   super.characters(text,cdata,unescaped);
}
