{
  StringBuffer buffer=new StringBuffer(128);
  buffer.append("<script>\n");
  buffer.append("document.location.href=\"");
  buffer.append(url);
  buffer.append("\";\n");
  buffer.append("</script>\n");
  try {
    res.getOutputStream().write(buffer.toString().getBytes());
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
