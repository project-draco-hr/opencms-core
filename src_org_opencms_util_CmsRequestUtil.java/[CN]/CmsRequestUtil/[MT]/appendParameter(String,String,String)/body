{
  if (CmsStringUtil.isEmpty(url)) {
    return null;
  }
  int pos=url.indexOf('?');
  StringBuffer result=new StringBuffer(256);
  result.append(url);
  if (pos >= 0) {
    result.append('&');
  }
 else {
    result.append('?');
  }
  result.append(paramName);
  result.append('=');
  result.append(paramValue);
  return result.toString();
}
