{
  try {
    URL imgURL=new URL(path);
    if (imgURL != null) {
      ImageIcon img=new ImageIcon(imgURL);
      return img;
    }
 else {
      System.err.println("Couldn't find file: " + path);
      return null;
    }
  }
 catch (  Exception e) {
    System.err.println(e);
    return null;
  }
}
