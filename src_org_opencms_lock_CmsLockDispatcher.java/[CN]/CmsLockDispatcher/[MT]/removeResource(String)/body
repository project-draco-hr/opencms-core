{
  String lockedPath=null;
  Iterator i=null;
  m_locks.remove(resourcename);
  if (resourcename.endsWith(I_CmsConstants.C_FOLDER_SEPARATOR)) {
    i=m_locks.keySet().iterator();
    while (i.hasNext()) {
      lockedPath=(String)i.next();
      if (lockedPath.startsWith(resourcename)) {
        i.remove();
      }
    }
  }
}
