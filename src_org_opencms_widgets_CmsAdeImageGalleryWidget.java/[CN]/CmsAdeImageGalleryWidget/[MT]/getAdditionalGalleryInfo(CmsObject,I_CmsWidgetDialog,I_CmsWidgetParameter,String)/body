{
  CmsVfsImageWidgetConfiguration config=new CmsVfsImageWidgetConfiguration(cms,widgetDialog,param,configurationParam);
  JSONObject result=new JSONObject();
  result.put(ImageWidgetInfo.useFormats.name(),config.isShowFormat());
  result.put(ImageWidgetInfo.imageFormats.name(),new JSONArray(config.getFormatValues()));
  result.put(ImageWidgetInfo.imageFormatNames.name(),CmsEncoder.escape(config.getSelectFormatString(),CmsEncoder.ENCODING_UTF_8));
  return result;
}
