{
  for (  ParamPublish param : params) {
    String value=request.getParameter(param.getName());
    if (value == null) {
      if (result != null) {
        result.put(CmsADEServer.JsonResponse.ERROR.getName(),Messages.get().getBundle().key(Messages.ERR_JSON_MISSING_PARAMETER_1,param.getName()));
      }
      return false;
    }
  }
  return true;
}
