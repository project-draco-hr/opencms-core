{
  String entryPoint=null;
  for (  Map.Entry<String,String> entry : active.entrySet()) {
    if (!entry.getValue().equals(sitemapPath)) {
      continue;
    }
    if (!entry.getKey().startsWith(cms.getRequestContext().getLocale().toString())) {
      continue;
    }
    entryPoint=entry.getKey();
    break;
  }
  return entryPoint;
}
