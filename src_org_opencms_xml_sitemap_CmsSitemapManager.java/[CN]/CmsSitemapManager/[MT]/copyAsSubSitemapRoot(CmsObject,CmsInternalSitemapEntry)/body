{
  CmsInternalSitemapEntry clone=new CmsInternalSitemapEntry(entry.getId(),"",entry.getResourceId(),"",entry.getTitle(),entry.getProperties(),new ArrayList<CmsInternalSitemapEntry>(),entry.getContentId());
  clone.setRuntimeInfo(entry.getSitePath(cms),0,new HashMap<String,String>());
  return clone;
}
