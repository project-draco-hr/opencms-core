{
  String permissions;
  try {
    permissions=getCms().getPermissions(getCms().getSitePath(getResource())).getPermissionString();
  }
 catch (  Throwable e) {
    try {
      permissions=getCms().getPermissions(getResource().getRootPath()).getPermissionString();
    }
 catch (    Throwable e1) {
      permissions=e1.getMessage();
    }
  }
  return permissions;
}
