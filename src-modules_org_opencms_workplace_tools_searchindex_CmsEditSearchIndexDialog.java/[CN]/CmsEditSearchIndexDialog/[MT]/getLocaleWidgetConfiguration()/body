{
  List<CmsSelectWidgetOption> result=new ArrayList<CmsSelectWidgetOption>();
  for (  Locale locale : m_searchManager.getAnalyzers().keySet()) {
    CmsSelectWidgetOption option=new CmsSelectWidgetOption(locale.toString(),locale.equals(m_index.getLocale()));
    result.add(option);
  }
  return result;
}
