{
  checkIsClosed();
  String key=str + ":" + param+ ":"+ param2+ ":"+ param3;
  if (!m_statementPool.containsKey(key)) {
    m_statementPool.put(key,new CmsPreparedStatement(m_originalConnection.prepareStatement(str,param,param2,param3),this));
  }
  return (PreparedStatement)m_statementPool.get(key);
}
