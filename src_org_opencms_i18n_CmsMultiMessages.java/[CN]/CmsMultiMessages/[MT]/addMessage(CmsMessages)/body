{
  if (!m_locale.equals(message.getLocale())) {
    throw new CmsIllegalArgumentException(Messages.get().container(Messages.ERR_MULTIMSG_LOCALE_DOES_NOT_MATCH_2,message.getLocale(),m_locale));
  }
  if (!m_messages.contains(message)) {
    if (m_messageCache != null) {
      m_messageCache=new Hashtable();
    }
    m_messages.add(message);
  }
}
