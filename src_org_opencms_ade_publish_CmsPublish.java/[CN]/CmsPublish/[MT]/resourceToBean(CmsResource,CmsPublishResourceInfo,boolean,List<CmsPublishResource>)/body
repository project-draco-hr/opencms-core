{
  CmsResourceUtil resUtil=new CmsResourceUtil(m_cms,resource);
  CmsPermissionInfo permissionInfo=OpenCms.getADEManager().getPermissionInfo(m_cms,resource,null);
  CmsPublishResource pubResource=new CmsPublishResource(resource.getStructureId(),resUtil.getFullPath(),resUtil.getTitle(),resUtil.getResourceTypeName(),resource.getState(),permissionInfo,resource.getDateLastModified(),resUtil.getUserLastModified(),CmsVfsService.formatDateTime(m_cms,resource.getDateLastModified()),removable,info,related);
  return pubResource;
}
