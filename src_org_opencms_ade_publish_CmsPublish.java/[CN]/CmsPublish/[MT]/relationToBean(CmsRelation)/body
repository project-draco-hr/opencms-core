{
  CmsPermissionInfo permissionInfo=OpenCms.getADEManager().getPermissionInfo(m_cms,relation.getTarget(m_cms,CmsResourceFilter.ALL),null);
  return new CmsPublishResource(relation.getTargetId(),relation.getTargetPath(),relation.getTargetPath(),CmsResourceTypePlain.getStaticTypeName(),null,CmsResourceState.STATE_UNCHANGED,permissionInfo,0,null,null,false,null,null);
}
