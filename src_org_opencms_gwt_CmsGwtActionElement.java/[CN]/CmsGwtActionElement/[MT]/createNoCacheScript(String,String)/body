{
  String result="<script type=\"text/javascript\" src=\"" + CmsWorkplace.getResourceUri("ade/" + moduleName + "/"+ moduleName+ ".nocache.js");
  if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(moduleVersion)) {
    result+="?version=" + moduleVersion;
  }
  result+="\"></script>";
  return result;
}
