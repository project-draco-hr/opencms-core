{
  List<I_CmsResourceType> result=new ArrayList<I_CmsResourceType>();
  try {
    Collection<CmsResource> resources=OpenCms.getADEManager().getSearchableResourceTypes(getCmsObject(),getCmsObject().getRequestContext().getUri(),getThreadLocalRequest());
    for (    CmsResource resource : resources) {
      result.add(getResourceManager().getResourceType(resource));
    }
  }
 catch (  CmsException e) {
    error(e);
  }
  return result;
}
