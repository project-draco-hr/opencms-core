{
  if (h == null) {
    return null;
  }
  Map g=new HashMap();
  Iterator en=h.keySet().iterator();
  while (en.hasNext()) {
    Object key=en.next();
    Object value=h.get(key);
    Vector List=(Vector)g.get(value);
    if (List == null) {
      Vector newEntry=new Vector();
      newEntry.addElement(key);
      g.put(value,newEntry);
    }
 else {
      List.addElement(key);
    }
  }
  return g;
}
