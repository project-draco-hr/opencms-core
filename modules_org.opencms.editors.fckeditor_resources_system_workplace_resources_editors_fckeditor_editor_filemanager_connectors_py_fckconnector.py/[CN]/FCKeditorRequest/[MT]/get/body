def get(self, key, default=None):
    if (key in self.request.keys()):
        field = self.request[key]
    elif (key in self.get_request.keys()):
        field = self.get_request[key]
    else:
        return default
    if (hasattr(field, 'filename') and field.filename):
        return field
    else:
        return field.value
