{
  String currentUrl=Window.Location.getHref();
  String connector="?";
  if (currentUrl.indexOf('?') >= 0) {
    connector="&";
  }
  String targetUrl=currentUrl + connector + CmsGwtConstants.PARAM_DISABLE_DIRECT_EDIT+ "=true"+ "&"+ CmsGwtConstants.PARAM_TEMPLATE_CONTEXT+ "="+ context;
  return targetUrl;
}
