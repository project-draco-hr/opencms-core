{
  int length=className.length();
  boolean nodot=true;
  for (int i=0; i < length; i++) {
    char ch=className.charAt(i);
    if (nodot) {
      if (ch == '.') {
        return false;
      }
 else       if (Character.isJavaIdentifierStart(ch)) {
        nodot=false;
      }
 else {
        return false;
      }
    }
 else {
      if (ch == '.') {
        nodot=true;
      }
 else       if (Character.isJavaIdentifierPart(ch)) {
        nodot=false;
      }
 else {
        return false;
      }
    }
  }
  return true;
}
