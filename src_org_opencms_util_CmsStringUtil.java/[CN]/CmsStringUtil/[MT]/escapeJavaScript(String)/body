{
  source=CmsStringUtil.substitute(source,"\\","\\\\");
  source=CmsStringUtil.substitute(source,"\"","\\\"");
  source=CmsStringUtil.substitute(source,"\'","\\\'");
  source=CmsStringUtil.substitute(source,"\r\n","\\n");
  source=CmsStringUtil.substitute(source,"\n","\\n");
  source=CmsStringUtil.substitute(source,"/","\\/");
  return source;
}
