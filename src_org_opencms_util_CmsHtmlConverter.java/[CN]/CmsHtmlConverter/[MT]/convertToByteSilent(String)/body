{
  try {
    return convertToByte(htmlInput.getBytes(m_encoding));
  }
 catch (  Exception e) {
    if (OpenCms.getLog(this).isWarnEnabled()) {
      OpenCms.getLog(this).warn("[" + this.getClass().getName() + "] "+ " convertToByteSilent, error converting HTML, ",e);
    }
    try {
      return htmlInput.getBytes(m_encoding);
    }
 catch (    UnsupportedEncodingException e1) {
      if (OpenCms.getLog(this).isWarnEnabled()) {
        OpenCms.getLog(this).warn("[" + this.getClass().getName() + "] "+ " convertToByteSilent error converting HTML, ",e1);
      }
      return htmlInput.getBytes();
    }
  }
}
