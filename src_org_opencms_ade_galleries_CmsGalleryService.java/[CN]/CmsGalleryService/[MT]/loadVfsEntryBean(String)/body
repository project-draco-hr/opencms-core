{
  try {
    CmsObject cms=OpenCms.initCmsObject(getCmsObject());
    cms.getRequestContext().setSiteRoot("");
    CmsResource optionRes=cms.readResource(path);
    CmsVfsEntryBean entryBean=new CmsVfsEntryBean(path,optionRes.getStructureId(),path,true,isEditable(cms,optionRes));
    return entryBean;
  }
 catch (  Throwable e) {
    error(e);
    return null;
  }
}
