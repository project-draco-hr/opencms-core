{
  if (m_openClose == null) {
    return;
  }
  if (visible) {
    addStyleName(CmsListItemWidget.OPENCLASS);
    m_openClose.setDown(true);
    OpenEvent.fire(this,this);
  }
 else {
    removeStyleName(CmsListItemWidget.OPENCLASS);
    m_openClose.setDown(false);
    CloseEvent.fire(this,this);
  }
}
