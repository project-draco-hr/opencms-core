{
  m_panel.clear();
  if (m_labelText != null) {
    Label label=new Label(m_labelText);
    label.addStyleName(I_CmsInputLayoutBundle.INSTANCE.inputCss().formInfo());
    m_panel.add(label);
  }
  for (  I_CmsFormField field : fields) {
    CmsFormRow row=createRow(field);
    m_panel.add(row);
  }
}
