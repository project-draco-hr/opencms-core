{
  String origSiteRoot=getCmsObject().getRequestContext().getSiteRoot();
  try {
    getCmsObject().getRequestContext().setSiteRoot("");
    internalSaveProperties(changes);
  }
 catch (  Throwable t) {
    error(t);
  }
 finally {
    getCmsObject().getRequestContext().setSiteRoot(origSiteRoot);
  }
}
