{
  List errors=new ArrayList();
  try {
    boolean found=false;
    Iterator itMappings=m_field.getMappings().iterator();
    while (itMappings.hasNext()) {
      CmsSearchFieldMapping curMapping=(CmsSearchFieldMapping)itMappings.next();
      if (curMapping.getType().toString().equals(m_mapping.getType().toString()) && (((curMapping.getParam() == null) && (m_mapping.getParam() == null)) || (curMapping.getParam().equals(m_mapping.getParam())))) {
        found=true;
      }
    }
    if (!found) {
      m_field.addMapping(m_mapping);
    }
    writeConfiguration();
  }
 catch (  Throwable t) {
    errors.add(t);
  }
  setCommitErrors(errors);
}
