{
  int deleteOption=-1;
  if (isFolder && !DIALOG_WAIT.equals(getParamAction())) {
    return false;
  }
  deleteOption=Boolean.valueOf(getParamDeleteVfsLinks()).booleanValue() ? CmsResource.DELETE_REMOVE_SIBLINGS : CmsResource.DELETE_PRESERVE_SIBLINGS;
  checkLock(getParamResource());
  getCms().deleteResource(getParamResource(),deleteOption);
  return true;
}
