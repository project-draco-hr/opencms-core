{
  cms.doUndoChanges(resource);
  if (C_BODY_MIRROR) {
    String bodyPath=I_CmsWpConstants.C_VFS_PATH_BODIES + resource.substring(1);
    boolean hasBodyFolder=false;
    try {
      cms.readFileHeader(bodyPath);
      hasBodyFolder=true;
    }
 catch (    CmsException e) {
    }
    if (hasBodyFolder) {
      cms.doUndoChanges(bodyPath);
    }
  }
}
