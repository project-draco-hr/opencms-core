{
  m_defaultPropertiesOnStructure=Boolean.valueOf(defaultPropertiesOnStructure).booleanValue();
  if (CmsLog.INIT.isInfoEnabled()) {
    CmsLog.INIT.info(Messages.get().getBundle().key(m_defaultPropertiesOnStructure ? Messages.INIT_PROP_ON_STRUCT_TRUE_0 : Messages.INIT_PROP_ON_STRUCT_FALSE_0));
  }
}
