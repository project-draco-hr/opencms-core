{
  super(jsp);
  listRecovery(listId);
  if (getList() == null) {
    setList(createList());
    getList().setMaxItemsPerPage(getSettings().getUserSettings().getExplorerFileEntries());
    getList().addAllItems(getListItems());
    getList().setSortedColumn(sortedColId,getLocale());
    listSave();
  }
}
