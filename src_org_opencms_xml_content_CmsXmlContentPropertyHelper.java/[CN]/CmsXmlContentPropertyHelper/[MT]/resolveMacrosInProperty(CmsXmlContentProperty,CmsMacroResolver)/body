{
  String propName=property.getPropertyName();
  CmsXmlContentProperty result=new CmsXmlContentProperty(propName,property.getPropertyType(),property.getWidget(),resolver.resolveMacros(property.getWidgetConfiguration()),property.getRuleRegex(),property.getRuleType(),property.getDefault(),resolver.resolveMacros(property.getNiceName()),resolver.resolveMacros(property.getDescription()),resolver.resolveMacros(property.getError()),property.getAdvanced(),property.getSelectInherit());
  return result;
}
