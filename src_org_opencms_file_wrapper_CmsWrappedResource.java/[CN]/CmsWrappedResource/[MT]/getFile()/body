{
  if (m_base instanceof CmsFile) {
    CmsFile file=(CmsFile)m_base;
    return new CmsFile(file.getStructureId(),file.getResourceId(),m_rootPath,m_typeId,file.getFlags(),file.getProjectLastModified(),file.getState(),file.getDateCreated(),file.getUserCreated(),file.getDateLastModified(),file.getUserLastModified(),file.getDateReleased(),file.getDateExpired(),file.getSiblingCount(),file.getLength(),file.getDateContent(),file.getVersion(),file.getContents());
  }
  return new CmsFile(getResource());
}
