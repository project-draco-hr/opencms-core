{
  StringBuffer result=new StringBuffer();
  result.append("[User]");
  result.append(" name:");
  result.append(m_name);
  result.append(" id:");
  result.append(m_id);
  result.append(" flags:");
  result.append(getFlags());
  result.append(" type:");
  result.append(getType());
  result.append(" description:");
  result.append(m_description);
  return result.toString();
}
