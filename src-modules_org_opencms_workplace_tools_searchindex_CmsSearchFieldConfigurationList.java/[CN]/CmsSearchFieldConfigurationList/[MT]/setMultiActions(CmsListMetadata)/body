{
  CmsListMultiAction deleteMultiAction=new CmsListMultiAction(LIST_MACTION_DELETECONFIGURATION);
  deleteMultiAction.setName(Messages.get().container(Messages.GUI_LIST_FIELDCONFIGURATION_MACTION_DELETECONFIGURATION_NAME_0));
  deleteMultiAction.setHelpText(Messages.get().container(Messages.GUI_LIST_FIELDCONFIGURATION_MACTION_DELETECONFIGURATION_NAME_HELP_0));
  deleteMultiAction.setConfirmationMessage(Messages.get().container(Messages.GUI_LIST_FIELDCONFIGURATION_MACTION_DELETECONFIGURATION_CONF_0));
  deleteMultiAction.setIconPath(ICON_MULTI_DELETE);
  metadata.addMultiAction(deleteMultiAction);
}
