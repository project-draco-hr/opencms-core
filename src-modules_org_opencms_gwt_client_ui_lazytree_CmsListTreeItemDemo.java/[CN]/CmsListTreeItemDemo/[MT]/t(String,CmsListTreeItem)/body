{
  CmsListInfoBean info=new CmsListInfoBean();
  info.setTitle(a);
  info.setSubTitle(a);
  CmsListItemWidget l=new CmsListItemWidget(info);
  CmsListTreeItem item=new CmsListTreeItem(true,new CmsCheckBox(),l);
  for (  CmsListTreeItem child : children) {
    item.addChild(child);
  }
  return item;
}
