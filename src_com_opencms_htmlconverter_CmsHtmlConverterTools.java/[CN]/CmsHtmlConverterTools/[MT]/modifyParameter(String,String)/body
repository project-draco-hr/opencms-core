{
  try {
    URL myURL=new URL(parameter);
    parameter=myURL.getFile();
  }
 catch (  IOException e) {
    System.err.println("Error: " + e.toString());
  }
  if (prefix != null && !"".equals(prefix)) {
    if (parameter.startsWith(prefix)) {
      parameter=parameter.substring(prefix.length());
    }
  }
  return parameter;
}
