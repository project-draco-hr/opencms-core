{
  m_groupName=null;
  m_userType=null;
  m_flags=null;
  if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(configuration)) {
    int flagsIndex=configuration.indexOf(CONFIGURATION_FLAGS);
    if (flagsIndex != -1) {
      String flags=configuration.substring(CONFIGURATION_FLAGS.length() + 1);
      if (flags.indexOf('|') != -1) {
        flags=flags.substring(0,flags.indexOf('|'));
      }
      try {
        m_flags=Integer.valueOf(flags);
      }
 catch (      Throwable t) {
      }
    }
    int typeIndex=configuration.indexOf(CONFIGURATION_USERTYPE);
    if (typeIndex != -1) {
      String type=configuration.substring(CONFIGURATION_USERTYPE.length() + 1);
      if (type.indexOf('|') != -1) {
        type=type.substring(0,type.indexOf('|'));
      }
      try {
        m_userType=Integer.valueOf(type);
      }
 catch (      Throwable t) {
      }
    }
    int groupIndex=configuration.indexOf(CONFIGURATION_GROUP);
    if (groupIndex != -1) {
      String group=configuration.substring(CONFIGURATION_GROUP.length() + 1);
      if (group.indexOf('|') != -1) {
        group=group.substring(0,group.indexOf('|'));
      }
      m_groupName=group;
    }
  }
  super.setConfiguration(configuration);
}
