{
  try {
    CmsJlanNetworkFile file=getFileForPath(session,connection,path);
    if (file == null) {
      return FileStatus.NotExist;
    }
 else {
      return file.isDirectory() ? FileStatus.DirectoryExists : FileStatus.FileExists;
    }
  }
 catch (  Exception e) {
    System.out.println(e);
    return FileStatus.NotExist;
  }
}
