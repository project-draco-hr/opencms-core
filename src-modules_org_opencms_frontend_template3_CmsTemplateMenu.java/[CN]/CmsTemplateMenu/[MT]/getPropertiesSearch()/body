{
  if (m_propertiesSearch == null) {
    CmsResource resUri;
    try {
      resUri=getResource();
      m_propertiesSearch=CmsCollectionsGenericWrapper.createLazyMap(new CmsPropertyLoaderSingleTransformer(resUri,true));
    }
 catch (    CmsException e) {
      LOG.error(e.getLocalizedMessage(),e);
    }
  }
  return m_propertiesSearch;
}
