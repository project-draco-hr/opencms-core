{
  CmsJSONMap imageInfos=CmsPreviewUtil.getImageInfo();
  if (imageInfos.containsKey(Attribute.hash.name()) && !imageInfos.getString(Attribute.hash.name()).equals(String.valueOf(m_handler.getImageIdHash()))) {
    imageInfos=CmsJSONMap.createJSONMap();
  }
  m_form.fillContent(imageInfo,imageInfos);
}
