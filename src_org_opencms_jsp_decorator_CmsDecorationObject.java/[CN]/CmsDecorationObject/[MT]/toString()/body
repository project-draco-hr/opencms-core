{
  StringBuffer buf=new StringBuffer();
  buf.append(this.getClass().getName());
  buf.append(" [name = '");
  buf.append(m_decorationKey);
  buf.append("', decoration = '");
  buf.append(m_decoration);
  buf.append("', locale = '");
  buf.append(m_locale);
  buf.append("' decorationDefinition ='");
  buf.append(m_decorationDefinition);
  buf.append("']");
  return buf.toString();
}
