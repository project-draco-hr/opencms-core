{
  @SuppressWarnings("unchecked") List<Node> nodes=node.selectNodes("documenttypes-indexed/name[text()='msword' or text()='msexcel' or text()='mspowerpoint' or text()='msoffice-ooxml' or text()='openoffice' or text()='msoffice-ole2']");
  for (  Node nodeToRemove : nodes) {
    nodeToRemove.detach();
  }
}
