{
  if (!forReal) {
    return true;
  }
  Node node=doc.selectSingleNode(m_xpath);
  if (node != null) {
    Element parent=node.getParent();
    node.detach();
    try {
      Element element=createElementFromXml(m_replacementXml);
      parent.add(element);
      return true;
    }
 catch (    DocumentException e) {
      e.printStackTrace(System.out);
      return false;
    }
  }
 else {
    return false;
  }
}
