{
  return new CmsXmlUpdateAction(){
    @Override public boolean executeUpdate(    Document doc,    String xpath,    boolean forReal){
      Node node=doc.selectSingleNode(xpath);
      if (node != null) {
        return false;
      }
      CmsSetupXmlHelper.setValue(doc,xpath + "/text()",type);
      return true;
    }
  }
;
}
