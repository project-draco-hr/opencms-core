{
  Node node=document.selectSingleNode(xpath);
  if (node == null) {
    if (xpath.equals(getXPathsToUpdate().get(1))) {
      createAnalyzer(document,xpath,CmsGallerySearchAnalyzer.class,"all");
    }
 else     if (xpath.equals(getXPathsToUpdate().get(2))) {
      createIndex(document,xpath,CmsGallerySearchIndex.class,CmsGallerySearchIndex.GALLERY_INDEX_NAME,"offline","Offline","all","gallery_fields",new String[]{"gallery_source"});
    }
 else     if (xpath.equals(getXPathsToUpdate().get(3))) {
      createIndexSource(document,xpath,"gallery_source",CmsVfsIndexer.class,new String[]{"/sites/","/shared/"},new String[]{"xmlpage-galleries","xmlcontent-galleries","jsp","page","text","pdf","rtf","html","image","generic","openoffice","msoffice-ole2","msoffice-ooxml"});
    }
 else     if (xpath.equals(getXPathsToUpdate().get(4))) {
