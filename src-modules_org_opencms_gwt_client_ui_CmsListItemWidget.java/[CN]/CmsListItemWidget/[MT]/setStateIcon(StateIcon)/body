{
  if (m_stateIcon == null) {
    m_stateIcon=new HTML();
    m_contentPanel.add(m_stateIcon);
  }
switch (icon) {
case export:
    m_stateIcon.setStyleName(I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().stateIcon() + " " + I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().export());
  m_stateIcon.setTitle(Messages.get().key(Messages.GUI_ICON_TITLE_EXPORT_0));
break;
case secure:
m_stateIcon.setStyleName(I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().stateIcon() + " " + I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().secure());
m_stateIcon.setTitle(Messages.get().key(Messages.GUI_ICON_TITLE_SECURE_0));
break;
default :
m_stateIcon.setStyleName(I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().stateIcon());
m_stateIcon.setTitle(null);
break;
}
}
