{
  String listMethod=n.getAttribute(CmsWorkplaceDefault.C_PROJECTLIST_METHOD);
  CmsXmlWpTemplateFile listdef=getProjectlistDefinitions(cms);
  Method callingMethod=null;
  Vector list=new Vector();
  try {
    callingMethod=callingObject.getClass().getMethod(listMethod,new Class[]{CmsObject.class,CmsXmlLanguageFile.class});
    list=(Vector)callingMethod.invoke(callingObject,new Object[]{cms,lang});
  }
 catch (  NoSuchMethodException exc) {
    throwException("Could not find method " + listMethod + " in calling class "+ callingObject.getClass().getName()+ " for generating projectlist content.",CmsLegacyException.C_NOT_FOUND);
  }
catch (  InvocationTargetException targetEx) {
    Throwable e=targetEx.getTargetException();
    if (!(e instanceof CmsException)) {
      throwException("User method " + listMethod + " in calling class "+ callingObject.getClass().getName()+ " throwed an exception. "+ e);
    }
 else {
      throw (CmsException)e;
    }
  }
catch (  Exception exc2) {
    throwException("User method " + listMethod + " in calling class "+ callingObject.getClass().getName()+ " was found but could not be invoked. "+ exc2,CmsLegacyException.C_XML_NO_USER_METHOD);
  }
  StringBuffer result=new StringBuffer();
  String state=CmsWorkplaceDefault.C_PROJECTLIST_STATE_UNLOCKED;
  String snaplock=listdef.getProcessedDataValue(CmsWorkplaceDefault.C_TAG_PROJECTLIST_SNAPLOCK,callingObject,parameters);
  for (int i=0; i < list.size(); i++) {
    CmsProject project=(CmsProject)list.elementAt(i);
    if (cms.countLockedResources(project.getId()) == 0) {
      state=CmsWorkplaceDefault.C_PROJECTLIST_STATE_UNLOCKED;
    }
 else {
      state=CmsWorkplaceDefault.C_PROJECTLIST_STATE_LOCKED;
    }
    setListEntryData(cms,lang,listdef,project);
    if (state.equals(CmsWorkplaceDefault.C_PROJECTLIST_STATE_UNLOCKED)) {
      listdef.setData(CmsWorkplaceDefault.C_PROJECTLIST_LOCKSTATE,"");
      listdef.setData(CmsWorkplaceDefault.C_PROJECTLIST_MENU,C_PROJECT_UNLOCK);
    }
 else {
      listdef.setData(CmsWorkplaceDefault.C_PROJECTLIST_LOCKSTATE,snaplock);
      listdef.setData(CmsWorkplaceDefault.C_PROJECTLIST_MENU,C_PROJECT_LOCK);
    }
    listdef.setData(CmsWorkplaceDefault.C_PROJECTLIST_IDX,new Integer(i).toString());
    result.append(listdef.getProcessedDataValue(CmsWorkplaceDefault.C_TAG_PROJECTLIST_DEFAULT,callingObject,parameters));
  }
  return result.toString();
}
