{
  try {
    String groupName=(String)getItem().get(A_CmsUserGroupsList.LIST_COLUMN_NAME);
    List dGroups=getCms().getGroupsOfUser(getUserName(),true);
    CmsGroup group=getCms().readGroup(groupName);
    if (isDirect()) {
      return dGroups.contains(group);
    }
 else {
      return !dGroups.contains(group);
    }
  }
 catch (  Exception e) {
    return false;
  }
}
