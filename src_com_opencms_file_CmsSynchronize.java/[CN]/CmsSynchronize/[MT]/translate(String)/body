{
  String translation=null;
  Iterator i=m_checkFile.iterator();
  while (i.hasNext()) {
    try {
      translation=((I_CmsSyncModification)i.next()).translate(m_cms,name);
    }
 catch (    CmsSyncModificationException e) {
      break;
    }
  }
  if (translation == null) {
    translation=m_cms.getRequestContext().getFileTranslator().translateResource(name);
  }
  return translation;
}
