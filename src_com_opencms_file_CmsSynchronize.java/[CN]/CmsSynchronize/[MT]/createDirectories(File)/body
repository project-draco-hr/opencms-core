{
  File filePath=new File(newFile.getPath().substring(0,newFile.getPath().lastIndexOf("/") + 1));
  Stack directories=new Stack();
  directories.push(filePath);
  String pathName=filePath.getPath().substring(0,filePath.getPath().lastIndexOf("/"));
  pathName=pathName.substring(0,pathName.lastIndexOf("/") + 1);
  File nextPath=new File(pathName);
  while (filePath.getPath().indexOf("/") != nextPath.getPath().lastIndexOf("/")) {
    directories.push(nextPath);
    pathName=pathName.substring(0,pathName.lastIndexOf("/"));
    pathName=pathName.substring(0,pathName.lastIndexOf("/") + 1);
    nextPath=new File(pathName);
  }
  while (directories.size() > 0) {
    filePath=(File)directories.pop();
    filePath.mkdir();
  }
  filePath=new File(newFile.getPath().substring(0,newFile.getPath().lastIndexOf("/") + 1));
  return (filePath.exists());
}
