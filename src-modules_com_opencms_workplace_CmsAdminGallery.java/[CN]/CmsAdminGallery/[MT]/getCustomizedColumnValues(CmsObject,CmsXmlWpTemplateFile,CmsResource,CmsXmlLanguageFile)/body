{
  getConfigFile(cms);
  filelistTemplate.fastSetXmlData(CmsWorkplaceDefault.C_FILELIST_ICON_VALUE,CmsXmlTemplateLoader.getRequest(cms.getRequestContext()).getServletUrl() + getGalleryIconPath(cms));
  filelistTemplate.setData(CmsWorkplaceDefault.C_FILELIST_NAME_VALUE,res.getName());
  filelistTemplate.setData(CmsWorkplaceDefault.C_FILELIST_TITLE_VALUE,cms.readProperty(cms.getSitePath(res),CmsPropertyDefinition.PROPERTY_TITLE));
}
