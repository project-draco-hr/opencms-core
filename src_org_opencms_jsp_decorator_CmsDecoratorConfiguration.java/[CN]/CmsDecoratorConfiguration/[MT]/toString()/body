{
  StringBuffer buf=new StringBuffer();
  buf.append(this.getClass().getName());
  buf.append(" [configFile = '");
  buf.append(m_configFile);
  buf.append("', decorations = '");
  buf.append(m_decorations);
  buf.append("', locale = '");
  buf.append(m_locale);
  buf.append("']");
  return buf.toString();
}
