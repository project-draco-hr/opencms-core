{
  CmsFolder cmsFolder;
  try {
    cmsFolder=cmsFolder=m_fileRb.readFolder(currentProject,folder + folderName);
  }
 catch (  CmsException exc) {
    if (currentProject.equals(onlineProject(currentUser,currentProject))) {
      throw exc;
    }
 else {
      cmsFolder=cmsFolder=m_fileRb.readFolder(onlineProject(currentUser,currentProject),folder + folderName);
    }
  }
  if (accessRead(currentUser,currentProject,(A_CmsResource)cmsFolder)) {
    return (cmsFolder);
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ folder+ folderName,CmsException.C_NO_ACCESS);
  }
}
