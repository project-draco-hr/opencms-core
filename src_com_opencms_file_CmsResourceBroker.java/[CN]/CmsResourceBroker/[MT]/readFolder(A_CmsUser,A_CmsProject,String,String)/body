{
  CmsFolder cmsFolder=m_fileRb.readFolder(currentProject,folder + folderName);
  if (accessRead(currentUser,currentProject,(A_CmsResource)cmsFolder)) {
    return (cmsFolder);
  }
 else {
    throw new CmsException(this.getClass().getName() + ": " + folder+ folderName,CmsException.C_NO_ACCESS);
  }
}
