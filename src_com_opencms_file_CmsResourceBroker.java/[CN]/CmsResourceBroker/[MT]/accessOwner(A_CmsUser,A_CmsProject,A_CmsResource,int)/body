{
  if (isAdmin(currentUser,currentProject)) {
    return (true);
  }
  if (resource.getOwnerId() == currentUser.getId()) {
    if ((resource.getAccessFlags() & flags) == flags) {
      return (true);
    }
  }
  System.err.println("rb.accessOwner() failed " + resource.getAbsolutePath());
  return (false);
}
