{
  if (accessWrite(currentUser,currentProject,(A_CmsResource)file)) {
    m_fileRb.writeFileHeader(currentProject,onlineProject(currentUser,currentProject),file);
    fileSystemChanged(currentProject.getName(),file.getAbsolutePath());
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ file.getAbsolutePath(),CmsException.C_ACCESS_DENIED);
  }
}
