{
  CmsFolder cmsFolder=m_fileRb.readFolder(currentProject,foldername);
  if ((!cmsFolder.isLocked()) && accessCreate(currentUser,currentProject,(A_CmsResource)cmsFolder)) {
    m_fileRb.deleteFolder(currentProject,foldername,false);
  }
 else {
    throw new CmsException(foldername,CmsException.C_NO_ACCESS);
  }
}
