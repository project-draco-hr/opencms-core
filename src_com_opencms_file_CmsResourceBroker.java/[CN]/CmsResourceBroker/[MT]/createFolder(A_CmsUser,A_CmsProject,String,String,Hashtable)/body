{
  validFilename(newFolderName);
  CmsFolder cmsFolder=m_fileRb.readFolder(currentProject,folder);
  if (accessCreate(currentUser,currentProject,(A_CmsResource)cmsFolder)) {
    return (m_fileRb.createFolder(currentUser,currentProject,folder + newFolderName + C_FOLDER_SEPERATOR,0));
  }
 else {
    throw new CmsException(folder + newFolderName,CmsException.C_NO_ACCESS);
  }
}
