{
  validFilename(newFolderName);
  CmsFolder cmsFolder=m_fileRb.readFolder(currentProject,folder);
  if (accessCreate(currentUser,currentProject,(A_CmsResource)cmsFolder)) {
    return (m_fileRb.createFolder(currentUser,currentProject,folder + newFolderName + C_FOLDER_SEPERATOR,C_ACCESS_DEFAULT_FLAGS));
  }
 else {
    throw new CmsException(CmsException.C_EXTXT[CmsException.C_NO_ACCESS],CmsException.C_NO_ACCESS);
  }
}
