{
  CmsFile cmsFile=m_fileRb.readFile(currentProject,onlineProject(currentUser,currentProject),filename);
  if (accessRead(currentUser,currentProject,(A_CmsResource)cmsFile)) {
    return (cmsFile);
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ filename,CmsException.C_NO_ACCESS);
  }
}
