{
  CmsFile cmsFile=null;
  try {
    cmsFile=m_fileRb.readFile(currentProject,onlineProject(currentUser,currentProject),filename);
  }
 catch (  CmsException exc) {
    if (currentProject.equals(onlineProject(currentUser,currentProject))) {
      throw exc;
    }
 else {
      cmsFile=m_fileRb.readFile(onlineProject(currentUser,currentProject),onlineProject(currentUser,currentProject),filename);
    }
  }
  if (accessRead(currentUser,currentProject,(A_CmsResource)cmsFile)) {
    return (cmsFile);
  }
 else {
    System.err.println(">>> readFile(2) error for\n" + currentUser.toString() + "\n"+ currentProject.toString()+ "\n"+ filename+ "\n"+ cmsFile+ " <<<");
    throw new CmsException("[" + this.getClass().getName() + "] "+ filename,CmsException.C_ACCESS_DENIED);
  }
}
