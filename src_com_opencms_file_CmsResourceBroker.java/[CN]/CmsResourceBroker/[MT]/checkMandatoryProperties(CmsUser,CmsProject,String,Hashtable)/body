{
  Vector metadefs=readAllPropertydefinitions(currentUser,currentProject,resourceType,C_PROPERTYDEF_TYPE_MANDATORY);
  for (int i=0; i < metadefs.size(); i++) {
    if (metainfos.containsKey(metadefs.elementAt(i))) {
      throw new CmsException("[" + this.getClass().getName() + "] "+ (String)metadefs.elementAt(i),CmsException.C_MANDATORY_PROPERTY);
    }
  }
}
