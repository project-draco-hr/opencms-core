{
  validFilename(newname);
  A_CmsResource file=m_fileRb.readFileHeader(currentProject,oldname);
  if (accessWrite(currentUser,currentProject,file)) {
    m_fileRb.renameFile(currentProject,onlineProject(currentUser,currentProject),oldname,newname);
  }
 else {
    throw new CmsException(oldname,CmsException.C_NO_ACCESS);
  }
}
