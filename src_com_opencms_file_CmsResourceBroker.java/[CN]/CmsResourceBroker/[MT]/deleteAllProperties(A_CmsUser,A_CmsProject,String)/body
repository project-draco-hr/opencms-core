{
  A_CmsResource res=m_fileRb.readFileHeader(currentProject,resource);
  if (!accessWrite(currentUser,currentProject,res)) {
    throw new CmsException("[" + this.getClass().getName() + "] "+ resource,CmsException.C_NO_ACCESS);
  }
  if (m_metadefRb.readAllMetadefinitions(res.getType(),C_PROPERTYDEF_TYPE_MANDATORY).size() == 0) {
    m_metadefRb.deleteAllMetainformations(res);
    fileSystemChanged(currentProject.getName(),resource);
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ resource,CmsException.C_MANDATORY_PROPERTY);
  }
}
