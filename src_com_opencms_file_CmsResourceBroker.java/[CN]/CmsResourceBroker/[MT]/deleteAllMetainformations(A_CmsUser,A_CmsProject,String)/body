{
  A_CmsResource res=m_fileRb.readFileHeader(currentProject,resource);
  if (!accessWrite(currentUser,currentProject,res)) {
    throw new CmsException(resource,CmsException.C_NO_ACCESS);
  }
  if (m_metadefRb.readAllMetadefinitions(res.getType(),C_METADEF_TYPE_MANDATORY).size() == 0) {
    m_metadefRb.deleteAllMetainformations(res);
  }
 else {
    throw new CmsException(resource,CmsException.C_MANDATORY_METAINFO);
  }
}
