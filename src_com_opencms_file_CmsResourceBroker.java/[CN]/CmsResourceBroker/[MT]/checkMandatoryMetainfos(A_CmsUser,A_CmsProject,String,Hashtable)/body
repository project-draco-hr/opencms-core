{
  Vector metadefs=readAllMetadefinitions(currentUser,currentProject,resourceType,C_METADEF_TYPE_MANDATORY);
  for (int i=0; i < metadefs.size(); i++) {
    if (metainfos.containsKey(metadefs.elementAt(i))) {
      throw new CmsException((String)metadefs.elementAt(i),CmsException.C_MANDATORY_METAINFO);
    }
  }
}
