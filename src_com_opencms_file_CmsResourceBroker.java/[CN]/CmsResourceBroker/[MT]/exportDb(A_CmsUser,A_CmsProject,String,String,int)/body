{
  boolean admin=false;
  try {
    admin=this.isAdmin(currentUser,currentProject);
    if (admin) {
      CmsDbExport export=new CmsDbExport(this,currentUser,currentProject,exportFile,exportPath,exportType);
      export.export();
    }
  }
 catch (  CmsException e) {
  }
catch (  Exception e) {
  }
  return admin;
}
