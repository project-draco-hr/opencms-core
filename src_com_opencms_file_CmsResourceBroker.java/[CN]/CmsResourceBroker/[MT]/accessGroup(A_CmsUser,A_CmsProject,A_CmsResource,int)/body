{
  if (userInGroup(currentUser,currentProject,currentUser.getName(),readGroup(currentUser,currentProject,resource.getGroupId()).getName())) {
    if ((resource.getAccessFlags() & flags) == flags) {
      return (true);
    }
  }
  System.err.println("rb.accessGroup() failed " + resource.getAbsolutePath());
  return (false);
}
