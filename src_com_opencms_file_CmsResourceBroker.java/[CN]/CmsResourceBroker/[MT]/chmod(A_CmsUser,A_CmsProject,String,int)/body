{
  A_CmsResource resource=m_fileRb.readFileHeader(currentProject,filename);
  if (accessWrite(currentUser,currentProject,resource)) {
    m_fileRb.chmod(currentProject,onlineProject(currentUser,currentProject),filename,flags);
  }
 else {
    throw new CmsException(CmsException.C_EXTXT[CmsException.C_NO_ACCESS],CmsException.C_NO_ACCESS);
  }
}
