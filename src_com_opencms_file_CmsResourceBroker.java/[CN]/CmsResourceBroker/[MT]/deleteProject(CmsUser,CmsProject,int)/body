{
  CmsProject deleteProject=m_projectRb.readProject(id);
  if (isAdmin(currentUser,currentProject) || isManagerOfProject(currentUser,deleteProject) || (deleteProject.getFlags() == C_PROJECT_STATE_UNLOCKED)) {
    m_projectRb.deleteProject(deleteProject);
    m_fileRb.deleteProject(deleteProject);
    m_metadefRb.deleteProject(deleteProject);
  }
 else {
    throw new CmsException("[" + this.getClass().getName() + "] "+ id,CmsException.C_NO_ACCESS);
  }
}
