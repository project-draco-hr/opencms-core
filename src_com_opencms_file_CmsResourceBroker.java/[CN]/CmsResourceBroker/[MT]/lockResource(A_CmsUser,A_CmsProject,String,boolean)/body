{
  A_CmsResource cmsResource=m_fileRb.readFileHeader(currentProject,resourcename);
  if (accessCreate(currentUser,currentProject,cmsResource)) {
    m_fileRb.lockResource(currentUser,currentProject,onlineProject(currentUser,currentProject),resourcename,force);
  }
 else {
    throw new CmsException(resourcename,CmsException.C_NO_ACCESS);
  }
}
