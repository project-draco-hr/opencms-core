{
  A_CmsProject online=onlineProject(currentUser,currentProject);
  if ((!currentProject.equals(online)) && (currentProject.getOwnerId() == currentUser.getId())) {
    m_fileRb.copyResourceToProject(currentProject,online,resource);
    resource=getParentOfResource(resource);
    try {
      while (resource != null) {
        m_fileRb.copyResourceToProject(currentProject,online,resource);
        resource=getParentOfResource(resource);
      }
    }
 catch (    CmsException exc) {
    }
  }
 else {
    throw new CmsException(CmsException.C_EXTXT[CmsException.C_NO_ACCESS],CmsException.C_NO_ACCESS);
  }
}
