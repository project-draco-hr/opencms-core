{
  if (locale != null) {
    m_locales.add(locale);
    m_bookmarks.put(locale.toString() + "|" + name,element);
    if (enabled) {
      Object o=m_elementLocales.get(name);
      if (o != null) {
        Set set=(Set)o;
        set.add(locale);
      }
 else {
        Set set=new HashSet();
        set.add(locale);
        m_elementLocales.put(name,set);
      }
    }
  }
 else {
    int warning=0;
    m_bookmarks.put(name,element);
  }
}
