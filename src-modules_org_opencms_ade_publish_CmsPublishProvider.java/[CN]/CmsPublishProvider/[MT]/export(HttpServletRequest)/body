{
  CmsObject cms=CmsFlexController.getCmsObject(request);
  StringBuffer sb=new StringBuffer();
  sb.append(org.opencms.gwt.CmsCoreProvider.get().export(request));
  sb.append(I_CmsPublishProviderConstants.DICT_NAME.replace('.','_')).append("=").append(getData(cms,request).toString()).append(";");
  sb.append(ClientMessages.get().export(request));
  return sb.toString();
}
