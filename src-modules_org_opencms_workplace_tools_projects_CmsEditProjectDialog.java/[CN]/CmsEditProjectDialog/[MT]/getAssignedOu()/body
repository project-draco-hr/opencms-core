{
  try {
    CmsOrganizationalUnit ou;
    if (!isNewProject()) {
      ou=OpenCms.getOrgUnitManager().readOrganizationalUnit(getCms(),m_project.getOuFqn());
    }
 else {
      ou=(CmsOrganizationalUnit)OpenCms.getRoleManager().getOrgUnitsForRole(getCms(),CmsRole.PROJECT_MANAGER,true).get(0);
    }
    return ou.getDisplayName(getLocale());
  }
 catch (  CmsException e) {
    return null;
  }
}
