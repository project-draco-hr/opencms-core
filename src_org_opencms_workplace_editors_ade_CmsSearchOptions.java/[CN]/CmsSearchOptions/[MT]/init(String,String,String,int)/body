{
  m_text=text;
  if (CmsStringUtil.isEmptyOrWhitespaceOnly(m_text)) {
    throw new CmsIllegalArgumentException(Messages.get().container(Messages.ERR_JSON_MISSING_PARAMETER_1,CmsADEServer.PARAMETER_TEXT));
  }
  m_location=location;
  if (m_location == null) {
    m_location="/";
  }
  if (CmsStringUtil.isEmptyOrWhitespaceOnly(type)) {
    throw new CmsIllegalArgumentException(Messages.get().container(Messages.ERR_JSON_MISSING_PARAMETER_1,CmsADEServer.PARAMETER_TYPE));
  }
  m_type=type;
  m_page=page;
  if (m_page < 0) {
    m_page=0;
  }
}
