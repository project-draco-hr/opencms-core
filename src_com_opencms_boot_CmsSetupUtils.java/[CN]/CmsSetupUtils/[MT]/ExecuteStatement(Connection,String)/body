{
  Statement stat;
  if (statement != null) {
    try {
      stat=con.createStatement();
      stat.executeUpdate(statement);
    }
 catch (    Exception e) {
      if (m_errorLogging) {
        m_errors.addElement(e.toString() + "\n");
      }
      System.err.println();
      System.err.println("Exception:  " + e.toString());
      System.err.println();
    }
  }
}
