{
  Statement stat;
  if (statement != null) {
    try {
      System.err.println("-----------------------------------------");
      System.err.println("-----------------------------------------");
      System.err.println("JETZT HIER DENN DA: " + statement);
      stat=con.createStatement();
      stat.executeUpdate(statement);
    }
 catch (    Exception e) {
      if (m_errorLogging) {
        m_errors.addElement(e.toString() + "\n");
      }
      e.printStackTrace();
    }
  }
}
