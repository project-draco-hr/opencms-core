{
  try {
    LineNumberReader lnr=new LineNumberReader(new FileReader(new File(m_configFolder + originalFile)));
    FileWriter fw=new FileWriter(new File(m_configFolder + backupFile));
    while (true) {
      String line=lnr.readLine();
      if (line == null)       break;
      fw.write(line + '\n');
    }
    lnr.close();
    fw.close();
  }
 catch (  IOException e) {
    CmsSetup.setErrors("Could not save " + originalFile + " to "+ backupFile+ " \n");
    CmsSetup.setErrors(e.toString());
  }
}
