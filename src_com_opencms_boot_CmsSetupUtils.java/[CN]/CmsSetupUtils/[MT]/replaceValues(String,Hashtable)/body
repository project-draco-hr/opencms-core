{
  StringTokenizer tokenizer=new StringTokenizer(source);
  String temp="";
  while (tokenizer.hasMoreTokens()) {
    String token=tokenizer.nextToken();
    if (token.startsWith("$$") && token.endsWith("$$")) {
      Object value=replacers.get(token);
      if (value != null) {
        token=value.toString();
      }
    }
    temp+=token + " ";
  }
  return temp;
}
