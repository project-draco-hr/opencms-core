{
  byte[] result=null;
  try {
    CmsXmlPage page=CmsXmlPage.read(cms,file);
    String localeProp=OpenCms.getUserDefaultLanguage();
    result=page.getContent(cms,elementName,localeProp).getBytes();
  }
 catch (  Exception exc) {
    throw new CmsException("Error in CmsXmlPageLoader",exc);
  }
  return result;
}
