{
  CmsXmlPage page=CmsXmlPageFactory.unmarshal(cms,resource,req);
  String element=req.getParameter(I_CmsResourceLoader.PARAMETER_ELEMENT);
  List locales=page.getLocales(element);
  Locale locale;
  if ((locales == null) || (locales.size() == 0)) {
    return;
  }
 else {
    locale=OpenCms.getLocaleManager().getBestMatchingLocale(cms.getRequestContext().getLocale(),OpenCms.getLocaleManager().getDefaultLocales(cms,cms.getSitePath(resource)),locales);
  }
  if (locale == null) {
    throw new CmsLoaderException(Messages.get().container(Messages.ERR_LOADER_UNKNOWN_LOCALE_4,new Object[]{resource.getRootPath(),cms.getRequestContext().getLocale(),CmsLocaleManager.getLocaleNames(locales),CmsLocaleManager.getLocaleNames(OpenCms.getLocaleManager().getDefaultLocales(cms,cms.getSitePath(resource)))}));
  }
  String value=page.getStringValue(cms,element,locale);
  if (value != null) {
    byte[] result=value.getBytes(page.getEncoding());
    res.getOutputStream().write(result);
  }
}
