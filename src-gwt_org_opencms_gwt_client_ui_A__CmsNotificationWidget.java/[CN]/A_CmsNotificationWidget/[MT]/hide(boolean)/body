{
  if (m_timer != null) {
    m_timer.cancel();
    m_timer=null;
  }
  if ((m_mode == Mode.STICKY) && !force) {
    return;
  }
  m_type=null;
  if (getAnimation() != null) {
    getAnimation().cancel();
    setAnimation(null);
  }
  if (getElement().getStyle().getVisibility().equalsIgnoreCase(Visibility.HIDDEN.getCssName())) {
    return;
  }
  if (!force) {
    animateHide();
  }
 else {
    onHideComplete();
  }
}
