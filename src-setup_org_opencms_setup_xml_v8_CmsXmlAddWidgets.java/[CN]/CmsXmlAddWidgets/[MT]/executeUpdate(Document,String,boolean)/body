{
  Node node=document.selectSingleNode(xpath);
  if (node == null) {
    for (int i=0; i < m_widgets.length; i++) {
      if (xpath.equals(getXPathsToUpdate().get(i))) {
        CmsSetupXmlHelper.setValue(document,xpathForWidgetAlias(m_widgets[i][0]),m_widgets[i][1]);
      }
    }
    return true;
  }
  return false;
}
