{
  String sitePath=getHoverbar().getSitePath();
  CmsSitemapController controller=getHoverbar().getController();
  CmsClientSitemapEntry entry=controller.getEntry(sitePath);
  boolean show=(entry != null) && controller.isDetailPage(entry) && controller.getData().canEditDetailPages()&& !controller.getDetailPageTable().isDefaultDetailPage(entry.getId());
  setVisible(show);
}
