{
  CmsSitemapController controller=getHoverbar().getController();
  CmsClientSitemapEntry entry=getHoverbar().getEntry();
  boolean show=!CmsSitemapView.getInstance().isSpecialMode() && (entry != null) && controller.isDetailPage(entry)&& controller.getData().canEditDetailPages()&& !controller.getDetailPageTable().isDefaultDetailPage(entry.getId());
  setVisible(show);
}
