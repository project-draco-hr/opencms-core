{
  getJsp().getRequest().setAttribute(C_SESSION_WORKPLACE_CLASS,this);
switch (getAction()) {
case ACTION_CONFIRMED:
default :
    try {
      OpenCms.getWorkplaceManager().initialize(getCms());
      CmsStaticExportThread thread=new CmsStaticExportThread(getCms());
      setParamAction(DIALOG_CANCEL);
      setParamThread(thread.getUUID().toString());
      actionCloseDialog();
    }
 catch (    CmsException e) {
      CmsWorkplaceException ereinit=new CmsWorkplaceException(Messages.get().container(Messages.LOG_ERR_WORKPLACE_REINIT_0),e);
      LOG.error(ereinit.getLocalizedMessage(),ereinit);
      getJsp().getRequest().setAttribute(ATTRIBUTE_THROWABLE,ereinit);
      getJsp().include(C_FILE_DIALOG_SCREEN_ERRORPAGE);
    }
  break;
}
}
