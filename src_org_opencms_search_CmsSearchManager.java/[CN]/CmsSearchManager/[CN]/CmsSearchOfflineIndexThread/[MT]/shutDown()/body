{
  m_isAlive=false;
  interrupt();
  if (m_isUpdating) {
    long waitTime=getOfflineUpdateFrequency() / 2;
    int waitSteps=0;
    do {
      try {
        Thread.sleep(waitTime);
      }
 catch (      InterruptedException e) {
      }
      waitSteps++;
    }
 while ((waitSteps < 5) && m_isUpdating);
  }
}
