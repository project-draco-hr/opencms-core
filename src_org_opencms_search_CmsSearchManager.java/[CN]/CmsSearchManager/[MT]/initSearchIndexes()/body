{
  CmsSearchIndex index=null;
  for (int i=0, n=m_indexes.size(); i < n; i++) {
    index=(CmsSearchIndex)m_indexes.get(i);
    try {
      index.initialize();
    }
 catch (    CmsException exc) {
      OpenCms.getLog(CmsLog.CHANNEL_INIT).info(". Search index         : Initialization of index \"" + index.getName() + "\" failed",exc);
    }
  }
}
