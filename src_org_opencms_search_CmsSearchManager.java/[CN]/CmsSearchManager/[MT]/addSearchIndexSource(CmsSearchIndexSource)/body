{
  m_indexSources.put(searchIndexSource.getName(),searchIndexSource);
  if (OpenCms.getLog(CmsLog.CHANNEL_INIT).isInfoEnabled()) {
    OpenCms.getLog(CmsLog.CHANNEL_INIT).info(". Search index source  : adding '" + searchIndexSource.getName() + "' using indexer "+ searchIndexSource.getIndexerClassName());
  }
}
