{
  m_toolbar.removeFromParent();
  m_toolbar=null;
  m_cancelButton=null;
  m_localeSelect=null;
  m_openFormButton=null;
  m_saveButton=null;
  m_entityId=null;
  m_onClose=null;
  m_locale=null;
  m_contentDefinition=null;
  if (m_basePanel != null) {
    m_basePanel.removeFromParent();
    m_basePanel=null;
  }
}
