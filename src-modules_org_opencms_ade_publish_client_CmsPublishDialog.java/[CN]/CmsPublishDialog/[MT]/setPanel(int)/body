{
  m_panel.showWidget(panelId);
  removeAllButtons();
  if (panelId == PANEL_SELECT) {
    for (    CmsButton button : m_publishSelectPanel.getButtons()) {
      addButton(button);
    }
  }
 else   if (panelId == PANEL_BROKEN_LINKS) {
    for (    CmsButton button : m_brokenLinksPanel.getButtons()) {
      addButton(button);
    }
  }
}
