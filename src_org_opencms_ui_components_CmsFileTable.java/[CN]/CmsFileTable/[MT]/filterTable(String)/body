{
  m_container.removeAllContainerFilters();
  if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(search)) {
    m_container.addContainerFilter(new Or(new SimpleStringFilter(CmsResourceTableColumn.PROPERTY_RESOURCE_NAME,search,true,false),new SimpleStringFilter(CmsResourceTableColumn.PROPERTY_NAVIGATION_TEXT,search,true,false),new SimpleStringFilter(CmsResourceTableColumn.PROPERTY_TITLE,search,true,false)));
  }
}
