{
  PreparedStatement stmt=null;
  Connection conn=null;
  try {
    conn=m_sqlManager.getConnection();
    stmt=m_sqlManager.getPreparedStatement(conn,"check_module_master");
    stmt.executeQuery();
    return true;
  }
 catch (  SQLException exc) {
    return false;
  }
 finally {
    m_sqlManager.closeAll(conn,stmt,null);
  }
}
