{
  String id=param.getId();
  StringBuffer result=new StringBuffer(16);
  result.append("<td class=\"xmlTd\"><input class=\"xmlInput maxwidth\" name=\"");
  result.append(id);
  result.append("\" id=\"");
  result.append(id);
  result.append("\" value=\"");
  result.append(param.getStringValue(cms));
  result.append("\">");
  result.append("</td>");
  return result.toString();
}
