{
  List<CmsClientSitemapEntry> cache=(List<CmsClientSitemapEntry>)getRequest().getSession().getAttribute(SESSION_ATTR_ADE_SITEMAP_RECENT_LIST_CACHE);
  if (cache == null) {
    cache=new ArrayList<CmsClientSitemapEntry>();
    getRequest().getSession().setAttribute(SESSION_ATTR_ADE_SITEMAP_RECENT_LIST_CACHE,cache);
  }
  return cache;
}
