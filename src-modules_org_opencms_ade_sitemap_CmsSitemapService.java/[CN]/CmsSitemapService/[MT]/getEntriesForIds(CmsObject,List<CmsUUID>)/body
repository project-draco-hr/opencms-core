{
  List<CmsInternalSitemapEntry> entries=new ArrayList<CmsInternalSitemapEntry>();
  for (  CmsUUID id : ids) {
    CmsInternalSitemapEntry entry=(CmsInternalSitemapEntry)OpenCms.getSitemapManager().getEntryForId(cms,id);
    if (entry != null) {
      entries.add(entry);
    }
  }
  return entries;
}
