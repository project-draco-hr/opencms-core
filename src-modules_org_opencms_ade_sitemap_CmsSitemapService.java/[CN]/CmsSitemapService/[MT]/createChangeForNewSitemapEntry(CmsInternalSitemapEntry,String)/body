{
  CmsObject cms=getCmsObject();
  CmsUUID resourceId=entry.getStructureId();
  CmsResource resource=cms.readResource(resourceId);
  CmsSitemapChangeNew result=new CmsSitemapChangeNewSubSitemapEntry(entry.getSitePath(cms),entry.getPosition(),entry.getTitle(),cms.getSitePath(resource),entry.getProperties(),entryPoint,entry.getId());
  return result;
}
