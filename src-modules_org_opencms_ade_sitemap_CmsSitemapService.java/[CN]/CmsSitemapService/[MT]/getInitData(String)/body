{
  CmsSitemapData result=null;
  CmsObject cms=getCmsObject();
  try {
    CmsResource sitemap=cms.readResource(sitemapUri);
    result=new CmsSitemapData(getDefaultTemplate(sitemapUri),getTemplates(),CmsXmlContentPropertyHelper.getPropertyInfo(cms,sitemap),getCachedRecentList());
  }
 catch (  Throwable e) {
    error(e);
  }
  return result;
}
