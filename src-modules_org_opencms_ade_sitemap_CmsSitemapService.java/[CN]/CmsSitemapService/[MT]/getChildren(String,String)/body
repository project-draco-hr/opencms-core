{
  List<CmsClientSitemapEntry> children=null;
  try {
    CmsObject cms=getCmsObject();
    CmsResource sitemapRes=cms.readResource(sitemapUri);
    Map<String,CmsXmlContentProperty> propertyConfig=OpenCms.getSitemapManager().getElementPropertyConfiguration(cms,sitemapRes);
    children=getChildren(root,1,propertyConfig);
  }
 catch (  Throwable e) {
    error(e);
  }
  return children;
}
