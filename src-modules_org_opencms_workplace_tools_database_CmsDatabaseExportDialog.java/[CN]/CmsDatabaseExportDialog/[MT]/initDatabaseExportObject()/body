{
  Object o;
  if (CmsStringUtil.isEmpty(getParamAction())) {
    o=new CmsVfsImportExportHandler();
  }
 else {
    o=getDialogObject();
  }
  if (!(o instanceof CmsVfsImportExportHandler)) {
    m_exportHandler=new CmsVfsImportExportHandler();
  }
 else {
    m_exportHandler=(CmsVfsImportExportHandler)o;
  }
}
