  }
  if (navPos == null) {
    navPos="";
  }
  retValue.append("<input type=\"hidden\" name=\"" + PREFIX_HIDDEN + I_CmsConstants.C_PROPERTY_NAVPOS+ "\" value=\""+ navPos+ "\">");
  retValue.append("</td>\n");
  retValue.append("\t<td class=\"textcenter\">");
