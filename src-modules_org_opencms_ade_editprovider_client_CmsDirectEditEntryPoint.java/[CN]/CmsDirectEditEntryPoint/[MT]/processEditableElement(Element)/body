{
  RootPanel root=RootPanel.get(elem.getId());
  CmsDirectEditButtons result=new CmsDirectEditButtons(elem,null);
  root.add(result);
  result.setPosition(m_positions.get(elem.getId()),m_buttonPositions.get(elem.getId()),(com.google.gwt.user.client.Element)elem.getParentElement());
  return result;
}
