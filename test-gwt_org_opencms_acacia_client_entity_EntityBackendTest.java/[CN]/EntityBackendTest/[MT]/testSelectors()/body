{
  Document.get().getBody().setInnerHTML("<div about='http://testEntity'><div property='" + ATTRIBUTE_NAME + "'>my value</div></div>");
  List<com.google.gwt.dom.client.Element> elements=getVieInstance().getAttributeElements("http://testEntity",ATTRIBUTE_NAME,null);
  assertNotNull(elements);
  assertEquals(1,elements.size());
  assertEquals("my value",elements.get(0).getInnerText());
  Document.get().getBody().setInnerHTML("<div about='http://testEntity' property='" + ATTRIBUTE_NAME + "'>my value</div>");
  elements=getVieInstance().getAttributeElements("http://testEntity",ATTRIBUTE_NAME,null);
  assertNotNull(elements);
  assertEquals(1,elements.size());
  assertEquals("my value",elements.get(0).getInnerText());
}
