{
  StringBuffer result=new StringBuffer();
  result.append("[");
  result.append(this.getClass().getName());
  result.append(", role: ");
  result.append(getRoleName());
  result.append(", org unit: ");
  result.append(getOuFqn());
  result.append(", group: ");
  result.append(getGroupName());
  result.append("]");
  return result.toString();
}
