{
  String roleName;
  if (m_systemRole) {
    roleName=Messages.get().key(context.getLocale(),"GUI_ROLENAME_" + m_roleName + "_0",null);
  }
 else {
    roleName=getRoleName();
  }
  return new CmsRoleViolationException(Messages.get().container(Messages.ERR_NOT_IN_ROLE_2,context.currentUser().getName(),roleName));
}
