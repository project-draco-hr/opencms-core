{
  ArrayList commands=container.getCommands();
  for (int i=0; i < commands.size(); i++) {
    try {
      if (C_DEBUG) {
        System.err.println("        invoke methode " + ((CmsCommand)commands.get(i)).toString());
      }
      invokeMethod(cms,(CmsCommand)commands.get(i));
      if (C_DEBUG) {
        System.err.println("        ok.");
      }
    }
 catch (    Exception e) {
      if (C_DEBUG) {
        System.err.println("        ... EXCEPTION:");
        e.printStackTrace();
        System.err.println("");
      }
      if (I_CmsLogChannels.C_PREPROCESSOR_IS_LOGGING && A_OpenCms.isLogging()) {
        A_OpenCms.log(A_OpenCms.C_MODULE_DEBUG," module com.opencms.modules.cluster.comm cant invoke methode " + ((CmsCommand)commands.get(i)).getMethodName() + e.getMessage());
      }
    }
  }
}
