{
  try {
    CmsCommandContainer container=readContainerFromRequest(cms);
    cms.loginUser(container.getUsername(),container.getPassword());
    invokeMethods(cms,container);
  }
 catch (  Exception exc) {
    if (I_CmsLogChannels.C_PREPROCESSOR_IS_LOGGING && A_OpenCms.isLogging()) {
      A_OpenCms.log(A_OpenCms.C_MODULE_DEBUG," module com.opencms.modules.cluster.comm cant invoce methods." + exc.getMessage());
    }
    throw new CmsException(CmsException.C_UNKNOWN_EXCEPTION,exc);
  }
  return null;
}
