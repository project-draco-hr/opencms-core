{
  if ((getParamCloseLink() != null) && Boolean.valueOf(getParamDirectpublish()).booleanValue()) {
    try {
      getJsp().getResponse().sendRedirect(getParamCloseLink());
    }
 catch (    Exception e) {
      throw new JspException(e.getMessage(),e);
    }
  }
 else {
    super.actionCloseDialog();
  }
}
