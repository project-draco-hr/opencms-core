{
  List errors=new ArrayList();
  if (getUsers().isEmpty()) {
    setCommitErrors(Collections.singletonList(new CmsIllegalStateException(Messages.get().container(Messages.ERR_NO_SELECTED_USER_WITH_EMAIL_0))));
    return;
  }
  try {
    Iterator itUsers=getUsers().iterator();
    while (itUsers.hasNext()) {
      CmsUser user=(CmsUser)itUsers.next();
      OpenCms.getSessionManager().sendBroadcast(getCms().getRequestContext().getCurrentUser(),m_msgInfo.getMsg(),user);
    }
  }
 catch (  Throwable t) {
    errors.add(t);
  }
  setCommitErrors(errors);
}
