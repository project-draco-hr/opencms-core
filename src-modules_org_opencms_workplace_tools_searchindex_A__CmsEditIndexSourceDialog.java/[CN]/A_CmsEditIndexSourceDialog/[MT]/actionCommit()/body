{
  List errors=new ArrayList();
  try {
    if (!m_searchManager.getSearchIndexSources().keySet().contains(m_indexsource.getName())) {
      m_searchManager.addSearchIndexSource(m_indexsource);
    }
    writeConfiguration();
  }
 catch (  Throwable t) {
    errors.add(t);
  }
  setCommitErrors(errors);
}
