{
  StringBuffer html=new StringBuffer(512);
  html.append("<table width='100%' cellspacing='0' class='buttons'>\n");
  html.append("\t<tr>\n");
  html.append(m_metadata.htmlSearchBar(wp));
  html.append(m_metadata.htmlMultiActionBar(wp));
