{
  this(resource.getPublishTag(),resource.getStructureId(),resource.getResourceId(),resource.getRootPath(),resource.getTypeId(),resource.getFlags(),resource.getProjectLastModified(),resource.getState(),resource.getDateCreated(),resource.getUserCreated(),resource.getDateLastModified(),resource.getUserLastModified(),resource.getDateReleased(),resource.getDateExpired(),resource.getLength(),resource.getDateContent(),resource.getVersion(),resource.getParentId(),resource.isFile() ? ((CmsFile)resource).getContents() : null,resource.getResourceVersion(),resource.getStructureVersion());
}
