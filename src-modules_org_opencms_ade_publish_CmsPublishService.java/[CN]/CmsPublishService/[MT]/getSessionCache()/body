{
  CmsADESessionCache cache=(CmsADESessionCache)this.getRequest().getSession().getAttribute(CmsADESessionCache.SESSION_ATTR_ADE_CACHE);
  if (cache == null) {
    cache=new CmsADESessionCache(getCmsObject());
    getRequest().getSession().setAttribute(CmsADESessionCache.SESSION_ATTR_ADE_CACHE,cache);
  }
  return cache;
}
