{
  if (m_active == active) {
    if (active) {
      fireChangeEvent();
    }
    return;
  }
  m_active=active;
  for (int i=0; i < m_arrayRadioButtons.length; i++) {
    m_arrayRadioButtons[i].setEnabled(active);
    if (!active) {
      m_arrayRadioButtons[i].setChecked(active);
    }
 else {
      if (m_defaultCheckBox != null) {
        m_defaultCheckBox.setChecked(active);
        fireChangeEvent();
      }
    }
  }
  if (active) {
    fireChangeEvent();
  }
}
