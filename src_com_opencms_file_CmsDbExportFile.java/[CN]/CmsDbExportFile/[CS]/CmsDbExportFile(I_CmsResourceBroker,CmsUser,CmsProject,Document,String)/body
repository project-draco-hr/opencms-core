{
  m_RB=eRB;
  m_user=luser;
  m_project=lproject;
  this.m_docXml=m_docXml;
  m_resourcepath=lpath;
  if (m_resourcepath.equals("/") == false) {
    CmsFolder folder=m_RB.readFolder(m_user,m_project,m_resourcepath,"");
    m_startFolder="/" + folder.getName();
  }
 else {
    m_startFolder="/";
  }
}
