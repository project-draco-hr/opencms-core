{
  Vector users=m_RB.getUsers(m_user,m_project);
  if (userId != (-1)) {
    Enumeration uenum=users.elements();
    while (uenum.hasMoreElements()) {
      A_CmsUser pg=(A_CmsUser)uenum.nextElement();
      if (pg.getId() == userId) {
        return pg.getName();
      }
    }
  }
  return "none";
}
