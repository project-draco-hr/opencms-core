{
  Vector groups=RB.getGroups(user,project);
  if (groupId != (-1)) {
    Enumeration genum=groups.elements();
    while (genum.hasMoreElements()) {
      A_CmsGroup pg=(A_CmsGroup)genum.nextElement();
      if (pg.getId() == groupId) {
        return pg.getName();
      }
    }
  }
  return "none";
}
