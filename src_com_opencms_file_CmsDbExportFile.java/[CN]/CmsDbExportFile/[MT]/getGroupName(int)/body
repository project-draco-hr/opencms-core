{
  Vector groups=m_RB.getGroups(m_user,m_project);
  if (groupId != (-1)) {
    Enumeration genum=groups.elements();
    while (genum.hasMoreElements()) {
      CmsGroup pg=(CmsGroup)genum.nextElement();
      if (pg.getId() == groupId) {
        return pg.getName();
      }
    }
  }
  return "none";
}
