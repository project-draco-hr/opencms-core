{
  CmsResourceUtil resUtil=new CmsResourceUtil(m_cms,resource);
  CmsPublishResource pubResource=new CmsPublishResource(resource.getStructureId(),resUtil.getFullPath(),resUtil.getTitle(),resUtil.getResourceTypeName(),resource.getState(),resource.getDateLastModified(),resUtil.getUserLastModified(),CmsVfsService.formatDateTime(m_cms,resource.getDateLastModified()),false,null,new ArrayList<CmsPublishResource>());
  return pubResource;
}
