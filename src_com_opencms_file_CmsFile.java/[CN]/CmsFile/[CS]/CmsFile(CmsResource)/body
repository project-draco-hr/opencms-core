{
  this(resource.getStructureId(),resource.getResourceId(),resource.getParentStructureId(),resource.getFileId(),resource.getName(),resource.getType(),resource.getFlags(),resource.getProjectLastModified(),resource.getState(),resource.getLoaderId(),resource.getDateCreated(),resource.getUserCreated(),resource.getDateLastModified(),resource.getUserLastModified(),0,resource.getLinkCount(),new byte[0]);
  if (resource.hasFullResourceName()) {
    setFullResourceName(resource.getRootPath());
  }
}
