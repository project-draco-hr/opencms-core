{
  boolean isVisible=false;
  Logger logger=null;
  if (getItem() != null) {
    String loggerName=getItem().getId();
    if (loggerName != null) {
      logger=LogManager.getLogger(loggerName);
    }
  }
  if (logger != null) {
    isVisible=!logger.getEffectiveLevel().equals(logLevel);
  }
  return isVisible;
}
