{
  Fieldable field=new Field(name,DateTools.dateToString(new Date(date),DateTools.Resolution.MILLISECOND),Field.Store.YES,Field.Index.NOT_ANALYZED);
  field.setBoost(0.0F);
  m_doc.add(field);
  if (analyzed) {
    field=new Field(name + CmsSearchField.FIELD_DATE_LOOKUP_SUFFIX,getDateTerms(date),Field.Store.NO,Field.Index.ANALYZED);
    m_doc.add(field);
  }
}
