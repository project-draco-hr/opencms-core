{
  if (!(event.getTarget() instanceof CmsLazyListTreeItem)) {
    return;
  }
  CmsLazyListTreeItem target=(CmsLazyListTreeItem)event.getTarget();
  if (target.getLoadState() != CmsLazyListTreeItem.LoadState.UNLOADED) {
    return;
  }
  target.onStartLoading();
  target.setOpen(true);
  load(target);
}
