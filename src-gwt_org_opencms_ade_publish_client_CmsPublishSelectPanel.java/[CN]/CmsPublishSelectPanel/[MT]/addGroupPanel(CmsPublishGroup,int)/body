{
  String header=group.getName();
  CmsPublishGroupPanel groupPanel=new CmsPublishGroupPanel(group,header,currentIndex,this,m_model,m_selectionControllers,m_showProblemsOnly);
  if (m_model.hasSingleGroup()) {
    groupPanel.hideGroupSelectCheckBox();
  }
  m_groupPanels.add(groupPanel);
  m_groupPanelContainer.add(groupPanel);
  return groupPanel;
}
