{
  String sitemapUri=CmsCoreProvider.get().link(m_controller.getData().getSitemapUri());
  if (sitemapUri.equals("")) {
    return;
  }
  String target=sitemapUri + "?" + CmsCoreProvider.PARAM_RETURNCODE+ "="+ m_controller.getReturnCode()+ "&path="+ CmsContainerpageController.getCurrentUri();
  leavePage(target);
}
