{
  getConfigFile(cms);
  filelistTemplate.fastSetXmlData(C_FILELIST_ICON_VALUE,cms.getRequestContext().getRequest().getServletUrl() + getGalleryIconPath(cms));
  filelistTemplate.setData(C_FILELIST_NAME_VALUE,res.getResourceName());
  filelistTemplate.setData(C_FILELIST_TITLE_VALUE,cms.readProperty(cms.readAbsolutePath(res),C_PROPERTY_TITLE));
}
