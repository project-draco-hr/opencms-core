{
  List languages=new ArrayList();
  for (Iterator i=getBookmarks().iterator(); i.hasNext(); ) {
    String name=(String)i.next();
    if (name.indexOf("_") >= 0) {
      String language=name.substring(0,name.indexOf("_"));
      if (!languages.contains(language)) {
        languages.add(language);
      }
    }
  }
  return languages;
}
