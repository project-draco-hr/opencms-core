{
  if (m_selected != null) {
    CmsRadioButton selectedButton=m_radioButtons.get(m_selected);
    selectedButton.setDown(false);
  }
  m_selected=key;
  CmsRadioButton button=m_radioButtons.get(m_selected);
  if (button != null) {
    button.setDown(true);
  }
  fireValueChangedEvent(getSelected());
}
