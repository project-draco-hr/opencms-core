{
  String result="";
  if (state.contains(STATE_SEPARATOR)) {
    if (!state.startsWith(STATE_SEPARATOR) && !state.startsWith("/")) {
      result="/" + state;
    }
 else {
      result=state;
      while (result.startsWith("//")) {
        result=result.substring(1);
      }
    }
  }
  return result;
}
