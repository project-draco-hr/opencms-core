{
  if ("opencms:uri".equals(element)) {
    return resourceName;
  }
  if (content == null) {
    return null;
  }
  if (content.hasValue(element,locale,index)) {
    CmsFlexController controller=(CmsFlexController)req.getAttribute(CmsFlexController.ATTRIBUTE_NAME);
    CmsObject cms=controller.getCmsObject();
    try {
      return content.getStringValue(cms,element,locale,index);
    }
 catch (    CmsXmlException e) {
      OpenCms.getLog(CmsJspTagContentShow.class).error("Error processing content element '" + element + '['+ index+ "]'",e);
      return null;
    }
  }
 else {
    return null;
  }
}
