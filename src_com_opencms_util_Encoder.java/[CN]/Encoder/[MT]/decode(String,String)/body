{
  if (source == null)   return null;
  if (encoding != null) {
    try {
      return URLDecoder.decode(source,encoding);
    }
 catch (    java.io.UnsupportedEncodingException e) {
    }
  }
  try {
    return URLDecoder.decode(source,C_UTF8_ENCODING);
  }
 catch (  java.io.UnsupportedEncodingException e) {
  }
  return source;
}
