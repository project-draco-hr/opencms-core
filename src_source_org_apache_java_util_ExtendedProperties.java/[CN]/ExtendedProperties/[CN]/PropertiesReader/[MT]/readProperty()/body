{
  StringBuffer buffer=new StringBuffer();
  try {
    while (true) {
      String line=readLine().trim();
      if ((line.length() != 0) && (line.charAt(0) != '#')) {
        if (line.endsWith("\\")) {
          line=line.substring(0,line.length() - 1);
          buffer.append(line);
        }
 else {
          buffer.append(line);
          break;
        }
      }
    }
  }
 catch (  NullPointerException e) {
    return null;
  }
  return buffer.toString();
}
