{
  if (formatter.isMatchAll()) {
    return true;
  }
  if (formatter.isTypeFormatter()) {
    return formatter.getContainerTypes().contains(m_type);
  }
 else {
    return (m_width == MATCH_ALL_CONTAINER_WIDTH) || ((formatter.getMinWidth() <= m_width) && (m_width <= formatter.getMaxWidth()));
  }
}
