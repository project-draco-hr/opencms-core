{
  FlowPanel tab=new FlowPanel();
  tab.setStyleName(org.opencms.gwt.client.ui.css.I_CmsLayoutBundle.INSTANCE.tabbedPanelCss().tabPanel());
  list.setStyleName(I_CmsLayoutBundle.INSTANCE.clipboardCss().itemList());
