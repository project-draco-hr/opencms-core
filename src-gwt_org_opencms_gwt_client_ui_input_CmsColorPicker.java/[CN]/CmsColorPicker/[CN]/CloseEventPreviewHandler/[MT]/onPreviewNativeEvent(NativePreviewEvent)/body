{
  Event nativeEvent=Event.as(event.getNativeEvent());
switch (DOM.eventGetType(nativeEvent)) {
case Event.ONMOUSEMOVE:
    break;
case Event.ONMOUSEUP:
  int x_coord=nativeEvent.getClientX();
int y_coord=nativeEvent.getClientY();
if (((x_coord > (m_xcoordspopup + 450 + 10)) || (x_coord < (m_xcoordspopup - 10))) || ((y_coord > ((m_ycoordspopup + 300) - 10)) || (y_coord < ((m_ycoordspopup) - 10)))) {
m_previewHandlerRegistration.removeHandler();
m_previewHandlerRegistration=null;
closePopup();
}
break;
case Event.ONKEYDOWN:
break;
case Event.ONMOUSEWHEEL:
break;
default :
}
}
