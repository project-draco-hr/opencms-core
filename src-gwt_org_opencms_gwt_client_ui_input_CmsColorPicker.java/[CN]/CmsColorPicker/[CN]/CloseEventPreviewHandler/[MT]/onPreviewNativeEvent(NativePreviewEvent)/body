{
  Event nativeEvent=Event.as(event.getNativeEvent());
switch (DOM.eventGetType(nativeEvent)) {
case Event.ONMOUSEMOVE:
    break;
case Event.ONMOUSEUP:
  int x_coord=nativeEvent.getClientX();
int y_coord=(nativeEvent.getClientY() + Window.getScrollTop());
if (((x_coord > (m_xcoordspopup + 450)) || (x_coord < (m_xcoordspopup))) || ((y_coord > ((m_ycoordspopup + 280))) || (y_coord < ((m_ycoordspopup))))) {
closePopup();
}
break;
case Event.ONKEYDOWN:
break;
case Event.ONMOUSEWHEEL:
closePopup();
break;
default :
}
}
