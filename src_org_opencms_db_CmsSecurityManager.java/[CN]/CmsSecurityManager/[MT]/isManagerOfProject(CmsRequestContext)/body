{
  try {
    return getAllManageableProjects(context,readOrganizationalUnit(context,context.currentProject().getOuFqn()),false).contains(context.currentProject());
  }
 catch (  CmsException e) {
    if (LOG.isErrorEnabled()) {
      LOG.error(e.getLocalizedMessage(),e);
    }
    return false;
  }
}
