{
  CmsResource resource=m_driverManager.availableResource(dbc,resourcePath,filter);
  if (resource == null) {
    return false;
  }
  boolean ret=true;
  try {
    checkPermissions(dbc,resource,CmsPermissionSet.ACCESS_READ,true,filter);
  }
 catch (  CmsException e) {
    ret=false;
  }
  return ret;
}
