{
  List result=null;
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    result=m_driverManager.readAllBackupFileHeaders(dbc,resource);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_ALL_BKP_FILE_HEADERS_1,dbc.getRequestContext().removeSiteRoot(resource.getRootPath())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
