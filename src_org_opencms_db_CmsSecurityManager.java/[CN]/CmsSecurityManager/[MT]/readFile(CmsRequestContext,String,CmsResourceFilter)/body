{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsResource resource=readResource(dbc,filename,filter);
  CmsFile result=null;
  try {
    result=m_driverManager.readFile(dbc,resource,filter);
  }
 catch (  Exception e) {
    dbc.report(null,null,e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
