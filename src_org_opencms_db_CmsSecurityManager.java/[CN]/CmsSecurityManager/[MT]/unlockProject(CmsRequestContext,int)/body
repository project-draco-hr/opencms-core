{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsProject project=m_driverManager.readProject(dbc,projectId);
  try {
    checkManagerOfProjectRole(dbc,project);
    m_driverManager.unlockProject(project);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_UNLOCK_PROJECT_2,new Integer(projectId),dbc.currentUser().getName()),e);
  }
 finally {
    dbc.clear();
  }
}
