{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] changeUserType() "+ userId.toString(),CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.changeUserType(dbc,userId,userType);
  }
 catch (  Exception e) {
    dbc.report(null,"Error changing type of user ID " + userId.toString(),e);
  }
 finally {
    dbc.clear();
  }
}
