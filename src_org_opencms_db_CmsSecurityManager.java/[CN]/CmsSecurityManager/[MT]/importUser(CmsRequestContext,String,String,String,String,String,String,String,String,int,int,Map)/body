{
  CmsUser newUser=null;
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  checkRole(dbc,CmsRole.USER_MANAGER);
  try {
    newUser=m_driverManager.importUser(dbc,id,name,password,description,firstname,lastname,email,address,flags,type,additionalInfos);
  }
 catch (  Exception e) {
    dbc.report(null,"Error importing user " + name,e);
  }
 finally {
    dbc.clear();
  }
  return newUser;
}
