{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readPath(dbc,projectId,path,filter);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_PATH_3,String.valueOf(projectId),path,String.valueOf(filter)),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
