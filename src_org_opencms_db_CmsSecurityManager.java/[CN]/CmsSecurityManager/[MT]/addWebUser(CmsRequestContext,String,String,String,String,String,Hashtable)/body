{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsUser result=null;
  try {
    result=m_driverManager.addWebUser(dbc,name,password,group,additionalGroup,description,additionalInfos);
  }
 catch (  Exception e) {
    dbc.report(null,"Error adding web user " + name,e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
