{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsProject result=null;
  try {
    result=m_driverManager.readProject(dbc,task);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_PROJECT_FOR_TASK_2,task.getName(),new Integer(task.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
