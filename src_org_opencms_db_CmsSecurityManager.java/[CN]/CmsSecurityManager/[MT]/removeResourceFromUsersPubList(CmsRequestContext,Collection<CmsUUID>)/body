{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.removeResourceFromUsersPubList(dbc,context.currentUser().getId(),structureIds);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_REMOVE_RESOURCE_FROM_PUBLIST_2,context.currentUser().getName(),structureIds),e);
  }
 finally {
    dbc.clear();
  }
}
