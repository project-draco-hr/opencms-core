{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsUser result=null;
  try {
    result=m_driverManager.readOwner(dbc,project);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_OWNER_FOR_PROJECT_2,project.getName(),new Integer(project.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
