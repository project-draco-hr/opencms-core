{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  String result=null;
  try {
    result=m_driverManager.moveToLostAndFound(dbc,resourcename,returnNameOnly);
  }
 catch (  Exception e) {
    dbc.report(null,"Error moving resource " + resourcename + " to lost+found",e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
