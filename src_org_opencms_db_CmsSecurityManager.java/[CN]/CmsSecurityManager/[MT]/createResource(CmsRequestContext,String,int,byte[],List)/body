{
  CmsResource newResource;
  I_CmsRuntimeInfo runtimeInfo=m_runtimeInfoFactory.getRuntimeInfo(m_driverManager,I_CmsRuntimeInfo.C_RUNTIMEINFO_VFS + I_CmsRuntimeInfo.C_RUNTIMEINFO_BACKUP);
  try {
    newResource=m_driverManager.createResource(context,runtimeInfo,resourcename,type,content,properties);
    runtimeInfo.pop();
  }
 catch (  CmsException e) {
    runtimeInfo.report(null,"Error creating resource " + resourcename,e);
    throw e;
  }
 finally {
    runtimeInfo.clear();
  }
  return newResource;
}
