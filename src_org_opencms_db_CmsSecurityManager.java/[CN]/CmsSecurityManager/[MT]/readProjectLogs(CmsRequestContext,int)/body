{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readProjectLogs(dbc,projectId);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_TASKLOGS_FOR_PROJECT_1,new Integer(projectId)),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
