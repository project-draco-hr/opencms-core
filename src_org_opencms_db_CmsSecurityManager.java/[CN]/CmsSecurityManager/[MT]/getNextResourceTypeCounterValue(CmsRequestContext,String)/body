{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    return m_driverManager.getNextResourceTypeCounterValue(dbc,resourceType);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_GET_NEXT_RESOURCE_TYPE_COUNTER_VALUE_1,resourceType),e);
    return -1;
  }
 finally {
    dbc.clear();
  }
}
