{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsUser result=null;
  try {
    result=m_driverManager.readOriginalAgent(dbc,task);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_ORIGINAL_TASK_OWNER_2,task.getName(),new Integer(task.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
