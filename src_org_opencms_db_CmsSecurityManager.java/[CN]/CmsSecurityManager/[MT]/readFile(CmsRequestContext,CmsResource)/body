{
  CmsFile result=null;
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    result=m_driverManager.readFile(dbc,resource);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_FILE_1,context.getSitePath(resource)),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
