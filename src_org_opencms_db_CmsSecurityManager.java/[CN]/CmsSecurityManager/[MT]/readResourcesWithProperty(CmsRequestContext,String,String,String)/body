{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readResourcesWithProperty(dbc,path,propertyDefinition,value);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_RESOURCES_FOR_PROP_VALUE_3,path,propertyDefinition,value),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
