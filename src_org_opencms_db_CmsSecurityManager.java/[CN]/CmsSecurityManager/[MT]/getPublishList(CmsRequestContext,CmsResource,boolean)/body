{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsPublishList result=null;
  try {
    result=m_driverManager.getPublishList(dbc,directPublishResource,publishSiblings);
  }
 catch (  Exception e) {
    dbc.report(null,null,e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
