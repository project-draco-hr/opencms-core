{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsPublishList result=null;
  try {
    result=m_driverManager.getPublishList(dbc,directPublishResource,publishSiblings);
  }
 catch (  Exception e) {
    if (directPublishResource != null) {
      dbc.report(null,Messages.get().container(Messages.ERR_GET_PUBLISH_LIST_DIRECT_1,dbc.getRequestContext().removeSiteRoot(directPublishResource.getRootPath())),e);
    }
 else {
      dbc.report(null,Messages.get().container(Messages.ERR_GET_PUBLISH_LIST_PROJECT_1,context.currentProject().getName()),e);
    }
  }
 finally {
    dbc.clear();
  }
  return result;
}
