{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsUser result=null;
  try {
    result=m_driverManager.readOwner(dbc,log);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_OWNER_FOR_TASKLOG_1,String.valueOf(log.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
