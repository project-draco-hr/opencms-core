{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readProjectResources(dbc,project);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_PROJECT_RESOURCES_2,project.getName(),new Integer(project.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
