{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readTasksForUser(dbc,projectId,userName,taskType,orderBy,sort);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_TASK_FOR_PROJECT_AND_USER_AND_TYPE_3,new Integer(projectId),userName,org.opencms.workflow.Messages.toTaskTypeString(taskType,context)),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
