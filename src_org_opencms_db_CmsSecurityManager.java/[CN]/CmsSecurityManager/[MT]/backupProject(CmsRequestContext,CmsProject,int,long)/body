{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.backupProject(dbc,backupProject,tagId,publishDate);
  }
 catch (  Exception e) {
    CmsMessageContainer errMsg=Messages.get().container(Messages.ERR_BACKUP_PROJECT_4,new Object[]{new Integer(tagId),backupProject.getName(),new Integer(backupProject.getId()),new Long(publishDate)});
    dbc.report(null,errMsg,e);
  }
 finally {
    dbc.clear();
  }
}
