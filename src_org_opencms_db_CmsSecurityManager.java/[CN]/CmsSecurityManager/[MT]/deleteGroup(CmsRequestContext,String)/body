{
  if (isAdmin(context)) {
    I_CmsRuntimeInfo runtimeInfo=m_runtimeInfoFactory.getRuntimeInfo(m_driverManager,I_CmsRuntimeInfo.C_RUNTIMEINFO_USER);
    try {
      m_driverManager.deleteGroup(context,runtimeInfo,name);
      runtimeInfo.pop();
    }
 catch (    CmsException e) {
      runtimeInfo.report(null,"Error deleteing group " + name,e);
      throw e;
    }
 finally {
      runtimeInfo.clear();
    }
  }
 else {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] deleteGroup() "+ name,CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
}
