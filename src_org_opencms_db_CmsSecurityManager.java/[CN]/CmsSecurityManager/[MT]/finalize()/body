{
  try {
    if (m_driverManager != null) {
      m_driverManager.destroy();
    }
  }
 catch (  Throwable t) {
    if (LOG.isErrorEnabled()) {
      LOG.error(Messages.get().key(Messages.LOG_ERR_CLOSE_DRIVERMANAGER_0),t);
    }
  }
  m_driverManager=null;
  m_dbContextFactory=null;
  super.finalize();
}
