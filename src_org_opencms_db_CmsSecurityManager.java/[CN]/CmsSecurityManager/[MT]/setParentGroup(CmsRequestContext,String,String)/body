{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] setParentGroup() "+ groupName,CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.setParentGroup(dbc,groupName,parentGroupName);
  }
 catch (  Exception e) {
    dbc.report(null,"Error setting parent group to " + parentGroupName + " of group "+ groupName,e);
  }
 finally {
    dbc.clear();
  }
}
