{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.changeLock(dbc,resource);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_CHANGE_LOCK_OF_RESOURCE_1,dbc.getRequestContext().removeSiteRoot(resource.getRootPath())),e);
  }
 finally {
    dbc.clear();
  }
}
