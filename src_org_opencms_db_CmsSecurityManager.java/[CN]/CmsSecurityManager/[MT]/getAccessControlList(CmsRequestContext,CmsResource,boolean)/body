{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsAccessControlList result=null;
  try {
    result=m_driverManager.getAccessControlList(dbc,resource,inheritedOnly);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_GET_ACL_ENTRIES_1,dbc.getRequestContext().removeSiteRoot(resource.getRootPath())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
