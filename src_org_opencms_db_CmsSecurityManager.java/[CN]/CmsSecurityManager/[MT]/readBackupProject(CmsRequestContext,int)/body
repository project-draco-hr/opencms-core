{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsBackupProject result=null;
  try {
    result=m_driverManager.readBackupProject(dbc,tagId);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_BKP_PROJECT_2,new Integer(tagId),dbc.currentProject().getName()),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
