{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.setPriority(dbc,taskId,priority);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_SET_TASK_PRIORITY_2,String.valueOf(taskId),String.valueOf(priority)),e);
  }
 finally {
    dbc.clear();
  }
}
