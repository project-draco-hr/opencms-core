{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readAllPropertyDefinitions(dbc,mappingtype);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_ALL_PROPDEF_MAPPING_TYPE_1,(mappingtype == CmsProperty.C_RESOURCE_RECORD_MAPPING) ? "CmsProperty.C_RESOURCE_RECORD_MAPPING" : "CmsProperty.C_STRUCTURE_RECORD_MAPPING"),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
