{
  List result=null;
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    result=m_driverManager.getUsers(dbc,orgUnit,recursive);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_ORGUNIT_USERS_1,orgUnit.getName()),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
