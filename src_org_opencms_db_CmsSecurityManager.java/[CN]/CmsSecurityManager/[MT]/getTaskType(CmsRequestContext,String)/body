{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  int result=0;
  try {
    result=m_driverManager.getTaskType(dbc,taskName);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_GET_TASK_TYPE_1,taskName),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
