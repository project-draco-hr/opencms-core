{
  CmsFile result=null;
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    result=m_driverManager.readFile(dbc,resource,filter);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_FILE_2,context.getSitePath(resource),filter),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
