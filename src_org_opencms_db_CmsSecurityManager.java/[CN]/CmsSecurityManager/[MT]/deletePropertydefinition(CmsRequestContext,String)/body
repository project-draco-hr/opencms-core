{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] deletePropertydefinition() "+ name,CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.deletePropertydefinition(dbc,name);
  }
 catch (  Exception e) {
    dbc.report(null,"Error deleting property definition " + name,e);
  }
 finally {
    dbc.clear();
  }
}
