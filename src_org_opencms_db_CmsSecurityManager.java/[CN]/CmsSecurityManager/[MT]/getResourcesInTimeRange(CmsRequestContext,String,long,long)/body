{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.getResourcesInTimeRange(dbc,folder,starttime,endtime);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_GET_RESOURCES_IN_TIME_RANGE_3,folder,CmsDateUtil.getDate(new Date(starttime),DateFormat.MEDIUM,context.getLocale()),CmsDateUtil.getDate(new Date(endtime),DateFormat.MEDIUM,context.getLocale())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
