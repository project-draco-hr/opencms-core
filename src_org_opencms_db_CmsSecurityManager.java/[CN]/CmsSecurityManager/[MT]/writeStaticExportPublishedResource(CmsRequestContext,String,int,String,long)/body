{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.writeStaticExportPublishedResource(dbc,resourceName,linkType,linkParameter,timestamp);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_WRITE_STATEXP_PUBLISHED_RESOURCES_3,resourceName,linkParameter,CmsDateUtil.getDateTime(new Date(timestamp),DateFormat.SHORT,context.getLocale()).toString()),e);
  }
 finally {
    dbc.clear();
  }
}
