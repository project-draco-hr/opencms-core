{
  CmsDbContext dbc=m_dbContextFactory.getDbContext();
  CmsUser result=null;
  try {
    result=m_driverManager.readUser(dbc,username);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_USER_FOR_NAME_1,username),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
