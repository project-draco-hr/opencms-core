{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsUser result=null;
  try {
    result=m_driverManager.readOwner(dbc,task);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_OWNER_FOR_TASK_2,task.getName(),String.valueOf(task.getId())),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
