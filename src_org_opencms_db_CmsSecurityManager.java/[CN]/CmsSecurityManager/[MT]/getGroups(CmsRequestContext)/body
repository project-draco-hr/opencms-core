{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    checkRole(dbc,CmsRole.SYSTEM_USER);
    result=m_driverManager.getGroups(dbc);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_GET_GROUPS_0),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
