{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsPublishList result=null;
  try {
    result=m_driverManager.fillPublishList(dbc,publishList);
  }
 catch (  Exception e) {
    if (publishList.isDirectPublish()) {
      dbc.report(null,Messages.get().container(Messages.ERR_GET_PUBLISH_LIST_DIRECT_1,CmsFileUtil.formatResourceNames(context,publishList.getDirectPublishResources())),e);
    }
 else {
      dbc.report(null,Messages.get().container(Messages.ERR_GET_PUBLISH_LIST_PROJECT_1,context.currentProject().getName()),e);
    }
  }
 finally {
    dbc.clear();
  }
  return result;
}
