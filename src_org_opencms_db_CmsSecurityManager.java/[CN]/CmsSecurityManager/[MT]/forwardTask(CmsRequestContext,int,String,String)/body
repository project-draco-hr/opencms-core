{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.forwardTask(dbc,taskid,newRoleName,newUserName);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_FORWARD_TASK_3,String.valueOf(taskid),newUserName,newRoleName),e);
  }
 finally {
    dbc.clear();
  }
}
