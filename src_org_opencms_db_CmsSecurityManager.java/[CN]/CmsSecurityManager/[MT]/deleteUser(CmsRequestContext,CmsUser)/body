{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] deleteUser() "+ user.getName(),CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  if (OpenCms.getDefaultUsers().isDefaultUser(user.getName())) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] deleteUser() "+ user.getName(),CmsSecurityException.C_SECURITY_NO_PERMISSIONS);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.deleteUser(dbc,context.currentProject(),user.getId());
  }
 catch (  Exception e) {
    dbc.report(null,"Error deleting user " + user.getName(),e);
  }
 finally {
    dbc.clear();
  }
}
