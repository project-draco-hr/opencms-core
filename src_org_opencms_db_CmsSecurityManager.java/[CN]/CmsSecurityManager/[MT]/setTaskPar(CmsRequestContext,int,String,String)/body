{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.setTaskPar(dbc,taskId,parName,parValue);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_SET_TASK_PARAM_3,parName,parValue,String.valueOf(taskId)),e);
  }
 finally {
    dbc.clear();
  }
}
