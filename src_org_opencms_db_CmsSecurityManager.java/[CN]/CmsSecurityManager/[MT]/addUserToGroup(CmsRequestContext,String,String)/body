{
  I_CmsRuntimeInfo runtimeInfo=m_runtimeInfoFactory.getRuntimeInfo(m_driverManager,I_CmsRuntimeInfo.C_RUNTIMEINFO_USER);
  try {
    m_driverManager.addUserToGroup(context,runtimeInfo,username,groupname);
    runtimeInfo.pop();
  }
 catch (  CmsException e) {
    runtimeInfo.report(null,"Error adding user " + username + " to group "+ groupname,e);
    throw e;
  }
 finally {
    runtimeInfo.clear();
  }
}
