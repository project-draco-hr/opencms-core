{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readGivenTasks(dbc,projectId,ownerName,taskType,orderBy,sort);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(org.opencms.workflow.Messages.toTaskTypeString(taskType,context),new Integer(projectId),ownerName),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
