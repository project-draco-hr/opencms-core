{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] createGroup() "+ name,CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsGroup result=null;
  try {
    result=m_driverManager.createGroup(dbc,name,description,flags,parent);
  }
 catch (  Exception e) {
    dbc.report(null,"Error creating group " + name,e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
