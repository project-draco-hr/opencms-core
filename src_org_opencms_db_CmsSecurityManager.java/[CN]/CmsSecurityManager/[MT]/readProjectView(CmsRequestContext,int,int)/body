{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  List result=null;
  try {
    result=m_driverManager.readProjectView(dbc,projectId,state);
  }
 catch (  Exception e) {
    dbc.report(null,Messages.get().container(Messages.ERR_READ_PROJECT_VIEW_2,String.valueOf(projectId),org.opencms.workflow.Messages.toTaskTypeString(state,context)),e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
