{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  CmsTask result=null;
  try {
    result=m_driverManager.createTask(dbc,currentUser,projectid,agentName,roleName,taskName,taskComment,taskType,timeout,priority);
  }
 catch (  Exception e) {
    dbc.report(null,"",e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
