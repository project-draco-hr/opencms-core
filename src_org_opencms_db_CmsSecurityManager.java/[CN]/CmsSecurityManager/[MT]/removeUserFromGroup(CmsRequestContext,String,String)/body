{
  if (!isAdmin(context)) {
    throw new CmsSecurityException("[" + this.getClass().getName() + "] removeUserFromGroup()",CmsSecurityException.C_SECURITY_ADMIN_PRIVILEGES_REQUIRED);
  }
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  try {
    m_driverManager.removeUserFromGroup(dbc,username,groupname);
  }
 catch (  Exception e) {
    dbc.report(null,"Error removing user " + username + " from group "+ groupname,e);
  }
 finally {
    dbc.clear();
  }
}
