{
  CmsDbContext dbc=m_dbContextFactory.getDbContext(context);
  readResource(dbc,foldername,CmsResourceFilter.ALL);
  int result=0;
  try {
    result=m_driverManager.countLockedResources(dbc,foldername);
  }
 catch (  Exception e) {
    dbc.report(null,null,e);
  }
 finally {
    dbc.clear();
  }
  return result;
}
