{
  RootPanel parentPanel=RootPanel.get(parentElementId);
  GalleryMode mode=I_CmsGalleryProviderConstants.GalleryMode.valueOf(galleryMode);
  CmsBinaryPreviewDialog previewDialog=new CmsBinaryPreviewDialog(mode,parentPanel.getOffsetHeight(),parentPanel.getOffsetWidth());
  m_controller=new CmsBinaryPreviewController(new CmsBinaryPreviewHandler(previewDialog,this));
  parentPanel.add(previewDialog);
  m_controller.loadResourceInfo(resourcePath);
}
