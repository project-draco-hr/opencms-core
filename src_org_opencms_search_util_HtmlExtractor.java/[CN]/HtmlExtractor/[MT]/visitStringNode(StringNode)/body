{
  if (!m_isScript) {
    String text=string.getText();
    if (!m_isPre) {
      text=Translate.decode(text);
      if (getReplaceNonBreakingSpaces()) {
        text=text.replace('\u00a0',' ');
      }
      if (getCollapse()) {
        collapse(m_buffer,text);
      }
 else {
        m_buffer.append(text);
      }
    }
 else {
      m_buffer.append(text);
    }
  }
}
