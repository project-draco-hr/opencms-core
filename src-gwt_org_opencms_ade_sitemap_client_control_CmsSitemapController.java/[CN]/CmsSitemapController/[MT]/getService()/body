{
  if (m_service == null) {
    m_service=GWT.create(I_CmsSitemapService.class);
    String serviceUrl=CmsStringUtil.joinPaths(CmsCoreProvider.get().getContext(),"org.opencms.ade.sitemap.CmsVfsSitemapService.gwt");
    ((ServiceDefTarget)m_service).setServiceEntryPoint(serviceUrl);
  }
  return m_service;
}
