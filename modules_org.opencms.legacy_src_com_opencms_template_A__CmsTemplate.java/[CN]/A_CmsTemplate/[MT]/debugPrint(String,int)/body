{
  System.err.print("* " + s);
  for (int j=0; j < (15 - s.length()); j++) {
    System.err.print(" ");
  }
  System.err.print(" " + ((i & 1) == 1 ? "X" : " ") + "  ");
  System.err.print(" " + ((i & 2) == 2 ? "X" : " ") + "  ");
  System.err.print(" " + ((i & 4) == 4 ? "X" : " ") + "  ");
