{
  if (m_throwable == null) {
    m_throwable=throwable;
    m_throwableResourceUri=resource;
  }
 else {
    if (OpenCms.getLog(this).isDebugEnabled()) {
      OpenCms.getLog(this).debug("Ignored additional exception" + ((resource != null) ? " on resource " + resource : ""),throwable);
    }
  }
  return m_throwable;
}
