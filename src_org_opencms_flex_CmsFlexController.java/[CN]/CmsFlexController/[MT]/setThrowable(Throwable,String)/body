{
  if (m_throwable == null) {
    m_throwable=throwable;
    m_throwableResourceUri=resource;
  }
 else {
    if (LOG.isDebugEnabled()) {
      LOG.debug(Messages.get().key(Messages.LOG_FLEXCONTROLLER_IGNORED_EXCEPTION_1,((resource != null) ? "on resource " + resource : "")));
    }
  }
  return m_throwable;
}
