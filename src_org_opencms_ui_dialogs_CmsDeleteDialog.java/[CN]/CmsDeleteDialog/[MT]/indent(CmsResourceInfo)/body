{
  boolean simple=false;
  if (simple) {
    return resourceInfo;
  }
 else {
    HorizontalLayout hl=new HorizontalLayout();
    Label label=new Label("");
    label.setWidth("35px");
    hl.addComponent(label);
    hl.addComponent(resourceInfo);
    hl.setExpandRatio(resourceInfo,1.0f);
    hl.setWidth("100%");
    return hl;
  }
}
