{
  CmsButtonCell<CmsHistoryResourceBean> replaceCell=new CmsButtonCell<CmsHistoryResourceBean>(CmsHistoryMessages.titleRevert(),I_CmsImageBundle.INSTANCE.style().tableReplaceIcon(),new ActionCell.Delegate<CmsHistoryResourceBean>(){
    @SuppressWarnings("synthetic-access") public void execute(    CmsHistoryResourceBean historyRes){
      m_handler.revert(historyRes);
    }
  }
);
  addColumn(CmsHistoryMessages.columnReplace(),30,new IdentityColumn<CmsHistoryResourceBean>(replaceCell));
}
