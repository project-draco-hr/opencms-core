{
  List result=new ArrayList();
  for (int i=0; i < m_folderList.size(); i++) {
    CmsResource res=(CmsResource)m_folderList.get(i);
    if (res.getState() == I_CmsConstants.C_STATE_DELETED) {
      result.add(res);
    }
  }
  Collections.sort(result,Collections.reverseOrder());
  return result;
}
