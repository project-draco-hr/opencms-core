{
  try {
    InputStream in=new ByteArrayInputStream(getFileBytes(C_EXPORT_XMLFILENAME));
    try {
      m_docXml=A_CmsXmlContent.getXmlParser().parse(in);
    }
  finally {
      try {
        in.close();
      }
 catch (      Exception e) {
      }
    }
  }
 catch (  Exception exc) {
    m_report.println(exc);
    throw new CmsException(CmsException.C_UNKNOWN_EXCEPTION,exc);
  }
}
