{
  if (!isVisible()) {
    return "";
  }
  String onClic="listSearchAction('" + getListId() + "', '"+ getId()+ "', '"+ CmsStringUtil.escapeJavaScript(wp.resolveMacros(getConfirmationMessage().key(wp.getLocale())))+ "');";
  return A_CmsHtmlIconButton.defaultButtonHtml(wp.getJsp(),CmsHtmlIconButtonStyleEnum.SMALL_ICON_TEXT,getId(),getName().key(wp.getLocale()),getHelpText().key(wp.getLocale()),isEnabled(),getIconPath(),null,onClic);
}
