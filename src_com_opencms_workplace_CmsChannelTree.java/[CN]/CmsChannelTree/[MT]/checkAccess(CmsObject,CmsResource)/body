{
  boolean access=false;
  if (res.getState() == C_STATE_DELETED) {
    return false;
  }
  int accessflags=res.getAccessFlags();
  boolean groupAccess=false;
  Enumeration allGroups=cms.getGroupsOfUser(cms.getRequestContext().currentUser().getName()).elements();
  while ((!groupAccess) && allGroups.hasMoreElements()) {
    groupAccess=cms.readGroup(res).equals((CmsGroup)allGroups.nextElement());
  }
  if (((accessflags & C_ACCESS_PUBLIC_VISIBLE) > 0) || (cms.readOwner(res).equals(cms.getRequestContext().currentUser()) && (accessflags & C_PERMISSION_VIEW) > 0) || (groupAccess && (accessflags & C_ACCESS_GROUP_VISIBLE) > 0)|| (cms.getRequestContext().isAdmin())) {
    access=true;
  }
  return access;
}
