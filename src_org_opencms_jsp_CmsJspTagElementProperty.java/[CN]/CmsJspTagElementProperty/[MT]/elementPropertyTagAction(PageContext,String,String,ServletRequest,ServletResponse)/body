{
  String propValue="";
  CmsContainerElementBean element=null;
  try {
    element=OpenCms.getADEManager().getCurrentElement(req);
  }
 catch (  CmsException e) {
    if (!LOG.isDebugEnabled()) {
      LOG.warn(e.getLocalizedMessage());
    }
    LOG.debug(e.getLocalizedMessage(),e);
    return;
  }
  if (!CmsStringUtil.isEmptyOrWhitespaceOnly(defaultValue)) {
    propValue=defaultValue;
  }
  if (element != null) {
    CmsObject cms=CmsFlexController.getCmsObject(req);
    Map<String,CmsProperty> properties=OpenCms.getADEManager().getElementProperties(cms,element);
    if (properties.containsKey(propertyName)) {
      CmsProperty property=properties.get(propertyName);
      if (property != null) {
        propValue=property.getValue(defaultValue);
      }
    }
  }
  pageContext.getOut().print(propValue);
}
