{
  super();
  if (items.containsKey(NO_SELECTION_TEXT)) {
    m_noSelectionText=items.get(NO_SELECTION_TEXT);
    items.remove(NO_SELECTION_TEXT);
  }
  if (addNullOption) {
    String text=Messages.get().key(Messages.GUI_SELECTBOX_EMPTY_SELECTION_0);
    items.put("",text);
  }
  setItems(items);
  if (addNullOption) {
    selectValue("");
  }
}
