{
  String message=null;
switch (variant) {
case applet:
    message=key(Messages.GUI_PREF_USE_UPLOAD_APPLET_0);
  break;
case basic:
message=key(Messages.GUI_PREF_USE_UPLOAD_BASIC_0);
break;
case gwt:
message=key(Messages.GUI_PREF_USE_UPLOAD_GWT_0);
break;
default :
message=key(Messages.ERR_PREF_UPLOAD_VARIANT_NOT_FOUND_0);
break;
}
return message;
}
