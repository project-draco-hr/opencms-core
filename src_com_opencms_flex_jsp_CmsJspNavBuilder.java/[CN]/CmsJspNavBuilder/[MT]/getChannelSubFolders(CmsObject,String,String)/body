{
  String channel=null;
  if (subChannel == null) {
    subChannel="";
  }
 else   if (subChannel.startsWith("/")) {
    subChannel=subChannel.substring(1);
  }
  if (parentChannel == null)   parentChannel="";
  if (parentChannel.endsWith("/")) {
    channel=parentChannel + subChannel;
  }
 else {
    channel=parentChannel + "/" + subChannel;
  }
  return getChannelSubFolders(cms,channel);
}
