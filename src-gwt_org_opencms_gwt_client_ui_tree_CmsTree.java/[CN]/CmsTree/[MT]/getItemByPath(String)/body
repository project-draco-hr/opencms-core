{
  String[] names=CmsStringUtil.splitAsArray(path,"/");
  I result=null;
  for (  String name : names) {
    if (CmsStringUtil.isEmptyOrWhitespaceOnly(name)) {
      continue;
    }
    if (result != null) {
      result=(I)result.getChild(name);
    }
 else {
      result=getItem(name);
    }
    if (result == null) {
      break;
    }
  }
  return result;
}
