{
  if (active) {
    String mem="";
    if (memory) {
      mem=getMem();
    }
    this.logQueue.put(new LogRecord(channel,mem + message,null));
    if (this.logQueue.size() > queue_maxsize) {
      this.logQueue.put(new LogRecord(CH_QUEUE_STATUS,"Log queue size limit exceeded",null));
      flush();
    }
  }
}
