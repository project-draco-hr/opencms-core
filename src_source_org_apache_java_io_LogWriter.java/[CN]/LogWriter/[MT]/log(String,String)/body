{
  if (m_active) {
    String mem="";
    if (m_memory) {
      mem=getMem();
    }
    this.m_logQueue.put(new LogRecord(channel,mem + message,null));
    if (this.m_logQueue.size() > m_queueMaxsize) {
      this.m_logQueue.put(new LogRecord(CH_QUEUE_STATUS,"Log queue size limit exceeded",null));
      flush();
    }
  }
}
