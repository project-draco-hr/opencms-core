{
  if (m_resource == null) {
    if (getGalleryItem().getResource() != null) {
      m_resource=getGalleryItem().getResource();
    }
 else     if (!CmsStringUtil.isEmptyOrWhitespaceOnly(getGalleryItem().getPath())) {
      m_resource=getCmsObject().readResource(getGalleryItem().getPath());
    }
  }
  return m_resource;
}
