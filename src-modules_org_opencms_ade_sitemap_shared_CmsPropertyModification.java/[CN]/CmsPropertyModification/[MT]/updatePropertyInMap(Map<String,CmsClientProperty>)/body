{
  CmsClientProperty prop=props.get(getName());
  if (prop == null) {
    prop=new CmsClientProperty(getName(),"","");
    props.put(getName(),prop);
  }
  if (isStructureValue()) {
    prop.setStructureValue(getValue());
  }
 else {
    prop.setResourceValue(getValue());
    prop.setStructureValue(getValue());
  }
}
