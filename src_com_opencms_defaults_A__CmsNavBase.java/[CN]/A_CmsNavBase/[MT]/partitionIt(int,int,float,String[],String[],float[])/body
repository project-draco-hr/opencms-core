{
  int leftPtr=left - 1;
  int rightPtr=right;
  while (true) {
    while (navPos[++leftPtr] < pivot)     ;
    while (rightPtr > 0 && navPos[--rightPtr] > pivot)     ;
    if (leftPtr >= rightPtr) {
      break;
    }
 else {
      swap(leftPtr,rightPtr,navLink,navText,navPos);
    }
  }
  swap(leftPtr,right,navLink,navText,navPos);
  return leftPtr;
}
