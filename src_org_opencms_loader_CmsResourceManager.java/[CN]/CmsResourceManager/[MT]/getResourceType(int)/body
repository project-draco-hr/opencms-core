{
  I_CmsResourceType result=null;
  if (typeId < m_configuration.m_resourceTypes.length) {
    result=m_configuration.m_resourceTypes[typeId];
  }
  if (result == null) {
    throw new CmsLoaderException(Messages.get().container(Messages.ERR_UNKNOWN_RESTYPE_ID_REQ_1,new Integer(typeId)));
  }
  return result;
}
