{
  for (int i=0; i < m_resourceTypeList.size(); i++) {
    I_CmsResourceType type=(I_CmsResourceType)m_resourceTypeList.get(i);
    if (type.getTypeName().equals(typeName)) {
      return type;
    }
  }
  throw new CmsException("[" + this.getClass().getName() + "] Unknown resource type name requested: "+ typeName,CmsException.C_NOT_FOUND);
}
