{
  Set<String> subItems=new HashSet<String>();
  Iterator<Widget> it=m_subContainer.iterator();
  while (it.hasNext()) {
    Widget w=it.next();
    if (w instanceof CmsContainerPageElement) {
      subItems.add(((CmsContainerPageElement)w).getId());
    }
  }
  subItems.add(m_subContainer.getId());
  return subItems;
}
