{
  List<Element> itemWidget=CmsDomUtil.getElementsByClass(org.opencms.gwt.client.ui.css.I_CmsLayoutBundle.INSTANCE.listItemWidgetCss().itemContainer(),element);
  if ((itemWidget != null) && (itemWidget.size() > 0)) {
    itemWidget.get(0).addClassName(I_CmsLayoutBundle.INSTANCE.sitemapItemCss().contentHide());
  }
}
