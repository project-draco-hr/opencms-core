{
  CmsListInfoBean infoBean=new CmsListInfoBean(galleryType.getNiceName(),Messages.get().key(Messages.GUI_GALLERY_ROOT_SUBTITLE_0),null);
  CmsListItemWidget result=new CmsListItemWidget(infoBean);
  result.setIcon(CmsIconUtil.getResourceIconClasses(galleryType.getTypeName(),null,false));
  return result;
}
