{
  CmsLock lock=cmsObject.getLock(cmsObject.getSitePath(cmsResource));
  if ((lock != null) && lock.isOwnedBy(cmsObject.getRequestContext().currentUser()) && lock.isOwnedInProjectBy(cmsObject.getRequestContext().currentUser(),cmsObject.getRequestContext().currentProject())) {
    return true;
  }
 else   if ((lock != null) && !lock.isUnlocked() && !lock.isOwnedBy(cmsObject.getRequestContext().currentUser())) {
    m_lockedFiles+=1;
    return false;
  }
 else   if ((lock != null) && !lock.isUnlocked() && lock.isOwnedBy(cmsObject.getRequestContext().currentUser())&& !lock.isOwnedInProjectBy(cmsObject.getRequestContext().currentUser(),cmsObject.getRequestContext().currentProject())) {
    cmsObject.changeLock(cmsObject.getSitePath(cmsResource));
  }
 else   if ((lock != null) && lock.isUnlocked()) {
    cmsObject.lockResource(cmsObject.getSitePath(cmsResource));
  }
  lock=cmsObject.getLock(cmsObject.getSitePath(cmsResource));
  if ((lock != null) && lock.isOwnedBy(cmsObject.getRequestContext().currentUser()) && !lock.isOwnedInProjectBy(cmsObject.getRequestContext().currentUser(),cmsObject.getRequestContext().currentProject())) {
    m_lockedFiles+=1;
    return false;
  }
  return true;
}
