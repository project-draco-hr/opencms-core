{
  String fileName=CmsResource.getName(cms.getRequestContext().getUri());
  if (fileName == null) {
    return TYPE_UNLOCK;
  }
 else   if (fileName.equals("lock.html")) {
    return TYPE_LOCK;
  }
 else   if (fileName.endsWith("lockchange.html")) {
    return TYPE_LOCKCHANGE;
  }
 else {
    return TYPE_UNLOCK;
  }
}
