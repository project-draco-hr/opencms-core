{
  try {
    if (element == null) {
      element="";
    }
    StringBuffer partKey=new StringBuffer(32);
    partKey.append(target);
    partKey.append("_");
    partKey.append(element);
    partKey.append("_");
    partKey.append(layout);
    partKey.append("_");
    partKey.append(locale);
    partKey.append("_");
    partKey.append(project);
    return partKey.toString();
  }
 catch (  Exception e) {
    return KEY_ILLEGAL;
  }
}
