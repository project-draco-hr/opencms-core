{
  String folderName=propertyInheritedFrom(propName,propVal);
  if (CmsStringUtil.isNotEmpty(folderName)) {
    return new StringBuffer(key("label.inherit")).append(' ').append(propVal).append(' ').append(key("label.from")).append(' ').append(folderName).toString();
  }
 else {
    return "not set";
  }
}
