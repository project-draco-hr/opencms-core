{
  String propVal=null;
  try {
    propVal=getCms().readPropertyObject(getParamResource(),propertyName,false).getValue();
  }
 catch (  CmsException e) {
    if (OpenCms.getLog(this).isInfoEnabled()) {
      OpenCms.getLog(this).info(e);
    }
  }
  if (CmsStringUtil.isEmpty(propVal)) {
    propVal="";
  }
  return propVal;
}
