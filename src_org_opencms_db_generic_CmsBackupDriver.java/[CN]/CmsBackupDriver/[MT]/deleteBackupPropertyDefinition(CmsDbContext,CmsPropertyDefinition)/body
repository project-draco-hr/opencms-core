{
  Connection conn=null;
  PreparedStatement stmt=null;
  try {
    if (internalCountProperties(dbc,metadef,I_CmsConstants.C_PROJECT_ONLINE_ID) != 0 || internalCountProperties(dbc,metadef,Integer.MAX_VALUE) != 0) {
      throw new CmsDbConsistencyException(Messages.get().container(Messages.ERR_ERROR_DELETING_PROPERTYDEF_1,metadef.getName()));
    }
    conn=m_sqlManager.getConnection(dbc);
    stmt=m_sqlManager.getPreparedStatement(conn,"C_PROPERTYDEF_DELETE_BACKUP");
    stmt.setString(1,metadef.getId().toString());
    stmt.executeUpdate();
  }
 catch (  SQLException exc) {
    throw new CmsDbSqlException(this,stmt,exc);
  }
 finally {
    m_sqlManager.closeAll(dbc,conn,stmt,null);
  }
}
