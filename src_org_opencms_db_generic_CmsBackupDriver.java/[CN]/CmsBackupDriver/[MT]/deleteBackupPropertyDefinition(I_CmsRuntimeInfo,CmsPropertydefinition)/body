{
  Connection conn=null;
  PreparedStatement stmt=null;
  try {
    if (internalCountProperties(runtimeInfo,metadef,I_CmsConstants.C_PROJECT_ONLINE_ID) != 0 || internalCountProperties(runtimeInfo,metadef,Integer.MAX_VALUE) != 0) {
      throw new CmsException("[" + this.getClass().getName() + "] "+ metadef.getName()+ "could not be deleted because property is attached to resources",CmsException.C_UNKNOWN_EXCEPTION);
    }
    conn=m_sqlManager.getConnection(runtimeInfo);
    stmt=m_sqlManager.getPreparedStatement(conn,"C_PROPERTYDEF_DELETE_BACKUP");
    stmt.setString(1,metadef.getId().toString());
    stmt.executeUpdate();
    m_sqlManager.closeAll(null,conn,stmt,null);
  }
 catch (  SQLException exc) {
    throw m_sqlManager.getCmsException(this,null,CmsException.C_SQL_ERROR,exc,false);
  }
 finally {
    m_sqlManager.closeAll(runtimeInfo,conn,stmt,null);
  }
}
