{
  if (user.isManaged() || OpenCms.getDefaultUsers().isDefaultUser(user.getName()) || OpenCms.getRoleManager().hasRole(cms,user.getName(),CmsRole.ROOT_ADMIN)) {
    return false;
  }
  String lastPasswordChangeStr=(String)user.getAdditionalInfo().get(CmsUserSettings.ADDITIONAL_INFO_LAST_PASSWORD_CHANGE);
  if (lastPasswordChangeStr == null) {
    return false;
  }
  long lastPasswordChange=Long.valueOf(lastPasswordChangeStr).longValue();
  if ((System.currentTimeMillis() - lastPasswordChange) > getPasswordChangeInterval()) {
    return true;
  }
  return false;
}
