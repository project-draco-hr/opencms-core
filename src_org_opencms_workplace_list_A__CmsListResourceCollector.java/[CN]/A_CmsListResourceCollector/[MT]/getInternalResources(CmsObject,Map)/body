{
synchronized (this) {
    if (m_resources == null) {
      m_resources=getResources(cms,params);
      Iterator it=m_resources.iterator();
      while (it.hasNext()) {
        CmsResource resource=(CmsResource)it.next();
        m_resCache.put(resource.getStructureId().toString(),resource);
      }
    }
  }
  return m_resources;
}
