{
  ZipFile zipFile=new ZipFile(in);
  try {
    ZipEntry entry=zipFile.getEntry(name);
    if (entry != null) {
      byte[] data=getBytesFromStream(zipFile.getInputStream(entry),(int)entry.getSize());
      if (data != null) {
        InputStream newStream=new ByteArrayInputStream(data);
        return newStream;
      }
    }
  }
  finally {
    zipFile.close();
  }
  return null;
}
