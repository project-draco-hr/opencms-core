{
  String fileName=CmsResource.getName(cms.getRequestContext().getUri());
  if (fileName == null) {
    return TYPE_UNLOCK;
  }
 else   if (fileName.equalsIgnoreCase("lock.jsp")) {
    return TYPE_LOCK;
  }
 else   if (fileName.indexOf("change") != -1) {
    return TYPE_LOCKCHANGE;
  }
 else {
    return TYPE_UNLOCK;
  }
}
