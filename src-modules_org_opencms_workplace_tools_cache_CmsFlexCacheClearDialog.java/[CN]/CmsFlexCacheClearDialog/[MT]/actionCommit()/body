{
  try {
    int action=-1;
    if (isOnline() && isOffline()) {
      if (getMode().equals(MODE_ALL)) {
        action=CmsFlexCache.CLEAR_ALL;
      }
 else {
        action=CmsFlexCache.CLEAR_ENTRIES;
      }
    }
 else     if (isOnline()) {
      if (getMode().equals(MODE_ALL)) {
        action=CmsFlexCache.CLEAR_ONLINE_ALL;
      }
 else {
        action=CmsFlexCache.CLEAR_ONLINE_ENTRIES;
      }
    }
 else     if (isOffline()) {
      if (getMode().equals(MODE_ALL)) {
        action=CmsFlexCache.CLEAR_OFFLINE_ALL;
      }
 else {
        action=CmsFlexCache.CLEAR_OFFLINE_ENTRIES;
      }
    }
 else {
      if (getMode().equals(MODE_ALL)) {
        action=CmsFlexCache.CLEAR_ALL;
      }
 else {
        action=CmsFlexCache.CLEAR_ENTRIES;
      }
    }
    OpenCms.fireCmsEvent(new CmsEvent(I_CmsEventListener.EVENT_FLEX_CACHE_CLEAR,Collections.singletonMap("action",new Integer(action))));
  }
 catch (  Exception e) {
    setCommitErrors(Collections.singletonList(e));
  }
}
