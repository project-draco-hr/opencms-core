def getCurrentFolder(currentFolder):
    if (not currentFolder):
        currentFolder = '/'
    if (currentFolder[(-1)] != '/'):
        currentFolder += '/'
    if (currentFolder[0] != '/'):
        currentFolder = ('/' + currentFolder)
    while ('//' in currentFolder):
        currentFolder = currentFolder.replace('//', '/')
    if ('..' in currentFolder):
        return None
    return currentFolder
