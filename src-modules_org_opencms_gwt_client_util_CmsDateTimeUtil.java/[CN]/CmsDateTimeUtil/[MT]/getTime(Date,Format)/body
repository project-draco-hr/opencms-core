{
  DateTimeFormat df;
switch (format) {
case FULL:
    df=DateTimeFormat.getFullTimeFormat();
  break;
case LONG:
df=DateTimeFormat.getLongTimeFormat();
break;
case MEDIUM:
df=DateTimeFormat.getMediumTimeFormat();
break;
case SHORT:
df=DateTimeFormat.getShortTimeFormat();
break;
default :
return "";
}
return df.format(date);
}
