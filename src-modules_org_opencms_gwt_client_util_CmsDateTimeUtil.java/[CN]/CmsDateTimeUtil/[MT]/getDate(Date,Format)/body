{
  DateTimeFormat df;
switch (format) {
case FULL:
    df=DateTimeFormat.getFullDateFormat();
  break;
case LONG:
df=DateTimeFormat.getLongDateFormat();
break;
case MEDIUM:
df=DateTimeFormat.getMediumDateFormat();
break;
case SHORT:
df=DateTimeFormat.getShortDateFormat();
break;
default :
return "";
}
return df.format(date);
}
