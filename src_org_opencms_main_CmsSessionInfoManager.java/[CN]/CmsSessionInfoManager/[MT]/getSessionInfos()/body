{
  List result=new ArrayList();
synchronized (m_sessions) {
    Iterator i=getConcurrentSessionIterator();
    while (i.hasNext()) {
      CmsSessionInfo sessionInfo=(CmsSessionInfo)m_sessions.get(i.next());
      if (sessionInfo != null) {
        result.add(sessionInfo);
      }
    }
  }
  return result;
}
