{
  boolean resourceExists;
  try {
    driverManager.getVfsDriver().readResource(dbc,dbc.currentProject().getId(),resourceName,false);
    resourceExists=true;
  }
 catch (  CmsVfsResourceNotFoundException e) {
    resourceExists=false;
  }
  if (resourceExists) {
    throw new CmsLockException(Messages.get().container(Messages.ERR_REMOVING_UNDELETED_RESOURCE_1,dbc.getRequestContext().removeSiteRoot(resourceName)));
  }
  m_exclusiveLocks.remove(resourceName);
}
