{
  String contextPath=OpenCms.getSystemInfo().getOpenCmsContext();
  String vfsPath="/system/modules/org.opencms.ade.editprovider/resources/resources.nocache.js";
  StringBuffer buffer=new StringBuffer();
  buffer.append(String.format("<script type=\"text/javascript\" src=\"%1$s\"></script>",CmsStringUtil.joinPaths(contextPath,vfsPath)));
  CmsPublishActionElement actionElement=new CmsPublishActionElement(context,(HttpServletRequest)context.getRequest(),(HttpServletResponse)context.getResponse());
  String exportedData=actionElement.exportAll();
  buffer.append(exportedData);
  return buffer.toString();
}
