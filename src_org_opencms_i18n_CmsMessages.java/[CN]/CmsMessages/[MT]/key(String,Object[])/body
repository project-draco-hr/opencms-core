{
  if ((params == null) || (params.length == 0)) {
    return key(keyName);
  }
  String result=key(keyName,true);
  if (result == null) {
    result=formatUnknownKey(keyName);
  }
 else {
    MessageFormat formatter=new MessageFormat(result,m_locale);
    result=formatter.format(params);
  }
  return result;
}
