{
  if (m_resourceBundle != null) {
    try {
      return m_resourceBundle.getString(keyName);
    }
 catch (    MissingResourceException e) {
      throw new CmsMessageException(Messages.get().container(Messages.ERR_CANT_FIND_RESOURCE_FOR_BUNDLE_2,keyName,m_baseName));
    }
  }
 else {
    throw new CmsMessageException(Messages.get().container(Messages.ERR_MESSAGE_BUNDLE_NOT_INITIALIZED_1,m_baseName));
  }
}
