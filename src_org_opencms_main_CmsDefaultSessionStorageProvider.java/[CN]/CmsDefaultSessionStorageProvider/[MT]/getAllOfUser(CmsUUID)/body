{
  List userSessions=new ArrayList();
  Iterator i=m_sessions.keySet().iterator();
  while (i.hasNext()) {
    CmsSessionInfo sessionInfo=(CmsSessionInfo)m_sessions.get(i.next());
    if (userId.equals(sessionInfo.getUserId())) {
      userSessions.add(sessionInfo);
    }
  }
  return userSessions;
}
