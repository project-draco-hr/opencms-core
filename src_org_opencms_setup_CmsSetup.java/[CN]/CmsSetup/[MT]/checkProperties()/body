{
  if (getProperties() == null) {
    return false;
  }
  String size=getFileMaxUploadSize();
  if (size == null || "".equals(size)) {
    setFileMaxUploadSize("-1");
  }
 else {
    try {
      Integer.parseInt(size);
    }
 catch (    Exception e) {
      setFileMaxUploadSize("-1");
    }
  }
  return true;
}
