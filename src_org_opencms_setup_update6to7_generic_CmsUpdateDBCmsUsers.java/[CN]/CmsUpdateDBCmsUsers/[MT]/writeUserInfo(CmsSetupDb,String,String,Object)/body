{
  String query=readQuery(QUERY_INSERT_CMS_USERDATA);
  try {
    List params=new ArrayList();
    params.add(id);
    params.add(key);
    params.add(value);
    params.add(value.getClass().getName());
    dbCon.updateSqlStatement(query,null,params);
  }
 catch (  SQLException e) {
    e.printStackTrace();
  }
}
