{
  I_CmsLauncher launcher=getLoaderManager().getLauncher(file.getLauncherType());
  if ((launcher instanceof CmsDumpLoader) || (launcher instanceof CmsJspLoader)) {
    I_CmsResourceLoader loader=(I_CmsResourceLoader)launcher;
    try {
      loader.load(cms,file,req,res);
    }
 catch (    IOException ioe) {
      throw new CmsException("Could not load file " + file.getResourceName(),ioe);
    }
catch (    ServletException se) {
      throw new CmsException("Could not load file " + file.getResourceName(),se);
    }
  }
 else {
    showResource(cms,file);
  }
}
