{
  CmsObject cms=m_repository.getCmsObject(context);
  RelationKey rk=parseRelationKey(objectId);
  rk.fillRelation(cms);
  Set<String> filterSet=CmsCmisUtil.splitFilter(filter);
  ObjectData result=collectObjectData(context,cms,rk.getSource(),rk.getRelation(),filterSet,(includeAllowableActions.booleanValue()),(includeAcl.booleanValue()),objectInfos);
  return result;
}
