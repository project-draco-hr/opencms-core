{
  if (value == null) {
    return;
  }
  String mode=getExplorerMode();
  if (mode == null) {
    mode="explorerview";
  }
  if ("explorerview".equals(mode)) {
    mode+="_" + getSite() + "/";
  }
  if (value.startsWith(CmsResource.VFS_FOLDER_SYSTEM + "/") && (!value.startsWith(m_currentSite)) && (!"galleryview".equals(getExplorerMode()))) {
    m_explorerResource.put(mode,"/");
  }
 else {
    m_explorerResource.put(mode,value);
  }
}
