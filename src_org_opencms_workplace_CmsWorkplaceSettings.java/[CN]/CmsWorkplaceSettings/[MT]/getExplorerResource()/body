{
  String mode=getExplorerMode();
  if (mode == null) {
    mode=CmsExplorer.VIEW_EXPLORER;
  }
  if (CmsExplorer.VIEW_EXPLORER.equals(mode)) {
    mode+="_" + getSite() + "/";
  }
  String resource=(String)m_explorerResource.get(mode);
  if (resource == null) {
    resource="/";
  }
  return resource;
}
