{
  String jsShow;
  String jsHide;
  if (widgetDialog.useNewStyle()) {
    jsShow="sMH";
    jsHide="hMH";
  }
 else {
    jsShow="showHelp";
    jsHide="hideHelp";
  }
  StringBuffer result=new StringBuffer(128);
  result.append(" onmouseover=\"");
  result.append(jsShow);
  result.append("('");
  result.append(key);
  result.append("');\" onmouseout=\"");
  result.append(jsHide);
  result.append("('");
  result.append(key);
  result.append("');\"");
  return result.toString();
}
