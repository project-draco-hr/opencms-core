{
  m_widget=((ComponentConnector)target).getWidget();
  m_widget.addDomHandler(new KeyUpHandler(){
    public void onKeyUp(    KeyUpEvent event){
      handle();
    }
  }
,KeyUpEvent.getType());
  m_widget.addDomHandler(new FocusHandler(){
    public void onFocus(    FocusEvent event){
      handle();
    }
  }
,FocusEvent.getType());
  handle();
}
