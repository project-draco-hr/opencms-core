{
  try {
    I_CmsRegistry reg=m_cms.getRegistry();
    int ver=Integer.parseInt(version);
    long date=Long.parseLong(createDate);
    reg.createModule(modulename,niceModulename,description,author,type,new HashMap(),date,ver);
  }
 catch (  Exception exc) {
