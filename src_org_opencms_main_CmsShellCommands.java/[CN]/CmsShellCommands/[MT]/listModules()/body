{
  Set modules=OpenCms.getModuleManager().getModuleNames();
  System.out.println("\n" + Messages.get().key(m_shell.getLocale(),Messages.GUI_SHELL_LIST_MODULES_1,new Object[]{new Integer(modules.size())}));
  Iterator i=modules.iterator();
  while (i.hasNext()) {
    String moduleName=(String)i.next();
    System.out.println(moduleName);
  }
  System.out.println();
}
