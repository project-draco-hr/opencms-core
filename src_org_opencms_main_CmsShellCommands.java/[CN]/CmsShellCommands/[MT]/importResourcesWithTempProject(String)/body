{
  CmsProject project=m_cms.createProject("SystemUpdate",Messages.get().key(m_shell.getLocale(),Messages.GUI_SHELL_IMPORT_TEMP_PROJECT_NAME_0,null),OpenCms.getDefaultUsers().getGroupAdministrators(),OpenCms.getDefaultUsers().getGroupAdministrators(),CmsProject.PROJECT_TYPE_TEMPORARY);
  int id=project.getId();
  m_cms.getRequestContext().setCurrentProject(project);
  m_cms.copyResourceToProject("/");
  OpenCms.getImportExportManager().importData(m_cms,importFile,"/",new CmsShellReport(m_cms.getRequestContext().getLocale()));
  m_cms.unlockProject(id);
  m_cms.publishProject();
}
