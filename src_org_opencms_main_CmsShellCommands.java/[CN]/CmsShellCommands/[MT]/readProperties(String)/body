{
  try {
    Map properties=m_cms.readProperties(resource);
    Iterator i=properties.keySet().iterator();
    Object key;
    while (i.hasNext()) {
      key=i.next();
      System.out.print(key + "=");
      System.out.println(properties.get(key));
    }
  }
 catch (  Exception exc) {
    CmsShell.printException(exc);
  }
}
