{
  Locale locale=CmsLocaleManager.getLocale(localeName);
  System.out.println(Messages.get().key(m_shell.getLocale(),Messages.GUI_SHELL_SETLOCALE_2,new Object[]{locale.toString(),m_cms.getRequestContext().currentUser().getName()}));
  CmsUserSettings settings=m_shell.getSettings();
  settings.setLocale(locale);
  settings.save(m_cms);
  System.out.println(Messages.get().key(m_shell.getLocale(),Messages.GUI_SHELL_SETLOCALE_POST_1,new Object[]{locale.toString()}));
}
