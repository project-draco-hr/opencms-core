{
  super(listId,id,name,helpText,icon,true,confirmationMessage);
  m_columnId=columnId;
  m_defaultShowAllAction=new CmsListIndependentAction(listId,SHOWALL_ACTION_ID){
    /** 
 * @see org.opencms.workplace.tools.I_CmsHtmlIconButton#buttonHtml(CmsWorkplace)
 */
    public String buttonHtml(    CmsWorkplace wp){
      String onClic=getListId() + "ListSearchAction('" + getId()+ "', '"+ CmsStringUtil.escapeJavaScript(wp.resolveMacros(getConfirmationMessage().key(wp.getLocale())))+ "');";
      return A_CmsHtmlIconButton.defaultButtonHtml(getId(),getName().key(wp.getLocale()),getHelpText().key(wp.getLocale()),isEnabled(),getIconPath(),onClic);
    }
  }
;
  m_defaultShowAllAction.setName(new CmsMessageContainer(Messages.get(),Messages.GUI_LIST_ACTION_SHOWALL_NAME_0));
  m_defaultShowAllAction.setHelpText(new CmsMessageContainer(Messages.get(),Messages.GUI_LIST_ACTION_SHOWALL_HELP_0));
  m_defaultShowAllAction.setIconPath("list/showall.gif");
  m_defaultShowAllAction.setEnabled(true);
  m_defaultShowAllAction.setConfirmationMessage(new CmsMessageContainer(Messages.get(),Messages.GUI_LIST_ACTION_SHOWALL_CONF_0));
}
