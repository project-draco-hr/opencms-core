{
  if (m_currentTarget != null) {
    elementDropAction();
    m_currentTarget=null;
  }
 else {
    elementCancelAction();
  }
  restoreElementAfterDrag();
  Document.get().getBody().removeClassName(I_CmsLayoutBundle.INSTANCE.dragdropCss().dragStarted());
  m_dragElement=null;
  m_placeholder=null;
  m_targets=null;
  m_currentTarget=null;
}
