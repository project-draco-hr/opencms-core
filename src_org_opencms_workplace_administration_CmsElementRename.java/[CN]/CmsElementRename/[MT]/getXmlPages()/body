{
  boolean isRecursive=Boolean.valueOf(getParamRecursive()).booleanValue();
  CmsResourceFilter filter=CmsResourceFilter.ONLY_VISIBLE_NO_DELETED.addRequireType(CmsResourceTypeXmlPage.C_RESOURCE_TYPE_ID);
  List xmlPages=null;
  try {
    xmlPages=getCms().readResources(getParamResource(),filter,isRecursive);
  }
 catch (  CmsException e) {
    if (OpenCms.getLog(this).isErrorEnabled()) {
      OpenCms.getLog(this).error(e);
    }
  }
  return xmlPages;
}
