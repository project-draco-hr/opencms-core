{
  Map result=new TreeMap();
  if (m_priority > 0) {
    result.put(PRIORITY,new Integer(m_priority));
  }
  if (!m_createExcerpt) {
    result.put(EXCERPT,new Boolean(m_createExcerpt));
  }
  if (m_dontCheckPermissions) {
    result.put(PERMISSIONS,new Boolean(!m_dontCheckPermissions));
  }
  return result;
}
