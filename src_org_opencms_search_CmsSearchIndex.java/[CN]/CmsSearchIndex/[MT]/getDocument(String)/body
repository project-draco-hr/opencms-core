{
  Document result=null;
  IndexSearcher searcher=getSearcher();
  if (searcher != null) {
    Term pathTerm=new Term(CmsSearchField.FIELD_PATH,rootPath);
    try {
      Hits hits=searcher.search(new TermQuery(pathTerm));
      if (hits.length() > 0) {
        result=hits.doc(0);
      }
    }
 catch (    IOException e) {
    }
  }
  return result;
}
