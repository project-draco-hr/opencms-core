{
  if (!m_indexExists) {
    return;
  }
  indexSearcherClose();
  try {
    IndexReader reader=new LazyContentReader(IndexReader.open(m_path));
    m_searcher=new IndexSearcher(reader);
  }
 catch (  IOException e) {
    LOG.error(Messages.get().getBundle().key(Messages.ERR_INDEX_SEARCHER_1,getName()),e);
  }
}
