{
  if (m_collector == null) {
    m_collector=new CmsSearchResourcesCollector(this,getSearchParams().getQuery(),getSearchParams().getSortOrder(),getSearchParams().getFields(),Collections.singletonList(getSearchParams().getSearchPath()));
    CmsResourceUtil resUtil=getResourceUtil();
    resUtil.setAbbrevLength(50);
    resUtil.setSiteMode(CmsResourceUtil.SITE_MODE_MATCHING);
  }
  return m_collector;
}
