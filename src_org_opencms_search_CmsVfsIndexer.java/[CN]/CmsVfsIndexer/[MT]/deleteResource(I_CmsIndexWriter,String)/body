{
  try {
    if (LOG.isInfoEnabled()) {
      LOG.info(Messages.get().getBundle().key(Messages.LOG_DELETING_FROM_INDEX_1,rootPath));
    }
    indexWriter.deleteDocuments(rootPath);
  }
 catch (  IOException e) {
    if (LOG.isWarnEnabled()) {
      LOG.warn(Messages.get().getBundle().key(Messages.LOG_IO_INDEX_DOCUMENT_DELETE_2,rootPath,m_index.getName()),e);
    }
  }
}
