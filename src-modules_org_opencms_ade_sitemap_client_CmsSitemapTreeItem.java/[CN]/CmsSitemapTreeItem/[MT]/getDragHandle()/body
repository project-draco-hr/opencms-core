{
  if (!m_dndEnabled) {
    return null;
  }
  CmsSitemapHoverbar hoverbar=getHoverbar();
  if (hoverbar == null) {
    return null;
  }
  for (  Widget button : hoverbar) {
    if (!(button instanceof CmsHoverbarMoveButton)) {
      continue;
    }
    if (!((CmsHoverbarMoveButton)button).isEnabled()) {
      return null;
    }
    return button.getElement();
  }
  return null;
}
