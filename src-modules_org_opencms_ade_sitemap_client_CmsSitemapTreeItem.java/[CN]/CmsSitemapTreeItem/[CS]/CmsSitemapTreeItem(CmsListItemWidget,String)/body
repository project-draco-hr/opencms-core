{
  super(widget);
  String name=CmsResource.getName(sitePath);
  if (name.endsWith("/")) {
    name=name.substring(0,name.length() - 1);
  }
  setId(name);
  m_decoratedPanel.addDecorationBoxStyle(CSS.sitemapEntryDecoration());
  m_listItemWidget=widget;
  m_originalPath=sitePath;
  m_sitePath=sitePath;
}
