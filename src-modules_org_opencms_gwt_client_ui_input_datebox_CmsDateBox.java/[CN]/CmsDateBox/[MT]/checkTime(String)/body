{
  if (!CmsDateConverter.validateTime(time)) {
    m_timeErr.setText(Messages.get().key(Messages.ERR_DATEBOX_INVALID_TIME_FORMAT_1,CmsDateConverter.cutSuffix(time)));
    updateCloseBehavior(false);
    m_popup.sync(true);
  }
 else {
    m_timeErr.setText(null);
    updateCloseBehavior(true);
    m_popup.sync(true);
  }
}
