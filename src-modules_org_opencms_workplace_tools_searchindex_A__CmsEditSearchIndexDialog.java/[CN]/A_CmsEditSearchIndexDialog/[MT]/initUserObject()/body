{
  try {
    m_searchIndex=m_searchManager.getIndex(getParamIndexName());
    if (m_searchIndex == null) {
      m_searchIndex=createDummySearchIndex();
    }
  }
 catch (  Exception e) {
    m_searchIndex=createDummySearchIndex();
  }
  if (m_searchIndex instanceof CmsSearchIndex) {
    m_index=(CmsSearchIndex)m_searchIndex;
  }
}
