{
  String values="";
  for (int i=0; i < m_selectBox.getItemCount(); i++) {
    if (m_selectBox.isItemSelected(i)) {
      values+=m_selectBox.getItemText(i) + ",";
    }
  }
  if (values.length() > 0) {
    values=values.substring(0,values.lastIndexOf(","));
  }
  ValueChangeEvent.fire(this,values);
}
