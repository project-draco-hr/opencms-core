{
  float navPos=navs.get(insertPosition).getNavPosition();
  if (insertPosition > 0) {
    assertTrue(navs.get(insertPosition - 1).getNavPosition() < navPos);
  }
  if (insertPosition < (navs.size() - 1)) {
    assertTrue(navs.get(insertPosition + 1).getNavPosition() > navPos);
  }
  assertTrue(checkNonDecreasing(navs));
}
