{
  storeEventPos(event);
  if (!m_dragging) {
    return;
  }
  if ((m_statusHandler != null) && !m_statusHandler.canDropNow(m_currentTarget,m_dragElement)) {
    cancelDragging();
    return;
  }
switch (event.getNativeButton()) {
case NativeEvent.BUTTON_LEFT:
    stopDragging(event);
  break;
default :
cancelDragging();
}
}
