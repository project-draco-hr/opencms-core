{
  if (isChoiceHandler()) {
    Entity choice=m_entity.getAttribute(Type.CHOICE_ATTRIBUTE_NAME).getComplexValues().get(valueIndex);
    if (choice != null) {
      for (      String option : getAttributeType().getAttributeNames()) {
        if (choice.hasAttribute(option)) {
          return option;
        }
      }
    }
  }
  return null;
}
