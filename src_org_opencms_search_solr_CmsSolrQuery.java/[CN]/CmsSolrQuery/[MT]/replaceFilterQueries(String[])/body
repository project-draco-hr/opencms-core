{
  for (  String fq : fqs) {
    int idx=fq.indexOf(':');
    if (idx != -1) {
      String fieldName=fq.substring(0,idx);
      if (getFilterQueries() != null) {
        for (        String sfq : getFilterQueries()) {
          if (sfq.startsWith(fieldName + ":")) {
            removeFilterQuery(sfq);
          }
        }
      }
    }
  }
  addFilterQuery(fqs);
}
