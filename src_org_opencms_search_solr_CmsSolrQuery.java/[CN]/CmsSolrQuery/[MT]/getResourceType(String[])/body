{
  String ret=null;
  int count=0;
  if (fqs != null) {
    for (    String fq : fqs) {
      if (fq.startsWith(CmsSearchField.FIELD_TYPE + ":")) {
        String val=fq.substring((CmsSearchField.FIELD_TYPE + ":").length());
        if (OpenCms.getResourceManager().hasResourceType(val)) {
          count++;
          ret=val;
        }
      }
    }
  }
  return (count == 1) ? ret : null;
}
