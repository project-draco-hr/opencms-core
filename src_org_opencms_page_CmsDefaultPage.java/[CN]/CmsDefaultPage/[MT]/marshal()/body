{
  try {
    ByteArrayOutputStream out=new ByteArrayOutputStream();
    XMLWriter writer=new XMLWriter(out);
    writer.write(m_document);
    writer.close();
    setContents(out.toByteArray());
  }
 catch (  Exception exc) {
    throw new CmsPageException("Marshalling xml page failed",exc);
  }
  return this;
}
