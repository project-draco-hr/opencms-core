{
  CmsXmlContentProperty prop=pref.getPropertyDefinition();
  for (  String[] attrToSet : new String[][]{{I_CmsXmlConfiguration.A_VALUE,pref.getDefaultValue()},{CmsWorkplaceConfiguration.A_TAB,pref.getTab()},{CmsWorkplaceConfiguration.A_NICE_NAME,prop.getNiceName()},{CmsWorkplaceConfiguration.A_DESCRIPTION,prop.getDescription()},{CmsWorkplaceConfiguration.A_WIDGET,prop.getWidget()},{CmsWorkplaceConfiguration.A_WIDGET_CONFIG,prop.getWidgetConfiguration()},{CmsWorkplaceConfiguration.A_RULE_REGEX,prop.getRuleRegex()},{CmsWorkplaceConfiguration.A_RULE_TYPE,prop.getRuleType()},{CmsWorkplaceConfiguration.A_ERROR,prop.getError()}}) {
    String attrName=attrToSet[0];
    String value=attrToSet[1];
    if (value != null) {
      elem.addAttribute(attrName,value);
    }
  }
}
