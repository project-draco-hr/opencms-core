{
  if (!isNewJob()) {
    if (CmsStringUtil.isNotEmptyOrWhitespaceOnly(getParamJobid())) {
      Iterator it=OpenCms.getScheduleManager().getJobs().iterator();
      while (it.hasNext()) {
        CmsScheduledJobInfo job=(CmsScheduledJobInfo)it.next();
        if (job.getId().equals(getParamJobid())) {
          return;
        }
      }
    }
    throw new Exception();
  }
}
