{
  String content=null;
  I_CmsFormatterBean formatter=getFormatterForContainer(element,container,configs,allowNested);
  if (formatter != null) {
    element.initSettings(m_cms,formatter);
    try {
      content=getElementContent(element,m_cms.readResource(formatter.getJspStructureId()),container);
    }
 catch (    Exception e) {
      LOG.error(e.getLocalizedMessage(),e);
    }
    if (content != null) {
      content=removeScriptTags(content);
    }
  }
  return content;
}
