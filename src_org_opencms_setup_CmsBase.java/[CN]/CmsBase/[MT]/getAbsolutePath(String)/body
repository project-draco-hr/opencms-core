{
  if (s == null) {
    return null;
  }
  File f=new File(s);
  if (!f.isAbsolute()) {
    if (OpenCms.getSystemInfo().getBasePath() == null) {
      return null;
    }
 else {
      return (OpenCms.getSystemInfo().getBasePath() + s).replace(m_replaceSep,File.separatorChar);
    }
  }
 else {
    return s.replace(m_replaceSep,File.separatorChar);
  }
}
