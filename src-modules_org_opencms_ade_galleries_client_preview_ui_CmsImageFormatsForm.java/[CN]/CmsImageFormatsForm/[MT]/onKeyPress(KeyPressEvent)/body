{
  char key=event.getCharCode();
  if (((key >= '0') && (key <= '9')) || (key == KeyCodes.KEY_BACKSPACE) || (key == KeyCodes.KEY_DELETE)|| (key == KeyCodes.KEY_TAB)|| (key == KeyCodes.KEY_LEFT)|| (key == KeyCodes.KEY_RIGHT)|| (key == KeyCodes.KEY_ENTER)) {
    return;
  }
  event.stopPropagation();
  event.preventDefault();
}
