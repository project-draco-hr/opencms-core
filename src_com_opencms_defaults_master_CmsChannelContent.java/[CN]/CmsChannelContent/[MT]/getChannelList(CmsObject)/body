{
  Vector content=new Vector();
  cms.getRequestContext().saveSiteRoot();
  cms.setContextToCos();
  try {
    getAllResources(cms,"/",content);
  }
 catch (  CmsException e) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MODULE).isWarnEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MODULE).warn("[CmsChannelContent]: error when reading subfolders of cos root: " + e.getMessage());
    }
  }
 finally {
    cms.getRequestContext().restoreSiteRoot();
  }
  return content;
}
