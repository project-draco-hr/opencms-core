{
  cms.getRequestContext().saveSiteRoot();
  cms.setContextToCos();
  try {
    cms.undeleteResource(cms.readAbsolutePath(m_channel));
  }
 catch (  CmsException exc) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isInfoEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).info("[CmsChannelContent] Could not undelete channel " + cms.readAbsolutePath(m_channel));
    }
  }
 finally {
    cms.getRequestContext().restoreSiteRoot();
  }
}
