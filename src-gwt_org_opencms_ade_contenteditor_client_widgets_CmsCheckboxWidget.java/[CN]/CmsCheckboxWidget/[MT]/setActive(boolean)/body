{
  Element parent=CmsDomUtil.getAncestor(getElement(),I_LayoutBundle.INSTANCE.form().attributeValue());
  if (parent != null) {
    parent.addClassName(I_LayoutBundle.INSTANCE.form().shallowWidget());
  }
  if (m_active == active) {
    return;
  }
  m_active=active;
  if (active) {
    fireChangeEvent();
  }
  m_checkbox.setEnabled(active);
}
