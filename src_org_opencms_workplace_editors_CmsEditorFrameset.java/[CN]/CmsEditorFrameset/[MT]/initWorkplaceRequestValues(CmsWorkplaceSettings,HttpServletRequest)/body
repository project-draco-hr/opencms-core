{
  fillParamValues(request);
  if (getDialogUri().endsWith("editor.jsp")) {
    if (!checkResourcePermissions(CmsPermissionSet.ACCESS_WRITE,true)) {
      try {
        actionClose();
      }
 catch (      Exception e) {
        if (LOG.isInfoEnabled()) {
          LOG.info(e);
        }
      }
    }
  }
}
