{
  try {
    performSaveContent(getParamOldbodyname(),getParamOldbodylanguage());
  }
 catch (  CmsException e) {
    try {
      showErrorPage(this,e,"save");
    }
 catch (    JspException exc) {
    }
  }
  if (!getParamBodylanguage().equals(getParamOldbodylanguage())) {
    initBodyElementName();
  }
  initContent();
}
