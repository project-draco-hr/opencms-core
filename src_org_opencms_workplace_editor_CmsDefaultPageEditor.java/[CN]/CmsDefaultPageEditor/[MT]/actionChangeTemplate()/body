{
  try {
    switchToTempProject();
    getCms().writeProperty(getParamTempfile(),I_CmsConstants.C_PROPERTY_TEMPLATE,getParamPagetemplate());
    switchToCurrentProject();
  }
 catch (  CmsException e) {
    try {
      showErrorPage(this,e,"save");
    }
 catch (    JspException exc) {
    }
  }
}
