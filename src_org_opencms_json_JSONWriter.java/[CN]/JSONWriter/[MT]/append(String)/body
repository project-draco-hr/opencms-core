{
  if (s == null) {
    throw new JSONException("Null pointer");
  }
  if ((this.m_mode == 'o') || (this.m_mode == 'a')) {
    try {
      if (this.m_comma && (this.m_mode == 'a')) {
        this.m_writer.write(',');
      }
      this.m_writer.write(s);
    }
 catch (    IOException e) {
      throw new JSONException(e);
    }
    if (this.m_mode == 'o') {
      this.m_mode='k';
    }
    this.m_comma=true;
    return this;
  }
  throw new JSONException("Value out of sequence.");
}
