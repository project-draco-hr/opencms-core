{
  if (s == null) {
    throw new JSONException("Null key.");
  }
  if (this.m_mode == 'k') {
    try {
      if (this.m_comma) {
        this.m_writer.write(',');
      }
      this.m_writer.write(JSONObject.quote(s));
      this.m_writer.write(':');
      this.m_comma=false;
      this.m_mode='o';
      return this;
    }
 catch (    IOException e) {
      throw new JSONException(e);
    }
  }
  throw new JSONException("Misplaced key.");
}
