{
  CmsObject cms=getCmsObject();
  try {
    CmsResource resource=cms.readResource(structureId);
    tryUnlock(resource);
  }
 catch (  CmsException e) {
    return e.getLocalizedMessage(OpenCms.getWorkplaceManager().getWorkplaceLocale(cms));
  }
catch (  Throwable e) {
    error(e);
  }
  return null;
}
