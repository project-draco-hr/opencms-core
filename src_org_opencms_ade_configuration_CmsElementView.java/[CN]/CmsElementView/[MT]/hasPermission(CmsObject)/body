{
  try {
    if (m_resource != null) {
      return cms.hasPermissions(m_resource,CmsPermissionSet.ACCESS_READ);
    }
 else {
      return OpenCms.getRoleManager().hasRole(cms,CmsRole.ELEMENT_AUTHOR);
    }
  }
 catch (  CmsException e) {
    LOG.error(e.getLocalizedMessage(),e);
  }
  return false;
}
