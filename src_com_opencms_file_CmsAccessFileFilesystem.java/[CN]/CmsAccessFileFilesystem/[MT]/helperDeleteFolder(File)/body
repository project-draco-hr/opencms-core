{
  String[] fileList=folder.list();
  File currentFile;
  for (int i=0; i < fileList.length; i++) {
    currentFile=new File(folder.getAbsolutePath() + m_fileseperator + fileList[i]);
    if (currentFile.isDirectory()) {
      helperDeleteFolder(currentFile);
    }
 else {
      currentFile.delete();
    }
  }
  folder.delete();
}
