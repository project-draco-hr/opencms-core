{
  int pos=filename.indexOf(m_mountpoint.getMountpoint());
  int len=m_mountpoint.getMountpoint().length();
  String path=null;
  String name=null;
  if (pos != -1) {
    name=filename.substring(pos + len);
  }
 else {
    name=filename;
  }
  path=m_mountpoint.getMountPath() + name;
  path=path.replace('/',m_fileseperator);
  return path;
}
