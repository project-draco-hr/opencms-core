{
  File projectFolder=new File(m_mountpoint.getMountPath() + C_INV_PROJECTS + C_INV_CHAR+ project.getId());
  if ((projectFolder != null) && (projectFolder.exists())) {
    helperDeleteFolder(projectFolder);
    File projectFile=new File(m_mountpoint.getMountPath() + C_INV_PROJECTS + C_INV_CHAR+ C_INV_CHAR+ C_INV_CHAR+ project.getId());
    projectFile.delete();
  }
}
