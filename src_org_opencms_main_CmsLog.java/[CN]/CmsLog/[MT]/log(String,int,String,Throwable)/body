{
  Log log=getLogger(channel);
  if (log == null) {
    return;
  }
switch (level) {
case LEVEL_TRACE:
    log.trace(message,throwable);
  break;
case LEVEL_DEBUG:
log.debug(message,throwable);
break;
case LEVEL_INFO:
log.info(message,throwable);
break;
case LEVEL_WARN:
log.warn(message,throwable);
break;
case LEVEL_ERROR:
log.error(message,throwable);
break;
case LEVEL_FATAL:
default :
log.fatal(message,throwable);
}
}
