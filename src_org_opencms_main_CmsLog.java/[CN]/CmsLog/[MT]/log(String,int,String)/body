{
  Log log=getLogger(channel);
  if (log == null) {
    return;
  }
switch (level) {
case LEVEL_TRACE:
    log.trace(message);
  break;
case LEVEL_DEBUG:
log.debug(message);
break;
case LEVEL_INFO:
log.info(message);
break;
case LEVEL_WARN:
log.warn(message);
break;
case LEVEL_ERROR:
log.error(message);
break;
case LEVEL_FATAL:
default :
log.fatal(message);
}
}
