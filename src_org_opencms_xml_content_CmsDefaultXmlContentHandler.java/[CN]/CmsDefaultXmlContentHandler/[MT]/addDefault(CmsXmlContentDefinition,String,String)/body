{
  if (contentDefinition.getSchemaType(elementName) == null) {
    throw new CmsXmlException(org.opencms.xml.types.Messages.get().container(org.opencms.xml.types.Messages.ERR_XMLCONTENT_INVALID_ELEM_DEFAULT_1,elementName));
  }
  String xpath=CmsXmlUtils.createXpath(elementName,1);
  m_defaultValues.put(xpath,defaultValue);
}
