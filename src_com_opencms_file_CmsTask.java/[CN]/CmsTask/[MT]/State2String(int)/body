{
  String result=null;
switch (state) {
case C_TASK_STATE_PREPARE:
{
      result="Prepared";
      break;
    }
case C_TASK_STATE_START:
{
    result="Start";
    break;
  }
case C_TASK_STATE_STARTED:
{
  result="Started";
  break;
}
case C_TASK_STATE_NOTENDED:
{
result="Not Ended";
break;
}
case C_TASK_STATE_ENDED:
{
result="Ended";
break;
}
case C_TASK_STATE_HALTED:
{
result="Halted";
break;
}
case C_TASK_STATE_MAIL:
{
result="Mail";
break;
}
case C_TASK_STATE_ONHOLD:
{
result="On Hold";
break;
}
default :
result="Unkown";
}
return result;
}
