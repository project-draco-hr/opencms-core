{
  if (m_exceptionTrhown) {
    return;
  }
  m_bytesRead=done;
  m_contentLength=total;
  m_item=item;
  if (isCanceled()) {
    String eName=m_exception.getClass().getName();
    LOG.info(Messages.get().getBundle().key(Messages.INFO_UPLOAD_USER_CANCELED_3,getId(),new Long(m_bytesRead),eName));
    m_exceptionTrhown=true;
    throw m_exception;
  }
  if ((CmsUploadBean.DEFAULT_SLOW_DELAY_MILLIS > 0) && (done < total)) {
    try {
      Thread.sleep(CmsUploadBean.DEFAULT_SLOW_DELAY_MILLIS);
    }
 catch (    Exception e) {
      m_exception=new RuntimeException(e);
    }
  }
  if (getPercent() >= 100) {
    stopWatcher();
  }
}
