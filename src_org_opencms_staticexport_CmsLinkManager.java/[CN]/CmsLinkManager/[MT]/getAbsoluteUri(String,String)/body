{
  if ((relativeUri == null) || (relativeUri.charAt(0) == '/')) {
    return relativeUri;
  }
  try {
    URL url=new URL(new URL(m_baseUrl,baseUri),relativeUri);
    if (url.getQuery() == null) {
      return url.getPath();
    }
 else {
      return url.getPath() + "?" + url.getQuery();
    }
  }
 catch (  MalformedURLException e) {
    return relativeUri;
  }
}
