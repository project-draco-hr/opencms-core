{
  JSONArray infoValue=new JSONArray();
  try {
    String siteRoot=cms.getRequestContext().getSiteRoot();
    Set<CmsResource> resources=getPublishResources().getResources();
    for (    CmsRelation relation : cms.getRelationsForResource(resource,CmsRelationFilter.SOURCES.filterStrong().filterIncludeChildren())) {
      CmsResource source=null;
      try {
        source=relation.getSource(cms,CmsResourceFilter.ALL);
      }
 catch (      CmsException e) {
        if (LOG.isErrorEnabled()) {
          LOG.error(e.getLocalizedMessage(),e);
        }
        continue;
      }
      if (resources.contains(source)) {
        infoValue.put(getResourceName(cms,source.getRootPath(),siteRoot) + "&nbsp;<span style='color: #666666;'>(" + relation.getType().getLocalizedName(locale)+ ")</span>");
      }
    }
  }
 catch (  CmsException e) {
    if (LOG.isErrorEnabled()) {
      LOG.error(e.getLocalizedMessage(),e);
    }
  }
  return infoValue.length() > 0 ? infoValue : null;
}
