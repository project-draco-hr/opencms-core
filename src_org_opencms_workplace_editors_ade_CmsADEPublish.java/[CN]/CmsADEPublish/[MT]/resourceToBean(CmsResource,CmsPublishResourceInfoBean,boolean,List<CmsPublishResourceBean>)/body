{
  CmsResourceUtil resUtil=new CmsResourceUtil(m_cms,resource);
  CmsPublishResourceBean pubResource=new CmsPublishResourceBean(resource.getStructureId(),CmsStringUtil.formatResourceName(resUtil.getFullPath(),PATH_LENGTH),resUtil.getTitle(),CmsWorkplace.getResourceUri(resUtil.getIconPathExplorer()),"" + resUtil.getStateAbbreviation(),removable,info,related);
  return pubResource;
}
