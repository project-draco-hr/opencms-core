{
  if (m_cntPageId == 0) {
    try {
      m_cntPageId=OpenCms.getResourceManager().getResourceType(CmsResourceTypeXmlContainerPage.getStaticTypeName()).getTypeId();
    }
 catch (    CmsLoaderException e) {
      m_cntPageId=CmsResourceTypeXmlContainerPage.getStaticTypeId();
    }
  }
  return m_cntPageId;
}
