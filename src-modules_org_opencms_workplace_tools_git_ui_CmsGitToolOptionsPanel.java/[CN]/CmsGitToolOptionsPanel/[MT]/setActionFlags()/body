{
switch (m_mode) {
case checkIn:
    m_checkinBean.setResetHead(false);
  m_checkinBean.setResetRemoteHead(false);
break;
case resetHead:
m_checkinBean.setResetHead(true);
m_checkinBean.setResetRemoteHead(false);
break;
case resetRemoteHead:
m_checkinBean.setResetHead(false);
m_checkinBean.setResetRemoteHead(true);
break;
default :
break;
}
}
