{
switch (getAction()) {
case ACTION_DELETE:
    Iterator it=CmsStringUtil.splitAsList(getGroupName(),CmsHtmlList.ITEM_SEPARATOR,true).iterator();
  while (it.hasNext()) {
    String name=(String)it.next();
    try {
      getCms().deleteGroup(name);
    }
 catch (    CmsException e) {
      throw new CmsRuntimeException(e.getMessageContainer(),e);
    }
  }
setAction(ACTION_CANCEL);
actionCloseDialog();
break;
case ACTION_TRANSFER:
Map params=new HashMap();
params.put(CmsDialog.PARAM_ACTION,CmsDialog.DIALOG_INITIAL);
params.put(PARAM_GROUPID,getParamGroupid());
getToolManager().jspForwardPage(this,getJsp().getRequestContext().getFolderUri() + "group_transfer.jsp",params);
break;
default :
super.actionDialog();
}
}
