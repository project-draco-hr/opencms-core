{
  CacheItem item=head;
  Vector ret=new Vector();
  while (item != null) {
    try {
      if (((A_CmsElement)item.value).getCacheDirectives().shouldRenew()) {
        Vector actItem=new Vector();
        actItem.add(0,item.key);
        actItem.add(1,item.value);
        ret.add(actItem);
        removeItem(item);
      }
    }
 catch (    NullPointerException e) {
      Vector actItem=new Vector();
      actItem.add(0,item.key);
      actItem.add(1,item.value);
      ret.add(actItem);
      removeItem(item);
    }
    item=item.next;
  }
  return ret;
}
