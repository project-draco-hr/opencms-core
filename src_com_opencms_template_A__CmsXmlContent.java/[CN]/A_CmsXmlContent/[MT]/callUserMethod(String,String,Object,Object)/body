{
  Object[] params=new Object[]{m_cms,parameter,this,userObj};
  Object result=null;
  try {
    result=getUserMethod(methodName,callingObject).invoke(callingObject,params);
  }
 catch (  NoSuchMethodException exc) {
    handleException("User method " + methodName + " was not found in class "+ callingObject.getClass().getName()+ ".");
  }
catch (  InvocationTargetException targetEx) {
    Throwable e=targetEx.getTargetException();
    if (!(e instanceof CmsException)) {
      handleException("User method " + methodName + " throwed an exception. "+ e);
    }
 else {
      throw (CmsException)e;
    }
  }
catch (  Exception exc2) {
    handleException("User method " + methodName + " was found but could not be invoked. "+ exc2);
  }
  if (!(result instanceof String || result instanceof Integer || result instanceof NodeList|| result instanceof byte[])) {
    handleException("User method " + methodName + " in class "+ callingObject.getClass().getName()+ " returned an unsupported Object: "+ result.getClass().getName());
  }
  return (result);
}
