{
  try {
    A_CmsXmlContent newDoc=(A_CmsXmlContent)getClass().newInstance();
    newDoc.init(m_cms,(Document)m_content.cloneNode(true),m_filename);
    return newDoc;
  }
 catch (  Exception e) {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isErrorEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).error(getClassName() + "Error while trying to clone object.");
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).error(getClassName() + e);
    }
    throw new CloneNotSupportedException(e.toString());
  }
}
