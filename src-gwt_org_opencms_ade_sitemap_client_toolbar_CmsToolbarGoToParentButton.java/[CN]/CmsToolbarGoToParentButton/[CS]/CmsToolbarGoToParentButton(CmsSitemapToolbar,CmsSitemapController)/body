{
  setImageClass(I_CmsImageBundle.INSTANCE.buttonCss().hoverbarParent());
  setTitle(Messages.get().key(Messages.GUI_HOVERBAR_PARENT_0));
  setButtonStyle(ButtonStyle.IMAGE,null);
  addClickHandler(new ClickHandler(){
    /** 
 * @see com.google.gwt.event.dom.client.ClickHandler#onClick(com.google.gwt.event.dom.client.ClickEvent)
 */
    public void onClick(    ClickEvent event){
      toolbar.onButtonActivation(CmsToolbarGoToParentButton.this);
      String sitemapLocation=CmsCoreProvider.get().getUri() + "?path=" + controller.getData().getParentSitemap();
      controller.leaveEditor(sitemapLocation);
    }
  }
);
}
