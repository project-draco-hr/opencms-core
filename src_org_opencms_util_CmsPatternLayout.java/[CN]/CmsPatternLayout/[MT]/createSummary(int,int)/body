{
  StringBuffer result=new StringBuffer(128);
  result.append("\t... ");
  result.append(filtered + truncated);
  result.append(" more (");
  result.append(filtered);
  result.append(" filtered; ");
  result.append(truncated);
  result.append(" truncated)");
  result.append(Layout.LINE_SEP);
  return result.toString();
}
