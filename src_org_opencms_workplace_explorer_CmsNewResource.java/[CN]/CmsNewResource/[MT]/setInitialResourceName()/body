{
  if (isCreateIndexMode()) {
    String defaultFile="";
    try {
      defaultFile=(String)OpenCms.getDefaultFiles().get(0);
    }
 catch (    IndexOutOfBoundsException e) {
    }
    if (CmsStringUtil.isEmpty(defaultFile)) {
      defaultFile="index.html";
    }
    setParamResource(defaultFile);
  }
 else {
    setParamResource("");
  }
}
