{
  HttpServletRequest httpReq=CmsXmlTemplateLoader.getRequest(cms.getRequestContext()).getOriginalRequest();
  String queryString="";
  if (httpReq != null) {
    queryString=httpReq.getQueryString();
  }
  boolean result=isProxyPrivateCacheable(cms,templateFile,elementName,parameters,templateSelector) && (queryString == null || "".equals(queryString));
  try {
    CmsFile file=cms.readFile(templateFile);
    result=result && (file.getFlags() & CmsResource.FLAG_INTERNAL) != CmsResource.FLAG_INTERNAL;
  }
 catch (  Exception e) {
    result=false;
  }
  return result;
}
