{
  String datablockName=getTemplateDatablockName(sectionName);
  Element data=null;
  try {
    data=getData(datablockName);
  }
 catch (  Exception e) {
    if (OpenCms.isLogging(I_CmsLogChannels.C_OPENCMS_INFO)) {
      OpenCms.log(I_CmsLogChannels.C_OPENCMS_INFO,"Cannot set title for template section \"" + sectionName + "\" in file "+ getAbsoluteFilename()+ ". Section doesn't exist.");
    }
    return;
  }
  data.setAttribute("title",title);
}
