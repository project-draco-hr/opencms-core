{
  Element result=null;
  if (hasData("bodyTag")) {
    result=getData("bodytag");
  }
 else {
    if (OpenCms.isLogging(I_CmsLogChannels.C_OPENCMS_DEBUG)) {
      OpenCms.log(I_CmsLogChannels.C_OPENCMS_DEBUG,getClassName() + "Cannot find \"bodytag\" tag in XML template file " + getFilename()+ ".");
    }
  }
  return result;
}
