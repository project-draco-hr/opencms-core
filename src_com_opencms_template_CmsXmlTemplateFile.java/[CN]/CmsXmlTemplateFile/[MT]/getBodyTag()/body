{
  Element result=null;
  if (hasData("bodyTag")) {
    result=getData("bodytag");
  }
 else {
    if (OpenCms.getLog(CmsLog.CHANNEL_MAIN).isDebugEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_MAIN).debug(getClassName() + "Cannot find \"bodytag\" tag in XML template file " + getFilename()+ ".");
    }
  }
  return result;
}
