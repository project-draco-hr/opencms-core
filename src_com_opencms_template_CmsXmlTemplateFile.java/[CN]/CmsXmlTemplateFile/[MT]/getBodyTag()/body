{
  Element result=null;
  if (hasData("bodyTag")) {
    result=getData("bodytag");
  }
 else {
    if (OpenCms.getLog(CmsLog.CHANNEL_TEMPLATE_XML).isDebugEnabled()) {
      OpenCms.getLog(CmsLog.CHANNEL_TEMPLATE_XML).debug("Cannot find \"bodytag\" tag in XML template file " + getFilename());
    }
  }
  return result;
}
