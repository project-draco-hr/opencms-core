{
  long now=new Date().getTime();
  long created=res.getDateCreated();
  long diff=(now - created) / 1000;
  if (diff <= TIME_DELAY) {
    if (TMP_FILE_TABLE.containsKey(res.getRootPath())) {
      return true;
    }
    return false;
  }
 else {
    TMP_FILE_TABLE.remove(res.getRootPath());
  }
  return true;
}
