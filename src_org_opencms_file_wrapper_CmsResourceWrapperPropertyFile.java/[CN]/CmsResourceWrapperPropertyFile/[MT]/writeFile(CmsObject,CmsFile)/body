{
  CmsResource res=getResource(cms,cms.getRequestContext().removeSiteRoot(resource.getRootPath()),CmsResourceFilter.ALL);
  if (res != null) {
    CmsWrappedResource.writePropertyFile(cms,cms.getRequestContext().removeSiteRoot(res.getRootPath()),resource.getContents());
    return resource;
  }
  return null;
}
