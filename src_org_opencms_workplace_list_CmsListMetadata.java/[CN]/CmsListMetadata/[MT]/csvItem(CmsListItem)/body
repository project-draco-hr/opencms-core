{
  StringBuffer csv=new StringBuffer(1024);
  Iterator<CmsListColumnDefinition> itCols=m_columns.elementList().iterator();
  while (itCols.hasNext()) {
    CmsListColumnDefinition col=itCols.next();
    if (!col.isVisible()) {
      continue;
    }
    csv.append(col.csvCell(item));
    csv.append("\t");
  }
  csv.append("\n");
  return csv.toString();
}
