{
  String retValue=m_dataSet.m_userId + "";
  if (m_dataSet.m_userName == null || "".equals(m_dataSet.m_userName.trim())) {
    try {
      retValue=m_cms.readUser(m_dataSet.m_userId).getName();
    }
 catch (    CmsException exc) {
    }
  }
 else {
    retValue=m_dataSet.m_userName;
  }
  return retValue;
}
