{
  return new CmsResource(res.getStructureId(),res.getResourceId(),addFileExtension(cms,res.getRootPath()),res.getTypeId(),res.isFolder(),res.getFlags(),res.getProjectLastModified(),res.getState(),res.getDateCreated(),res.getUserCreated(),res.getDateLastModified(),res.getUserLastModified(),res.getDateReleased(),res.getDateExpired(),res.getSiblingCount(),res.getLength());
}
