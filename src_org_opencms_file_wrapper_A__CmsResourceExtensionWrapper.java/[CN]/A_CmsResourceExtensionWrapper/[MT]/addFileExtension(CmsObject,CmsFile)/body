{
  return new CmsFile(file.getStructureId(),file.getResourceId(),file.getContentId(),addFileExtension(cms,file.getRootPath()),file.getTypeId(),file.getFlags(),file.getProjectLastModified(),file.getState(),file.getDateCreated(),file.getUserCreated(),file.getDateLastModified(),file.getUserLastModified(),file.getDateReleased(),file.getDateExpired(),file.getSiblingCount(),file.getLength(),file.getContents());
}
