{
  A_CmsGroup group=null;
  try {
    m_statementGroupRead.setString(1,groupname);
    ResultSet res=m_statementGroupRead.executeQuery();
    if (res.next()) {
      group=new CmsGroup(res.getInt(C_GROUP_ID),res.getInt(C_PARENT_GROUP_ID),res.getString(C_GROUP_NAME),res.getString(C_GROUP_DESCRIPTION),res.getInt(C_GROUP_FLAGS));
    }
  }
 catch (  SQLException e) {
    throw new CmsException(e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  return group;
}
