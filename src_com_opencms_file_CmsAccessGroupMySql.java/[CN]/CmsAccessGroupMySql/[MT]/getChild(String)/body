{
  Vector childs=new Vector();
  A_CmsGroup group;
  A_CmsGroup parent;
  ResultSet res=null;
  try {
    parent=readGroup(groupname);
    if (parent != null) {
synchronized (m_statementGroupChilds) {
        m_statementGroupChilds.setInt(1,parent.getId());
        res=m_statementGroupChilds.executeQuery();
      }
      while (res.next()) {
        group=new CmsGroup(res.getInt(C_GROUP_ID),res.getInt(C_PARENT_GROUP_ID),res.getString(C_GROUP_NAME),res.getString(C_GROUP_DESCRIPTION),res.getInt(C_GROUP_FLAGS));
        childs.addElement(group);
      }
    }
  }
 catch (  SQLException e) {
    throw new CmsException("[CmsAccessGroupMySql/getChild(groupname)]:" + e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  if (childs.size() == 0) {
    childs=null;
  }
  return childs;
}
