{
  Vector childs=new Vector();
  CmsGroup group;
  CmsGroup parent;
  ResultSet res=null;
  Connection con=null;
  try {
    parent=readGroup(groupname);
    if (parent != null) {
      con=getConnection();
      PreparedStatement statementGroupChilds=con.prepareStatement(C_GROUP_CHILDS);
      statementGroupChilds.setInt(1,parent.getId());
      res=statementGroupChilds.executeQuery();
      putConnection(con);
      while (res.next()) {
        group=new CmsGroup(res.getInt(C_GROUP_ID),res.getInt(C_PARENT_GROUP_ID),res.getString(C_GROUP_NAME),res.getString(C_GROUP_DESCRIPTION),res.getInt(C_GROUP_FLAGS));
        childs.addElement(group);
      }
    }
  }
 catch (  SQLException e) {
    putConnection(con);
    throw new CmsException("[" + this.getClass().getName() + "] "+ e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  if (childs.size() == 0) {
    childs=null;
  }
  return childs;
}
