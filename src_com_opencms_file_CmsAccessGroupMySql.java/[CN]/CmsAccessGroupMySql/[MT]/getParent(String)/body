{
  A_CmsGroup parent=null;
  A_CmsGroup group=readGroup(groupname);
  Connection con=null;
  ResultSet res=null;
  try {
    con=getConnection();
    PreparedStatement statementGroupParent=con.prepareStatement(C_GROUP_PARENT);
    statementGroupParent.setInt(1,group.getParentId());
    res=statementGroupParent.executeQuery();
    putConnection(con);
    if (res.next()) {
      parent=new CmsGroup(res.getInt(C_GROUP_ID),res.getInt(C_PARENT_GROUP_ID),res.getString(C_GROUP_NAME),res.getString(C_GROUP_DESCRIPTION),res.getInt(C_GROUP_FLAGS));
    }
  }
 catch (  SQLException e) {
    putConnection(con);
    throw new CmsException("[" + this.getClass().getName() + "] "+ e.getMessage(),CmsException.C_SQL_ERROR,e);
  }
  return parent;
}
