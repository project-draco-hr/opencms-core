{
  if (m_tabInitialized) {
    m_tab.onContentChange();
  }
 else {
    String key=m_controller.getPreselectOption(m_controller.getStartSiteRoot(),m_controller.getVfsSiteSelectorOptions());
    m_tab.setSortSelectBoxValue(key);
    m_controller.loadVfsEntryBean(m_controller.getDefaultVfsTabSiteRoot(),new AsyncCallback<CmsVfsEntryBean>(){
      public void onFailure(      Throwable caught){
      }
      public void onSuccess(      CmsVfsEntryBean result){
        m_tab.fillInitially(Collections.singletonList(result));
        m_tabInitialized=true;
        m_tab.onContentChange();
      }
    }
);
  }
}
