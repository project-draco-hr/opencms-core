{
  List result=new ArrayList();
  Iterator i=getSystemSubNodes(node).iterator();
  while (i.hasNext()) {
    String classname=(String)i.next();
    try {
      result.add(Class.forName(classname).newInstance());
      if (OpenCms.isLogging(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN))       OpenCms.log(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN,". CmsSyncModification init : " + classname + " instanciated");
    }
 catch (    Exception e1) {
      if (OpenCms.isLogging(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN))       OpenCms.log(CmsLog.C_OPENCMS_INFO,CmsLog.LEVEL_WARN,". CmsSyncModification init : non-critical error " + e1.toString());
    }
  }
  return result;
}
