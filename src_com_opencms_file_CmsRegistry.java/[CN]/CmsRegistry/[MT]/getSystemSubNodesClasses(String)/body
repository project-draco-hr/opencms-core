{
  List result=new ArrayList();
  Iterator i=getSystemSubNodes(node).iterator();
  while (i.hasNext()) {
    String classname=(String)i.next();
    try {
      result.add(Class.forName(classname).newInstance());
      if (OpenCms.getLog(this).isInfoEnabled())       OpenCms.getLog(this).info(". CmsSyncModification init : " + classname + " instanciated");
    }
 catch (    Exception e1) {
      if (OpenCms.getLog(this).isWarnEnabled())       OpenCms.getLog(this).warn(". CmsSyncModification init : non-critical error" + e1.toString());
    }
  }
  return result;
}
