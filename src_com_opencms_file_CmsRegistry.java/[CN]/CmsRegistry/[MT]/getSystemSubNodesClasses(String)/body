{
  List result=new ArrayList();
  Iterator i=getSystemSubNodes(node).iterator();
  while (i.hasNext()) {
    String classname=(String)i.next();
    try {
      result.add(Class.forName(classname).newInstance());
      if (C_LOGGING && A_OpenCms.isLogging(C_OPENCMS_INFO))       A_OpenCms.log(C_OPENCMS_INFO,". CmsSyncModification init : " + classname + " instanciated");
    }
 catch (    Exception e1) {
      if (C_LOGGING && A_OpenCms.isLogging(C_OPENCMS_INFO))       A_OpenCms.log(C_OPENCMS_INFO,". CmsSyncModification init : non-critical error " + e1.toString());
    }
  }
  return result;
}
