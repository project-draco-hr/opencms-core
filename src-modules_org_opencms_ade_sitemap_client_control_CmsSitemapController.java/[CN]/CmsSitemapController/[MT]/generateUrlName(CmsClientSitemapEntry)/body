{
  Set<String> subUrlNames=new HashSet<String>();
  for (  CmsClientSitemapEntry subEntry : entry.getSubEntries()) {
    subUrlNames.add(subEntry.getName());
  }
  int counter=1;
  String prefix="item_";
  while (subUrlNames.contains(prefix + counter)) {
    counter+=1;
  }
  return prefix + counter;
}
