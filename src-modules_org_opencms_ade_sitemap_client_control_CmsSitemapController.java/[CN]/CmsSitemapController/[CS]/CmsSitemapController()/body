{
  m_undone=new ArrayList<I_CmsClientSitemapChange>();
  m_data=(CmsSitemapData)CmsRpcPrefetcher.getSerializedObject(getService(),CmsSitemapData.DICT_NAME);
  m_hiddenProperties=new HashSet<String>();
