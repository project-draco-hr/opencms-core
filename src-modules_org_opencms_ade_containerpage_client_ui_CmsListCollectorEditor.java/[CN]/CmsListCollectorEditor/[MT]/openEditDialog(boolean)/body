{
  if (isNew) {
    CmsContainerpageController.get().getContentEditorHandler().openDialog(m_parentResourceId,m_editableData.getSitePath() + "&amp;newlink=" + URL.encodeQueryString(m_editableData.getNewLink()),true);
  }
 else {
    CmsContainerpageController.get().getContentEditorHandler().openDialog(m_editableData.getStructureId(),m_editableData.getSitePath(),false,m_parentResourceId);
  }
}
