{
  Object source=event.getSource();
  if (source == m_delete) {
    removeHighlighting();
    openWarningDialog();
    CmsDomUtil.ensureMouseOut(m_delete.getElement());
    CmsDomUtil.ensureMouseOut(getElement());
  }
  if (source == m_edit) {
    openEditDialog(false);
    removeHighlighting();
  }
  if (source == m_new) {
    openEditDialog(true);
    removeHighlighting();
  }
}
