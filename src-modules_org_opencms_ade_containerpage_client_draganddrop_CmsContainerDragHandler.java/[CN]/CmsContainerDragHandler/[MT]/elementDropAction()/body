{
  if (((m_current != m_startInfo) || (Math.abs(m_currentTarget.getWidgetIndex((Widget)m_current.getDraggable()) - m_currentTarget.getWidgetIndex(m_current.getPlaceholder())) > 1)) && (m_current != m_dropZoneInfo)) {
    m_controller.addToRecentList(m_dragElement.getClientId());
    m_controller.setPageChanged();
  }
  m_currentTarget.insert((Widget)m_current.getDraggable(),m_currentTarget.getWidgetIndex(m_current.getPlaceholder()));
}
