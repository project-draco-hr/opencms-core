{
  return (!resourcename.startsWith(I_CmsWpConstants.C_VFS_PATH_SYSTEM) || resourcename.equalsIgnoreCase(I_CmsWpConstants.C_VFS_PATH_SYSTEM) || resourcename.startsWith(I_CmsWpConstants.C_VFS_PATH_BODIES)|| resourcename.startsWith(I_CmsWpConstants.C_VFS_PATH_GALLERIES)|| (m_includeSystem && resourcename.startsWith(I_CmsWpConstants.C_VFS_PATH_SYSTEM)));
}
