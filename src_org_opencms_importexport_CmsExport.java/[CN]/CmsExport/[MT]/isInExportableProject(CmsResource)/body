{
  boolean retValue=true;
  if (m_inProject) {
    if (res.getState() == CmsResource.STATE_CHANGED || res.getState() == CmsResource.STATE_NEW) {
      if (res.getProjectLastModified() != m_cms.getRequestContext().currentProject().getId()) {
        retValue=false;
      }
    }
 else {
      retValue=false;
    }
  }
  return retValue;
}
