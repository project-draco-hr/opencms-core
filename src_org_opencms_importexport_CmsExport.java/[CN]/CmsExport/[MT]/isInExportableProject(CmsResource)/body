{
  boolean retValue=true;
  if (m_inProject) {
    if ((res.getState() == CmsResource.STATE_CHANGED) || (res.getState() == CmsResource.STATE_NEW)) {
      if (!res.getProjectLastModified().equals(m_cms.getRequestContext().currentProject().getUuid())) {
        retValue=false;
      }
    }
 else {
      retValue=false;
    }
  }
  return retValue;
}
